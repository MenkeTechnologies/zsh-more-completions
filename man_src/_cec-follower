#compdef cec-follower

# Autogenerated from man page /usr/share/man/man1/cec-follower.1.gz

local arguments

arguments=(
    {-d,--device}'[Use device <dev> as the CEC device.]':dev
    {-D,--driver}'[Use a cec device that has driver name <drv>, as returned by the CEC_ADAP_G_CA…]':drv
    {-a,--adapter}'[Use a cec device that has adapter name <adap-name>, as returned by the CEC_AD…]':adap-name
    {-v,--verbose}'[Turn on verbose reporting.]'
    {-w,--wall-clock}'[Show timestamps as wall-clock time. This also turns on verbose reporting.]'
    {-T,--trace}'[Trace all called ioctls. Useful for debugging.]'
    '(- * :)'{-h,--help}'[Prints the help message.]'
    {-n,--no-warnings}'[Turn off warning messages.]'
    {-m,--show-msgs}'[Show received messages.]'
    {-s,--show-state}'[Show state changes from the emulated device.]'
    '*:filename:_files'
)
_arguments -s $arguments
