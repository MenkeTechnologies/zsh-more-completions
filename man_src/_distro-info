#compdef distro-info

# Autogenerated from man page /usr/share/man/man1/distro-info.1.gz

local arguments

arguments=(
    '--date[date for calculating the version (default: today).]:DATE'
    {-h,--help}'[display help message and exit.]'
    {-a,--all}'[list all known versions.]'
    {-y,--days}'[display number of days until specified version reaches the specified mileston…]':MILESTONE
    {-d,--devel}'[latest development version.]'
    '--series[series to calculate the version for.]:SERIES'
    {-s,--stable}'[latest stable version.]'
    '--supported[list of all supported stable versions.]'
    '--unsupported[list of all unsupported stable versions.]'
    {-c,--codename}'[print the codename (default).]'
    {-r,--release}'[print the release version.]'
    {-f,--fullname}'[print the full name SEE ALSO debian-distro-info (1), ubuntu-distro-info (1) A…]'
    '*:filename:_files'
)
_arguments -s $arguments
