#compdef plasmapkg

# Autogenerated from man page /usr/share/man/man1/plasmapkg.1.gz

local arguments

arguments=(
    {-h,--hash}'[Generate a SHA1 hash for the package at \\(lqpath\\(rq.]'
    {-g,--global}'[For install or remove, operates on packages installed for all users.]'
    {-t,--type}'[The type of package, e. g.]'
    {-i,--install}'[Install the package at \\(lqpath\\(rq.]'
    {-u,--upgrade}'[Upgrade the package at \\(lqpath\\(rq.]'
    {-l,--list}'[List installed packages.]'
    {-r,--remove}'[Remove the package named \\(lqname\\(rq.]'
    {-p,--packageroot}'[Absolute path to the package root.]'
    '*:filename:_files'
)
_arguments -s $arguments
