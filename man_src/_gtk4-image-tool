#compdef gtk4-image-tool

# Autogenerated from man page /opt/homebrew/share/man/man1/gtk4-image-tool.1

local arguments

arguments=(
    '--undecorated[.]'
    '--output[Save the differences as a png image in FILE.]:FILE'
    '--quiet[Don\t write results to stdout. Conversion.]'
    '--format[Convert to the given format.]:FORMAT'
    '--color-state[Convert to the given color state.]:COLORSTATE'
    '--cicp[Convert to a color state that is specified as a cicp tuple.]:CICP'
    '*:filename:_files'
)
