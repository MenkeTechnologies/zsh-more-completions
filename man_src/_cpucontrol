#compdef cpucontrol

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/cpucontrol.8.gz

local arguments

arguments=(
    '-d[Directory paths where to look for microcode images.]'
    '-n[Do not look for the microcode images in the standard directories.]'
    '-m[Show value of the specified MSR.]'
    '-i[Retrieve CPUID info. Level should be given as a hex number.]'
    '-u[Apply CPU firmware updates.]'
    '-e[Re-evaluate the kernel flags indicating the present CPU features.]'
    '-v[Increase the verbosity level.]'
    '-h[Show help message.]'
    '*:filename:_files'
)
_arguments -s $arguments
