#compdef vidcontrol

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/vidcontrol.1.gz

local arguments

arguments=(
    '-b[Set border color to color.]'
    '-C[Clear the history buffer.]'
    '-c[Change the cursor appearance.]'
    '-d[Print out current output screen map.]'
    '-g[Set the geometry of the text mode for the modes with selectable geometry.]'
    '-h[Set the size of the history (scrollback) buffer to size lines.]'
    '-i[Shows the active vty number.]'
    '-l[Install screen output map file from screen_map.]'
    '-L[Install default screen output map.]'
    '-M[Sets the base character used to render the mouse pointer to char.]'
    '-m[Switch the mouse pointer on or off.]'
    '-p[Capture the current contents of the video buffer corresponding to the termina…]'
    '-P[Same as p, but dump contents of the video buffer in a plain text format ignor…]'
    '-H[When used with p or P, it instructs vidcontrol to dump full history buffer in…]'
    '-r[Change reverse mode colors to foreground and background.]'
    '-S[Turn vty switching on or off.]'
    '-s[Set the active vty to number.]'
    '-T[Switch between xterm and cons25 style terminal emulation.]'
    '-t[Set the screensaver timeout to N seconds, or turns it off.]'
    '-x[Use hexadecimal digits for output.]'
    '*:filename:_files'
)
_arguments -s $arguments
