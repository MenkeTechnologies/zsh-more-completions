#compdef plocate

# Autogenerated from man page /usr/share/man/man1/plocate.1.gz

local arguments

arguments=(
    {-A,--all}'[Ignored for compatibility with mlocate (1).]'
    {-b,--basename}'[Match only against the file name portion of the path name, ie.]'
    {-c,--count}'[Do not print each match.]'
    {-d,--database}'[Find matches in the given database, instead of /var/lib/plocate/plocate. db.]':DBPATH
    {-e,--existing}'[Print only entries that refer to files existing at the time locate is run.]'
    {-i,--ignore-case}'[Do a case-insensitive match as given by the current locale (default is case-s…]'
    {-l,--limit}'[Stop searching after LIMIT matches have been found.]':LIMIT
    {-N,--literal}'[Print entry names without quoting.]'
    {-0,--null}'[Instead of writing a newline after every match, write a NUL (ASCII 0).]'
    {-r,--regexp}'[Patterns are taken to be POSIX basic regular expressions.]'
    '--regex[Like --regexp, but patterns are instead taken to be POSIX extended regular e…]'
    {-w,--wholename}'[Match against the entire path name.]'
    '--help[Print out usage information, then exit successfully.]'
    '--version[Print out version information, then exit successfully.]'
    '*:filename:_files'
)
_arguments -s $arguments
