#compdef kldconfig

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/kldconfig.8.gz

local arguments

arguments=(
    '-d[Remove the specified paths from the module search path.]'
    '-f[Do not display a diagnostic message if a path specified for adding is already…]'
    '-i[Add the specified paths to the beginning of the search path, not to the end.]'
    '-m[Instead of replacing the module search path with the set of paths specified, …]'
    '-n[Do not actually change the module search path.]'
    '-r[Display the current search path.]'
    '-S[Specify the sysctl name to use instead of the default kern. module_path.]'
    '-U[Unique-ify the current search path - if any of the directories is repeated on…]'
    '-v[Verbose output: display the new module search path.]'
    '*:filename:_files'
)
_arguments -s $arguments
