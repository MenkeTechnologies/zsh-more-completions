#compdef gpio

# Autogenerated from man page /usr/local/share/man/man1/gpio.1

local arguments

arguments=(
    '-v[Output the current version including the board revision of the Raspberry Pi.]'
    '-g[Use the BCM_GPIO pins numbers rather than wiringPi pin numbers.]'
    '-1[Use the physical pin numbers rather than wiringPi pin numbers.]'
    '-x[This causes the named extension to be initialised.]:extension'
    '-p[Use the PiFace interface board and its corresponding pin numbers.]'
    '*:filename:_files'
)
