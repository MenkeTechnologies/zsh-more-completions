#compdef rsyslogd

# Autogenerated from man page /usr/share/man/man8/rsyslogd.8.gz

local arguments

arguments=(
    '-D[Runs the Bison config parser in debug mode.]'
    '-d[Turns on debug mode. See the DEBUGGING section for more information.]'
    '-f[Specify an alternative configuration file instead of R /etc/rsyslog.]'
    '-i[Specify an alternative pid file instead of the default one.]'
    '-n[Avoid auto-backgrounding.]'
    '-N[Do a coNfig check.]:"level'
    '-C[This prevents rsyslogd from changing to the root directory.]'
    '-v[Print version and exit.]'
    '*:filename:_files'
)
_arguments -s $arguments
