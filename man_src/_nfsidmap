#compdef nfsidmap

# Autogenerated from man page /usr/share/man/man8/nfsidmap.8.gz

local arguments

arguments=(
    '-c[Clear the keyring of all the keys.]'
    '-d[Display the system\s effective NFSv4 domain name on R stdout .]'
    '-g[Revoke the gid key of the given user.]:user'
    '-h[Display usage message.]'
    '-l[Display on stdout all keys currently in the keyring used to cache ID mapping…]'
    '-r[Revoke both the uid and gid key of the given user.]:user'
    '-t[Set the expiration timer, in seconds, on the key.]:timeout'
    '-u[Revoke the uid key of the given user.]:user'
    '-v[Increases the verbosity of the output to syslog (can be specified multiple t…]'
    '*:filename:_files'
)
_arguments -s $arguments
