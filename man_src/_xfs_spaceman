#compdef xfs_spaceman

# Autogenerated from man page /usr/share/man/man8/xfs_spaceman.8.gz

local arguments

arguments=(
    '-c[xfs_spaceman commands may be run interactively (the default) or as arguments …]:cmd'
    '-a[Collect free space information from this allocation group.]:agno'
    '-b[This argument establishes that the histogram bin sizes are successive powers …]'
    {-e,-h,-m}'[.]'
    '-d[Print debugging information such as the raw free space extent information.]'
    '-g[Print the free space block and extent counts for each AG.]'
    '-r[Query the realtime device for free space information.]'
    '-s[Display a summary of the free space information found.]'
    {-u,-p}'[options are given, this command acts on all files.]'
    '-f[offset / length pair are required. ).]'
    '*:filename:_files'
)
_arguments -s $arguments
