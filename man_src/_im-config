#compdef im-config

# Autogenerated from man page /usr/share/man/man8/im-config.8.gz

local arguments

arguments=(
    '-a[List all; force to list all possible input methods in menu or STDOUT even if …]'
    '-c[Use console dialogue.]'
    '-x[Use X dialogue with zenity.]'
    '-s[No action; perform a simulation of events that would occur but do not actuall…]'
    '-l[List all available input method configuration settings to STDOUT.]'
    '-m[List configuration values to STDOUT: . RS 12 . ie n \\{\\ \\h\-04\\\(bu\\h\+03\\\c .]'
    '-n[<input method name> Set input method to <input method name>.]'
    '-o[<input method name>.]'
    '*:filename:_files'
)
_arguments -s $arguments
