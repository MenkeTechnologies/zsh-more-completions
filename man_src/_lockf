#compdef lockf

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/lockf.1.gz

local arguments

arguments=(
    '-k[Causes the lock file to be kept (not removed) after the command completes.]'
    '-s[Causes lockf to operate silently.]'
    '-n[Causes lockf to fail if the specified lock file does not exist.]'
    '-t[Specifies a timeout for waiting for the lock.]'
    '*:filename:_files'
)
_arguments -s $arguments
