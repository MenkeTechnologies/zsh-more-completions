#compdef regexp-assemble

# Autogenerated from man page /usr/share/man/man1/regexp-assemble.1p.gz

local arguments

arguments=(
    '-a[look Ahead. Insert \\f(CW\\*(C`(?=.]'
    '-b[Blank. Ignore blank lines.]'
    '-c[Comment. Basic comment filtering.]'
    '-d[Debug. Turns on debugging output. See Regexp::Assemble for suitable values.]'
    '-i[Indent.]'
    '-n[No newline. Do not print a newline after the pattern.]'
    '-p[Print. Print the pattern.]'
    '-r[Reduce. The default behaviour is to reduce the assembled pattern.]'
    '-s[Statistics. Print some statistics about the assembled pattern.]'
    '-S[Statistics only. Like -s, except that the pattern itself is not output.]'
    '-t[Test. Test the assembled expression against the contents of a file.]'
    '-T[Time. Print statistics on the time taken to reduce and assemble the pattern.]'
    '-u[Unique. Carp if duplicate patterns are found.]'
    '-U[Unroll.]'
    '-v[Version. Print the version of the regexp-assemble script.]'
    '*:filename:_files'
)
_arguments -s $arguments
