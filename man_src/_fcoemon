    {-d,--debug}'[Enable debugging messages.]'
    {-l,--legacy}'[Force fcoemon to use the legacy /sys/module/libfcoe/parameters/ interface.]'
    {-s,--syslog}'[Use syslogd for logging. The default behavior is to log to stdout and stderr.]'
