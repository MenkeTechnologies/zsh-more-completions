#compdef rotatelogs

# Autogenerated from man page /usr/share/man/man8/rotatelogs.8.gz

local arguments

arguments=(
    '-l[Causes the use of local time rather than GMT as the base for the interval or …]'
    '-L[Causes a hard link to be made from the current logfile to the specified link …]:linkname'
    '-p[If given, rotatelogs will execute the specified program every time a new log …]:program'
    '-f[Causes the logfile to be opened immediately, as soon as rotatelogs starts, in…]'
    '-t[Causes the logfile to be truncated instead of rotated.]'
    '-v[Produce verbose output on STDERR.]'
    '-e[Echo logs through to stdout.]'
    '-c[Create log file for each interval, even if empty.]'
    '-n[Use a circular list of filenames without timestamps.]:number-of-files'
    '*:filename:_files'
)
_arguments -s $arguments
