#compdef trace-summary

# Autogenerated from man page /opt/zeek/share/man/man1/trace-summary.1

local arguments

arguments=(
    '--version[show program\s version number and exit.]'
    {-h,--help}'[show this help message and exit.]'
    {-b,--bytes}'[count fractions in terms of bytes rather than packets/connections.]'
    {-c,--conn-summaries}'[input file contains Zeek connection summaries.]'
    '--conn-version[when used with -c, specify \1\ for use with Bro version 1.]:CONN_VERSION'
    {-C,--chema}'[for packets: include only TCP, ignore when seq==0.]'
    {-e,--external}'[ignore strictly internal traffic.]'
    {-E,--exclude-nets}'[excludes CIDRs in file from analysis.]'
    {-i,--intervals}'[create summaries for time intervals of given length (seconds, or use suffix o…]'
    {-l,--local-nets}'[differentiate in/out based on CIDRs in file.]'
    {-n,--topn}'[show top <n>.]'
    {-p,--ports}'[include only ports listed in file.]'
    {-P,--write-ports}'[write top total/incoming/outgoing ports into file.]'
    {-r,--resolve-host-names}'[resolve host names.]'
    {-R,--R}'[write output suitable for R into files <tag. *>.]'
    {-s,--sample-factor}'[sample factor of input.]'
    {-S,--do-sample}'[sample input with probability (0. 0 < prob < 1. 0).]'
    {-m,--save-mem}'[do not make memory-expensive statistics.]'
    {-t,--tcp}'[include only TCP.]'
    {-u,--udp}'[include only UDP.]'
    {-U,--min-time}'[minimum time in ISO format (e. g. 2005-12-31-23-59-00).]'
    {-v,--verbose}'[show top-n for every interval.]'
    {-V,--max-time}'[maximum time in ISO format AUTHOR trace-summary was written by The Zeek Proje…]'
    '*:filename:_files'
)
_arguments -s $arguments
