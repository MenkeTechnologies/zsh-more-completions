#compdef dh_installdocs

# Autogenerated from man page /usr/share/man/man1/dh_installdocs.1.gz

local arguments

arguments=(
    {-A,--all}'[Install all files specified by command line parameters in \\s-1ALL\\s0 packages…]'
    {--Xitem,--exclude}'[Exclude files that contain item anywhere in their filename from being install…]':item
    '--sourcedir[Look in the specified directory for files to be installed.]:dir'
    '--doc-main-package[Set the main package for a documentation package.]:main-package'
    '--link-doc[Make the documentation directory of all packages acted on be a symlink to the…]:package'
    '*:filename:_files'
)
_arguments -s $arguments
