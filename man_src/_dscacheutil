#compdef dscacheutil

# Autogenerated from man page /usr/share/man/man1/dscacheutil.1

local arguments

arguments=(
    '-h[Lists the options for calling dscacheutil.]'
    '-q[Initiate a query using standard calls.]'
    '-a[Optional flag to q for a specific key with a value.]'
    '--cachedump[Dumps an overview of the cache by default.]'
    '--buckets[Used in conjunction with cachedump to also print hash bucket usage of the cur…]'
    '--entries[Used in conjunction with cachedump to dump detailed information about cache e…]'
    '--configuration[Prints current configuration information, such as the search policy from Dire…]'
    '--flushcache[Flushes the entire cache. This should only be used in extreme cases.]'
    '--statistics[Prints statistics from the cache including an overview and detailed call stat…]'
    '*:filename:_files'
)
_arguments -s $arguments
