#compdef polkit-action

# Autogenerated from man page /usr/local/man/man1/polkit-action.1.gz

local arguments

arguments=(
    '--action[Show detailed information about a specific action.]:action'
    '--reset-defaults[Reset the defaults for the specified action to the factory defaults.]:action'
    '--show-overrides[Prints all actions for where the defaults are overridden.]'
    '--set-defaults-any[Override the any stanza for the given action with the supplied value.]:value'
    '--set-defaults-inactive[Override the inactive stanza for the given action with the supplied value.]:value'
    '--set-defaults-active[Override the active stanza for the given action with the supplied value.]:value'
    '--version[Show version and exit.]'
    '--help[Show usage information and exit.]'
    '*:filename:_files'
)
_arguments -s $arguments
