#compdef cmake-gui

# Autogenerated from man page /usr/share/man/man1/cmake-gui.1.gz

local arguments

arguments=(
    '-S[Path to root directory of the CMake project to build.]:path-to-source>'
    '-B[Path to directory which CMake will use as the root of build directory.]:path-to-build'
    {--help,--help,--usage,-h,-H}'[Print usage information and exit.]'
    {--version,--version}'[Show program name/version banner and exit.]:f'
    '--help-full[Print all help manuals and exit.]:f'
    '--help-manual[Print one help manual and exit.]:f'
    '--help-manual-list[List help manuals available and exit.]:f'
    '--help-command[Print help for one command and exit.]:f'
    '--help-command-list[List commands with help available and exit.]:f'
    '--help-commands[Print cmake-commands manual and exit.]:f'
    '--help-module[Print help for one module and exit.]:f'
    '--help-module-list[List modules with help available and exit.]:f'
    '--help-modules[Print cmake-modules manual and exit.]:f'
    '--help-policy[Print help for one policy and exit.]:f'
    '--help-policy-list[List policies with help available and exit.]:f'
    '--help-policies[Print cmake-policies manual and exit.]:f'
    '--help-property[Print help for one property and exit.]:f'
    '--help-property-list[List properties with help available and exit.]:f'
    '--help-properties[Print cmake-properties manual and exit.]:f'
    '--help-variable[Print help for one variable and exit.]:f'
    '--help-variable-list[List variables with help available and exit.]:f'
    '--help-variables[Print cmake-variables manual and exit.]:f'
    '*:filename:_files'
)
_arguments -s $arguments
