#compdef mutt_pgpring

# Autogenerated from man page /usr/local/share/man/man1/mutt_pgpring.1

local arguments

arguments=(
    '-k[Dump the contents of the specified keyring.]:keyring"'
    '-2[Use the default keyring for PGP 2. x.]'
    '-5[Use the default keyring for PGP 5.]'
    '-s[Dump the secret keyring.]'
    '-S[Dump signatures.]'
    '-f[Dump fingerprints.]'
    '*:filename:_files'
)
