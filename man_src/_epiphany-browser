#compdef epiphany-browser

# Autogenerated from man page /usr/share/man/man1/epiphany-browser.1.gz

local arguments

arguments=(
    {-\?,--help}'[Show help options . PP Application options.]'
    {-n,--new-tab}'[Open a new tab in an existing browser window.]'
    '--new-window[Open a new browser window.]'
    {-b,--bookmarks-editor}'[Launch the bookmarks editor.]'
    '--import-bookmarks[Import bookmarks from the given file.]:FILE'
    {-l,--load-session}'[Load the given session file.]':FILE
    {-t,--add-bookmark}'[Add a bookmark.]':URL
    '--private-instance[Start a private instance.]'
    '--profile[Profile directory to use in the private instance.]:FILE'
    '--display[X display to use.]:DISPLAY'
    '--version[Show version.]'
    '*:filename:_files'
)
_arguments -s $arguments
