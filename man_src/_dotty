#compdef dotty

# Autogenerated from man page /usr/share/man/man1/dotty.1.gz

local arguments

arguments=(
    '-V[Prints the version.]'
    '--lm[Sets the layout mode. The mode can be sync or async. The default is async.]:mode'
    '--el[Sets the mesage level. The lev can be 0 or 1. The default is 0.]:lev'
    '*:filename:_files'
)
_arguments -s $arguments
