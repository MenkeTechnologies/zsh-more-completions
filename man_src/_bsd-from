#compdef bsd-from

# Autogenerated from man page /usr/share/man/man1/bsd-from.1.gz

local arguments

arguments=(
    '-c[Just print a count of messages and exit.]'
    '-f[The supplied file is examined instead of the invoker\s mailbox.]'
    '-s[Only mail from addresses containing the supplied string are printed.]'
    '*:filename:_files'
)
_arguments -s $arguments
