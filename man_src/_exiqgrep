#compdef exiqgrep

# Autogenerated from man page /usr/share/man/man8/exiqgrep.8.gz

local arguments

arguments=(
    '-h[Print help.]'
    '-f[Match sender address (field is \\(lq< >\\(rq wrapped).]:regexp'
    '-r[Match recipient address.]:regexp'
    '-s[Match against the site field from long output.]:regexp'
    '-y[Message younger than.]:seconds'
    '-o[Message older than.]:seconds'
    '-z[Frozen messages only (exclude non-frozen).]'
    '-x[Non-frozen messages only (exclude frozen).]'
    '-c[Display match count.]'
    '-l[Long Format \[Default\].]'
    '-i[Message IDs only.]'
    '-b[Brief Format.]'
    '-R[Reverse order.]'
    '*:filename:_files'
)
_arguments -s $arguments
