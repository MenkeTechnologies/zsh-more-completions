#compdef pear

# Autogenerated from man page /usr/share/man/man1/pear.1.gz

local arguments

arguments=(
    '-v[increase verbosity level (default 1).]'
    '-q[be quiet, decrease verbosity level.]'
    '-c[find user configuration in file.]:file'
    '-C[find system configuration in file.]:file'
    '-d[set user config variable foo to bar.]:bar'
    '-D[set system config variable foo to bar.]:bar'
    '-G[start in graphical (Gtk) mode.]'
    '-S[store system configuration.]'
    '-s[store user configuration.]'
    '-y[unset foo in the user configuration.]:foo'
    '-V[version information.]'
    '-h[.]'
    '-\?[display help/usage.]'
    '*:filename:_files'
)
_arguments -s $arguments
