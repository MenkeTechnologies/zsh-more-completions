#compdef priditherpng

local arguments

arguments=(
  {-h,--help}'[show this help message and exit]'
  '--bitdepth[bitdepth of output]'
  '--cutoff[cutoff to select adjacent output values]'
  '--defaultgamma[gamma value to use when no gamma in input]'
  '--linear[force linear input]'
  '--targetgamma[gamma to use in output (target), defaults to input]'
  '*:filename:_files'
)

_arguments -s $arguments
