#compdef brltty-cldr

local arguments

arguments=(
  {-f,--output-format}'[string  Format of each output line.]'
  {-h,--help}'[print a usage summary (commonly used options only)]'
  {-H,--full-help}'[print a usage summary (all options), and then exit.]'
  '*:filename:_files'
)

_arguments -s $arguments
