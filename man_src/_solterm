local arguments

arguments=(
    '-e[The character to use to escape, or exit, the program.]:escape_char'
    '--notencrypted[Do not encrypt the SOL data going over the connection.]'
    '--notauthenticated[Do not do integrity checks on the SOL data going over the connection.]'
    '--bitrate[Set the bitrate to the given value.]'
    '--alerts[This specifies what to do if a alert that requires a callout occurs on the li…]'
    '--holdoff[Specifies that CTS, DTR, and DSR are to be deasserted at the start of the SoL…]'
    '-v[Be more verbose. Maybe specified multiple time for more output.]'
    '-q[Be less verbose. Maybe specified multiple time for less output.]'
    '*:filename:_files'
)
_arguments -s $arguments
