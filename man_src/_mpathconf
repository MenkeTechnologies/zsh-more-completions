#compdef mpathconf

# Autogenerated from man page /usr/share/man/man8/mpathconf.8.gz

local arguments

arguments=(
    '--with_module[If set to y, this runs modprobe dm_multipath to install the multipath module…]'
    '--with_multipathd[option is not set by default.]'
    '--user_friendly_names[and.]:n'
    '--find_multipaths[options COMMANDS.]:n'
    '--enable[Removes any line that blacklists all device nodes from the /etc/multipath.]'
    '--disable[Adds a line that blacklists all device nodes to the /etc/multipath.]'
    '--allow[Modifies the /etc/multipath/conf blacklist to blacklist all wwids and the bla…]:<device>'
    '--user_friendly_name[If set to y, this adds the line user_friendly_names yes to the /etc/multipath.]'
    '--outfile[Write the resulting multipath configuration to <filename> instead of /etc/mul…]:<filename>'
    '*:filename:_files'
)
