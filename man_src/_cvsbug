#compdef cvsbug

# Autogenerated from man page /usr/share/man/man8/cvsbug.8.gz

local arguments

arguments=(
    '-f[specify a file (problem-report) which already contains a complete problem rep…]:problem-report"'
    '-t[Change mail address at the support site for problem reports.]:mail-address"'
    '-P[print the form specified by the environment variable PR_FORM on standard o…]'
    '-L[print the list of available categories. No mail is sent.]'
    '--request-id[sends mail to the default support site, or site if specified, with a request…]'
    '-v[Display the cvsbug version number.]'
    '*:filename:_files'
)
_arguments -s $arguments
