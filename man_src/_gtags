#compdef gtags

# Autogenerated from man page /usr/local/share/man/man1/gtags.1

local arguments

arguments=(
    '--accept-dotfiles[Accept files and directories whose names begin with a dot.]'
    {-c,--compact}'[Make \GTAGS\ in compact format.]'
    '--config[Print the value of config variable name.]:name'
    {-d,--dump}'[Dump a tag file as text to the standard output.]':tag-file
    '--explain[Explain handling files.]'
    {-f,--file}'[Give a list of candidates of target files.]':file
    '--gtagsconf[Set environment variable GTAGSCONF to file.]:file'
    '--gtagslabel[Set environment variable GTAGSLABEL to label.]:label'
    {-I,--idutils}'[In addition to tag files, make ID database for idutils(1).]'
    {-i,--incremental}'[Update tag files incrementally.]'
    {-O,--objdir}'[Use BSD-style obj directory as the location of tag files.]'
    '--single-update[Update tag files for a single file.]:file'
    '--skip-unreadable[Skip unreadable files.]'
    '--skip-symlink[Skip symbolic links.]:type'
    '--sqlite3[Use Sqlite 3 API to make tag files. By default, BSD/DB 1. 85 API is used.]'
    '--statistics[Print statistics information.]'
    {-q,--quiet}'[Quiet mode.]'
    {-v,--verbose}'[Verbose mode.]'
    {-w,--warning}'[Print warning messages.]'
    '--with-sqlite3[.]'
    '*:filename:_files'
)
_arguments -s $arguments
