local arguments

arguments=(
    '-h[Show help options.]'
    {-i,--images}'[Use IMAGES in the document list.]'
    {-c,--command}'[Use COMMAND instead of xdg-open to open recent documents.]'
    {-a,--age}'[Limit documents to AGE number of days.]'
    '--limit[Display up to LIMIT items.]'
    {-s,--sort-type}'[Sort items according to SORT TYPE.]'
    '*:filename:_files'
)
