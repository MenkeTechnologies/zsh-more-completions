local arguments

arguments=(
    '-U[database server login name.]'
    '-P[database server password.]'
    '-S[database server to which to connect.]'
    '-D[database to use.]'
    '-i[a script to apply to the database. Not currently implemented.]'
    '-o[a file to hold the script, defaults to standard output.]'
    '-v[Show version information and copyright notice.]'
    '*:filename:_files'
)
