#compdef tpm2_policycphash

# Autogenerated from man page /usr/share/man/man1/tpm2_policycphash.1.gz

local arguments

arguments=(
    {-L,--policy}'[File to save the compounded policy digest.]':FILE:
    {-S,--session}'[The policy session file generated via the -S option to tpm2_startauthsession(…]':FILE:
    '--cphash-input[The file containing the command parameter hash of the command.]:FILE:'
    '--cphash[DEPRECATED, use \[en\]cphash-input instead.]:FILE:'
    {-h,--help}'[By default, it attempts to invoke the manpager for the tool, however, on fail…]'
    {-v,--version}'[tool, supported tctis and exit. \[bu\] 2.]'
    {-V,--verbose}'[tool prints to the console during its execution.]'
    {-Q,--quiet}'[\[bu\] 2.]'
    {-Z,--enable-errata}'[errata fixups.]'
    {-R,--autoflush}'[objects created by the command.]'
    '-n[tpm2_startauthsession -S session.]:signing_key.name'
    '--out[tpm2_verifysignature -c signing_key. ctx -g sha256 -m policy. cphash \[rs\].]:policy.cphash'
    {-s,-t,-f}'[Satisfy policycphash and execute nvsetbits tpm2_startauthsession -S session.]'
    '*:filename:_files'
)
_arguments -s $arguments
