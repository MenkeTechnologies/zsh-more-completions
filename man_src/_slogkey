#compdef slogkey

# Autogenerated from man page /usr/share/man/man1/slogkey.1.gz

local arguments

arguments=(
    '-m[Generates a mew master key.]:filename'
    '-d[Derive a host key using a previously generated master key. sp . if n \\{\\.]'
    '-s[Display the current log entry counter to which the key relates. sp . if n \\{\\.]'
    '*:filename:_files'
)
_arguments -s $arguments
