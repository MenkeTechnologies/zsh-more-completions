#compdef paccache

# Autogenerated from man page /usr/share/man/man8/paccache.8.gz

local arguments

arguments=(
    {-a,--arch}'[Scan for packages for a specific architecture.]':<arch>
    {--min-atime,--min-mtime}'[Only consider packages for removal with atime respectively mtime older than s…]'
    {-c,--cachedir}'[Specify a different cache directory. This option can be used more than once.]':<dir>
    {-f,--force}'[Apply force to mv and rm operations.]'
    {-h,--help}'[Display syntax and command line options.]'
    {-i,--ignore}'[Specify packages to ignore, comma-separated.]':<pkgs>
    {-k,--keep}'[Specify how many versions of each package are kept in the cache directory, de…]':<num>
    '--nocolor[Remove color from the output.]'
    {-q,--quiet}'[Minimize the output.]'
    {-u,--uninstalled}'[Target uninstalled packages.]'
    {-v,--verbose}'[Increase verbosity, can be specified up to 3 times.]'
    {-z,--null}'[Use null delimiters for candidate names (only with -v and -vv).]'
    {-d,--dryrun}'[.]'
    {-m,--move}'[.]':<dir>
    {-r,--remove}'[.]'
)
_arguments -s $arguments
