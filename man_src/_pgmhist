local arguments

arguments=(
    '--median[sp This option causes pgmhist to print the median gray value.]'
    '--quartile[sp This option causes pgmhist to print the four quartile gray values.]'
    '--decile[sp This option causes pgmhist to print the ten decile gray values.]'
    '--forensic[sp With this option, pgmhist works on images that contain invalid gray values.]'
    '--machine[.]'
    '*:filename:_files'
)
