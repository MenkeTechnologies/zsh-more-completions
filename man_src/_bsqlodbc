#compdef bsqlodbc

# Autogenerated from man page /usr/local/share/man/man1/bsqlodbc.1

local arguments

arguments=(
    '-U[Database server login name.]'
    '-P[Database server password.]'
    '-S[Database server to which to connect.]'
    '-D[Database to use.]'
    '-i[Name of script file, containing SQL.]'
    '-o[Name of output file, holding result data.]'
    '-e[Name of file for errors.]'
    '-t[Specifies the field terminator. Default is two spaces ( \\ ).]'
    '-h[Print column headers with the data to the same file.]'
    '-q[Do not print column metadata, return status, or rowcount. Overrides h.]'
    '-v[Verbose mode, for more information about the ODBC interaction.]'
    '-V[Specify ODBC version (2 or 3).]'
    '*:filename:_files'
)
_arguments -s $arguments
