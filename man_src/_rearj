    '-f[Convert diskette archives.]'
    '-g[Skip creation of output archive.]'
    '-i[Check integrity of rearj program.]:name'
    '-j[Skip if output archive size is larger than the original.]'
    '-k[Skip archive timestamping.]'
    '-l[Write append log file (default name is R rearj. log ).]:name'
    '-m[Select archives before date in YYMMDDHHMMSS format.]:date'
    '-n[Select archives on or after date in YYMMDDHHMMSS format.]:date'
    '-o[Allow overwrite of existing target archive.]'
    '-p[Ignore long filenames under Windows 95.]'
    '-q[Query for each archive to convert.]'
    '-r[Recurse through subdirectories.]'
    '-s[Skip verify of file count and total size.]'
    '-t[Create suffix type archives.]:suffix'
    '-u[Allow update of archive with backup (default is R BAK ).]:bak'
    '-v[Execute configured command on extracted files.]'
    '-w[Assign work directory.]:dir'
    '-x[Exclude by filename, wildname, or listfile.]:!file'
    '-y[Delete output archive and write text to log (testing mode).]:text'
    '-z[Simulate operation.]'
    '*:filename:_files'
