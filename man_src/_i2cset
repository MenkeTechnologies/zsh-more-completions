#compdef i2cset

# Autogenerated from man page /usr/share/man/man8/i2cset.8.gz

local arguments

arguments=(
    '-V[Display the version and exit.]'
    '-f[Force access to the device even if it is already busy.]'
    '-y[Disable interactive mode.]'
    '-m[The mask parameter, if specified, describes which bits of value will be actua…]:mask'
    '-r[Read back the value right after writing it, and compare the result with the v…]'
    '-a[Allow using addresses between 0x00 - 0x02 and 0x78 - 0x7f. Not recommended.]'
    '*:filename:_files'
)
_arguments -s $arguments
