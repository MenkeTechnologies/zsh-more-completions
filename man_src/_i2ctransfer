#compdef i2ctransfer

# Autogenerated from man page /usr/share/man/man8/i2ctransfer.8.gz

local arguments

arguments=(
    '-f[Force access to the device even if it is already busy.]'
    '-y[Disable interactive mode.]'
    '-v[Enable verbose output. It will print infos about all messages sent, i. e.]'
    '-V[Display the version and exit.]'
    '-a[Allow using addresses between 0x00 - 0x02 and 0x78 - 0x7f. Not recommended.]'
    '*:filename:_files'
)
_arguments -s $arguments
