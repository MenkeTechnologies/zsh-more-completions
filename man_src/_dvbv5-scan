#compdef dvbv5-scan

# Autogenerated from man page /usr/share/man/man1/dvbv5-scan.1.gz

local arguments

arguments=(
    {-3,--dvbv3}'[Force dvbv5-scan to use DVBv3 only.]'
    {-a,--adapter}'[Use the given adapter. Default value: 0.]':adapter#
    {-C,--cc}'[Set the default country to be used by the MPEG-TS parsers, in ISO 3166-1 two …]':country_code
    {-d,--demux}'[Use the given demux. Default value: 0.]':demux#
    {-f,--frontend}'[Use the given frontend. Default value: 0.]':frontend#
    {-F,--file-freqs-only}'[Don\t use the other frequencies discovered during scan.]'
    {-G,--get_frontend}'[Use data from get_frontend on the output file.]'
    {-I,--input-format}'[Format of the input file. Please notice that caps is ignored.]':format
    {-l,--lnbf}'[Type of LNBf to use \help\ lists the available ones.]':LNBf_type
    {-N,--nit}'[Use data from NIT table on the output file.]'
    {-o,--output}'[output filename (default: dvb_channel. conf).]':file
    {-O,--output-format}'[Output format: channel - for dvb-apps compatible channel file;.]':format
    {-p,--parse-other-nit}'[Parse the other NIT/SDT tables that could be found mainly on some DVB-C carri…]'
    {-S,--sat_number}'[Satellite number. Used only on satellite delivery systems.]':satellite_number
    {-T,--timeout-multiply}'[Multiply the scan lock wait time and MPEG-TS table parsing by this factor.]':factor
    {-U,--freq_bpf}'[SCR/Unicable band-pass filter frequency to use, in kHz.]':frequency
    {-v,--verbose}'[Be (very) verbose. Useful to check if the MPEG-TS is happenning fine.]'
    {-w,--lna}'[Enable, disable or put LNA power in auto mode. Not all frontends support it.]':LNA
    '-1[.]:auto'
    {-W,--wait}'[Adds additional wait time for DISEqC command completion.]':time
    {-\?,--help}'[Outputs the usage help.]'
    '--usage[Gives a short usage message.]'
    {-V,--version}'[Prints the program version.]'
    '*:filename:_files'
)
_arguments -s $arguments
