    '-d[check only for dynamic shared library files and return status.]'
    '-i[ignore non-prebound files (useful when running on all types of files).]'
    '-z[zero out the prebind check sum in the output if it has one.]'
    '-u[unprebind, rather than reprebind (-c, -p, -d, -e ignored).]'
    '-e[replace any dependent library\s "@executable_path" prefix with the executabl…]:executable_path'
    '--seg_addr_table[The -seg_addr_table option is used when the input a dynamic library and if s…]:table_file_name'
    '--seg_addr_table_filename[Use pathname instead of the install name of the library for matching an entr…]:pathname'
    '--seg1addr[Move the input library to base address address.]:address'
    '-r[prepend the rootdir argument to all dependent libraries.]:rootdir'
    '-o[write the updated file into output_file rather than back into the input file.]:output_file'
    '-s[write the updated file to standard output DIAGNOSTICS With no -c, -p or -d an…]'
    '*:filename:_files'
)
_arguments -s $arguments
