#compdef wirefilter

# Autogenerated from man page /opt/homebrew/share/man/man1/wirefilter.1

local arguments

arguments=(
    '-f[use a startup configuration file.]:"rcfile"'
    '-l[percentage of loss as a floating point number.]:"loss"'
    '-L[when this is not zero, wirefilter uses the Gilbert model for bursty errors.]:"lostburst"'
    '-d[Extra delay (in milliseconds).]:"delay"'
    '-D[percentage of dup packet. It has the same syntax of -l.]:"dup"'
    '-b[Channel bandwidth in Bytes/sec. It has the same syntax of -d.]:"bandwidth"'
    '-s[Interface speed in Bytes/sec. It has the same syntax of -b.]:"speed"'
    '-c[Channel buffer size (in Bytes): maximum size of the packet queue.]:"channel_bufsize"'
    '-n[Number of bits damaged/one megabyte.]:factor"'
    '-m[Packets longer than mtu_size are discarded.]:size"'
    '-N[nofifo. with -N packets can be reordered.]'
    '-M[the unix socket where the parameters (loss percentage, delay etc) can be chec…]:socket"'
    '-v[If this option is used, the two local vde_plugs (vde_plug1 and vde_plug2) wil…]:"vde_plug1:vde_plug2"'
    '--mgmtmode[this option sets the access mode of the mgmt socket.]:"mode"'
    '--daemon[wirefilter becomes a daemon.]'
    '--pidfile[wirefilter saves its pid into the file.]:"pathnamefP"'
    '--blinkid[This option defines the id sent for each packet to the blink server (see the…]:"name"'
    '--blink[wirefilter sends a log message to the specified PF_UNIX/DATAGRAM socket for …]'
    '*:filename:_files'
)
_arguments -s $arguments
