#compdef xdg-email

# Autogenerated from man page /usr/share/man/man1/xdg-email.1.gz

local arguments

arguments=(
    '--utf8[Indicates that all command line options that follow are in utf8.]'
    '--cc[Specify a recipient to be copied on the e-mail.]:address'
    '--bcc[Specify a recipient to be blindly copied on the e-mail.]:address'
    '--subject[Specify a subject for the e-mail.]:text'
    '--body[Specify a body for the e-mail.]:text'
    '--attach[Specify an attachment for the e-mail. file must point to an existing file.]:file'
    '--help[Show command synopsis.]'
    '--manual[Show this manual page.]'
    '--version[Show the xdg-utils version information.]'
    '*:filename:_files'
)
_arguments -s $arguments
