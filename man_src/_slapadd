#compdef slapadd

# Autogenerated from man page /usr/share/man/man8/slapadd.8

local arguments

arguments=(
    '-b[Use the specified suffix to determine which database to add entries to.]:suffix'
    '-c[enable continue (ignore errors) mode.]'
    '-d[enable debugging messages as defined by the specified R debug-level ; see sl…]:debug-level'
    '-f[specify an alternative slapd. conf (5) file.]:slapd.conf'
    '-F[specify a config directory.]:confdir'
    '-g[disable subordinate gluing.]'
    '-j[Jump to the specified line number in the LDIF file before processing any entr…]:lineno'
    '-l[Read LDIF from the specified file instead of standard input.]:ldif-file'
    '-n[Add entries to the dbnum-th database listed in the configuration file.]:dbnum'
    '-o[Specify an option with a(n optional) R value .]'
    '-q[enable quick (fewer integrity checks) mode.]'
    '-s[disable schema checking.]'
    '-S[Server ID to use in generated entryCSN.]:SID'
    '-u[enable dry-run (don\t write to backend) mode.]'
    '-v[enable verbose mode.]'
    '-w[write syncrepl context information.]'
    '*:filename:_files'
)
_arguments -s $arguments
