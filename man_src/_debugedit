#compdef debugedit

# Autogenerated from man page /usr/share/man/man8/debugedit.8.gz

local arguments

arguments=(
    {-\?,--help}'[Show summary of options.]'
    {-b,--base-dir}'[base build directory of objects.]':STRING
    {-d,--dest-dir}'[directory to rewrite base-dir into.]':STRING
    {-l,--list-file}'[file where to put list of source and header file names.]':STRING
    {-i,--build-id}'[recompute build ID note and print ID on stdout.]'
    '*:filename:_files'
)
_arguments -s $arguments
