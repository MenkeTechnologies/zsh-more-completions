#compdef xmessage

# Autogenerated from man page /opt/X11/share/man/man1/xmessage.1

local arguments

arguments=(
    '--buttons[This option will cause xmessage to create one button for each comma-separated…]:buttonbutton.|.|.'
    '--default[Defines the button with a matching label to be the default.]:label'
    '--file[File to display. The corresponding resource is file.]:filename'
    '--print[This will cause the program to write the label of the button pressed to stand…]'
    '--center[Pop up the window at the center of the screen.]'
    '--nearmouse[Pop up the window near the mouse cursor.]'
    '--timeout[Exit with status 0 after secs seconds if the user has not clicked on a button…]:secs'
    '*:filename:_files'
)
_arguments -s $arguments
