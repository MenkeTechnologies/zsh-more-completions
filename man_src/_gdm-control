#compdef gdm-control

# Autogenerated from man page /usr/share/man/man1/gdm-control.1.gz

local arguments

arguments=(
    '--help[Display some help and exit.]'
    '--none[Do nothing special when the current session ends.]'
    '--shutdown[Shutdown the computer when the current session ends.]'
    '--reboot[eboot the computer when the current session ends.]'
    '--suspend[Suspend the computer when the current session ends.]'
    '*:filename:_files'
)
