#compdef memusage

# Autogenerated from man page /usr/share/man/man1/memusage.1.gz

local arguments

arguments=(
    {-n,--progname}'[Name of the program file to profile.]'
    {-p,--png}'[Generate PNG graphic and store it in R file .]'
    {-d,--data}'[Generate binary data file and store it in R file .]'
    {-u,--unbuffered}'[Do not buffer output.]'
    {-b,--buffer}'[Collect size entries before writing them out.]'
    '--no-timer[Disable timer-based . RB ( SIGPROF ) sampling of stack pointer value.]'
    {-m,--mmap}'[Also trace mmap (2), mremap (2), and munmap (2).]'
    {-\?,--help}'[Print help and exit.]'
    '--usage[Print a short usage message and exit.]'
    {-V,--version}'[Print version information and exit.]'
    {-t,--time-based}'[Use time (rather than number of function calls) as the scale for the X axis.]'
    {-T,--total}'[Also draw a graph of total memory use.]'
    '--title[Use name as the title of the graph.]:name'
    {-x,--x-size}'[Make the graph size pixels wide.]'
    {-y,--y-size}'[Make the graph size pixels high.]'
    '*:filename:_files'
)
_arguments -s $arguments
