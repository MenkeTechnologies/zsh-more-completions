#compdef icedax

# Autogenerated from man page /usr/share/man/man1/icedax.1.gz

local arguments

arguments=(
    '--device[uses device as the source for CDDA reading.]:device'
    '--auxdevice[uses auxdevice as CDROM drive for ioctl usage.]:auxdevice'
    '--interface[specifies the interface for CDROM access: generic_scsi or (on Linux, and Fre…]:interface'
    {-c,--channels}'[uses 1 for mono, or 2 for stereo recording, or s for stereo recording with…]'
    {-s,--stereo}'[sets to stereo recording.]'
    {-m,--mono}'[sets to mono recording.]'
    {-x,--max}'[sets maximum (CD) quality.]'
    {-b,--bits-per-sample}'[sets bits per sample per channel: 8 , 12 or 16 .]'
    {-r,--rate}'[sets rate in samples per second.]'
    {-a,--divider}'[sets rate to 44100Hz / divider.]'
    {-R,--dump-rates}'[shows a list of all sample rates and their dividers.]'
    {-P,--set-overlap}'[sets the initial number of overlap sectors for jitter correction.]'
    {-n,--sectors-per-request}'[reads sectors per request.]'
    {-l,--buffers-in-ring}'[uses a ring buffer with buffers total.]'
    {-t,--track}'[selects the start track and optionally the end track.]'
    {-i,--index}'[selects the start index.]'
    {-o,--offset}'[starts offset sectors behind start track (one sector equivalents 1/75 second…]'
    {-O,--output-format}'[can be wav (for wav files) or aiff (for apple/sgi aiff files) or aifc (fo…]'
    {-C,--cdrom-endianess}'[sets endianess of the input samples to \little\, \big\ or \guess\ to override…]'
    {-E,--output-endianess}'[sets endianess of the output samples to \little\ or \big\ to override default…]'
    {-d,--duration}'[sets recording time in seconds or frames.]'
    {-B,--bulk,--alltracks}'[copies each track into a separate file.]'
    {-w,--wait}'[waits for signal, then start recording.]'
    {-F,--find-extremes}'[finds extreme amplitudes in samples.]'
    {-G,--find-mono}'[finds if input samples are in mono.]'
    {-T,--deemphasize}'[undo the effect of pre-emphasis in the input samples.]'
    {-e,--echo}'[copies audio data to sound device e. g. /dev/dsp .]'
    {-p,--set-pitch}'[changes pitch of audio data copied to sound device.]'
    {-v,--verbose-level}'[prints verbose information about the CD.]'
    {-N,--no-write}'[does not write to a file, it just reads (for debugging purposes).]'
    {-J,--info-only}'[does not write to a file, it just gives information about the disc.]'
    {-L,--cddb}'[does a cddbp album- and track title lookup based on the cddb id.]'
    {-H,--no-infofile}'[does not write an info file and a cddb file.]'
    {-g,--gui}'[formats the output to be better parsable by gui frontends.]'
    {-M,--md5}'[enables calculation of MD-5 checksum for \count\ bytes from a beginning of a …]'
    {-S,--speed}'[sets the cdrom device to one of the selectable speeds for reading.]'
    {-q,--quiet}'[quiet operation, no screen output.]'
    {-V,--verbose-SCSI}'[enable SCSI command logging to the console.]'
    {-Q,--silent-SCSI}'[suppress SCSI command error reports to the console.]'
    '--scanbus[Scan all SCSI devices on all SCSI busses and print the inquiry strings.]'
    '--devices[Like -scanbus but works in a more native way, respecting the device name spec…]'
    '--paranoia[use the paranoia library instead of icedax\s routines for reading.]'
    {-h,--help}'[display version of icedax on standard output.]'
    '-D[.]:device'
    '-I[option for details.]'
    '-A[.]:auxdevice'
    '*:filename:_files'
)
_arguments -s $arguments
