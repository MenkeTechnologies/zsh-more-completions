#compdef vdptool

# Autogenerated from man page /usr/share/man/man8/vdptool.8.gz

local arguments

arguments=(
    '-i[specifies the network interface to which the command applies.]:ifname'
    '-V[specifies the VDP tlv identifier to be set or queried.]:tlvid'
    '-n["neighbor" option for commands which can use it (e. g. get-tlv).]'
    '-c[Specifies additional parameters for TLV queries and associations commands.]:list>'
    '-r[show raw client interface messages.]'
    '-R[show only raw Client interface messages.]'
    '-W[Wait for the bridge response message .]'
    '-h[show usage information.]:help'
    '-v[show version information.]:version'
    '-t[get VSI information for the specified interface.]:get-vsi'
    '-T[set VSI information for the specified interface.]:set-vsi'
    '-p[display the process identifier of the running lldpad process.]:ping'
    '-q[exit from interactive mode.]:quit'
    '*:filename:_files'
)
