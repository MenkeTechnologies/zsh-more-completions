#compdef ninfod

# Autogenerated from man page /usr/share/man/man8/ninfod.8.gz

local arguments

arguments=(
    '-d[Debug mode. Do not go background.]'
    '-h[Show help.]'
    '-v[Verbose mode.]'
    '-u[Run as another user. user can either be username or user ID.]:user'
    '-p[File for process-id storage. user is required to be able to create the file.]:pidfile'
    '-V[Print version and exit.]'
    '*:filename:_files'
)
_arguments -s $arguments
