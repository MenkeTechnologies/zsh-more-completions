#compdef repomanage

# Autogenerated from man page /usr/share/man/man1/repomanage.1.gz

local arguments

arguments=(
    '(- * :)'{-h,--help}'[Display a help message, and then quit.]'
    {-o,--old}'[Print the oldest packages.]'
    {-n,--new}'[Print the newest packages.]'
    {-s,--space}'[Space-separated instead of newline-separated output.]'
    {-k,--keep}'[Newest N packages to keep - defaults to 1.]'
    '*:filename:_files'
)
_arguments -s $arguments
