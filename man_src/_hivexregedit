#compdef hivexregedit

# Autogenerated from man page /usr/share/man/man1/hivexregedit.1.gz

local arguments

arguments=(
    '--help[Display help.]'
    '--debug[Enable debugging in the hivex library.]'
    '--merge[Vb 2 hivexregedit --merge \[--prefix prefix\] \[--encoding enc\] hivef…]'
    '--export[Vb 1 hivexregedit --export \[--prefix prefix\] hivefile key > regfile . Ve .]'
    '--prefix[Hive files and Windows Registry key names are indirectly related.]:prefix'
    '--encoding[When merging (only), you may need to specify the encoding for strings to be u…]:UTF-16LE|ASCII'
    '--unsafe-printable-strings[When exporting (only), assume strings are \\s-1UTF-16LE\\s0 and print them as s…]'
    '--unsafe[Use heuristics to tolerate certain levels of corruption within hives.]'
    '*:filename:_files'
)
_arguments -s $arguments
