#compdef thin_dump

# Autogenerated from man page /usr/share/man/man8/thin_dump.8.gz

local arguments

arguments=(
    '(- * :)'{-h,--help}'[Print help and exit.]'
    {-V,--version}'[Print version information and exit.]'
        {-f,--format}'[Choose output format. PP . nf .]:format:(xml human_readable custom)'
    {-r,--repair}'[Repair the metadata whilst dumping it.]'
    {-m,--metadata-snap}'[Dump metadata snapshot. PP . nf .]'
    '--dev-id[Dump the specified device. PP . nf .]:{natural}'
    '--skip-mappings[Do not dump the mappings.]'
    '-o[Specify a file for the output rather than writing to stdout.]:file}'
    '*:filename:_files'
)
_arguments -s $arguments
