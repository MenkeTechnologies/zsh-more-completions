#compdef get-edid

# Autogenerated from man page /usr/share/man/man1/get-edid.1.gz

local arguments

arguments=(
    {-b,--bus}'[only scan the i2c bus BUS (if built with i2c support).]'
    {-c,--classiconly}'[only use the older VBE interface (if built with i2c and VBE support).]'
    '(- * :)'{-h,--help}'[display a basic help message.]'
    {-i,--i2conly}'[only use the newer i2c interface (if built with i2c and VBE support).]'
    {-m,--monitor}'[try to return information about monitor number NUM (VBE only).]'
    {-q,--quiet}'[do not output messages over stderr.]'
    '*:filename:_files'
)
_arguments -s $arguments
