#compdef twistd3

# Autogenerated from man page /usr/share/man/man1/twistd3.1.gz

local arguments

arguments=(
    {-q,--quiet}'[No-op for backwards compatibility.]'
    {-p,--profile}'[Run the application under the profiler, dumping results to the specified file.]'
    '--profiler[Specify the profiler to use. Defaults to the \hotshot\ profiler.]:name'
    '--savestats[Save the Stats object rather than the text output of the profiler.]'
    {-b,--debug}'[Run the application in the Python Debugger (implies --nodaemon option).]'
    {-e,--encrypted}'[The specified tap/aos file is encrypted.]':file
    '--euid[Set only effective user-id rather than real user-id.]'
    {-o,--no_save}'[Do not save shutdown state.]'
    '--originalname[Behave as though the specified Application has no process name set, and run w…]'
    {-l,--logfile}'[Log to a specified file, - for stdout (default: twistd. log).]':logfile
    '--logger[A fully-qualified name to a log observer factory to use for the initial log o…]'
    '--pidfile[Save pid in specified file (default: twistd. pid).]:pidfile'
    '--chroot[Chroot to a supplied directory before running (default: don\t chroot).]:directory'
    {-d,--rundir}'[Change to a supplied directory before running (default: . ).]':directory
    {-u,--uid}'[The uid to run as (default: don\t change).]':uid
    {-g,--gid}'[The gid to run as (default: don\t change).]':gid
    '--umask[The (octal) file creation mask to apply.]:mask'
    {-r,--reactor}'[Choose which reactor to use. See --help-reactors for a list of possibilities.]':reactor
    '--help-reactors[List the names of possibly available reactors.]'
    '--spew[Write an extremely verbose log of everything that happens.]'
    {-f,--file}'[Read the given . tap file (default: twistd. tap).]'
    {-s,--source}'[Load an Application from the given . tas (AOT Python source) file.]'
    {-y,--python}'[Use the variable "application" from the given Python file.]'
    '--syslog[Log to syslog instead of a file.]'
    '--version[Print version information and exit.]'
    {-n,--nodaemon}'[Don\t daemonize (stay in foreground).]'
    '--prefix[Use the specified prefix when logging to logfile. Default is "twisted".]:prefix'
    '*:filename:_files'
)
_arguments -s $arguments
