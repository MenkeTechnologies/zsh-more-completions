#compdef kismetdb_to_wiglecsv

# Autogenerated from man page /usr/share/man/man1/kismetdb_to_wiglecsv.1.gz

local arguments

arguments=(
    {-i,--in}'[Input kismetdb file.]':filename
    {-o,--out}'[Output Wigle CSV file.]':filename
    {-f,--force}'[Force writing to the target file, even if it exists.]'
    {-r,--rate-limit}'[Limit updated records to one update per \[rate\] seconds per device.]':rate
    {-c,--cache-limit}'[Maximum number of device to cache, defaults to 1000.]':limit
    {-v,--verbose}'[Verbose output.]'
    {-s,--skip-clean}'[Don\t clean (sql vacuum) input database.]'
    {-e,--exclude}'[Exclude records within \dist\ *meters* of the lat,lon provided.]':latlondist
    '*:filename:_files'
)
_arguments -s $arguments
