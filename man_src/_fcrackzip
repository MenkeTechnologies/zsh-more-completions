#compdef fcrackzip

# Autogenerated from man page /usr/share/man/man1/fcrackzip.1.gz

local arguments

arguments=(
    '(- * :)'{-h,--help}'[Prints the version number and (hopefully) some helpful insights.]'
    {-v,--verbose}'[Each -v makes the program more verbose.]'
    {-b,--brute-force}'[Select brute force mode.]'
    {-D,--dictionary}'[Select dictionary mode.]'
    {-c,--charset}'[Select the characters to use in brute-force cracking. Must be one of .]':characterset-specification
    {-p,--init-password}'[Set initial (starting) password for brute-force searching to string, or use t…]':string
    {-l,--length}'[Use an initial password of length min, and check all passwords up to password…]':min-max
    {-u,--use-unzip}'[Try to decompress the first file by calling unzip with the guessed password.]'
    {-m,--method}'[Use method number "name" instead of the default cracking method.]':name
    {-2,--modulo}'[Calculate only r/m of the password. Not yet supported.]':rm
    {-B,--benchmark}'[Make a small benchmark, the output is nearly meaningless.]'
    {-V,--validate}'[Make some basic checks whether the cracker works.]'
    '*:filename:_files'
)
_arguments -s $arguments
