#compdef ipfwpcap

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/ipfwpcap.8.gz

local arguments

arguments=(
    '-d[Turns on extra debugging messages.]'
    '-r[Writes packets back to the divert 4 socket.]'
    '--rr[Indicates that it is okay to quit if maxbytes or maxpkts are reached.]'
    '-b[Stop dumping after maxbytes bytes.]'
    '-p[Stop dumping after maxpkt packets.]'
    '-P[File to store PID number in. Default is /var/run/ipwfpcap. portnr. pid.]'
    '*:filename:_files'
)
_arguments -s $arguments
