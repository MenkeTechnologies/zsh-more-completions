    '-u[Username, if omitted null session assumed.]:USERNAME'
    '-p[Password or NTLM hash.]:PASSWORD'
    '-s[Specify a share (default C$), ex \C$\.]:SHARE'
    '-d[Domain name (default WORKGROUP).]:DOMAIN'
    '-P[SMB port (default 445) . SS "Command Execution:".]:PORT'
    '-x[Execute a command ex. \ipconfig /all\ . SS "Filesystem Search:".]:COMMAND'
    '-L[List all drives on the specified host.]'
    '-R[Recursively list dirs, and files (no sharepath lists ALL shares), ex.]:PATH'
    '-r[List contents of directory, default is to list root of all shares, ex.]:PATH'
    '-A[Define a file name pattern (regex) that auto downloads a file on a match (req…]:PATTERN'
    '-q[Disable verbose output.]'
    '--depth[Traverse a directory tree to a specific depth . SS "File Content Search:".]:DEPTH'
    '-F[File content search, -F \\[Pp\]assword\ (requries admin access to execute comma…]:PATTERN'
    '--search-path[Specify drive/path to search (used with -F, default C:Users), ex \D:HR\ .]:PATH'
    '--download[Download a file from the remote system, ex. \C$temppasswords. txt\.]:PATH'
    '--upload[Upload a file to the remote system ex. \/tmp/payload. exe C$temppayload.]:DST'
    '--delete[Delete a remote file, ex. \C$tempmsf. exe\.]'
    '--skip[Skip delete file confirmation prompt . SS "Optional arguments:".]'
    '(- * :)'{-h,--help}'[show help message and exit EXAMPLES smbmap -u jsmith -p password1 -d workgrou…]'
    '*:filename:_files'
