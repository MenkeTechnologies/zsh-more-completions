#compdef htpasswd

# Autogenerated from man page /usr/share/man/man1/htpasswd.1.gz

local arguments

arguments=(
    '-b[Use batch mode; i. e.]'
    '-i[Read the password from stdin without verification (for script usage).]'
    '-c[Create the passwdfile.]'
    '-n[Display the results on standard output rather than updating a file.]'
    '-m[Use MD5 encryption for passwords. This is the default (since version 2. 2.]'
    '-B[Use bcrypt encryption for passwords.]'
    '-C[This flag is only allowed in combination with -B (bcrypt encryption).]'
    '-d[Use crypt() encryption for passwords.]'
    '-s[Use SHA encryption for passwords.]'
    '-p[Use plaintext passwords.]'
    '-D[Delete user.]'
    '-v[Verify password.]'
    '*:filename:_files'
)
_arguments -s $arguments
