local arguments

arguments=(
    '--cflags[Print the compiler flags that are necessary to compile a program or library t…]'
    '--exec-prefix[If specified, use DIR instead of the installation exec prefix that libcaca w…]:DIR'
    '--libs[Print the linker flags that are necessary to link a program or library that u…]'
    '--prefix[If specified, use PREFIX instead of the installation prefix that libcaca was …]:DIR'
    '--version[Prints the currently installed version of libcaca on standard output.]'
