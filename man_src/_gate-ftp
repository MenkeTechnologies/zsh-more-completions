#compdef gate-ftp

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/gate-ftp.1.gz

local arguments

arguments=(
    '-4[Forces gate-ftp to only use IPv4 addresses.]'
    '-6[Forces gate-ftp to only use IPv6 addresses.]'
    '-A[Force active mode ftp.]'
    '-a[Causes gate-ftp to bypass normal login procedure, and use an anonymous login …]'
    '-d[Enables debugging.]'
    '-e[Disables command line editing. This is useful for Emacs ange-ftp mode.]'
    '-f[Forces a cache reload for transfers that go through the FTP or HTTP proxies.]'
    '-g[Disables file name globbing.]'
    '-i[Turns off interactive prompting during multiple file transfers.]'
    '-N[Use netrc instead of ~/. netrc. Refer to THE .]'
    '-n[Restrains gate-ftp from attempting auto-login upon initial connection for non…]'
    '-o[When auto-fetching files, save the contents in output.]'
    '-P[Sets the port number to port.]'
    '-p[Enable passive mode operation for use behind connection filtering firewalls.]'
    '-q[Quit if the connection has stalled for quittime seconds.]'
    '-R[Restart all non-proxied auto-fetches.]'
    '-r[Retry the connection attempt if it failed, pausing for wait seconds.]'
    '-s[Uses srcaddr as the local IP address for all connections.]'
    '-t[Enables packet tracing.]'
    '-T[Set the maximum transfer rate for direction to maximum bytes/second, and if s…]'
    '-u[Upload files on the command line to URL where URL is one of the ftp URL types…]'
    '-V[Disable verbose and progress, overriding the default of enabled when output i…]'
    '-v[Enable verbose and progress.]'
    '*:filename:_files'
)
_arguments -s $arguments
