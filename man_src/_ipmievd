local arguments

arguments=(
    '-a[Prompt for the remote server password.]'
    '-A[Specify an authentication type to use during IPMIv1. 5 lan session activation.]:<authtype>'
    '-c[Present output in CSV (comma separated variable) format.]'
    '-C[The remote server authentication, integrity, and encryption algorithms to use…]:<ciphersuite>'
    '-E[The remote server password is specified by the environment variable IPMI_PASS…]'
    '-f[Specifies a file containing the remote server password.]:<password_file>'
    '-h[Get basic usage help from the command line.]'
    '-H[Remote server address, can be IP address or hostname.]:<address>'
    '-I[Selects IPMI interface to use.]:<interface>'
    '-L[Force session privilege level. Can be CALLBACK, USER, OPERATOR, ADMIN.]:<privlvl>'
    '-m[Set the local IPMB address.]:<local_address>'
    '-o[Select OEM type to support.]:<oemtype>'
    '-O[Open selected file and read OEM SEL event descriptions to be used during SEL …]:oem>'
    '-p[Remote server UDP port to connect to. Default is 623.]:<port>'
    '-P[Remote server password is specified on the command line.]:<password>'
    '-S[Use local file for remote SDR cache.]:<sdr_cache_file>'
    '-t[Bridge IPMI requests to the remote target address.]:<target_address>'
    '-U[Remote server username, default is NULL user.]:<username>'
    '-d[Use device number N to specify the /dev/ipmiN (or /dev/ipmi/N or /dev/ipmide…]:N'
    '-v[Increase verbose output level.]'
    '*:filename:_files'
)
