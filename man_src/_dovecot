#compdef dovecot

# Autogenerated from man page /usr/share/man/man1/dovecot.1.gz

local arguments

arguments=(
    '-a[Dump all configuration settings to stdout and exit successfully.]'
    '-c[Start dovecot with an alternative configuration.]:config-file'
    '-F[Run dovecot in foreground, do not daemonize.]'
    '-n[Dump non-default settings to stdout and exit successfully.]'
    '-p[Prompt for the ssl key password for the configured ssl_key on startup.]'
    '--build-options[Show Dovecot\\(aqs build options and exit successfully.]'
    '--help[Print a usage message to stdout and exit successfully.]'
    '--hostdomain[Shows the current R host . domain name of the system.]'
    '--version[Show Dovecot\s version and exit successfully.]'
    '*:filename:_files'
)
_arguments -s $arguments
