#compdef hciattach

# Autogenerated from man page /usr/share/man/man1/hciattach.1.gz

local arguments

arguments=(
    '-b[Send break.]'
    '-n[Don\t detach from controlling terminal.]'
    '-p[Print the PID when detaching.]'
    '-t[Specify an initialization timeout. (Default is 5 seconds. ).]:timeout"'
    '-s[Specify an initial speed instead of the hardware default.]:speed"'
    '-l[List all available configurations.]'
    '-r[Set the HCI device into raw mode (the kernel and bluetoothd will ignore it).]'
    '*:filename:_files'
)
_arguments -s $arguments
