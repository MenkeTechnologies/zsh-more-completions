#compdef yum-debug-restore

# Autogenerated from man page /usr/share/man/man1/yum-debug-restore.1.gz

local arguments

arguments=(
    '--output[Output the commands that would be run to stdout.]'
    '--shell[Output the commands that would be run to a file.]:file'
    '--install-latest[Ask yum to install the latest version of the given packages, instead of the v…]'
    '--ignore-arch[Ignore the architecture of the packages, so you can "restore" an i386 debug-d…]'
    '--filter-types[Only perform the given types of commands, so you can filter to just upgrades …]:installremoveupdatedowngrade'
    '*:filename:_files'
)
_arguments -s $arguments
