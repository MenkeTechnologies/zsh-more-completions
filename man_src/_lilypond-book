#compdef lilypond-book

# Autogenerated from man page /usr/share/man/man1/lilypond-book.1.gz

local arguments

arguments=(
    {-F,--filter}'[pipe snippets through FILTER \[default: `convert-ly -n -\\].]':FILTER
    {-f,--format}'[use output format FORMAT (texi \[default\], texi-html, latex, html, docbook).]':FORMAT
    '(- * :)'{-h,--help}'[show this help and exit.]'
    {-I,--include}'[add DIR to include path.]':DIR
    '--info-images-dir[format Texinfo output so that Info will look for images of music in DIR.]:DIR'
    '--left-padding[pad left side of music to align music inspite of uneven bar numbers (in mm).]:PAD'
    '--lily-loglevel[Print lilypond log messages according to LOGLEVEL.]:LOGLEVEL'
    '--lily-output-dir[write lily-XXX files to DIR, link into --output dir.]:DIR'
    '--load-custom-package[Load the additional python PACKAGE (containing e. g. a custom output format).]:PACKAGE'
    {-l,--loglevel}'[Print log messages according to LOGLEVEL (NONE, ERROR, WARNING, PROGRESS (def…]':LOGLEVEL
    {-o,--output}'[write output to DIR.]':DIR
    {-P,--process}'[process ly_files using COMMAND FILE.]':COMMAND
    '--redirect-lilypond-output[Redirect the lilypond output.]'
    {-s,--safe}'[Compile snippets in safe mode.]'
    '--skip-lily-check[do not fail if no lilypond output is found.]'
    '--skip-png-check[do not fail if no PNG images are found for EPS files.]'
    '--use-source-file-names[write snippet output files with the same base name as their source file.]'
    {-V,--verbose}'[be verbose.]'
    '--version[show version number and exit.]'
    {-w,--warranty}'[show warranty and copyright .]'
    '--latex-program[run executable PROG instead of latex, or in case --pdf option is set instead …]:PROG'
    '--texinfo-program[run executable PROG instead of texi2pdf.]:PROG'
    '--pdf[create PDF files for use with PDFTeX]'
    '*:filename:_files'
)
_arguments -s $arguments
