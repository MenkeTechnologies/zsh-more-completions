    '-H[See \\-host.]'
    '-h[See \\-help.]'
    '--\\-help[Show help.]'
    '--\\-host[Set destination host. Default is "localhost".]'
    '--\\-mount[Set mountpoint. Default is "/example. ogg".]'
    '-P[See \\-port.]'
    '--\\-pass[Set source password. Default is "hackme".]'
    '--\\-port[Set destination port. Symbolic names are supported. Default is 8000.]'
    '--\\-proto[Set protocol. Default is "http".]'
    '--\\-station-description[Set stream description.]'
    '--\\-station-genre[Set stream genre.]'
    '--\\-station-meta[Set meta information. Both key and value are arbitrary strings.]'
    '--\\-station-name[Set stream name.]'
    '--\\-station-url[Set stream URL.]'
    '--\\-tls-mode[Set TLS mode.]'
    '--\\-usage[Set stream usages.]'
    '--\\-user[Set source user. Default is "source".]'
    '*:filename:_files'
)
