#compdef pap

# Autogenerated from man page /usr/share/man/man1/pap.1.gz

local arguments

arguments=(
    '-A[Connect to the printer with Appletalk address address and do not consult the .]:address'
    '-c[Take cuts.]'
    '-d[Enable debug output.]'
    '-e[Send any message from the printer to stderr instead of stdout.]'
    '-E[Don\t wait for EOF from the printer.]'
    '-p[Connect to the printer named nbpname and do not consult the .]:nbpname'
    '-s[Update the file called statusfile to contain the most recent status message f…]:statusfile'
    '-w[Wait for the printer status to contain the word "waiting" before sending the …]'
    '-W[Wait for the printer status to contain the word "idle" before sending the job.]'
    '*:filename:_files'
)
_arguments -s $arguments
