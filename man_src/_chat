local arguments

arguments=(
    '-f[Read the chat script from the chat file.]:file>'
    '-t[Set the timeout for the expected string to be received.]:<timeout>'
    '-r[Set the file for output of the report strings.]:file>'
    '-e[Start with the echo option turned on.]'
    '-E[Enables environment variable substituion within chat scripts using the standa…]'
    '-v[Request that the chat script be executed in a verbose mode.]'
    '-V[Request that the chat script be executed in a stderr verbose mode.]'
    '-s[Use stderr.]'
    '-S[Do not use the SYSLOG. By default, error messages are sent to the SYSLOG.]'
    '-T[Pass in an arbitary string, usually a phone number, that will be substituted …]:number>'
    '-U[Pass in a second string, usually a phone number, that will be substituted for…]'
    '*:filename:_files'
)
