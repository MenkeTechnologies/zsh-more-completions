#compdef zvbi-atsc-cc

# Autogenerated from man page /usr/share/man/man1/zvbi-atsc-cc.1.gz

local arguments

arguments=(
    {-?,-h,--help,--usage}'[Print a short usage message.]'
    {-q,--quiet}'[Suppress all progress and error messages.]'
    {-v,--verbose}'[Increase verbosity. SS "Device options".]'
    '--adapter-num[PD Select a different DVB device to capture the signal (/dev/dvb/adapterN).]:number'
    '--frontend-id[PD Select a different frontend on the DVB device (/dev/dvb/adapterN/frontendM…]:number'
    '--demux-id[PD Select a different demultiplexer on the DVB device (/dev/dvb/adapterN/demu…]:number'
    '--dvr-id[PD Select a different DVR interface on the DVB device (/dev/dvb/adapterN/dvrM…]:number'
    '--ts[PD Decode an MPEG-2 Transport Stream on standard input instead of opening a D…]'
    '--channel-conf[PD To tune in a TV station zvbi-atsc-cc needs a channel configuration file.]:name'
    {-L,--list}'[List all stations in the channel configuration file and their transponder fre…]'
    '--station[PD The station to tune in. Usually the -n can be omitted.]:name'
    {-c,--cc}'[Print any received caption on standard output.]'
    '--cc-file[PD Append any received caption to the specified file.]:name'
    '--channel[PD Print NTSC Closed Caption channel 1, 2, 3 or 4 on standard output.]:number'
    {--cc1-file,--cc4-file}'[PD Filter out NTSC Closed Caption channel CC1 .]'
    {--t1-file,--t4-file}'[PD Filter out NTSC Text service T1 .]'
    {--s1-file,--s2-file}'[PD Filter out DTVCC service 1 or 2 and append the text to the specified file.]'
    {-b,--no-webtv}'[Do not print WebTV links.]'
    {-m,--timestamps}'[Prepend timestamps YYYYMMDDHHMMSS to caption lines.]'
    {--Sdate,--stream-time}'[PD Prepend timestamps YYYYMMDDHHMMSS to caption lines generated from the time…]':date
    {-s,--sentences}'[Print caption one sentence at a time.]'
    '--format[PD When type is "plain" zvbi-atsc-cc prints caption and XDS text without any…]:type'
    {-p,--plain}'[Same as -jplain . SS "XDS options".]'
    {-x,--xds}'[Print all received XDS data on standard output.]'
    '--xds-file[PD Filter out eXtended Data Service data (station name, program name, program…]:name'
    '-a[.]:number'
    '-i[.]:number'
    '-d[.]:number'
    '-r[.]:number'
    '-T[.]'
    '--atsc[option to clarify that this is an ATSC TS and the program should expect an az…]'
    '-e[.]:name'
    '-n[.]:name'
    '-C[.]:name'
    '-l[.]:number'
    {-1,-4}'[.]'
    {-5,-8}'[.]'
    {-9,-0}'[.]'
    '-S[.]'
    '-j[.]:type'
    '-X[.]:name'
    '-f[.]:typetype*'
    '--filter[.]:typetype*'
    '*:filename:_files'
)
_arguments -s $arguments
