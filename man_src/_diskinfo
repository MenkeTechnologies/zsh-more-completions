#compdef diskinfo

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/diskinfo.8.gz

local arguments

arguments=(
    '-v[Print fields one per line with a descriptive comment.]'
    '-c[Perform a simple measurement of the I/O read command overhead.]'
    '-i[Perform a simple IOPS benchmark.]'
    '-p[Return the physical path of the disk.]'
    '-s[Return the disk ident, usually the serial number.]'
    '-S[Perform synchronous random write test (ZFS SLOG test), measuring time require…]'
    '-t[Perform a simple and rather naive benchmark of the disks seek and transfer pe…]'
    '-w[Allow disruptive write tests.]'
    '*:filename:_files'
)
_arguments -s $arguments
