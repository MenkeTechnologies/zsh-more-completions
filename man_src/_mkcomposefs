#compdef mkcomposefs

# Autogenerated from man page /usr/share/man/man1/mkcomposefs.1.gz

local arguments

arguments=(
    '--digest-store[This path will become a composefs "object store".]:PATH'
    '--print-digest[Print the fsverity digest of the composefs metadata file.]'
    '--print-digest-only[Print the fsverity digest of the composefs metadata file, but don\t write the…]'
    '--use-epoch[Use a zero time (unix epoch) as the modification time for all files.]'
    '--skip-devices[Don\t add device nodes to the image.]'
    '--skip-xattrs[Don\t add xattrs to files in the image.]'
    '--user-xattrs[Only add xattrs with the "user. " prefix to files in the image.]'
    '--from-file[The source is a file in the composefs-dump(5) format.]'
    '--version[The base version to use for the image format.]'
    '--max-version[If this specifies a version higher than --version, then the actual image form…]'
    '--threads[Number of threads to be used to calculate the file digests and copy.]:count'
    '*:filename:_files'
)
_arguments -s $arguments
