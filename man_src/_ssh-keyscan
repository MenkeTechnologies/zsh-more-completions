#compdef ssh-keyscan

# Autogenerated from man page /usr/share/man/man1/ssh-keyscan.1.gz

local arguments

arguments=(
    '-4[Forces ssh-keyscan to use IPv4 addresses only.]'
    '-6[Forces ssh-keyscan to use IPv6 addresses only.]'
    '-c[Request certificates from target hosts instead of plain keys.]'
    '-f[Read hosts or addrlist namelist pairs from file, one per line.]'
    '-H[Hash all hostnames and addresses in the output.]'
    '-p[Port to connect to on the remote host.]'
    '-T[Set the timeout for connection attempts.]'
    '-t[Specifies the type of the key to fetch from the scanned hosts.]'
    '-v[Verbose mode.]'
    '*:filename:_files'
)
_arguments -s $arguments
