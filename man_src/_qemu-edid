#compdef qemu-edid

local arguments

arguments=(
  '-h[print this text]'
  '-o[set output file (stdout by default)]'
  '-v[set monitor vendor (three letters)]'
  '-n[set monitor name]'
  '-s[set monitor serial]'
  '-d[set display resolution]'
  '-x[set preferred width]'
  '-y[set preferred height]'
  '-X[set maximum width]'
  '-Y[set maximum height]'
  '*:filename:_files'
)

_arguments -s $arguments
