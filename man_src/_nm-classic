local arguments

arguments=(
    '-a[Display all symbol table entries, including those inserted for use by debugg…]'
    '-g[Display only global (external) symbols.]'
    '-n[Sort numerically rather than alphabetically.]'
    '-o[Prepend file or archive element name to each output line, rather than only o…]'
    '-p[Don\t sort; display in symbol-table order.]'
    '-r[Sort in reverse order.]'
    '-u[Display only undefined symbols.]'
    '-U[Don\t display undefined symbols.]'
    '-m[Display the N_SECT type symbols (Mach-O symbols) as .]'
    '-x[Display the symbol table entry\s fields in hexadecimal, along with the name a…]'
    '-j[Just display the symbol names (no value or type).]'
    '-s[List only those symbols in the section (segname,sectname).]'
    '-l[List a pseudo symbol ".]'
    '--arch[Specifies the architecture, arch_type, of the file for R nm (1) to operate o…]:arch_type"'
    '-f[For R llvm-nm (1) this specifies the output format.]:format"'
    '-A[Write the pathname or library name of an object on each line.]'
    '-P[Write information in a portable output format.]'
    '-t[For the -P output, write the numeric value in the specified format.]:format"'
    '--help[option.]'
    '-T[option used on the object, that can be different than the dyld information.]'
    '-L[Display the symbols in the bitcode files in the (__LLVM,__bundle) section if …]'
    '*:filename:_files'
)
