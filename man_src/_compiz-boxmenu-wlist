local arguments

arguments=(
    '-h[Show help options.]'
    {-i,--images}'[Use IMAGES in the window list.]'
    {-m,--minimized}'[Show only MINIMIZED windows in the window list.]'
    {-c,--current-viewport}'[Show only windows in CURRENT VIEWPORT in the window list.]'
    '*:filename:_files'
)
_arguments -s $arguments
