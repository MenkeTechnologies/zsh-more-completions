#compdef bladeRF-cli

# Autogenerated from man page /usr/share/man/man1/bladeRF-cli.1.gz

local arguments

arguments=(
    {-d,--device}'[Use the specified bladeRF device.]':device
    {-f,--flash-firmware}'[Write the provided FX3 firmware file to flash.]':file
    {-l,--load-fpga}'[Load the provided FPGA bitstream.]':file
    {-L,--flash-fpga}'[Write the provided FPGA image to flash for autoloading.]':file
    {-p,--probe}'[Probe for devices, print results, then exit.]'
    {-e,--exec}'[Execute the specified interactive mode command.]':command
    {-s,--script}'[Run provided script.]':file
    {-i,--interactive}'[Enter interactive mode.]'
    '--lib-version[Print libbladeRF version and exit.]'
    {-v,--verbosity}'[Set the libbladeRF verbosity level.]':level
    '--version[Print CLI version and exit.]'
    '(- * :)'{-h,--help}'[Show this help text.]'
    '--help-interactive[Print help information for all interactive commands. SS "Notes:".]'
    '*:filename:_files'
)
_arguments -s $arguments
