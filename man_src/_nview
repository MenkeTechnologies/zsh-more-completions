#compdef nview

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/nview.1.gz

local arguments

arguments=(
    '-c[Execute cmd on the first file loaded.]'
    '-e[Start editing in ex mode, as if the command name were nview ex.]'
    '-F[Don\t copy the entire file when first starting to edit.]'
    '-R[Start editing in read-only mode, as if the command name was nview view, or th…]'
    '-r[Recover the specified files, or, if no files are specified, list the files th…]'
    '-S[Run with the secure edit option set, disallowing all access to external progr…]'
    '-s[Enter batch mode; applicable only to nview ex edit sessions.]'
    '-t[Start editing at the specified tag see Xr ctags 1.]'
    '-v[Start editing in vi mode, as if the command name was nview vi.]'
    '-w[Set the initial window size to the specified number of lines.]'
    '*:filename:_files'
)
_arguments -s $arguments
