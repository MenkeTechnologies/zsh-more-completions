#compdef vis

# Autogenerated from man page /usr/share/man/man1/vis.1

local arguments

arguments=(
    '-b[Turns off prepending of backslash before up-arrow control sequences and meta …]'
    '-c[Request a format which displays a small subset of the non-printable character…]'
    '-F[Causes vis to fold output lines to foldwidth columns (default 80), like fold …]'
    '-f[Same as F.]'
    '-l[Mark newlines with the visible sequence \\e$, followed by the newline.]'
    '-n[Turns off any encoding, except for the fact that backslashes are still double…]'
    '-o[Request a format which displays non-printable characters as an octal number, …]'
    '-s[Only characters considered unsafe to send to a terminal are encoded.]'
    '-t[Tabs are also encoded.]'
    '-w[White space (space-tab-newline) is also encoded.]'
    '*:filename:_files'
)
_arguments -s $arguments
