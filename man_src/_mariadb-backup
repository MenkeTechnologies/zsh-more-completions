#compdef mariadb-backup

local arguments

arguments=(
  '--print-defaults[print the program argument list and exit.]'
  '--no-defaults[dont read default options from any option file.]'
  '--defaults-file[only read default options from the given file #.]'
  '--defaults-extra-file[read this file after the global files are read.]'
  '--defaults-group-suffix[additionally read default groups with # appended as a suffix.]'
  {-V,--verbose}'[display verbose output]'
  {-v,--version}'[print version information]'
  '--target-dir[destination directory]'
  '--backup[take backup to target-dir]'
  '--prepare[prepare a backup for starting mysql server on the backup.]'
  '--export[create files to import to another database when prepare.]'
  '--print-param[print parameter of mysqld needed for copyback.]'
  '--use-memory[the value is used instead of buffer_pool_size]'
  '--throttle[limit count of IO operations (pairs of read&write) per]'
  '--log[ignored option for MySQL option compatibility]'
  '--log-copy-interval[time interval between checks done by log copying thread]'
  '--extra-lsndir[(for --backup): save an extra copy of the]'
  '--incremental-lsn[(for --backup): copy only .ibd pages newer than specified]'
  '--incremental-basedir[(for --backup): copy only .ibd pages newer than backup at]'
  '--incremental-dir[(for --prepare): apply .delta files and logfile in the]'
  '--tables[filtering by regexp for table names.]'
  '--tables-file[filtering by list of the exact database.table name in the]'
  '--databases[filtering by list of databases.]'
  '--databases-file[filtering by list of databases in the file.]'
  '--tables-exclude[filtering by regexp for table names. Operates the same]'
  '--tables[.]'
  '--databases-exclude[excluding databases based on name, Operates the same way]'
  '--databases[.]'
  '--stream[stream all backup files to the standard output in the]'
  '--compress[compress individual backup files using the specified]'
  '--compress-threads[number of threads for parallel data compression. The]'
  '--compress-chunk-size[size of working buffer(s) for compression threads in]'
  '--incremental-force-scan[perform a full-scan incremental backup even in the]'
  '--close-files[do not keep files opened. Use at your own risk.]'
  '--core-file[write core on fatal signals]'
  '--copy-back[copy all the files in a previously made backup from the]'
  '--move-back[move all the files in a previously made backup from the]'
  '--galera-info[this options creates the xtrabackup_galera_info file]'
  '--slave-info[this option is useful when backing up a replication slave]'
  '--no-lock[use this option to disable table lock with "FLUSH TABLES]'
  '--safe-slave-backup[slave SQL thread and wait to start backup until]'
  '--safe-slave-backup-timeout[. The slave SQL thread]'
  '--rsync[uses the rsync utility to optimize local file transfers.]'
  '--force-non-empty-directories[this option, when specified, makes --copy-back or]'
  '--move-back[files to non-empty directories. Note]'
  {--copy-back,--move-back}'[to copy a file from the]'
  '--no-version-check[this option disables the version check which is enabled]'
  '--no-backup-locks[this option controls if backup locks should be used]'
  '--decompress[decompresses all files with the .qp extension in a backup]'
  {-u,--user}'[this option specifies the MySQL username used when]'
  {-H,--host}'[this option specifies the host to use when connecting to]'
  {-P,--port}'[this option specifies the port to use when connecting to]'
  {-p,--password}'[this option specifies the password to use when connecting]'
  '--help[details.]'
  '--protocol[the protocol to use for connection (tcp, socket, pipe]'
  {-S,--socket}'[this option specifies the socket to use when connecting]'
  '--incremental-history-name[this option specifies the name of the backup series]'
  '--incremental[.]'
  '--incremental-history-uuid[this option specifies the UUID of the specific history]'
  {--incremental-history-name,--incremental-basedir}'[]'
  '--incremental-lsn[. If no valid lsn can be found (no]'
  '--remove-original[remove .qp files after decompression.]'
  '--ftwrl-wait-query-type[this option specifies which types of queries are allowed]'
  '--kill-long-query-type[this option specifies which types of queries should be]'
  '--history[this option enables the tracking of backup history in the]'
  '--kill-long-queries-timeout[this option specifies the number of seconds innobackupex]'
  '--ftwrl-wait-timeout[this option specifies time in seconds that innobackupex]'
  '--ftwrl-wait-threshold[this option specifies the query run time threshold which]'
  '--safe-slave-backup-timeout[how many seconds --safe-slave-backup should wait for]'
  '--binlog-info[this option controls how backup should retrieve servers]'
  '--secure-auth[refuse client connecting to server if it uses old]'
  '--log-innodb-page-corruption[continue backup if innodb corrupted pages are found. The]'
  '--ssl[enable SSL for connection (automatically enabled with]'
  '--ssl-ca[CA file in PEM format (check OpenSSL docs, implies]'
  '--ssl[).]'
  '--ssl-capath[CA directory (check OpenSSL docs, implies --ssl).]'
  '--ssl-cert[X509 cert in PEM format (implies --ssl).]'
  '--ssl-cipher[SSL cipher to use (implies --ssl).]'
  '--ssl-key[X509 key in PEM format (implies --ssl).]'
  '--ssl-crl[certificate revocation list (implies --ssl).]'
  '--ssl-crlpath[certificate revocation list path (implies --ssl).]'
  '--tls-version[TLS protocol version for secure connection.]'
  '--ssl-verify-server-cert[verify servers "Common Name" in its cert against]'
  {-h,--datadir}'[path to the database root.]'
  {-t,--tmpdir}'[path for temporary files. Several paths may be specified]'
  '--parallel[number of threads to use for parallel datafiles transfer.]'
  '--extended-validation[enable extended validation for Innodb data pages during]'
  '--encrypted-backup[in --backup, assume that nonzero key_version implies that]'
  '--skip-encrypted-backup[allow copying unencrypted that]'
  '--log[ignored option for MySQL option compatibility]'
  '--log-bin[base name for the log sequence]'
  '--innodb[ignored option for MySQL option compatibility]'
  '--innodb-adaptive-hash-index[enable InnoDB adaptive hash index (disabled by default).]'
  '--innodb-autoextend-increment[data file autoextend increment in megabytes]'
  '--innodb-data-file-path[path to individual files and their sizes.]'
  '--innodb-data-home-dir[the common part for InnoDB table spaces.]'
  '--innodb-doublewrite[enable InnoDB doublewrite buffer during --prepare.]'
  '--innodb-io-capacity[number of IOPs the server can do. Tunes the background IO]'
  '--innodb-file-io-threads[number of file I/O threads in InnoDB.]'
  '--innodb-read-io-threads[number of background read I/O threads in InnoDB.]'
  '--innodb-write-io-threads[number of background write I/O threads in InnoDB.]'
  '--innodb-file-per-table[stores each InnoDB table to an .ibd file in the database]'
  '--innodb-flush-method[with which method to flush data.. One of: fsync, O_DSYNC]'
  '--innodb-log-buffer-size[the size of the buffer which InnoDB uses to write log to]'
  '--innodb-log-file-size[ignored for mysqld option compatibility]'
  '--innodb-log-files-in-group[ignored for mysqld option compatibility]'
  '--innodb-log-group-home-dir[path to InnoDB log files.]'
  '--innodb-max-dirty-pages-pct[percentage of dirty pages allowed in bufferpool.]'
  '--innodb-use-native-aio[use native AIO if supported on this platform.]'
  '--innodb-page-size[the universal page size of the database.]'
  '--innodb-buffer-pool-filename[ignored for mysqld option compatibility]'
  '--innodb-checksum-algorithm[the algorithm InnoDB uses for page checksumming. \[CRC32]'
  '--innodb-undo-directory[directory where undo tablespace files live, this path can]'
  '--innodb-undo-tablespaces[number of undo tablespaces to use.]'
  '--innodb-compression-level[compression level used for zlib compression.]'
  '--defaults-group[defaults group in config file (default "mysqld").]'
  '--plugin-dir[server plugin directory. Used to load encryption plugin]'
  '--open-files-limit[the maximum number of file descriptors to reserve with]'
  '--lock-ddl-per-table[lock DDL for each table before backup starts to copy it]'
  '--rocksdb-datadir[rocksDB data directory.This option is only  used with]'
  {--copy-back,--move-back}'[]'
  '--rocksdb-backup[backup rocksdb data, if rocksdb plugin is installed.Used]'
  '--check-privileges[check database user privileges fro the backup user]'
  '--innodb-force-recovery[(for --prepare): Crash recovery mode (ignores page]'
  '--mysqld-args[all arguments that follow this argument are considered as]'
  '*:filename:_files'
)

_arguments -s $arguments
