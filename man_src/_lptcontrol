#compdef lptcontrol

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/lptcontrol.8.gz

local arguments

arguments=(
    '-e[Turn on extended mode.]'
    '-i[Turn on interrupt-driven mode.]'
    '-p[Turn on polled mode.]'
    '-s[Turn on standard mode, i. e. , turn off extended mode.]'
    '-d[Set the mode of the printer control device specified by control_device.]'
    '*:filename:_files'
)
_arguments -s $arguments
