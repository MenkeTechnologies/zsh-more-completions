#compdef pidof

# Autogenerated from man page /usr/share/man/man8/pidof.8.gz

local arguments

arguments=(
    '-s[Single shot - this instructs the program to only return one pid.]'
    '-c[Only return process ids that are running with the same root directory.]'
    '-n[Avoid stat (2) system function call on all binaries which are located on net…]'
    '-x[Scripts too - this causes the program to also return process id\s of shells r…]'
    '-o[Tells pidof to omit processes with that process id.]:omitpid'
    '*:filename:_files'
)
_arguments -s $arguments
