arguments=(
    '-I[option of xfsdump displays the inventory contents hierarchically.]'
    '-F[Don\t prompt the operator.]'
    '-i[Interactive mode.]'
    '-M[Prunes dump sessions identified by the given mount point which were created p…]:mmddyyyy'
    '-u[Like -M, except the matching filesystem is specified using its universally un…]:mmddyyyy'
    '-m[If specified, only sessions with at least one media file whose label matches …]:media_label'
    '-s[Prunes the dump session identified by the given session id.]:SESSION_ID'
    '-C[With this option, xfsinvutil performs consistency checks for all entries in…]'
    '*:filename:_files'
)
