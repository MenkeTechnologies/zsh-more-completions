#compdef coredumpctl

# Autogenerated from man page /usr/share/man/man1/coredumpctl.1.gz

local arguments

arguments=(
    '--no-legend[Do not print column headers.]'
    '-1[Show information of a single coredump only, instead of listing all known core…]'
    {-F,--field}'[Print all possible data values the specified field takes in matching coredump…]'
    {-o,--output}'[Write the core to FILE.]':FILE
    {-h,--help}'[Print a short help text and exit.]'
    '--version[Print a short version string and exit.]'
    '--no-pager[Do not pipe output into a pager.]'
    '--o/--output[gdb.]'
    '*:filename:_files'
)
_arguments -s $arguments
