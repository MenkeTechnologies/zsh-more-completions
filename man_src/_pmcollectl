    '-h[.]:host'
    {-c,--count}'[.]':samples
    {-f,--filename}'[.]':filename
    {-i,--interval}'[.]':interval
    {-R,--runtime}'[.]':duration
    {-p,--playback}'[.]':filename
    '--help[.]'
