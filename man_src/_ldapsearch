#compdef ldapsearch

# Autogenerated from man page /usr/share/man/man1/ldapsearch.1

local arguments

arguments=(
    '-n[Show what would be done, but don\t actually perform the search.]'
    '-c[Continuous operation mode.]'
    '-u[Include the User Friendly Name form of the Distinguished Name (DN) in the out…]'
    '-v[Run in verbose mode, with many diagnostics written to standard output.]'
    '-t[A single -t writes retrieved non-printable values to a set of temporary files.]'
    '-T[Write temporary files to directory specified by path (default: /var/tmp/).]:path'
    '-F[URL prefix for temporary files.]:prefix'
    '-A[Retrieve attributes only (no values).]'
    '-L[Search results are display in LDAP Data Interchange Format detailed in ldif …]'
    '-M[Enable manage DSA IT control. -MM makes control critical.]'
    '-S[Sort the entries returned based on attribute.]:attribute'
    '-d[Set the LDAP debugging level to debuglevel.]:debuglevel'
    '-f[Read a series of lines from file, performing one LDAP search for each line.]:file'
    '-x[Use simple authentication instead of SASL.]'
    '-D[Use the Distinguished Name binddn to bind to the LDAP directory.]:binddn'
    '-W[Prompt for simple authentication.]'
    '-w[Use passwd as the password for simple authentication.]:passwd'
    '-y[Use complete contents of passwdfile as the password for simple authentication.]:passwdfile'
    '-H[Specify URI(s) referring to the ldap server(s); a list of URI, separated by w…]:ldapuri'
    '-h[Specify an alternate host on which the ldap server is running.]:ldaphost'
    '-p[Specify an alternate TCP port where the ldap server is listening.]:ldapport'
    '-b[Use searchbase as the starting point for the search instead of the default.]:searchbase'
    '-s[Specify the scope of the search to be one of base , one , sub , or childr…]'
    '-a[Specify how aliases dereferencing is done.]'
    '-P[Specify the LDAP protocol version to use.]'
    '-E[Specify general extensions with -e and search extensions with -E.]'
    '-l[wait at most timelimit seconds for a search to complete.]:timelimit'
    '-z[retrieve at most sizelimit entries for a search.]:sizelimit'
    '-O[Specify SASL security properties.]:security-properties'
    '-I[Enable SASL Interactive mode. Always prompt.]'
    '-Q[Enable SASL Quiet mode. Never prompt.]'
    '-U[Specify the authentication ID for SASL bind.]:authcid'
    '-R[Specify the realm of authentication ID for SASL bind.]:realm'
    '-X[Specify the requested authorization ID for SASL bind.]:authzid'
    '-Y[Specify the SASL mechanism to be used for authentication.]:mech'
    '--MM[makes control critical.]'
    '-e[.]'
    '-Z[Issue StartTLS (Transport Layer Security) extended operation. If you use.]'
    '--ZZ[OUTPUT FORMAT If one or more entries are found, each entry is written to stan…]'
    '*:filename:_files'
)
