#compdef flexiblas

local arguments

arguments=(
  {-u,--user}'[edit the users flexiblasrc configuration]'
  {-g,--global}'[edit the global flexiblasrc configuration]'
  {-E,--environment}'[edit the configuration file given by the]'
  {-H,--host}'[edit the hosts flexiblasrc configuration]'
  {-p,--pipe}'[pipe/Script compatible output.]'
  {-h,--help}'[print this information and exit.]'
  {-v,--version}'[print the versionformation and exit.]'
  '*:filename:_files'
)

_arguments -s $arguments
