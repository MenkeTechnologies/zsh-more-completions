#compdef hexedit

# Autogenerated from man page /usr/local/share/man/man1/hexedit.1

local arguments

arguments=(
    {-s,--sector}'[Format the display to have entire sectors.]'
    {-m,--maximize}'[Try to maximize the display.]'
    '--color[Display colors.]'
    {--l,--linelength}'[Explicitly set the number of bytes to display per line to <n>.]:n'
    '(- * :)'{-h,--help}'[Show the usage. COMMANDS (quickly) Moving.]'
    '*:filename:_files'
)
_arguments -s $arguments
