#compdef systemd-vmspawn

# Autogenerated from man page /usr/share/man/man1/systemd-vmspawn.1.gz

local arguments

arguments=(
    {-D,--directory}'[Directory to use as file system root for the virtual machine.]'
    {-i,--image}'[Root file system disk image (or device node) for the virtual machine.]'
    '--qemu-smp[Configures the number of CPUs to start the virtual machine with.]:SMP'
    '--qemu-mem[Configures the amount of memory to start the virtual machine with.]:MEM'
    '--qemu-kvm[Configures whether to use KVM.]:BOOL'
    '--qemu-vsock[Configure whether to use VSock networking.]:BOOL'
    '--vsock-cid[Configure vmspawn to use a specific CID for the guest.]:CID'
    '--qemu-gui[Start QEMU in graphical mode. sp Added in version 255.]'
    '--secure-boot[Configure whether to search for firmware which supports Secure Boot.]:BOOL'
    {-M,--machine}'[Sets the machine name for this container.]'
    {--load-credential,--set-credential}'[Pass a credential to the container.]'
    '--no-pager[Do not pipe output into a pager.]'
    {-h,--help}'[Print a short help text and exit.]'
    '--version[Print a short version string and exit.]'
    '--private-users[to map into the users subuid namespace.]'
    '*:filename:_files'
)
_arguments -s $arguments
