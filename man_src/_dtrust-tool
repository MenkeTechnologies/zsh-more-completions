local arguments

arguments=(
    {--check-transport-protection,-c}'[In the delivery state the card is locked by a so called transport protection.]'
    {--help,-h}'[Print help message on screen.]'
    {--reader,-r}'[Number of the reader to use.]'
    {--pin-status,-s}'[Show the status of the various PINs.]'
    {--unlock-transport-protection,-u}'[This command removes the transport protection.]'
    {--verbose,-v}'[Causes dtrust-tool to be more verbose.]'
    {--wait,-w}'[Causes dtrust-tool to wait for the token to be inserted into reader.]'
