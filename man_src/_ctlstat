#compdef ctlstat

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/ctlstat.8.gz

local arguments

arguments=(
    '-t[Total mode.]'
    '-c[Display statistics this many times.]'
    '-C[Disable CPU statistics display.]'
    '-d[Display DMA operation time (latency) instead of overall I/O time (latency).]'
    '-D[Text dump mode.]'
    '-h[Suppress display of the header.]'
    '-j[JSON dump mode.]'
    '-l[Request statistics for the specified LUN.]'
    '-n[Display statistics for this many devices.]'
    '-p[Request statistics for the specified port.]'
    '-w[Wait this many seconds in between displays.]'
    '*:filename:_files'
)
_arguments -s $arguments
