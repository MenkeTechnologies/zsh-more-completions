#compdef pdftopng

# Autogenerated from man page /opt/homebrew/share/man/man1/pdftopng.1

local arguments

arguments=(
    '-f[Specifies the first page to convert.]:number"'
    '-l[Specifies the last page to convert.]:number"'
    '-r[Specifies the resolution, in DPI. The default is 150 DPI.]:number"'
    '--mono[Generate a monochrome image (instead of a color image).]'
    '--gray[Generate a grayscale image (instead of a color image).]'
    '--alpha[Generate an alpha channel in the PNG file.]'
    '--rot[Rotate pages by 0 (the default), 90, 180, or 270 degrees.]:angle"'
    '--freetype[Enable or disable FreeType (a TrueType / Type 1 font rasterizer).]'
    '--aa[Enable or disable font anti-aliasing. This defaults to "yes".]'
    '--aaVector[Enable or disable vector anti-aliasing. This defaults to "yes".]'
    '--opw[Specify the owner password for the PDF file.]:password"'
    '--upw[Specify the user password for the PDF file.]:password"'
    '--verbose[Print a status message (to stdout) before processing each page.]'
    '-q[Don\t print any messages or errors. RB "\[config file: " errQuiet \].]'
    '-v[Print copyright and version information.]'
    '-h[Print usage information. ( -help and.]'
    '--help[are equivalent. ) EXIT CODES The Xpdf tools use the following exit codes:.]'
    '*:filename:_files'
)
_arguments -s $arguments
