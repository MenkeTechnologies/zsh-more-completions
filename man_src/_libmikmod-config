#compdef libmikmod-config

# Autogenerated from man page /usr/local/share/man/man1/libmikmod-config.1

local arguments

arguments=(
    '--version[Print the currently installed version of libmikmod on the standard output.]'
    '--libs[Print the linker flags that are necessary to link a libmikmod program.]'
    '--ldadd[Print the extra linker flags that are necessary to link a libmikmod program i…]'
    '--cflags[Print the compiler flags that are necessary to compile a libmikmod program.]'
    '--prefix[If specified, use PREFIX instead of the installation prefix that libmikmod wa…]:PREFIX'
    '--exec-prefix[If specified, use PREFIX instead of the installation exec prefix that libmikm…]:PREFIX'
    '*:filename:_files'
)
_arguments -s $arguments
