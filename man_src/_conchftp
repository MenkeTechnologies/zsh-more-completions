#compdef conchftp

# Autogenerated from man page /usr/share/man/man1/conchftp.1.gz

local arguments

arguments=(
    '-B[Specifies the default size of the buffer to use for sending and receiving.]'
    '-b[File to read commands from, \-\ for stdin. (Default value: interactive/stdin.]'
    '-R[Number of requests to make before waiting for a reply.]'
    '-s[Subsystem/server program to connect to.]'
    '*:filename:_files'
)
_arguments -s $arguments
