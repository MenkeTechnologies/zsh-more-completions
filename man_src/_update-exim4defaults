#compdef update-exim4defaults

# Autogenerated from man page /usr/share/man/man8/update-exim4defaults.8.gz

local arguments

arguments=(
    {--help,-h}'[Print short usage instructions and exit.]'
    '--qflags[Set qflags, special flags given to exim directly after the \\(lq-q\\(rq.]:flags'
    '--queuerunner[Should the init script start one daemon that listens on the smtp port for inc…]:combined|queueonly|separate|ppp|no|nodaemon'
    '--queuetime[In which intervals should we run the queue? This passed as option -q<qflags>…]:time'
    '--commonoptions[Options passed both to queue-running and listening instances of Exim.]:options'
    '--queuerunneroptions[Options only passed to the instance of Exim running the queue.]:options'
    '--smtplisteneroptions[Options only passed to the instance of Exim listening on the SMTP port.]:options'
    {--force,-f}'[Without this option update-exim4defaults exits with an error if at least one…]'
    '--remove-common[Try to remove the given option from COMMONOPTIONS.]:option'
    '--remove-queue[Try to remove the given option from QUEUERUNNEROPTIONS.]:option'
    '--remove-smtp[Try to remove the given option from SMTPLISTENEROPTIONS.]:option'
    '--q<qflags>time[to Exim, e. g. -q30m or -qq1h.]'
    '--remove-commands[additionally remove whitespace at the beginning and the end of the option and…]'
    '--init[create a default /etc/default/exim4 file and exit immediately. Unless.]'
    '--remove-something[.]'
    '*:filename:_files'
)
_arguments -s $arguments
