#compdef dirb

# Autogenerated from man page /usr/share/man/man1/dirb.1.gz

local arguments

arguments=(
    '-a[RB Specify your custom USER_AGENT. (Default is: "Mozilla/4.]:<agent_string>'
    '-b[RB Don\t squash or merge sequences of /. / or /. / in the given URL.]'
    '-c[RB Set a cookie for the HTTP request.]:<cookie_string>'
    '-E[RB Use the specified client certificate file.]:<certificate>'
    '-f[RB Fine tunning of NOT_FOUND (404) detection.]'
    '-H[RB Add a custom header to the HTTP request.]:<header_string>'
    '-i[RB Use case-insensitive Search.]'
    '-l[RB Print "Location" header when found.]'
    '-N[RB Ignore responses with this HTTP code.]:<nf_code>'
    '-o[RB Save output to disk.]:<output_file>'
    '-p[RB Use this proxy. (Default port is 1080).]:<proxy:port>'
    '-P[RB Proxy Authentication.]:<proxy_username:proxy_password>'
    '-r[RB Don\t Search Recursively.]'
    '-R[RB Interactive Recursion. (Ask in which directories you want to scan).]'
    '-S[RB Silent Mode. Don\t show tested words. (For dumb terminals).]'
    '-t[RB Don\t force an ending \/\ on URLs.]'
    '-u[RB Username and password to use.]:<username:password>'
    '-v[RB Show Also Not Existent Pages.]'
    '-w[RB Don\t Stop on WARNING messages.]'
    '-x[RB Amplify search with the extensions on this file.]:<extensions_file>'
    '-X[RB Amplify search with this extensions.]:<extensions>'
    '-z[Amplify search with this extensions.]'
    '*:filename:_files'
)
_arguments -s $arguments
