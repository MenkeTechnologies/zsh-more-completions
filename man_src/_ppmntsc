#compdef ppmntsc

# Autogenerated from man page /usr/share/man/man1/ppmntsc.1.gz

local arguments

arguments=(
    '--pal[Use the PAL transform instead of the default NTSC.]'
    '--verbose[Print a grand total of the number of illegal pixels.]'
    '--debug[Produce a humongous listing of illegal colors and their legal counterparts.]'
    '--legalonly[Output only pixels that are already legal.]'
    '--illegalonly[Output only pixels that are illegal (and output them uncorrected).]'
    '--correctedonly[Output only pixels that are corrected versions of illegal pixels.]'
    '*:filename:_files'
)
_arguments -s $arguments
