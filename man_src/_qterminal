#compdef qterminal

# Autogenerated from man page /usr/share/man/man1/qterminal.1.gz

local arguments

arguments=(
    {-d,--drop}'[Start in "dropdown mode" (like Yakuake or Tilda).]'
    {-e,--execute}'[Execute command instead of shell.]'
    {-h,--help}'[Print this help.]'
    {-p,--profile}'[Load qterminal with specific options.]'
    {-v,--version}'[Prints application version and exits.]'
    {-w,--workdir}'[Start session with specified work directory.]'
    '*:filename:_files'
)
_arguments -s $arguments
