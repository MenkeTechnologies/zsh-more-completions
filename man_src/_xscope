#compdef xscope

# Autogenerated from man page /opt/X11/share/man/man1/xscope.1

local arguments

arguments=(
    '-i[Specify the port that xscope will use to take requests from clients (defaults…]:<input-port>'
    '-o[Determines the port that xscope will use to connect to X11 (defaults to 0).]:<output-port>'
    '-h[Determines the host that xscope will use to find its X11 server.]:<host>'
    '-d[Defines the display number.]:<display>'
    '-f[Instead of decoding a live session, reads previously captured raw data from a…]:<filename>'
    '-q[Quiet output mode.]'
    '-v[Determines the level of printing which xscope will provide.]:<print-level>'
    '-r[Print only raw data without decoding it.]'
    '-D[Print debugging information at the specified level.]:<debug-level>'
    '-I[Enter interactive mode at startup.]'
    '-S[Toggle output on or off when SIGUSR1 is received.]:<n>'
    '-V[Print version information and exit.]'
    '-t[Terminate xscope when all clients close. INTERACTIVE MODE.]'
    '--o3[SEE ALSO X (7), Xsecurity (7), X11 Protocol and extension protocol documents …]'
    '*:filename:_files'
)
_arguments -s $arguments
