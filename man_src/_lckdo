#compdef lckdo

# Autogenerated from man page /usr/share/man/man1/lckdo.1.gz

local arguments

arguments=(
    '-w[If the lock is already held by another process, wait for it to complete inste…]'
    '-W[The same as -w but wait not more than sec seconds.]:{sec}'
    '-e[Execute the program directly without forking and waiting (keeps an extra file…]'
    '-E[Set the file descriptor number to keep open when exec()ing (implies -e).]:{nnn}'
    '-n[Do not create the lock file if it does not exist.]'
    '-q[Produce no output if lock is already held.]'
    '-s[Lock in shared (read) mode.]'
    '-x[Lock in exclusive (write) mode (default).]'
    '-t[Test for lock existence.]'
    '*:filename:_files'
)
_arguments -s $arguments
