#compdef pacsort

# Autogenerated from man page /usr/share/man/man8/pacsort.8.gz

local arguments

arguments=(
    {-f,--files}'[If the input lists are in the format *-. pkg.]'
    {-h,--help}'[Display syntax and command line options.]'
    {-k,--key}'[Sort the input starting on the specified column.]':<index>
    {-r,--reverse}'[Sort in reverse order.]'
    {-t,--separator}'[Field separator. Default separator is space.]':<sep>
    {-z,--null}'[Lines end with null bytes rather than newlines.]'
    {-v,--version}'[.]'
)
_arguments -s $arguments
