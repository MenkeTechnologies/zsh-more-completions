#compdef patchview

# Autogenerated from man page /usr/share/man/man1/patchview.1.gz

local arguments

arguments=(
    {-n,--line-number}'[Display the line number that each patch begins at.]'
    {-N,--number-files}'[File numbers are listed, beginning at 1, before each filename.]'
    {-#,--hunks}'[Only list hunks within the specified RANGE.]'
    '--lines[Only list hunks that contain lines from the original file that lie within the…]:RANGE'
    {-F,--files}'[Only list files indicated by the specified RANGE.]'
    {-p,--strip-match}'[When matching, ignore the first n components of the pathname.]'
    '--strip[Remove the first n components of the pathname before displaying it.]:n'
    '--addprefix[Prefix the pathname with PREFIX before displaying it.]:PREFIX'
    {-s,--status}'[Show file additions, modifications and removals.]'
    {-E,--empty-files-as-removed}'[Treat empty files as absent for the purpose of displaying file additions, mod…]'
    {-i,--include}'[Include only files matching PATTERN.]'
    {-x,--exclude}'[Exclude files matching PATTERN.]'
    {-z,--decompress}'[Decompress files with extensions . gz and . bz2.]'
    {-H,--with-filename}'[Print the name of the patch file containing each patch.]'
    {-h,--no-filename}'[Suppress the name of the patch file containing each patch.]'
    {-v,--verbose}'[Verbose output.]'
    '--help[Display a short usage message.]'
    '--version[Display the version number of patchview.]'
    '--filter[Behave like filterdiff(1) instead.]'
    '--grep[Behave like grepdiff(1) instead.]'
    '--nv[.]'
    '--nvv[.]'
    '*:filename:_files'
)
_arguments -s $arguments
