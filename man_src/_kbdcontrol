#compdef kbdcontrol

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/kbdcontrol.1.gz

local arguments

arguments=(
    '-A[Detach the keyboard, specified by the keyboard device name, from the keyboard…]'
    '-a[Attach the keyboard, specified by the keyboard device name, to the keyboard m…]'
    '-b[duration .]'
    '-r[delay .]'
    '-l[Install keyboard map file from keymap_file.]'
    '-d[Dump the current keyboard map onto stdout.]'
    '-f[Set function key number # to send string.]'
    '-F[Set function keys back to the standard definitions.]'
    '-x[Use hexadecimal numbers in keyboard map dump.]'
    '-i[Print brief information about the keyboard.]'
    '-K[Disconnect the keyboard from the console.]'
    '-k[Use the specified device as the console keyboard.]'
    '-L[Load keyboard map file from keymap_file and write the "struct keymap" compile…]'
    '-P[Search for the keymap file in path.]'
    '*:filename:_files'
)
_arguments -s $arguments
