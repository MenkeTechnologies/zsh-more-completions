#compdef nginx

# Autogenerated from man page /usr/share/man/man8/nginx.8.gz

local arguments

arguments=(
    '-\?[Print help.]'
    '-c[Use an alternative configuration file.]'
    '-g[Set global configuration directives. See EXAMPLES for details.]'
    '-p[Set the prefix path. The default value is /usr/share/nginx.]'
    '-q[Suppress non-error messages during configuration testing.]'
    '-s[Send a signal to the master process.]'
    '-t[Do not run, just test the configuration file.]'
    '-T[Same as t, but additionally dump configuration files to standard output.]'
    '-V[Print the nginx version, compiler version, and configure script parameters.]'
    '-v[Print the nginx version.]'
    '*:filename:_files'
)
_arguments -s $arguments
