#compdef dmenu

# Autogenerated from man page /usr/share/man/man1/dmenu.1.gz

local arguments

arguments=(
    '-b[dmenu appears at the bottom of the screen.]'
    '-f[dmenu grabs the keyboard before reading stdin.]'
    '-i[dmenu matches menu items case insensitively.]'
    '-l[dmenu lists items vertically, with the given number of lines.]:lines"'
    '-m[dmenu is displayed on the monitor number supplied.]:monitor"'
    '-p[defines the prompt to be displayed to the left of the input field.]:prompt"'
    '--fn[defines the font or font set used.]:font"'
    '--nb[defines the normal background color.]:color"'
    '--nf[defines the normal foreground color.]:color"'
    '--sb[defines the selected background color.]:color"'
    '--sf[defines the selected foreground color.]:color"'
    '-v[prints version information to stdout, then exits.]'
    '-w[embed into windowid. USAGE dmenu is completely controlled by the keyboard.]:windowid'
    '*:filename:_files'
)
_arguments -s $arguments
