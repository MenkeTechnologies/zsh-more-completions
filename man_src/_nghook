#compdef nghook

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/nghook.8.gz

local arguments

arguments=(
    '-a[Output each packet read in human-readable decoded ASCII form instead of raw b…]'
    '-d[Increase the debugging verbosity level.]'
    '-e[Execute the program specified by the third argument.]'
    '-l[Loops all received data back to the hook in addition to writing it to standar…]'
    '-m[Before executing the program (in e mode) send the given ASCII control message…]'
    '-n[Do not attempt to read any data from standard input.]'
    '-S[Use file descriptor 0 for output instead of the default 1.]'
    '-s[Use file descriptor 1 for input instead of the default 0.]'
    '*:filename:_files'
)
_arguments -s $arguments
