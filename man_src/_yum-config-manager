#compdef yum-config-manager

# Autogenerated from man page /usr/share/man/man1/yum-config-manager.1.gz

local arguments

arguments=(
    '(- * :)'{-h,--help}'[Display a help message, and then quit.]'
    '--setopt[Set any config option in yum config or repo files.]:value'
    '--save[Save the current options (useful with --setopt).]'
    '--enable[Enable the specified repos (automatically saves).]'
    '--disable[Disable the specified repos (automatically saves).]'
    '*:filename:_files'
)
_arguments -s $arguments
