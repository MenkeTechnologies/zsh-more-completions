#compdef dh_installman

# Autogenerated from man page /usr/share/man/man1/dh_installman.1.gz

local arguments

arguments=(
    {-A,--all}'[Install all files specified by command line parameters in \\s-1ALL\\s0 packages…]'
    '--language[Use this to specify that the man pages being acted on are written in the spec…]:ll'
    '--sourcedir[Look in the specified directory for files to be installed.]:dir'
    '*:filename:_files'
)
_arguments -s $arguments
