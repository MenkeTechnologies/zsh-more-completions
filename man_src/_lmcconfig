#compdef lmcconfig

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/lmcconfig.8.gz

local arguments

arguments=(
    '-1[All parameters after this apply to the T1E1 card.]'
    '-3[All parameters after this apply to the T3 card.]'
    '-a[Set Transmitter clock source to number.]'
    '-b[Read BIOS ROM. Print the first 256 locations.]'
    '-B[Write BIOS ROM. Write the first 256 locations with an address pattern.]'
    '-c[Use HDLC\s 16-bit CRC polynomial: X^16+X^12+X^5+1 (default).]'
    '-C[Use HDLC\s 32-bit CRC polynomial: X^32+X^26+X^23+X^22+X^16+X^12+X^11+X^10+X^8…]'
    '-d[Clear the driver-level debug flag. Non-critical log messages are suppressed.]'
    '-D[Set the driver-level debug flag. The driver generates more log messages.]'
    '-e[Set DTE (Data Terminal Equipment) mode (default).]'
    '-E[Set DCE (Data Communication Equipment) mode.]'
    '-f[Set the frequency of the built-in synthesized oscillator to number bits/secon…]'
    '-F[Set SPPP line protocol to Frame-Relay. Only works for 5. 4 and later.]'
    '-h[Print help (usage message).]'
    '-i[Set interface name (e. g. \\& lmc0 ).]'
    '-L[Set loopback mode to number.]'
    '-m[Read Tulip MII registers.]'
    '-M[Write Tulip MII register. Write data into register addr.]'
    '-p[Read Tulip PCI configuration registers.]'
    '-P[Write Tulip PCI configuration register. Write data into register addr.]'
    '-s[Read Tulip SROM. Print the 64 16-bit locations.]'
    '-S[Write Tulip SROM. Initializes the Tulip SROM to card type number.]'
    '-t[Read Tulip CSRs. Print the 16 32-bit control and status registers.]'
    '-T[Write Tulip CSR. Write data into CSR number addr.]'
    '-u[Reset event counters to zero.]'
    '-U[Reset gate array. Not needed during normal operation; just for testing.]'
    '-v[Set verbose mode: print more stuff.]'
    '-V[Print the card configuration \\\[em\] see the EXAMPLES section.]'
    '-w[Load gate array from on-board ROM.]'
    '-W[Load gate array microcode from filename.]'
    '-x[Select RAWIP mode \\\[em\] bypass line protocol code.]'
    '-X[Select line protocol code rather than RAWIP mode.]'
    '-y[Disable SPPP keep-alive packets.]'
    '-Y[Enable SPPP keep-alive packets.]'
    '-z[Set SPPP line protocol to Cisco-HDLC.]'
    '-Z[Set SPPP line protocol to PPP.]'
    '--\?\\&[Print help (usage message).]'
    '-A[Start sending alarm signal (see table above).]'
    '-g[Set receiver gain range to number : Pp -column "0x00" "Medium" "auto-set base…]'
    '-I[Send a CSU loopback activate inband command (T1-SF only).]'
    '-l[Send a line loopback deactivate BOP message (T1-ESF only).]'
    '*:filename:_files'
)
_arguments -s $arguments
