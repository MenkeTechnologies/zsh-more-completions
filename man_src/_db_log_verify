#compdef db_log_verify

# Autogenerated from man page /usr/share/man/man1/db_log_verify.1.gz

local arguments

arguments=(
    '-C[Specify the cache size (in megabytes) of the temporary database environment i…]:cache-size'
    '-b[Specify the starting log record (by lsn) to verify.]:start-lsn'
    '-c[Specify whether to continue the verification after an error is detected.]'
    '-D[Specify a database name.]:database-name'
    '-d[Specify a database file name.]:database-filename'
    '-e[Specify the ending log record by lsn.]:end-lsn'
    '-h[Specify a home directory of the database environment whose log is to be verif…]:home-to-verify'
    '-H[Specify a home directory for this utility to create a temporarily database en…]:temporary-home'
    '-N[Do not acquire shared region mutexes while running.]'
    '-P[Specify an environment password.]:password'
    '-s[Specify the starting log record by time.]:start-time'
    '-V[Write the library version number to the standard output and exit.]'
    '-v[Enable verbose mode to display verbose output during the verification process.]'
    '-z[Specify the ending log record by time.]:end-time'
    '*:filename:_files'
)
_arguments -s $arguments
