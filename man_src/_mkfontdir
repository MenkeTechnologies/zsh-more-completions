#compdef mkfontdir

# Autogenerated from man page /opt/X11/share/man/man1/mkfontdir.1

local arguments

arguments=(
    '-e[Specify a directory containing encoding files.]'
    '-n[do not scan for fonts, do not write font directory files.]'
    '-p[Specify a prefix that is prepended to the encoding file path names when they …]'
    '-r[Keep non-absolute encoding directories in their relative form when writing th…]'
    '-x[Ignore fonts files of type R suffix .]:suffix'
    '*:filename:_files'
)
_arguments -s $arguments
