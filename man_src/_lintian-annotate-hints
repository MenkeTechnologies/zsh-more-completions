#compdef lintian-annotate-hints

local arguments

arguments=(
  {-a,--annotate}'[display descriptions of tags in Lintian overrides]'
  '--include-dir[check for Lintian data in DIR]'
  '--profile[use vendor profile X to determine severities]'
  '--output-width[set output width instead of probing terminal]'
  '--version[show version info and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
