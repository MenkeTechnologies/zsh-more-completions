    '--allow-conic[Allow conic BÃ©zier curves to be used in the generated path. Showing.]'
    '--fill[Fill the path (this is the default).]'
    '--stroke[Stroke the path instead of filling it.]'
    '--points[Show points on the path.]'
    '--controls[Show control points.]'
    '--fill-rule[The fill rule that is used to determine what areas are inside the path.]:VALUE'
    '--fg-color[The color that is used to fill the interior of the path or stroke the path.]:COLOR'
    '--bg-color[The color that is used to render the background behind the path.]:COLOR'
    '--point-color[The color that is used to render the points. If not specified, red is used.]:COLOR'
    '--line-width[The line width to use for the stroke. VALUE must be a positive number.]:VALUE'
    '--line-cap[The cap style to use at line ends.]:VALUE'
    '--line-join[The join style to use at line joins.]:VALUE'
    '--miter-limit[The limit at which to clip miters at line joins. The default value is 4.]:VALUE'
    '--dashes[The dash pattern to use for this stroke.]:VALUE'
    '--dash-offset[The offset into the dash pattern where dashing should begin.]:VALUE'
    '--output-file[The file to save the PNG image to. If not specified, "path. png" is used.]:FILE'
    '*:filename:_files'
