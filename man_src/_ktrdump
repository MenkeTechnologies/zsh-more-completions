#compdef ktrdump

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/ktrdump.8.gz

local arguments

arguments=(
    '-c[Print the CPU number that each entry was logged from.]'
    '-f[Print the file and line number that each entry was logged from.]'
    '-q[Quiet mode; do not print the column header.]'
    '-r[Print relative timestamps rather than absolute timestamps.]'
    '-t[Print the timestamp for each entry.]'
    '-H[Print the thread ID for each entry.]'
    '-i[File containing saved ktr trace events; for more information see the ktr 4 maâ€¦]'
    '-N[The kernel image to resolve symbols from.]'
    '-M[The core file or memory image to read from. The default is /dev/mem.]'
    '-o[The file to write the output to. The default is standard output.]'
    '*:filename:_files'
)
_arguments -s $arguments
