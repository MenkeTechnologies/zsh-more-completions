#compdef sandbox

# Autogenerated from man page /usr/share/man/man8/sandbox.8.gz

local arguments

arguments=(
    '--h--help[display usage message.]'
    '--H--homedir[Use alternate homedir to mount over your home directory.]'
    {-i,--include}'[Copy this file into the appropriate temporary sandbox directory.]'
    {-I,--includefile}'[Copy all files listed in inputfile into the appropriate temporary sandbox dir…]'
    {-l,--level}'[Specify the MLS/MCS Security Level to run the sandbox with.]'
    {-M,--mount}'[Create a Sandbox with temporary files for $HOME and /tmp.]'
    {-s,--shred}'[Shred temporary files created in $HOME and /tmp, before deleting.]'
    {-t,--type}'[Use alternate sandbox type, defaults to sandbox_t or sandbox_x_t for -X.]'
    {-T,--tmpdir}'[Use alternate temporary directory to mount on /tmp. Defaults to tmpfs.]'
    {-S,--session}'[Run a full desktop session, Requires level, and home and tmpdir.]'
    {-w,--windowsize}'[Specifies the windowsize when creating an X based Sandbox.]'
    {-W,--windowmanager}'[Select alternative window manager to run within sandbox -X.]'
    '-X[Create an X based Sandbox for gui apps, temporary files for $HOME and /tmp, s…]'
    {-d,--dpi}'[Set the DPI value for the sandbox X Server.]'
    '(- * :)'{-h,--help}'[display usage message.]'
    {-H,--homedir}'[Use alternate homedir to mount over your home directory.]'
    {-C,--capabilities}'[sandbox.]'
    '*:filename:_files'
)
_arguments -s $arguments
