#compdef odbcinst

# Autogenerated from man page /usr/local/share/man/man1/odbcinst.1

local arguments

arguments=(
    '-i[Install (add section to config file) new OBJECT.]'
    '-u[Uninstall (remove section from config file) existing OBJECT.]'
    '-q[Query the config files and print the options for specified OBJECT.]'
    '-j[Prints current configuration of unixODBC, listing (among others) the paths to…]'
    '-c[Calls SQLCreateDataSource.]'
    '-m[Calls SQLManageDataSources.]'
    '--version[Prints program version and exits. SS OBJECTS.]'
    '-d[The ACTION affect drivers (and thus the odbcinst. ini configuration file).]'
    '-s[The ACTION affect data sources (and thus the user or system odbc.]'
    '-f[The FILE is template file, describing the configuration of installed OBJECT (…]:FILE'
    '-r[Same as the -f OPTION, only take the standard input as the template file.]'
    '-n[Specifies the NAME of the OBJECT.]:NAME'
    '-v[Turn off verbose mode.]'
    '-l[The specified data source is system-wide.]'
    '-h[The specified data source is user-specific.]'
    '*:filename:_files'
)
_arguments -s $arguments
