#compdef qv4l2

# Autogenerated from man page /usr/share/man/man1/qv4l2.1.gz

local arguments

arguments=(
    {-d,--device}'[Use device <dev> as the video device.]:dev'
    {-V,--vbi-device}'[Use device <dev> as the vbi device.]:dev'
    {-r,--radio-device}'[Use device <dev> as the radio device.]:dev'
    {-S,--sdr-device}'[Use device <dev> as the SDR device.]:dev'
    {-R,--raw}'[Open device in raw mode, i. e. without using the libv4l2 wrapper functions.]'
    '(- * :)'{-h,--help}'[Prints the help message. HOTKEYS Main Window.]'
    '*:filename:_files'
)
