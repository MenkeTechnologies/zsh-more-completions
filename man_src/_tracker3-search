#compdef tracker3-search

# Autogenerated from man page /usr/share/man/man1/tracker3-search.1.gz

local arguments

arguments=(
    {-l,--limit}'[Limit search to limit results.]':limit
    {-o,--offset}'[Offset the search results by offset.]':offset
    {-r,--or-operator}'[Use OR for search terms instead of AND (the default).]'
    {-d,--detailed}'[Show the unique URN associated with each search result.]'
    {-a,--all}'[Show results which might not be available.]'
    '--disable-snippets[Results are shown with snippets.]'
    '--disable-fts[If Full Text Search (FTS) is available, this option allows it to be disabled â€¦]'
    '--disable-color[This disables any ANSI color use on the command line.]'
    '--folders[for example), then ALL items in that category are returned.]'
    {-f,--files}'[.]'
    '-s[.]'
    {-m,--music}'[.]'
    '--music-albums[.]'
    '--music-artists[.]'
    {-i,--images}'[.]'
    {-v,--videos}'[.]'
    {-t,--documents}'[.]'
    '--software[.]'
    '--software-categories[.]'
    '--feeds[.]'
    '*:filename:_files'
)
_arguments -s $arguments
