#compdef xdg-dbus-proxy

# Autogenerated from man page /usr/share/man/man1/xdg-dbus-proxy.1.gz

local arguments

arguments=(
    '--help[Print help and exit.]'
    '--version[Print version.]'
    '--fd[Write to FD when the proxies are ready, and stop when it is closed.]:FD'
    '--args[Parse nul-separated arguments from the given file descriptor.]:FD'
    '--filter[Enable filtering.]'
    '--log[Turn on logging.]'
    '--sloppy-names[Make all unique names visible.]'
    '--see[Set the SEE policy for the given name.]:NAME'
    '--talk[Set the TALK policy for the given name.]:NAME'
    '--own[Set the OWN policy for the given name.]:NAME'
    '--call[Set a rule for calls on the given name.]:RULE'
    '--broadcast[Set a rule for broadcast signals from the given name.]:RULE'
    '*:filename:_files'
)
_arguments -s $arguments
