#compdef morse

# Autogenerated from man page /usr/share/man/en.UTF-8/man6/morse.6.gz

local arguments

arguments=(
    '-l[The l option produces output suitable for led 4 devices.]'
    '-s[The s option produces dots and dashes rather than words.]'
    '-p[Send morse the real way.]'
    '-w[Set the sending speed in words per minute.]'
    '-c[Farnsworth support. Set the spacing between characters in words per minute.]'
    '-f[Set the sidetone frequency to something other than the default 600 Hz.]'
    '-d[Similar to p, but use the RTS line of device (which must be a TTY device) in …]'
    '-e[Echo each character before it is sent, used together with either p or d.]'
    '-r[Decode morse output consisting of dots and dashes (as generated by using the …]'
    '*:filename:_files'
)
_arguments -s $arguments
