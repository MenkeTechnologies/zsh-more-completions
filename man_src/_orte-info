#compdef orte-info

# Autogenerated from man page /usr/local/share/man/man1/orte-info.1

local arguments

arguments=(
    {-a,--all}'[Show all configuration options and MCA parameters.]'
    '--arch[Show architecture ORTE was compiled on.]'
    {-c,--config}'[Show configuration options.]'
    {--gmca,--gmca}'[Pass global MCA parameters that are applicable to all contexts.]':value
    '(- * :)'{-h,--help}'[Shows help / usage message.]'
    '--hostname[Show the hostname that ORTE was configured and built on.]'
    '--internal[Show internal MCA parameters (not meant to be modified by users).]'
    {--mca,--mca}'[Pass context-specific MCA parameters; they are considered global if --gmca is…]':value
    '--param[Show MCA parameters.]:component'
    '--parsable[When used in conjunction with other parameters, the output is displayed in a …]'
    '--path[Show paths that ORTE was configured with.]:type'
    '--pretty[When used in conjunction with other parameters, the output is displayed in \p…]'
    '--parseable[Synonym for --parsable.]'
    {-v,--version}'[Show version of ORTE or a component.]':scope
    '*:filename:_files'
)
_arguments -s $arguments
