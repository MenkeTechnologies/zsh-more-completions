#compdef susepaste

# Autogenerated from man page /usr/share/man/man1/susepaste.1.gz

local arguments

arguments=(
    '-n[nickname of the author of the paste.]:NICK'
    '-t[name of the paste.]:TITLE'
    '-k[API key to paste as you.]:KEY'
    '-f[what kind of syntax highlighting should be used.]:SYNTAX'
    '-e[for how log will be paste stored on the server.]:EXPIRE'
    '*:filename:_files'
)
_arguments -s $arguments
