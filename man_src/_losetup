    {-d,--detach}'[Detach the file or device associated with the specified loop device(s).]'
    {-D,--detach-all}'[Detach all associated loop devices.]'
    {-f,--find}'[Find the first unused loop device.]'
    '--show[Display the name of the assigned loop device if the -f option and a file argu…]'
    {-L,--nooverlap}'[Check for conflicts between loop devices to avoid situation when the same bac…]'
    {-j,--associated,-o}'[Show the status of all loop devices associated with the given file.]'
    '--offset[The data start is moved offset bytes into the specified file or device.]'
    '--sizelimit[The data end is set to no more than size bytes after the data start.]:size'
    {-b,--sector-size}'[Set the logical sector size of the loop device in bytes (since Linux 4. 14).]'
    {-c,--set-capacity}'[Force the loop driver to reread the size of the file associated with the spec…]'
    {-P,--partscan}'[Force the kernel to scan the partition table on a newly created loop device.]'
    {-r,--read-only}'[Set up a read-only loop device.]'
    '--direct-io[Enable or disable direct I/O for the backing file.]'
    {-v,--verbose}'[Verbose mode.]'
    {-l,--list}'[If a loop device or the -a option is specified, print the default columns for…]'
    {-O,--output}'[Specify the columns that are to be printed for the --list output.]'
    {-n,--noheadings}'[Don\t print headings for --list output format.]'
    '--raw[Use the raw --list output format.]'
    {-J,--json}'[Use JSON format for --list output.]'
    {-V,--version}'[Display version information and exit.]'
    '(- * :)'{-h,--help}'[Display help text and exit.]'
    '*:filename:_files'
)
