#compdef man-recode

# Autogenerated from man page /usr/share/man/man1/man-recode.1.gz

local arguments

arguments=(
    {-t,--to-code}'[Convert manual pages to R encoding .]'
    '--suffix[Form each output file name by appending R suffix to the input file name, afte…]:suffix'
    '--in-place[Overwrite each input file with the output, after removing any compression ext…]'
    {-q,--quiet}'[Do not issue error messages when the page cannot be converted.]'
    {-d,--debug}'[Print debugging information.]'
    '(- * :)'{-h,--help}'[Print a help message and exit.]'
    {-V,--version}'[Display version information. SEE ALSO iconv (1), man (1), manconv (1) AUTHOR.]'
    '*:filename:_files'
)
_arguments -s $arguments
