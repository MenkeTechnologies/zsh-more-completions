#compdef xfsettingsd

# Autogenerated from man page /usr/share/man/man1/xfsettingsd.1.gz

local arguments

arguments=(
    {-h,--help}'[Show help options.]'
    '--help-all[Show all help options.]'
    '--help-gtk[Show GTK+ Options.]'
    '--help-sm-client[Show session management options . PP GTK+ Options.]'
    '--class[Program class as used by the window manager.]:CLASS'
    '--name[Program name as used by the window manager.]:NAME'
    '--screen[X screen to use.]:SCREEN'
    '--sync[Make X calls synchronous.]'
    '--gtk-module[Load additional GTK+ modules.]:MODULES'
    '--g-fatal-warnings[Make all warnings fatal . PP Session management options.]'
    '--sm-client-id[Session management client ID.]:ID'
    '--sm-client-disable[Disable session management . SS "Application Options:".]'
    {-V,--version}'[Version information.]'
    '--no-daemon[Do not fork to the background.]'
    '--replace[Replace running xsettings daemon (if any).]'
    '--display[X display to use COPYRIGHT Copyright Â© 2008-2011 The Xfce development team.]:DISPLAY'
    '*:filename:_files'
)
_arguments -s $arguments
