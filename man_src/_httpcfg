local arguments

arguments=(
    '--add[Adds a new certificate/private key combination for being used at a given port…]'
    {--del,--delete}'[Deletes the certificates associated for the port specified (must be specified…]'
    '--list[List all known certificates.]'
    '--port[Specifies the port to add certificates to, or remove certificates from.]:PORT'
    '--pvk[Specifies the Private Key file.]:FILE'
    '--cert[Specifies the certificate file to use. EXAMPLES.]:CERT'
    '*:filename:_files'
)
