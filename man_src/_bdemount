local arguments

arguments=(
    '-h[shows this help.]'
    '-k[the full volume encryption key and tweak key formatted in base16 and separate…]'
    '-o[specify the volume offset in bytes.]'
    '-p[specify the password.]'
    '-r[specify the recovery password.]'
    '-s[specify the file containing the startup key.]'
    '-v[verbose output to stderr.]'
    '-V[print version.]'
    '-X[extended options to pass to sub system El ENVIRONMENT None FILES None EXAMPLE…]'
    '*:filename:_files'
)
