#compdef cups-genppd.5.3

local arguments

arguments=(
  '-h[show this help message.]'
  '-L[list available translations (message catalogs).]'
  '-M[list available printer models.]'
  '-V[show version information and defaults.]'
  '-N[localize numbers.]'
  '-l[output PPDs translated with messages for locale.]'
  '-p[output PPDs in directory prefix.]'
  '-d[embed directory prefix in PPD file.]'
  '-s[generate simplified PPD files.]'
  '-a[generate all (simplified and full) PPD files.]'
  '-q[quiet mode.]'
  '-v[verbose mode.]'
  '-z[compress PPD files.]'
  '-Z[dont compress PPD files.]'
  '-S[skip PPD files with duplicate model identifiers.]'
  '-R[generate every sizeth PPD file.]'
  '-r[generate the PPD files (N % size == divisor).]'
  '*:filename:_files'
)

_arguments -s $arguments
