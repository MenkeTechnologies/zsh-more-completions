#compdef hastd

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/hastd.8.gz

local arguments

arguments=(
    '-c[Specify alternative location of the configuration file.]'
    '-d[Print or log debugging information.]'
    '-F[Start the hastd daemon in the foreground.]'
    '-h[Print the hastd usage message.]'
    '-P[Specify alternative location of a file where main process PID will be stored.]'
    '*:filename:_files'
)
_arguments -s $arguments
