#compdef e2scrub

# Autogenerated from man page /usr/share/man/man8/e2scrub.8.gz

local arguments

arguments=(
    '-n[Print what commands e2scrub would execute to check the file system.]'
    '-r[Remove the e2scrub snapshot and exit without checking anything.]'
    '-t[Run fstrim(1) on the mounted filesystem if no errors are found.]'
    '-V[Print version information and exit.]'
    '*:filename:_files'
)
_arguments -s $arguments
