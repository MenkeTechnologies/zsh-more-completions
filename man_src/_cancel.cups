#compdef cancel.cups

local arguments

arguments=(
  '-a[cancel all jobs]'
  '-E[encrypt the connection to the server]'
  '-h[connect to the named server and port]'
  '-u[specify the owner to use for jobs]'
  '-U[specify the username to use for authentication]'
  '-x[purge jobs rather than just canceling]'
  '*:filename:_files'
)

_arguments -s $arguments
