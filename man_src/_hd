#compdef hexdump

# Autogenerated from man page /usr/share/man/man1/hexdump.1.gz

local arguments

arguments=(
    '-b[One-byte octal display.]'
    '-c[One-byte character display.]'
    '-C[Canonical hex+ASCII display.]'
    '-d[Two-byte decimal display.]'
    '-e[Specify a format string to be used for displaying data.]'
    '-f[Specify a file that contains one or more newline separated format strings.]'
    '-n[Interpret only length bytes of input.]'
    '-o[Two-byte octal display.]'
    '-s[Skip offset bytes from the beginning of the input.]'
    '-v[Cause hexdump to display all input data.]'
    '-x[Two-byte hexadecimal display.]'
    '*:filename:_files'
)
_arguments -s $arguments
