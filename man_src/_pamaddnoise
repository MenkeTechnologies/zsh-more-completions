local arguments

arguments=(
    '--lambda[Used for poisson noise only. The default value is 0. 05.]:value'
    '--lsigma[Used for laplacian noise only. The default value is 10. 0.]:value'
    '--mgsigma[Used for multiplicative gaussian noise only. The default value is 0. 5.]:value'
    '--seed[Used for all noise types.]:int'
    '--sigma1[Used for gaussian noise only. The default value is 4. 0.]:value'
    '--sigma2[Used for gaussian noise only. The default value is 20. 0.]:value'
    '--tolerance[Used for impulse noise only. The default value is 0. 10.]:ratio'
    '--type[Select type of noise by name.]:noise_type'
    '*:filename:_files'
)
