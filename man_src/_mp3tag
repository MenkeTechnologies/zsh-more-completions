#compdef mp3tag

# Autogenerated from man page /usr/share/man/man1/mp3tag.1.gz

local arguments

arguments=(
    '-a[Set the artist name to artist. Strings over thirty characters are truncated.]:artist'
    '-e[Set the comment to comment. Strings over thirty characters are truncated.]:comment'
    '-g[Set the genre to genre.]:genre'
    '-k[Set the track number to track.]:track'
    '-l[Set the album name to album. Strings over thirty characters are truncated.]:album'
    '-r[Merge new tag with any preexisting tag in the file.]'
    '-s[Set the song name to song. Strings over thirty characters are truncated.]:song'
    '-y[Set the year to year. Strings over four characters are truncated. EXAMPLES.]:year'
    '*:filename:_files'
)
_arguments -s $arguments
