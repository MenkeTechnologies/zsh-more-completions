#compdef editmap

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/editmap.8.gz

local arguments

arguments=(
    '-C[Use the specified sendmail configuration file for looking up the TrustedUser…]'
    '-N[Include the null byte that terminates strings in the map (for alias maps).]'
    '-f[Normally all upper case letters in the key are folded to lower case.]'
    '-q[Query the map for the specified key.]'
    '-u[Update the record for key with value or inserts a new record if one doesn\t…]'
    '-x[Deletes the specific key from the map.]'
    '*:filename:_files'
)
_arguments -s $arguments
