#compdef ctfmerge

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/ctfmerge.1.gz

local arguments

arguments=(
    '-f[Match global symbols to global CTF data.]'
    '-g[Don\t delete the original debugging sections.]'
    '-s[Use the . dynsym ELF section instead of the . symtab ELF section.]'
    '-t[Make sure that all object files have a CTF section.]'
    '-v[Enable verbose mode.]'
    '-l[Sets the label as label.]'
    '-L[Instructs ctfmerge to read the label from the environment variable labelenv.]'
    '-o[Use outfile to store the merged CTF data.]'
    '-d[Uniquify against uniqfile.]'
    '-w[Additive merge with withfile.]'
    '-c[Copy CTF data from srcfile into destfile.]'
    '*:filename:_files'
)
_arguments -s $arguments
