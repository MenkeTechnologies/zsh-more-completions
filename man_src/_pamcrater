local arguments

arguments=(
    '--number[This causes pamcrater to generate n craters.]:n'
    '--height[This sets the height of the generated image to height pixels.]:height'
    '--width[This sets the width of the generated image to width pixels.]:width'
    '--randomseed[This is the seed for the random number generator that generates the pixels.]:integer'
