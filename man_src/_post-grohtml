#compdef post-grohtml

# Autogenerated from man page /usr/share/man/man1/post-grohtml.1

local arguments

arguments=(
    '-a[Number of bits of antialiasing information to be used by text when generatin…]:aa-text-bits'
    '-b[Initialize the background color to white.]'
    '-D[Inform grohtml to place all image files into directory R dir .]:dir'
    '-F[Prepend directory B dir /dev name to the search path for font and device desc…]:dir'
    '-g[Number of bits of antialiasing information to be used by graphics when gener…]:aa-graphic-bits'
    '-h[Generate section and number headings by using <B> . \\|. \\|.]'
    '-i[Select the resolution for all images. By default this is 100 pixels per inch.]:resolution'
    '-I[Determine the image stem name. If omitted grohtml uses \\%grohtml- XXX .]:stem'
    '-j[Inform grohtml to split the html output into multiple files.]:filename'
    '-l[Turn off the production of automatic section links at the top of the document.]'
    '-n[Generate simple heading anchors whenever a section/number heading is found.]'
    '-o[Specify the vertical offset of images in points.]:vertical-offset'
    '-p[Display page rendering progress to stderr.]'
    '-r[Turn off the automatic header and footer line (html rule).]'
    '-s[Set the base point size of the source file.]:size'
    '-S[When splitting html output, split at the heading level (or higher) defined by…]:level'
    '--Thtml[option. If no files are given, grohtml will read the standard input.]'
    '-P[option. OPTIONS.]'
    '--i200[indicates 200 pixels per inch.]'
    '-v[Print the version number.]'
    '*:filename:_files'
)
