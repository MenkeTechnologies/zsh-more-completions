    '-A[Don\t abort when the server sends something other than the expected positive …]'
    '-c[Display a running counter that is incremented each time an SMTP DATA command …]'
    '-C[When a host sends RESET instead of SYN|ACK, try count times before giving up.]:count'
    '-d[Don\t disconnect after sending a message; send the next message over the same…]'
    '-f[Use the specified sender address (default: <foo@myhostname>).]:from'
    '-F[Send the pre-formatted message header and body in the specified file, while p…]:file'
    '-l[Send length bytes as message payload.]:length'
    '-L[Speak LMTP rather than SMTP.]'
    '-m[Send the specified number of messages (default: 1).]:message_count'
    '-M[Use the specified hostname or \[address\] in the HELO command and in the defaul…]:myhostname'
    '-N[Prepend a non-repeating sequence number to each recipient address.]'
    '-o[Old mode: don\t send HELO, and don\t send message headers.]'
    '-r[Send the specified number of recipients per transaction (default: 1).]:recipient_count'
    '-R[Wait for a random period of time 0 <= n <= interval between messages.]:interval'
    '-s[Run the specified number of SMTP sessions in parallel (default: 1).]:session_count'
    '-S[Send mail with the named subject line (default: none).]:subject'
    '-t[Use the specified recipient address (default: <foo@myhostname>).]:to'
    '-T[Override the default TCP window size.]:windowsize'
    '-v[Make the program more verbose, for debugging purposes.]'
    '-w[Wait a fixed time between messages.]:interval'
    '*:filename:_files'
)
