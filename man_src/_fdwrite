#compdef fdwrite

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/fdwrite.1.gz

local arguments

arguments=(
    '-v[Toggle verbosity on stdout. Default is ``on\\.]'
    '-y[Do not ask for presence of a floppy disk in the drive.]'
    '-f[Input file to read. If none is given, stdin is assumed.]'
    '-d[The name of the floppy device to write to. Default is /dev/fd0.]'
    '*:filename:_files'
)
_arguments -s $arguments
