#compdef rqinfo

# Autogenerated from man page /usr/share/man/man1/rqinfo.1.gz

local arguments

arguments=(
    {-i,--interval}'[Updates stats every N seconds (default: don\\\[cq\]t poll).]':FLOAT
    {-r,--raw}'[Print only the raw numbers, no bar charts.]'
    {-Q,--only-queues}'[Show only queue info.]'
    {-W,--only-workers}'[Show only worker info.]'
    {-R,--by-queue}'[Shows workers by queue.]'
    {-P,--path}'[TEXT Specify the import path.]'
    '--connection-class[Redis client class to use.]:TEXT'
    '--queue-class[RQ Queue class to use.]:TEXT'
    {-j,--job-class}'[RQ Job class to use.]':TEXT
    {-w,--worker-class}'[RQ Worker class to use.]':TEXT
    {-c,--config}'[Module containing RQ settings.]':TEXT
    {-u,--url}'[URL describing Redis connection details.]':TEXT
    '*:filename:_files'
)
_arguments -s $arguments
