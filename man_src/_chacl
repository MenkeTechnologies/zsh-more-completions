#compdef chacl

# Autogenerated from man page /usr/share/man/man1/chacl.1.gz

local arguments

arguments=(
    '-b[Indicates that there are two ACLs to change, the first is the file access ACL…]'
    '-d[Used to set only the default ACL of a directory.]'
    '-R[Removes the file access ACL only.]'
    '-D[Removes directory default ACL only.]'
    '-B[Remove all ACLs.]'
    '-l[Lists the access ACL and possibly the default ACL associated with the specifi…]'
    '-r[Set the access ACL recursively for each subtree rooted at pathname(s).]'
    '*:filename:_files'
)
_arguments -s $arguments
