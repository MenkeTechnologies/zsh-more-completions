#compdef raddebug

# Autogenerated from man page /usr/share/man/man8/raddebug.8.gz

local arguments

arguments=(
    '--ccondition[Set a specific debug condition.]'
    '--fsocket_file[The path to the control socket.]'
    '--iipv4-address[Show debug output for the client having the given IPv4 address.]'
    '-d[The radius configuration directory, usually /etc/freeradius/3. 0.]:directory'
    '-n[Read raddb/name. conf instead of raddb/radiusd. conf.]:mname'
    '--Iipv6-address[Show debug output for the client having the given IPv6 address.]'
    '--ttimeout[Stop printing debug output after "timeout" seconds.]'
    '-c[Set a specific debug condition.]:condition'
    '-f[The path to the control socket.]:socket_file'
    '-i[Show debug output for the client having the given IPv4 address.]:ipv4-address'
    '-I[Show debug output for the client having the given IPv6 address.]:ipv6-address'
    '-t[Stop printing debug output after "timeout" seconds.]:timeout'
    '-u[Show debug output for users having the given name.]:name'
    '*:filename:_files'
)
_arguments -s $arguments
