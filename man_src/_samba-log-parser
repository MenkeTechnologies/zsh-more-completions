#compdef samba-log-parser

# Autogenerated from man page /usr/share/man/man1/samba-log-parser.1.gz

local arguments

arguments=(
    '--pid[Display traces for winbind client with the matching PID.]:PID'
    '--traceid[Display traces with matching traceid debug header field.]:ID'
    '--breakdown[Break down all traces to separate files in the current working directory.]'
    '--merge-by-timestamp[Sort the trace lines according to the timestamp.]'
    '--flow[Display the request/sub-request flow.]'
    '--flow-compact[Display the request/sub-request flow without dcerpc call details.]'
    '*:filename:_files'
)
_arguments -s $arguments
