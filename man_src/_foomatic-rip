#compdef foomatic-rip

# Autogenerated from man page /usr/share/man/man1/foomatic-rip.1.gz

local arguments

arguments=(
    '-v[verbose mode for debugging.]'
    '-q[quiet mode - minimal information output.]'
    '-P[<printer> is the configured printer which should be used for this job.]:<printer>'
    '--ppd[The PPD file <ppdfile> should be applied for processing this job.]:<ppdfile>'
    '-o[Option settings for this job.]'
    '---------------------[This way you can print directly to your printer, use.]'
    '*:filename:_files'
)
_arguments -s $arguments
