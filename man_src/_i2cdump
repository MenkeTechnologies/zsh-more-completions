#compdef i2cdump

# Autogenerated from man page /usr/share/man/man8/i2cdump.8.gz

local arguments

arguments=(
    '-V[Display the version and exit.]'
    '-f[Force access to the device even if it is already busy.]'
    '-r[Limit the range of registers being accessed.]:first-last'
    '-y[Disable interactive mode.]'
    '-a[Allow using addresses between 0x00 - 0x02 and 0x78 - 0x7f. Not recommended.]'
    '*:filename:_files'
)
_arguments -s $arguments
