#compdef etherwake

# Autogenerated from man page /usr/share/man/man8/etherwake.8.gz

local arguments

arguments=(
    '-b[Send the wake-up packet to the broadcast address.]'
    '-D[Increase the Debug Level.]'
    '-i[Use interface ifname instead of the default "eth0".]:ifname'
    '-p[Append a four or six byte password to the packet.]:passwd'
    '-V[Show the program version information.]'
    '*:filename:_files'
)
_arguments -s $arguments
