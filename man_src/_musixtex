#compdef musixtex

# Autogenerated from man page /Library/TeX/texbin/man/man1/musixtex.1

local arguments

arguments=(
    '-v[output program name and version number, and quit.]'
    '-h[output usage summary and quit.]'
    '-l[assume LaTeX source; implied if the file has . ltx extension.]'
    '-p[change the TeX processor to pdfetex (1) or pdflatex (1) (and doesn\t run …]'
    '-c[preprocess pmx file using pmxchords (1).]'
    '-d[don\t generate a . ps file and change the dvi processor to dvipdfm (1).]'
    '-D[use dvix as the dvi processor; e. g. , -D "dvipdfm -m 0. 9".]:dvix'
    '-P[use ps2pdf as the Postscript processor; e. g. , -P "ps2pdf -sPAPERSIZE=a4".]:ps2pdf'
    '-F[use fmt as the TeX processor; e. g. , -F "luatex --output-format=dvi".]:fmt'
    '-m[stop processing at the pmx file.]'
    '-M[use mtxx as the mtx preprocessor; e. g. , -M "prepmx -i".]:mtxx'
    '-A[use aspcx as the aspc preprocessor; e. g. , -A "autosp -l".]:aspcx'
    '-t[stop processing at the tex/mid files.]'
    '-s[stop processing at the dvi file.]'
    '-g[stop processing at the ps file.]'
    '-i[retain intermediate and log files.]'
    '-1[one-pass \[pdf\]\[la\]tex processing; i. e. , without calling musixflx.]'
    '-x[call makeindex (1) to update index database.]'
    '-f[restore default processing.]'
    '-q[quiet mode (only musixtex\s own messages); other messages are diverted to a t…]'
    '--lx[Note that four TeX engines are available via the -l and -p options:.]'
    '*:filename:_files'
)
_arguments -s $arguments
