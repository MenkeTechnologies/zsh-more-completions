#compdef osacompile

# Autogenerated from man page /usr/share/man/man1/osacompile.1

local arguments

arguments=(
    '-l[Override the language for any plain text files.]'
    '-e[Enter one line of a script.]'
    '-o[Place the output in the file name.]'
    '-x[Save the resulting script as execute-only.]'
    '-s[Stay-open applet.]'
    '-u[Use startup screen.]'
    '-a[Create the applet or droplet for the specified target architecture arch.]'
    '-d[Place the resulting script in the data fork of the output file.]'
    '-r[Place the resulting script in the resource fork of the output file, in the sp…]'
    '-t[Set the output file type to type, where type is a four-character code.]'
    '-c[Set the output file creator to creator, where creator is a four-character cod…]'
    '*:filename:_files'
)
_arguments -s $arguments
