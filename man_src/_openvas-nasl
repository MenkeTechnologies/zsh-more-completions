#compdef openvas-nasl

# Autogenerated from man page /usr/share/man/man1/openvas-nasl.1.gz

local arguments

arguments=(
    '-T[Makes nasl write verbosely what the script does in the file tracefile , ala …]:tracefile'
    '-t[Apply the NASL script to target which may be a single host (127. 0. 0.]:target'
    '-e[Specifies the network interface to be used as the source for established conn…]:iface'
    '-s[Sets the return value of safe_checks() to 1.]'
    '-D[Only run the description part of the script.]'
    '-B[Runs in description mode before running the script.]'
    '-L[Lint the script (run extended checks).]'
    '-X[Run the script with disabled signature verification.]'
    '-h[Show help.]'
    '-v[Show the version of NASL.]'
    '-d[Output debug information to stderr.]'
    '-k[Set KB key to value. Can be used multiple times.]:value'
    '*:filename:_files'
)
_arguments -s $arguments
