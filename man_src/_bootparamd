#compdef bootparamd

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/bootparamd.8.gz

local arguments

arguments=(
    '-d[Display the debugging information.]'
    '-s[Log the debugging information with syslog 3.]'
    '-r[The default router (a machine or an IP-address).]'
    '-f[The file to use as boot parameter file instead of /etc/bootparams.]'
    '*:filename:_files'
)
_arguments -s $arguments
