#compdef mpstat

# Autogenerated from man page /usr/share/man/man1/mpstat.1.gz

local arguments

arguments=(
    '-A[This option is equivalent to specifying "-u -I ALL -P ALL".]'
    '-I[Report interrupts statistics.]'
    '-P[Indicate the processor number for which statistics are to be reported.]'
    '-u[Report CPU utilization. The following values are displayed: CPU . RS .]'
    '-V[Print version number then exit.]'
    '*:filename:_files'
)
_arguments -s $arguments
