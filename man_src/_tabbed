#compdef tabbed

# Autogenerated from man page /usr/share/man/man1/tabbed.1.gz

local arguments

arguments=(
    '-c[close tabbed when the last tab is closed. Mutually exclusive with -f.]'
    '-d[detaches tabbed from the terminal and prints its XID to stdout.]'
    '-f[fill up tabbed again by spawning the provided command, when the last tab is c…]'
    '-h[will print the usage of tabbed.]'
    '-g[defines the X11 geometry string, which will fixate the height and width of ta…]:geometry"'
    '-n[will set the WM_CLASS attribute to name.]:name"'
    '-p[will set the absolute or relative position of where to start a new tab.]'
    '-r[will replace the narg th argument in command with the window id, rather tha…]:narg"'
    '-s[will disable automatic spawning of the command.]'
    '-t[defines the selected background color.]:color"'
    '-T[defines the selected foreground color.]:color"'
    '-u[defines the normal background color.]:color"'
    '-U[defines the normal foreground color.]:color"'
    '-v[prints version information to stderr, then exits. USAGE.]'
    '*:filename:_files'
)
_arguments -s $arguments
