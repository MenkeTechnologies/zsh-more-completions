#compdef xfdesktop

# Autogenerated from man page /usr/share/man/man1/xfdesktop.1.gz

local arguments

arguments=(
    {-h,--help}'[Show help options.]'
    '--help-all[Show all help options.]'
    '--help-gtk[Show GTK+ Options.]'
    '--help-sm-client[Show session management options . SS APPLICATION OPTIONS:.]'
    {-V,--version}'[Display version information.]'
    {-R,--reload}'[Causes an already-running instance of xfdesktop to reload all its settings an…]'
    {-N,--next}'[Causes an already-running instance of xfdesktop to advance to the next wallpa…]'
    {-M,--menu}'[Causes an already-running instance of xfdesktop to pop up the applications me…]'
    {-W,--windowlist}'[Causes an already-running instance of xfdesktop to pop up the window list men…]'
    {-A,--arrange}'[Automatically arrange all the icons on the desktop.]'
    {-D,--disable-wm-check}'[Do not wait for a window manager on startup.]'
    {-e,--enable-debug}'[Enable debug messages.]'
    {-d,--disable-debug}'[Disable debug messages.]'
    {-Q,--quit}'[Cause xfdesktop to quit.]'
    '--display[X display to use.]:DISPLAY'
    '*:filename:_files'
)
