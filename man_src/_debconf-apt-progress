#compdef debconf-apt-progress

# Autogenerated from man page /usr/share/man/man1/debconf-apt-progress.1.gz

local arguments

arguments=(
    '--config[Print environment variables necessary to start up a progress bar frontend.]'
    '--start[Start up a progress bar, running from 0 to 100 by default.]'
    '--from[If used with --start, make the progress bar begin at waypoint rather than 0.]:waypoint'
    '--to[If used with --start, make the progress bar end at waypoint rather than 100.]:waypoint'
    '--stop[Stop a running progress bar.]'
    '--no-progress[Avoid starting, stopping, or stepping the progress bar.]'
    '--dlwaypoint[Specify what percent of the progress bar to use for downloading packages.]:percentage'
    '--logfile[Send the normal output from apt to the given file.]:file'
    '--logstderr[Send the normal output from apt to stderr.]'
    '*:filename:_files'
)
_arguments -s $arguments
