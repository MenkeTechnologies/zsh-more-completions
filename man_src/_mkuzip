#compdef mkuzip

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/mkuzip.8.gz

local arguments

arguments=(
    '-o[Name of the output file outfile.]'
    '-L[Use lzma 3 compression algorithm instead of the default zlib 3.]'
    '-s[Split the image into clusters of cluster_size bytes, 16384 bytes by default.]'
    '-v[Display verbose messages.]'
    '-Z[Disable zero-blocks detection and elimination.]'
    '-d[Enable de-duplication.]'
    '-S[Print summary about the compression ratio as well as output file size after f…]'
    '-j[Specify the number of compression jobs that mkuzip runs in parallel to speed …]'
    '*:filename:_files'
)
_arguments -s $arguments
