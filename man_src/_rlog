#compdef rlog

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/rlog.1.gz

local arguments

arguments=(
    '-R[Print only the name of the \\*r file.]'
    '-v[Print only the working pathname and tip-revision.]:"string"'
    '-h[Print only the \\*r pathname, working pathname, head, default branch, access l…]'
    '-t[Print the same as -h , plus the descriptive text.]'
    '-N[Do not print the symbolic names.]'
    '-b[Print information about the revisions on the default branch, normally the hig…]'
    '-d[Print information about revisions with a checkin date/time in the ranges give…]:"dates"'
    '-l[Print information about locked revisions only.]:f2lockers'
    '-r[prints information about revisions given in the comma-separated list revisio…]:f2revisions'
    '-s[prints information about revisions whose state attributes match one of the st…]:states'
    '-w[prints information about revisions checked in by users with login names appea…]:f2logins'
    '-T[This option has no effect; it is present for compatibility with other \\*r com…]'
    '-V[Print \\*r\s version number.]'
    '-x[Use suffixes to characterize \\*r files. See ci (1) for details.]:"suffixes"'
    '-z[Without options, rlog prints complete information.]:.'
    '-L[Ignore RCSThis is convenient in combination with.]'
    '--z 1990/01/12[.]:04:00:00 (default)'
    '--zLT 1990-01-11[.]:20:00:00-08'
    '--z+05:30 1990-01-12[EXAMPLES rlog -L -R RCS/* rlog -L -h RCS/* rlog -L -l RC…]:09:30:00+05:30'
    '*:filename:_files'
)
_arguments -s $arguments
