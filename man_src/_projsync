#compdef projsync

# Autogenerated from man page /usr/share/man/man1/projsync.1.gz

local arguments

arguments=(
    '--endpoint[Defines the URL where to download the master files.]:URL'
    '--local-geojson-file[Defines the filename for the master GeoJSON files that references resources.]:FILENAME'
    '--user-writable-directory[Specifies that resource files must be downloaded in the user writable directo…]'
    '--system-directory[Specifies that resource files must be downloaded in the ${installation_prefix…]'
    '--target-dir[Directory into which resource files must be downloaded. UNINDENT NDENT 0. 0.]:DIRNAME'
    '--bbox[Specify an area of interest to restrict the resources to download.]:west_longsouth_lateast_longnorth_lat'
    '--spatial-test[Specify how the extent of the resource files are compared to the area of use …]:contains|intersects'
    '--source-id[Restrict resource files to be downloaded to those whose source_id property co…]:ID'
    '--area-of-use[Restrict resource files to be downloaded to those whose area_of_use property …]:NAME'
    '--file[Restrict resource files to be downloaded to those whose name property contain…]:NAME'
    '--all[Ask to download all files. UNINDENT NDENT 0. 0.]'
    '--exclude-world-coverage[Exclude files which have world coverage. UNINDENT NDENT 0. 0.]'
    {-q,--quiet}'[Quiet mode . UNINDENT NDENT 0. 0.]'
    '--dry-run[Simulate the behavior of the tool without downloading resource files.]'
    '--list-files[List file names, with the source_id and area_of_use properties.]'
    '*:filename:_files'
)
_arguments -s $arguments
