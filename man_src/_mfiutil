#compdef mfiutil

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/mfiutil.8.gz

local arguments

arguments=(
    '-u[unit specifies the unit of the controller to work with.]'
    '-d[Print numeric device IDs as drive identifier. This is the default.]'
    '-e[Print drive identifiers in enclosure:slot form.]'
    '*:filename:_files'
)
_arguments -s $arguments
