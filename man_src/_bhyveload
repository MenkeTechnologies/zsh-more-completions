#compdef bhyveload

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/bhyveload.8.gz

local arguments

arguments=(
    '-c[cons-dev is a tty 4 device to use for bhyveload terminal I/O.]'
    '-d[The disk-path is the pathname of the guest\s boot disk image.]'
    '-e[Set the Fx loader environment variable name to value.]'
    '-h[The host-path is the directory at the top of the guest\s boot filesystem.]'
    '-l[Specify a different OS loader. By default bhyveload will use /boot/userboot.]'
    '-m[memsize is the amount of memory allocated to the guest.]'
    '-C[Include guest memory in the core file when bhyveload dumps core.]'
    '-S[Wire guest memory.]'
    '*:filename:_files'
)
_arguments -s $arguments
