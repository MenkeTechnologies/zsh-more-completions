#compdef usbhidctl

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/usbhidctl.1.gz

local arguments

arguments=(
    '-a[Show all items and their current values if device returns.]'
    '-f[Specify a path name for the device to operate on.]'
    '-l[Loop and dump the device data every time it changes.]'
    '-n[Suppress printing of the item name.]'
    '-r[Dump the report descriptor.]'
    '-t[Specify a path name for the HID usage table file.]'
    '-v[Be verbose.]'
    '-w[Change item values.]'
    '-x[Dump data in hexadecimal as well as decimal.]'
    '-z[Reset reports to zero before processing other arguments.]'
    '*:filename:_files'
)
_arguments -s $arguments
