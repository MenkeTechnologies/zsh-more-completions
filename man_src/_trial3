local arguments

arguments=(
    {-j,--jobs}'[Set the number of process workers to run.]':number
    {-b,--debug}'[Run the tests in a debugger.]'
    {-B,--debug-stacktraces}'[Report Deferred creation and callback stack traces.]'
    '--coverage[Generate coverage information in the `coverage\ subdirectory of the trial tem…]'
    '--debugger[Specifies the debugger to use when the --debug option is passed.]'
    '--disablegc[Disable the garbage collector for the duration of the test run.]'
    {-e,--rterrors}'[Print tracebacks to standard output as soon as they occur.]'
    '--force-gc[Run gc. collect() before and after each test case.]'
    '(- * :)'{-h,--help}'[Print a usage message to standard output, then exit.]'
    '--help-order[Print a list of possible orders that TestCase test methods can be run in, the…]'
    '--help-reporters[Print a list of valid reporters to standard output, then exit.]'
    '--help-reactors[Print a list of possible reactors to standard output, then exit.]'
    {-l,--logfile}'[Direct the log to a different file. The default file is `test. log\.]':logfile
    {-n,--dry-run}'[Go through all the tests and make them pass without running.]'
    {-N,--no-recurse}'[By default, trial recurses through packages to find every module inside every…]'
    '--nopm[Don\t automatically jump into debugger for post-mortem analysis of exceptions.]'
    '--order[Specify what order to run the individual test methods within the given TestCa…]:order'
    '--profile[Run tests under the Python profiler.]'
    {-r,--reactor}'[Choose which reactor to use. See --help-reactors for a list.]':reactor
    '--recursionlimit[Set Python\s recursion limit. See sys. setrecursionlimit().]:limit'
    '--reporter[Select the reporter to use for trial\s output.]:reporter'
    {-x,--exitfirst}'[Stop the test run after the first test which does not succeed.]'
    '--spew[Print an insanely verbose log of everything that happens.]'
    '--tbformat[Format to display tracebacks with.]:format'
    '--temp-directory[WARNING: Do not use this option unless you know what you are doing.]:directory'
    '--testmodule[Ask trial to look into filename and run any tests specified using the Emacs-s…]:filename'
    '--unclean-warnings[As of Twisted 8.]'
    {-u,--until-failure}'[Keep looping the tests until one of them raises an error or a failure.]'
    '--version[Prints the Twisted version number and exit.]'
    '--without-module[Simulate the lack of the specified comma-separated list of modules.]:modulenames'
    {-z,--random}'[Run the tests in random order using the specified seed.]':seed
    '*:filename:_files'
)
_arguments -s $arguments
