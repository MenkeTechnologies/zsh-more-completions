    {--summary,--no-summary}'[Enable/disable final summary (default enabled).]'
    '--only-summary[No messages, show only the final summary (default disabled).]'
    {--shipouts,--no-shipouts}'[Enable/disable reporting shipouts (default disabled with page-delay, enabled …]'
    {--repetitions,--no-repetitions}'[Allow/prevent repeated messages (default disabled, i. e.]'
    {--heartbeat,--no-heartbeat}'[Enable/disable progress gauge in page-delay mode (default enabled).]'
    {-l,--minlevel}'[Filter out messages with severity level lower than LEVEL.]'
    {-u,--unwrap-only}'[Do not filter messages and do not output the summary, only unwrap long, wrapp…]'
    '--silence-package[Filter out messages that can be identified as coming from the given package.]:PKGNAME'
    '--silence-string[Filter out messages that contain the given string (you only need to provide p…]'
    '--silence-file[Filter out messages that have been generated while the given file was being p…]'
    '--semisilence-file[Just like the previous option, but non-recursive.]'
    '--add-[debug[Add MESSAGE to the list of messages known to the program with the given sever…]:MESSAGE'
    {-c,--config-file}'[Read options from the given configuration file.]'
    {-h,--help}'[Show concise options description.]'
    '--version[Print program version.]'
    '*:filename:_files'
)
