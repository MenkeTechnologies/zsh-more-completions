#compdef memcontrol

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/memcontrol.8.gz

local arguments

arguments=(
    '-a[List all range slots, even those that are inactive.]'
    '-b[Memory range base address.]'
    '-l[Length of memory range in bytes, power of 2.]'
    '-o[Text identifier for this setting (7 char max).]'
    '*:filename:_files'
)
_arguments -s $arguments
