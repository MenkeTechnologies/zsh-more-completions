#compdef protect

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/protect.1.gz

local arguments

arguments=(
    '-c[Remove protection from the specified processes.]'
    '-d[Apply the operation to all current children of the specified processes.]'
    '-i[Apply the operation to all future children of the specified processes.]'
    '-g[Apply the operation to all processes in the specified process group.]'
    '-p[Apply the operation to the specified process.]'
    '*:filename:_files'
)
_arguments -s $arguments
