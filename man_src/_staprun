#compdef staprun

# Autogenerated from man page /usr/share/man/man8/staprun.8.gz

local arguments

arguments=(
    '-v[Verbose mode.]'
    '-V[Print version number and exit.]'
    '-w[Suppress warnings from the script.]'
    '-u[Load the uprobes. ko module.]'
    '-c[Command CMD will be run and the staprun program will exit when CMD does.]:CMD'
    '-x[The \_stp_target\ variable will be set to PID.]:PID'
    '-o[Send output to FILE.]:FILE'
    '-b[The systemtap module will specify a buffer size.]:BUFFER_SIZE'
    '-L[Load module and start probes, then detach from the module leaving the probes …]'
    '-A[Attach to loaded systemtap module.]'
    '-C[Control coloring of error messages. WHEN must be either .]:WHEN'
    '-d[Delete a module.]'
    '-D[Run staprun in background as a daemon and show it\s pid.]'
    '-R[Rename the module to a unique name before inserting it.]'
    '-r[Pass the given number and URI data to the tapset functions remote_id() and re…]:N:URI'
    '-S[Sets the maximum size of output file and the maximum number of output files.]:sizeN"'
    '-T[Sets maximum time reader thread will wait before dumping trace buffer.]:timeout'
    '--privilege[option, which staprun will later enforce.]'
    '*:filename:_files'
)
_arguments -s $arguments
