#compdef newfs_apfs

# Autogenerated from man page /usr/share/man/man8/newfs_apfs.8

local arguments

arguments=(
    '-b[The block size of the container.]'
    '-o[Additional (undocumented) formatting options.]'
    '-i[Creates a case-insensitive volume. This is the default on macOS.]'
    '-e[Creates a case-sensitive volume.]'
    '-U[The UID of the root volume. Defaults to current UID.]'
    '-G[The GID of the root volume. Defaults to current GID.]'
    '-q[Places a quota, or upper limit, on the size of the volume.]'
    '-r[Reserves an amount of space for this volume.]'
    '-s[The fixed size of the volume.]'
    '-v[The name of the volume. The default is untitled.]'
    '-A[Add a volume to an existing container.]'
    '-C[Create a container only, with no volume.]'
    '-E[Enable volume encryption. The volume metadata is also encrypted.]'
    '-F[Format as a Fusion device (with the secondary device specified here).]'
    '-R[Set the OS role of the volume within the container.]'
    '*:filename:_files'
)
