#compdef mailq

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/mailq.1.gz

local arguments

arguments=(
    '--Ac[Show the mail submission queue specified in /etc/mail/submit.]'
    '--qL[Show the "lost" items in the mail queue instead of the normal queue items.]'
    '--qQ[Show the quarantined items in the mail queue instead of the normal queue item…]'
    '--qI[Limit processed jobs to those containing substr as a substring of the queue …]:substr'
    '--qR[Limit processed jobs to those containing substr as a substring of one of the…]:substr'
    '--qS[Limit processed jobs to those containing substr as a substring of the sender…]:substr'
    '-v[Print verbose information.]'
    '-q[option is used to limit the processed jobs.]'
    '*:filename:_files'
)
_arguments -s $arguments
