#compdef tcpick

# Autogenerated from man page /usr/share/man/man8/tcpick.8.gz

local arguments

arguments=(
    {-i,--interface}'[listen on selected interface, (i. e. ppp0 or eth0).]':interface
    {-r,--readfile}'[reads raw packets from a file written with tcpdump -w instead of using a net…]'
    '-a[Displays host names instead of ip addresses.]'
    {-C,--colors}'[Uses terminal colors: very nice! It should help you to read the output of tcp…]'
    {-D,--dirs}'[Create directories to store sniffed sessions.]'
    '-e[Exits when count packets have been sniffed.]:count'
    '-E[Exit when number sniffed connections are detected as "CLOSED".]:number'
    '--Ef[Exit when the first number connections are detected as "CLOSED".]:number'
    {--F1,--F2,--filenaming}'[Choose the filenaming system.]'
    '-h[Shows source and destination ip and port; shows tcp flags as letters.]'
    '--help[Displays a short help summary.]'
    '-p[Don\t put the network interface in promiscuous mode.]'
    '-S[Suppresses the "status of the connection" banner.]'
    '--separator[Add a separator for the payloads displayed.]'
    '-t[Adds timestamp in hour:minutes:seconds:microseconds format.]'
    '--td[Like -tFP with date timestamp in day-month-year format.]'
    '-T[Track number connections.]:number'
    '--Tf[Track only the first number connections; the following will be discarded.]:number'
    '-v[Quite useless, yet. Set verbosity level.]:verbosity'
    '-X[Connections are considered EXPIRED when there is no traffic for at least time…]:timeout'
    '--version[Displays the tcpick version.]'
    '--yH[View data in hexadecimal-spaced mode (for the hexdump see -yx and.]'
    '--yX[.]:options.'
    '--yP[Shows data contained in the tcp packets.]'
    '--yR[Displays all kind of characters, printable and non printable.]'
    '--yx[Shows all data after the header in hexadecimal dump of 16 bytes per line.]'
    '--yU[Shows all data after the header, but Unprintable characters are displayed as …]'
    '--wR[ C (output only client data) and S (output only server data) to the -w set.]'
    '--wP[Unprintable characters are written like dots.]'
    '--wU[Unprintable characters are displayed as hexadecimal values between a "<" and …]'
    '--wH[The flow is written in hexadecimal-spaced mode.]'
    '*:filename:_files'
)
_arguments -s $arguments
