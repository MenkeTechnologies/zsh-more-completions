local arguments

arguments=(
    '-p[Returns the data in XML format.]'
    '-g[Returns the default Kerberos realm name.]'
    '-l[Returns a list of the services sso_util knows how to Kerberize.]'
    '-L[Returns the default Kerberos log file paths.]'
    '-r[Returns whether or not the given node has a Kerberos record associated with i…]'
    '-a[Account name of an administrator authorized to make changes in the Kerberos d…]'
    '-u[Forces the update, ignoring that the update may already have been installed.]'
    '-R[Name of the Computer record containing the secure config record.]'
    '-f[Specifies the directory node in which to find the given computer record.]'
    '*:filename:_files'
)
