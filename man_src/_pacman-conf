#compdef pacman-conf

# Autogenerated from man page /usr/share/man/man8/pacman-conf.8.gz

local arguments

arguments=(
    {-c,--config}'[Specify an alternate configuration file.]':<path>
    {-R,--rootdir}'[Specify an alternate installation root (default is /).]':<path>
    {-r,--repo}'[Query options for a specific repository.]':<remote>
    {-v,--verbose}'[Always shown directive names.]'
    {-l,--repo-list}'[List configured repositories.]'
    {-h,--help}'[Output syntax and command line options.]'
    {-V,--version}'[Display version and exit.]'
)
_arguments -s $arguments
