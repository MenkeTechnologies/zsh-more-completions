#compdef electrum

# Autogenerated from man page /usr/share/man/man1/electrum.1.gz

local arguments

arguments=(
    {-g,--gui}'[Select user interface, possible values are qt, lite, gtk or text.]'
    {-w,--wallet}'[Use wallet from WALLET_PATH, default is electrum. dat.]'
    {-o,--offline}'[Remain offline.]'
    {-a,--all}'[Show all addresses.]'
    {-b,--balance}'[Show the balance of listed addresses.]'
    {-l,--labels}'[Show the labels of listed addresses.]'
    {-f,--fee}'[set tx fee.]'
    {-F,--fromad}'[dr=FROM_ADDR . PP set source address for payto/mktx.]'
    {-c,--changeaddr}'[PP set the change address for payto/mktx.]'
    {-s,--server}'[set server host:port:protocol, where protocol is t or h.]'
    {-p,--proxy}'[set proxy \[type:\]host\[:port\], where type is socks4,socks5 or http.]'
    {-v,--verbose}'[show debugging information.]'
    {-P,--portable}'[portable wallet.]'
    {-L,--lang}'[default language used in GUI.]'
    {-u,--usb}'[Turn on support for hardware wallets (EXPERIMENTAL).]'
    '*:filename:_files'
)
_arguments -s $arguments
