#compdef mkzimage_cmdline

# Autogenerated from man page /usr/share/man/man8/mkzimage_cmdline.8.gz

local arguments

arguments=(
    '-a[activate or deactivate the the cmdline.]:0|1'
    '-s[pass this string to the kernel. It can be up to 511 chars long.]:cmdline'
    '-c[clear cmdline area in zImage AUTHOR Olaf Hering <olh@suse. de>.]'
    '*:filename:_files'
)
_arguments -s $arguments
