#compdef klogd

# Autogenerated from man page /usr/share/man/man8/klogd.8.gz

local arguments

arguments=(
    '-c[Sets the default log level of console messages to n.]:n'
    '-d[Enable debugging mode. This will generate LOTS of output to stderr.]'
    '-f[Log messages to the specified filename rather than to the syslog facility.]:file'
    {-i,-I}'[Signal the currently executing klogd daemon.]'
    '-n[Avoid auto-backgrounding.]'
    '-o[Execute in \one-shot\ mode.]'
    '-p[Enable paranoia.]'
    '-s[Force klogd to use the system call interface to the kernel message buffers.]'
    '-k[Use the specified file as the source of kernel symbol information.]:file'
    '-v[Print version and exit.]'
    '-x[Omits EIP translation and therefore doesn\t read the System. map file.]'
    '-2[When symbols are expanded, print the line twice.]'
    '*:filename:_files'
)
_arguments -s $arguments
