#compdef repo-rss

# Autogenerated from man page /usr/share/man/man1/repo-rss.1.gz

local arguments

arguments=(
    '(- * :)'{-h,--help}'[Help; display a help message and then quit.]'
    '-f[Specify the file to which the RSS will be written. Default is repo-rss. xml.]'
    '-l[Specify the URL to the RSS feed. Default is http://yum. baseurl. org/.]'
    '-t[Specify the title for the RSS feed.]:title'
    '-d[Specify the description of the RSS feed.]:description'
    '-r[Specify the number of days to consider most recent. Default is 3.]:days'
    '--tempcache[Enable the use of a temporary directory for the yum cache.]'
    '*:filename:_files'
)
_arguments -s $arguments
