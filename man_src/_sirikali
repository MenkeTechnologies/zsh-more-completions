#compdef sirikali

# Autogenerated from man page /usr/share/man/man1/sirikali.1.gz

local arguments

arguments=(
    '-d[Path to where a volume to be auto unlocked/mounted is located.]:dir'
    '-m[Tool to use to open a default file manager(default tool is xdg-open).]:tool'
    '-e[Start the application without showing the GUI.]'
    '-b[A name of a backend to retrieve a password from when a volume is open from CL…]:backend'
    '-k[When opening a volume from CLI,a value of "rw" will open the volume in read m…]:val'
    '-z[Full path of the mount point to be used when the volume is opened from CLI.]:path'
    '-c[Set Volume Configuration File Path when a volume is opened from CLI.]:path'
    '-i[Set inactivity timeout(in minutes) to dismount the volume when mounted from C…]:min'
    '-o[Set mount options when mounting a volume from CLI.]:val'
    '-u[Unmount volume.]:path'
    '-p[Print a list of unlocked volumes.]'
    '-f[Path to keyfile.]:path'
    '-s[Option to trigger generation of password hash.]'
    '(- * :)'{-h,--help}'[Show a help message. SEE ALSO.]'
    '*:filename:_files'
)
