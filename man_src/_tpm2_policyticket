#compdef tpm2_policyticket

# Autogenerated from man page /usr/share/man/man1/tpm2_policyticket.1.gz

local arguments

arguments=(
    {-L,--policy}'[File to save the compounded policy digest.]':FILE:
    {-S,--session}'[The policy session file generated via the -S option to tpm2_startauthsession(…]':FILE:
    {-n,--name}'[Name of the object that validated the authorization.]':FILE:
    '--ticket[The ticket file to record the authorization ticket structure.]:FILE:'
    '--timeout[The file path to record the timeout structure returned.]:FILE:'
    {-q,--qualification}'[Optional, the policy qualifier data that the signer can choose to include in …]':FILE_OR_HEX_STR:
    {-h,--help}'[By default, it attempts to invoke the manpager for the tool, however, on fail…]'
    {-v,--version}'[tool, supported tctis and exit. \[bu\] 2.]'
    {-V,--verbose}'[tool prints to the console during its execution.]'
    {-Q,--quiet}'[\[bu\] 2.]'
    {-Z,--enable-errata}'[errata fixups.]'
    {-R,--autoflush}'[objects created by the command.]'
    '-c[.]'
    {-C,-i}'[Create ticket-able policy tpm2_startauthsession -S session.]'
    '-x[.]'
    '-t[.]:0xFFFFFE0C'
    '*:filename:_files'
)
_arguments -s $arguments
