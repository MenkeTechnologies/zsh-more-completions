#compdef ir-keytable

# Autogenerated from man page /usr/share/man/man1/ir-keytable.1.gz

local arguments

arguments=(
    {-a,--auto-load}'[Auto-load keymaps, based on a configuration file. Only works with --sysdev.]':CFGFILE
    {-c,--clear}'[Clears the scancode to keycode mappings.]'
    {-D,--delay}'[Sets the delay before repeating a keystroke.]':DELAY
    {-k,--set-key}'[Set scancode to keycode mapping.]':SCANKEY
    {-p,--protocol}'[Comma separated list of kernel protocols or BPF protocol to enable.]':PROTOCOL
    {-e,--parameter}'[Comma separated list of parameters for the BPF protocol.]':PARAMETER
    {-P,--period\\}'[Sets the period to repeat a keystroke.]':fiPERIOD
    {-r,--read}'[Read and show the current scancode to keycode mapping.]'
    {-s,--sysdev}'[rc device to control, defaults to rc0 if not specified.]':SYSDEV
    {-t,--test}'[test if the rc device is generating events.]'
    {-v,--verbose}'[Enables debug messages.]'
    {-w,--write}'[Reads the protocols and scancode to keycode mapping from the KEYMAP file, andâ€¦]':KEYMAP
    {-\?,--help}'[Prints the help message.]'
    '--usage[Give a short usage message.]'
    {-V,--version}'[print the v4l2-utils version .]'
    '--period[Sets the period to repeat a keystroke.]:PERIOD'
    '*:filename:_files'
)
_arguments -s $arguments
