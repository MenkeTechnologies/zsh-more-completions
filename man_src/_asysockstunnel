#compdef asysockstunnel

local arguments

arguments=(
  {-h,--help}'[show this help message and exit]'
  {-l,--listen-ip}'[listener IP address to bind to]'
  {-p,--listen-port}'[listener port number to bind to]'
  {-t,--timeout}'[endpoint timeout]'
  {-v,--verbose}'[]'
  '*:filename:_files'
)

_arguments -s $arguments
