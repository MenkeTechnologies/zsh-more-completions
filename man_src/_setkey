#compdef setkey

# Autogenerated from man page /usr/share/man/man8/setkey.8

local arguments

arguments=(
    '-\?[Print short help.]'
    '-a[setkey usually does not display dead SAD entries with D.]'
    '-D[Dump the SAD entries. If P is also specified, the SPD entries are dumped.]'
    '-F[Flush the SAD entries. If P is also specified, the SPD entries are flushed.]'
    '-H[Add hexadecimal dump in x mode.]'
    '-h[On , synonym for H. On other systems, synonym for ?.]'
    '-k[Use semantics used in kernel. Available only in Linux. See also r.]'
    '-l[Loop forever with short output on D.]'
    '-n[No action.]'
    '-r[Use semantics described in IPsec RFCs. This mode is default.]'
    '-x[Loop forever and dump all the messages transmitted to the PF_KEY socket.]'
    '-V[Print version string.]'
    '-v[Be verbose.]'
    '-m[Specify a security protocol mode for use.]'
    '-u[Specify the identifier of the policy entry in the SPD. See policy.]'
    '-f[defines the content of the ESP padding.]'
    '--lh[.]'
    '--ls[Specify hard/soft life time duration of the SA measured in seconds.]'
    '--bh[.]'
    '--bs[Specify hard/soft life time duration of the SA measured in bytes transported.]'
    '-E[Specify an encryption algorithm ealgo for ESP.]'
    '-A[Specify an authentication algorithm for AH.]'
    '-C[Specify a compression algorithm for IPComp.]'
    '-P[.]'
    '*:filename:_files'
)
_arguments -s $arguments
