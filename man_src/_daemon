#compdef daemon

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/daemon.8.gz

local arguments

arguments=(
    '-c[Change the current working directory to the root /.]'
    '-f[Redirect standard input, standard output and standard error to /dev/null.]'
    '-S[Enable syslog output.]'
    '-o[Append output from the daemonized process to output_file.]'
    '-m[Redirect output from the child process stdout (1), stderr (2), or both (3).]'
    '-p[Write the ID of the created process into the child_pidfile using the pidfile …]'
    '-P[Write the ID of the daemon process into the supervisor_pidfile using the pidf…]'
    '-r[Supervise and restart the program after a one-second delay if it has been ter…]'
    '-R[Supervise and restart the program after the specified delay if it has been te…]'
    '-t[Set the title for the daemon process.]'
    '-u[Login name of the user to execute the program under.]'
    '-s[These priorities are accepted: emerg, alert, crit, err, warning, notice, info…]'
    '-l[These facilities are accepted: auth, authpriv, console, cron, daemon, ftp, ke…]'
    '-T[Set the tag which is appended to all syslog messages. The default is daemon.]'
    '*:filename:_files'
)
_arguments -s $arguments
