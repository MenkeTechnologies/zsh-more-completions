#compdef uhsoctl

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/uhsoctl.1.gz

local arguments

arguments=(
    '-a[Specify APN to connect to.]'
    '-c[Specify CID (Context ID) to use, by default CID 1 is used.]'
    '-p[Specify SIM PIN.]'
    '-u[Specify username.]'
    '-k[Specify username.]'
    '-r[Path to resolv. conf, default /etc/resolv. conf.]'
    '-f[Explicitly set the serial port to use as controlling terminal.]'
    '-b[Fork into background directly, before a connection has been established.]'
    '-n[Never fork into background, run entirely in foreground.]'
    '*:filename:_files'
)
_arguments -s $arguments
