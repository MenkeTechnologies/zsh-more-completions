#compdef dcraw

# Autogenerated from man page /usr/share/man/man1/dcraw.1.gz

local arguments

arguments=(
    '-v[Print verbose messages, not just warnings and errors.]'
    '-c[Write decoded images or thumbnails to standard output.]'
    '-e[Extract the camera-generated thumbnail, not the raw image.]'
    '-z[Change the access and modification times of an AVI, JPEG, TIFF or raw file to…]'
    '-i[Identify files but don\t decode them.]'
    '-I[Read the raw pixels from standard input in CPU byte order with no header.]'
    '-P[Read the dead pixel list from this file instead of ". badpixels".]:deadpixels.txt'
    '-K[Subtract a dark frame from the raw data.]:darkframe.pgm'
    '-k[When shadows appear foggy, you need to raise the darkness level.]:darkness'
    '-S[When highlights appear pink, you need to lower the saturation level.]:saturation'
    '-n[Use wavelets to erase noise while preserving real detail.]:noise_threshold'
    '-C[Enlarge the raw red and blue layers by the given factors, typically 0.]:blue_mag'
    '-H[Clip all highlights to solid white (default).]:0'
    '-w[Use the white balance specified by the camera.]'
    '-a[Calculate the white balance by averaging the entire image.]'
    '-A[Calculate the white balance by averaging a rectangular area.]'
    '-r[Specify your own raw white balance.]'
    '-M[otherwise.]'
    '-o[Select the output colorspace when the.]:0-6'
    '-p[option is not used:.]'
    '-d[Show the raw data as a grayscale image with no interpolation.]'
    '-D[Same as.]'
    '-E[Same as.]'
    '-h[Output a half-size color image. Twice as fast as.]'
    '-q[.]:.'
    '-f[Interpolate RGB as four colors.]'
    '-m[After interpolation, clean up color artifacts by repeatedly applying a 3x3 me…]:number_of_passes'
    '-W[Use a fixed white level, ignoring the image histogram.]'
    '-b[Divide the white level by this number, 1. 0 by default.]:brightness'
    '-g[Set the gamma curve, by default BT. 709 ( -g 2. 222 4. 5 ).]:toe_slope'
    '-6[Write sixteen bits per sample instead of eight.]'
    '-4[Linear 16-bit, same as.]'
    '-T[Write TIFF with metadata instead of PGM/PPM/PAM.]'
    '-t[Flip the output image.]:0-790180270'
    '-j[For Fuji Super CCD cameras, show the image tilted 45 degrees.]'
    {-s,-s}'[If a file contains N raw images, choose one or "all" to decode.]'
    '*:filename:_files'
)
_arguments -s $arguments
