#compdef fixfiles

# Autogenerated from man page /usr/share/man/man8/fixfiles.8.gz

local arguments

arguments=(
    '-B[If specified with onboot, this fixfiles will record the current date in the /.]'
    '-F[Force reset of context to match file_context for customizable files.]'
    '-f[Clear /tmp directory with out prompt for removal.]'
    '-R[Use the rpm database to discover all files within the specified packages and …]:rpmpackagenamerpmpackagename...'
    '-C[Run a diff on the PREVIOUS_FILECONTEXT file to the currently installed one, …]:PREVIOUS_FILECONTEXT'
    '-N[Only act on files created after the specified date.]:time'
    '*:filename:_files'
)
_arguments -s $arguments
