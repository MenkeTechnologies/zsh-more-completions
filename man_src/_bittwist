#compdef bittwist

# Autogenerated from man page /usr/share/man/man1/bittwist.1

local arguments

arguments=(
    '-d[Print a list of network interfaces available.]'
    '-v[Print timestamp for each packet.]'
    '--vv[Print timestamp and hex data for each packet.]'
    '-i[Send pcap-file(s) out onto the network through interface.]:interface'
    '-s[Packet length to send. Set length to:.]:length'
    '-l[Send pcap-file(s) out onto the network for loop times.]:loop'
    '-c[Send up to count packets. Default is to send all packets from pcap-file(s).]:count'
    '-m[Set interval multiplier to speed.]:speed'
    '-r[Limit the sending to rate Mbps. Value for rate must be between 1 to 1000.]:rate'
    '-p[Set interval to sleep (in seconds), ignoring the actual interval.]:sleep'
    '-1[or any other value from 14 to 1514.]'
    '-h[Print version information and usage.]'
    '*:filename:_files'
)
_arguments -s $arguments
