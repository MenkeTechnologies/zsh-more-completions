#compdef xl2tpd

# Autogenerated from man page /usr/share/man/man8/xl2tpd.8.gz

local arguments

arguments=(
    '-D[This option prevents xl2tpd from detaching from the terminal and daemonizing.]'
    '-l[This option tells xl2tpd to use syslog for logging even when -D was specified.]'
    '-c[Tells xl2tpd to use an alternate config file. Default is /etc/xl2tpd/xl2tpd.]:file>'
    '-s[Tells xl2tpd to use an alternate "secrets" file.]:file>'
    '-p[Tells xl2tpd to use an alternate pid file.]:file>'
    '*:filename:_files'
)
_arguments -s $arguments
