#compdef bibzbladd

# Autogenerated from man page /Library/TeX/texbin/man/man1/bibzbladd.1

local arguments

arguments=(
    '-d[Debug mode.]'
    '-e[If 1 (default), add an empty zblnumber if a zbl cannot be found.]'
    '-f[Force searching for Zbl numbers even if the entry already has one.]'
    '*:filename:_files'
)
_arguments -s $arguments
