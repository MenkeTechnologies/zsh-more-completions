#compdef sucrack

# Autogenerated from man page /usr/share/man/man1/sucrack.1.gz

local arguments

arguments=(
    '-h[print help message.]'
    '-a[use ansi escape codes for nice looking statistics (requires --enable-statisti…]'
    '-s[statistics display intervall (requires --enable-statistics configuration flag…]:<seconds>'
    '-c[only print statistics if a key other than `q\ is pressed.]'
    '-r[enable rewriting of dictionary words (see rules below).]'
    '-w[number of threads to run with.]:<num>'
    '-b[size of the word list buffer.]:<size>'
    '-u[user account to su to.]:<user>'
    '-l[specify certain rules for the rewriting process . PP.]:<rules>'
    '*:filename:_files'
)
_arguments -s $arguments
