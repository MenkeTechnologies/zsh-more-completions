#compdef mount_msdos

# Autogenerated from man page /usr/share/man/man8/mount_msdos.8

local arguments

arguments=(
    '-o[Use the specified mount options, as described in mount 8.]'
    '-u[Set the owner of the files in the file system to uid.]'
    '-g[Set the group of the files in the file system to gid.]'
    '-m[Specify the maximum file permissions for files in the file system.]'
    '*:filename:_files'
)
