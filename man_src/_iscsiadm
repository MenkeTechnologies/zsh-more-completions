#compdef iscsiadm

# Autogenerated from man page /usr/share/man/man8/iscsiadm.8.gz

local arguments

arguments=(
    {-a,--ip}'[ipaddr can be IPv4 or IPv6. This option is only valid for ping submode.]':ipaddr
    {-A,--portal_type}'[Specify the portal type for the new flash node entry to be created.]:portal type:(ipv4 ipv6)'
    {-b,--packetsize}'[Specify the ping packetsize. This option is only valid for ping submode.]':packetsize
    {-c,--count}'[count specify number of ping iterations.]':count
    {-C,--submode}'[Specify the submode for mode. op must be name of submode.]':op
    {-d,--debug}'[print debugging information. Valid values for debug_level are 0 to 8.]':debug_level
    '(- * :)'{-h,--help}'[display help text and exit.]'
    {-H,--host}'[The host argument specifies the SCSI host to use for the operation.]:hostno or MAC'
    {-i,--interval}'[interval specify delay between two ping iterations.]':interval
    {-I,--interface}'[The interface argument specifies the iSCSI interface to use for the operation.]':iface
    {-k,--killiscsid}'[Currently priority must be zero.]':priority
    {-D,--discover}'[Discover targets using the discovery record with the recid matching the the …]'
    {-l,--login}'[For node and fw mode, login to a specified record.]'
        {-L,--loginall}'[For node mode, login all sessions with the node or conn startup values passed…]:mode:(all manual automatic)'
    {-m,--mode}'[specify the mode.]':op
    {-n,--name}'[In node mode, specify a field name in a record.]':name
    {-o,--op}'[Specifies a database operator op.]':op
    {-p,--portal}'[Use target portal with ip-address ip and port.]:ip\:port:_hosts'
    {-P,--print}'[If in node mode print nodes in tree format.]'
    {-T,--targetname}'[Use target targetname.]':targetname
    {-r,--sid}'[Use session ID sid.]'
    {-R,--rescan}'[In session mode, if sid is also passed in rescan the session.]'
    {-s,--stats}'[Display session statistics.]'
    {-S,--show}'[When displaying records, do not hide masked values, such as the CHAP secret (…]'
    {-t,--type}'[type must be sendtargets (or abbreviated as st), slp, isns or fw.]':type
    {-u,--logout}'[logout for a specified record.]'
    {-U,--logoutall}'[logout all sessions with the node or conn startup values passed in or all run…]:mode:(all manual automatic)'
    {-v,--value}'[Specify a value for use with the update operator.]':value
    {-V,--version}'[display version and exit.]'
    {-x,--index}'[Specify the index of the entity to operate on.]':index
    {-I,--interface}'[Specify interface]:interface:_net_interfaces'
    '*:filename:_files'
)
_arguments -s $arguments
