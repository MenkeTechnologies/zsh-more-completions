#compdef antiword.bin

local arguments

arguments=(
  '-f[text output]'
  '-t[output (default)]'
  '-a[size name> Adobe PDF output]'
  '-p[size name> PostScript output]'
  '-x[XML output]'
  '-m[character mapping file]'
  '-w[in characters of text output]'
  '-i[image level (PostScript only)]'
  '-L[landscape mode (PostScript only)]'
  '-r[removed text]'
  '-s[hidden (by Word) text]'
  '*:filename:_files'
)

_arguments -s $arguments
