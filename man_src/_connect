#compdef connect-proxy

# Autogenerated from man page /usr/share/man/man1/connect-proxy.1.gz

local arguments

arguments=(
    '-H[specifies a hostname and port number of the http proxy server to relay.]'
    '-S[specifies the hostname and port number of the SOCKS server to relay.]'
    '-4[specifies SOCKS relaying and indicates protocol version to use.]'
    '-R[method to resolve the hostname.]'
    '-p[will forward a local TCP port instead of using the standard input and output.]'
    '-P[same to \-p\ except keep remote session.]'
    '-w[timeout in seconds for making connection with TARGET host.]'
    '*:filename:_files'
)
_arguments -s $arguments
