#compdef usbconfig

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/usbconfig.8.gz

local arguments

arguments=(
    '-u[Limit device range to USB devices connected to the given USBUS unit.]'
    '-a[Limit device range to the given USB device index.]'
    '-d[Limit device range to USB devices connected to the given unit and address.]'
    '-h[Show help and available commands.]'
    '*:filename:_files'
)
_arguments -s $arguments
