#compdef fonttosfnt

# Autogenerated from man page /opt/X11/share/man/man1/fonttosfnt.1

local arguments

arguments=(
    '-v[Be verbose.]'
    '-c[Do not crop glyphs.]'
    '-b[Write byte-aligned glyph data.]'
    '-r[Do not reencode fonts.]'
    '-g[Set the type of scalable glyphs that we write.]:n"'
    '-m[Set the type of scalable metrics that we write.]:n"'
    '*:filename:_files'
)
_arguments -s $arguments
