#compdef kdump

# Autogenerated from man page /usr/share/man/en.UTF-8/man1/kdump.1.gz

local arguments

arguments=(
    '-d[Display all numbers in decimal.]'
    '-E[Display elapsed timestamps (time since beginning of trace).]'
    '-f[Display the specified file instead of ktrace. out.]'
    '-H[List the thread ID (tid) of the thread with each trace record, if available.]'
    '-l[Loop reading the trace file, once the end-of-file is reached, waiting for mor…]'
    '-m[Display at most maxdata bytes when decoding I/O.]'
    '-n[Suppress ad hoc translations.]'
    '-p[Display only trace events that correspond to the process or thread pid.]'
    '-R[Display relative timestamps (time since previous entry).]'
    '-r[When decoding STRU records, display structure members such as UIDs, GIDs, dat…]'
    '-S[Display system call numbers.]'
    '-s[Suppress display of I/O data.]'
    '-T[Display absolute timestamps for each entry (seconds since epoch).]'
    '-A[Display description of the ABI of traced process.]'
    '-t[See the t option of ktrace 1.]'
    '*:filename:_files'
)
_arguments -s $arguments
