#compdef tzsetup

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/tzsetup.8.gz

local arguments

arguments=(
    '-C[Open all files and directories relative to chroot_directory.]'
    '-n[Do not create or copy files.]'
    '-r[Reinstall the zoneinfo file installed last time.]'
    '-s[Skip the initial question about adjusting the clock if not set to UTC.]'
    '*:filename:_files'
)
_arguments -s $arguments
