#compdef rmextattr

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/rmextattr.8.gz

local arguments

arguments=(
    '-f[(Force.]'
    '-h[(No follow.]'
    '-i[(From stdin. ) Read attribute data from stdin instead of as an argument.]'
    '-n[( NUL Ns -terminate. ) NUL Ns -terminate the extent content written out.]'
    '-q[(Quiet. ) Do not print out the pathname and suppress error messages.]'
    '-s[(Stringify. ) Escape nonprinting characters and put quotes around the output.]'
    '-x[(Hex. ) Print the output in hexadecimal.]'
    '*:filename:_files'
)
_arguments -s $arguments
