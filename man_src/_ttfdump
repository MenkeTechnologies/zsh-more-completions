#compdef ttfdump

# Autogenerated from man page /usr/share/man/man1/ttfdump.1.gz

local arguments

arguments=(
    '-h[Print a help message on standard output and exit.]'
    '-t[If this option is specified then dump the table "tablename", otherwise dump …]:tablename'
    '-g[If this option is specified then dump the glyph with index glyphnumber, othe…]:glyphnumber'
    '-c[Select a TrueType font in a TrueType Collection.]:collection'
    '-o[Output of the dump. If this option is not specified, dump to stdout.]:dumpfile'
    '-i[TrueType font file to be dumped.]:ttfile'
    '*:filename:_files'
)
_arguments -s $arguments
