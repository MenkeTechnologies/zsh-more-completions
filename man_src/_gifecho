#compdef gifecho

# Autogenerated from man page /usr/local/man/man1/gifecho.1.gz

local arguments

arguments=(
    '-v[Verbose mode (show progress). Enables printout of running scan lines.]'
    '-s[Explicitly defines the size of the color map of the resulting gif image.]:colormap-size'
    '-f[Select foreground index (background is always 0).]:foreground'
    '-c[The color to use as the foreground color. White by default.]'
    '-t[One line of text can be provided on the command line.]:text'
    '-h[Print one line command line help, similar to Usage above.]'
    '*:filename:_files'
)
_arguments -s $arguments
