#compdef susepaste-screenshot

# Autogenerated from man page /usr/share/man/man1/susepaste-screenshot.1.gz

local arguments

arguments=(
    '--all[takes screenshot of whole desktop instean.]'
    '-n[nickname of the author of the paste.]:NICK'
    '-t[name of the paste.]:TITLE'
    '-k[API key to paste as you.]:KEY'
    '-e[for how log will be paste stored on the server.]:EXPIRE'
    '*:filename:_files'
)
_arguments -s $arguments
