local arguments

arguments=(
    '--interlace[Produce an interlaced GIF file.]'
    '--sort[Produces a GIF file with a sorted color map.]'
    '--map[mapfile Uses the colors found in the mapfile to create the colormap in the …]'
    '--transparent[ppmtogif marks the specified color as transparent in the GIF image.]:color'
    '--alpha[This option names a PGM file that contains an alpha mask for the image.]:pgmfile'
    '--alphacolor[See -alpha .]'
    '--comment[Include a comment in the GIF output with comment text R text .]:text'
    '--floyd[mapfile.]'
    '--nolzw[This option causes the GIF output, and thus ppmtogif , not to use LZW (Lempe…]'
    '*:filename:_files'
)
