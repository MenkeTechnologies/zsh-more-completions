#compdef gvpack

# Autogenerated from man page /usr/local/share/man/man1/gvpack.1

local arguments

arguments=(
    '-g[Combines the graphs at the graph level.]'
    '--array[Combines the graphs at the graph level, placing them in an array.]'
    '-G[Specifies attributes to be added to the resulting union graph.]:value"'
    '-m[Packs the graphs allowing a margin of output points around the parts.]:"margin"'
    '-n[Combines the graphs at the node level. Clusters are ignored in the packing.]'
    '-o[Prints output to the file output. If not given, gvpack uses stdout.]:"output"'
    '-s[Use graph_name as the name of the root graph. By default, "root" is used.]:"graph_name"'
    '-u[Don\t pack the graphs. Just combine them into a single graph.]'
    '-v[Verbose mode.]'
    '-\?[Prints usage information and exit.]'
    '*:filename:_files'
)
_arguments -s $arguments
