#compdef hp-probe

# Autogenerated from man page /usr/share/man/man1/hp-probe.1.gz

local arguments

arguments=(
    {-b,--bus}'[: cups, usb*, net, bt, fw, par (*default) (Note: bt and fw not supported…]:bus:bus'
    {-t,--ttl}'[Set timeout:.]:ttl'
    {-o,--timeout}'[Filter by functionality:.]:timeout'
    {-e,--filter}'[<filter list>: comma separated list of one or more of: scan, pcard, fax, copy…]:filter'
    {-s,--search}'[<search re> must be a valid regular expression (not case sensitive) Network d…]:search'
    {-m,--method}'[Set the logging level:.]:method'
    {-l,--logging}'[<level>: none, info*, error, warn, debug (*default) Run in debug mode:.]:level'
    {-g,--ldebug}'[This help information:.]'
    '(- * :)'{-h,--help}'[EXAMPLES Find all devices on the network: hp-probe -bnet Find all devices on …]'
    '*:filename:_files'
)
_arguments -s $arguments
