#compdef ttystudio

# Autogenerated from man page /Users/wizard/.nvm/versions/node/v8.16.0/share/man/man1/ttystudio.1

local arguments

arguments=(
    {-l,--log}'[Log status to stderr (default now).]'
    {-q,--quiet}'[Do not log status to stderr.]'
    {-f,--font}'[Choose a BDF font in ttystudio\s JSON format.]'
    {-b,--font-bold}'[Choose a bold BDF font in ttystudio\s JSON format.]'
    {-d,--delay}'[Specify frame delay in ms (default: 100).]'
    {-i,--interval}'[Specify frame snapshot interval in ms (default: 100).]'
    {-k,--key}'[Choose a key combination to quit recording (default: C-q).]'
    {-n,--num-plays}'[Specify a number of plays for the animation (default: 0 - infinite).]'
    {-r,--range}'[Choose a range of frames to compile. e. g. 5-200.]'
    {-x,--ratio}'[Choose pixel to cell ratio.]'
    {-t,--term}'[Choose the terminal name for terminfo.]'
    '--palette[Use a global palette for APNGs instead of RGBA.]'
    {--no-palette,--rgba,--lct}'[Use RGBA for APNGs instead of a global palette.]'
    '--border[width[Add a border around the animation using the specified parameters.]'
    '--play[Replay a frames file in the terminal.]'
    '--record[Explicitly choose to record (not very useful).]'
    '--compile[Explicitly choose to compile (not very useful).]'
    '--screenshot[Start ttystudio in screenshot mode.]'
    '--screenshot-key[Set the screenshot key when in screenshot mode. Default is `C-p`.]'
    '--version[Display ttystudio version.]'
    '--border[Add a border around the animation using the specified parameters.]:widthrgba'
    '(- * :)'{-h,--help}'[Display help information.]'
    '*:filename:_files'
)
_arguments -s $arguments
