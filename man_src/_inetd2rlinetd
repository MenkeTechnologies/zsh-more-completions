#compdef inetd2rlinetd

# Autogenerated from man page /usr/share/man/man8/inetd2rlinetd.8.gz

local arguments

arguments=(
    '-f[File in inetd. conf (5) format which will be converted. Default in stdin.]:file"'
    '-l[A line in inetd. conf (5) format. Only that line will be converted.]:entry"'
    '--force-overwrite[Forces overwriting existing files with new version.]'
    '--print-file-names[Prints to standard output list of generated files.]'
    '--add-from-comment[Puts special comment with original inetd.]'
    '*:filename:_files'
)
_arguments -s $arguments
