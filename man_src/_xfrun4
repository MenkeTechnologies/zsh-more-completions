#compdef xfce4-appfinder

# Autogenerated from man page /usr/share/man/man1/xfce4-appfinder.1.gz

local arguments

arguments=(
    {-h,--help}'[Show help options.]'
    '--help-all[Show all help options.]'
    '--help-gtk[Show GTK+ Options . SS "Application Options:".]'
    {-c,--collapsed}'[Start in collapsed mode.]'
    {-V,--version}'[Print version information and exit.]'
    {-r,--replace}'[Replace the existing service.]'
    {-q,--quit}'[Quit all instances.]'
    '--disable-server[Do not try to use or become a D-Bus service.]'
    '--display[X display to use REPORTING BUGS Please report bugs to <http://bugzilla. xfce.]:DISPLAY'
    '*:filename:_files'
)
_arguments -s $arguments
