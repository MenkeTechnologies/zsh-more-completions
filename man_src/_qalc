#compdef qalc

# Autogenerated from man page /usr/share/man/man1/qalc.1.gz

local arguments

arguments=(
    {-b,--base}'[set the number base for results and, optionally, expressions . PP.]':base
    {-e,--exrates}'[update exchange rates . PP.]'
    {-f,--file}'[execute commands from a file first . PP.]':file
    '(- * :)'{-h,--help}'[display this help and exits . PP.]'
    {-i,--interactive}'[start in interactive mode . PP.]'
    {-l,--list}'[displays a list of all user-defined or matching variables, functions and unit…]'
    '--list-functions[displays a list of all or matching functions. PP.]:term'
    '--list-units[displays a list of all or matching units. PP.]:term'
    '--list-variables[displays a list of all or matching variables. PP.]:term'
    {-m,--time}'[terminate calculation and display of result after specified amount of time .]':MILLISECONDS
    {-n,--nodefs}'[do not load any functions, units, or variables from file . PP.]'
    '--nocurrencies[do not load any global currencies from file . PP.]'
    '--nodatasets[do not load any global data sets from file . PP.]'
    '--nofunctions[do not load any global functions from file . PP.]'
    '--nounits[do not load any global units from file . PP.]'
    '--novariables[do not load any global variables from file . PP.]'
    '-p[start in programming mode (same as -b "base base" -s "xor^", with base conver…]:base'
    {-s,--set}'[as set command in interactive program session (ex. --set "base 16") . PP.]'
    {-t,--terse}'[reduce output to just the result of the input expression . PP.]'
    '--/+u8[switch unicode support on/off . PP.]'
    {-v,--version}'[show application version and exit.]'
    '*:filename:_files'
)
_arguments -s $arguments
