#compdef sessreg

# Autogenerated from man page /opt/X11/share/man/man1/sessreg.1

local arguments

arguments=(
    '-w[This specifies an alternate wtmpx file, instead of _PATH_WTMPX .]:wtmpx-file'
    '-u[This specifies an alternate utmpx file, instead of "/var/run/utmpx" .]:utmpx-file'
    '-L[This specifies an alternate lastlog file, instead of "/var/log/lastlog" , if…]:lastlog-file'
    '-l[This describes the "line" name of the entry.]:line-name'
    '-h[This is set to indicate that the session was initiated from a remote host.]:host-name'
    '-s[\\" __BEGIN_UTMPX_ONLY__ This option is accepted for compatibility, but does n…]:slot-number'
    '-x[\\" __BEGIN_UTMPX_ONLY__ This option is accepted for compatibility, but does n…]:Xservers-file'
    '-t[\\" __BEGIN_UTMPX_ONLY__ This option is accepted for compatibility, but does n…]:ttys-file'
    '-V[This option causes the command to print its version and exit.]'
    '-a[This session should be added to utmpx/wtmpx.]'
    '-d[This session should be deleted from utmpx/wtmpx.]'
    '*:filename:_files'
)
_arguments -s $arguments
