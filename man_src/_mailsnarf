#compdef mailsnarf

# Autogenerated from man page /usr/share/man/man8/mailsnarf.8.gz

local arguments

arguments=(
    '-i[Specify the interface to listen on.]:interface'
    '-p[Process packets from the specified PCAP capture file instead of the network.]:pcapfile'
    '-v["Versus" mode. Invert the sense of matching, to select non-matching messages.]'
    '*:filename:_files'
)
_arguments -s $arguments
