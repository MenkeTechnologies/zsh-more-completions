local arguments

arguments=(
    {-f,--force}'[Force overwriting the output file.]'
    {-p,--path}'[The path of the object for which the blobs will be returned.]':STRING:
    {-u,--tpm2bPublic}'[(for stdout): The returned public area of the object as a marshalled TPM2B_PU…]'
    {-r,--tpm2bPrivate}'[(for stdout): The returned private area of the object as a marshalled TPM2B_P…]'
    {-l,--policy}'[stdout): The returned policy associated with the object, encoded in JSON.]'
