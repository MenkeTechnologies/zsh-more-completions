#compdef builder

# Autogenerated from man page /usr/share/man/man1/builder.1.gz

local arguments

arguments=(
    '--buildTimeout[Maximum time to wait for builds and tests.]'
    '--buildroot[Directory under which to build.]'
    '--cmd[Build command (specify relative to go/src/).]'
    '--cmdTimeout[Maximum time to wait for an external command.]'
    '--commitInterval[Time to wait between polling for new commits (0 disables commit poller).]'
    '--dashboard[Go Dashboard Host.]'
    '--fail[fail all builds.]'
    '--gccpath[Path to download gcc from.]'
    '--gcpath[Path to download gc from.]'
    '--parallel[Build multiple targets in parallel.]'
    '--release[Build and upload binary release archives.]'
    '--rev[Build specified revision and exit.]'
    '--tool[Tool to build.]'
    '-v[verbose.]'
    '*:filename:_files'
)
