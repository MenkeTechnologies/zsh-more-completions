#compdef btxld

# Autogenerated from man page /usr/share/man/en.UTF-8/man8/btxld.8.gz

local arguments

arguments=(
    '-q[Quiet: inhibit warnings.]'
    '-v[Verbose: display information about the files processed.]'
    '-b[Specify the BTX kernel to be bound with the client.]'
    '-E[Set the client entry point.]'
    '-e[Set the BTX loader entry point.]'
    '-f[Specify the output format, where format is one of bin, aout, or elf.]'
    '-l[Specify the BTX loader to be bound with the client.]'
    '-o[Name the output file. The default is a. out.]'
    '-P[Specify the first page of the client\s segment to be marked present, where pa…]'
    '-W[Specify the first page of the client\s segment to be marked writable, where p…]'
    '*:filename:_files'
)
_arguments -s $arguments
