#compdef zabbix_js

local arguments

arguments=(
  {-s,--script}'[specify the filename of script to execute. Specify - for]'
  {-i,--input}'[specify input parameter file name. Specify - for]'
  {-p,--param}'[specify input parameter]'
  {-l,--loglevel}'[specify log level]'
  {-t,--timeout}'[specify timeout in seconds]'
  {-h,--help}'[display this help message]'
  {-V,--version}'[display version number]'
  '*:filename:_files'
)

_arguments -s $arguments
