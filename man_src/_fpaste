#compdef fpaste

# Autogenerated from man page /usr/share/man/man1/fpaste.1.gz

local arguments

arguments=(
    '--version[show program\s version number and exit.]'
    '(- * :)'{-h,--help}'[show help message and exit.]'
    {-t,--title}'[title for paste; default is UNTITLED.]'
    {-d,--password}'[password for paste; default is "".]'
    '-l[language of content for syntax highlighting; default is "text"; use "list" to…]:"LANGUAGE"'
    '-x[time before paste is removed; default is 604800 seconds for unauthenticated p…]:"EXPIRES"'
    {-U,--URL}'[URL of fpaste server; default is http://fpaste. org.]':"URL"
    {-P,--private}'[make paste private; default is yes; valid options: yes, no.]'
    {-i,--clipin}'[read paste text from current X clipboard selection \[requires xsel\].]'
    {-o,--clipout}'[save returned paste URL to all the clipboards i.]'
    '--input-selection[specify which X clipboard to use.]:CLIPBOARD'
    '--fullpath[use pathname VS basename for file description(s).]'
    '--pasteself[paste this script itself.]'
    '--sysinfo[paste system information.]'
    '--printonly[print paste, but do not send.]'
    '*:filename:_files'
)
_arguments -s $arguments
