#compdef ether-wake

# Autogenerated from man page /usr/share/man/man8/ether-wake.8.gz

local arguments

arguments=(
    '-b[Send the wake-up packet to the broadcast address.]'
    '-D[Increase the Debug Level.]'
    '-i[Use interface ifname instead of sending a wake packet to all interfaces.]:ifname'
    '-p[Append a four or six byte password to the packet.]:passwd'
    '-V[Show the program version information.]'
    '*:filename:_files'
)
_arguments -s $arguments
