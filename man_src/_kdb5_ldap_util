#compdef kdb5_ldap_util

# Autogenerated from man page /usr/share/man/man8/kdb5_ldap_util.8.gz

local arguments

arguments=(
    '-r[Specifies the realm to be operated on.]:realm'
    '-D[Specifies the Distinguished Name (DN) of the user who has sufficient rights t…]:user_dn'
    '-w[Specifies the password of user_dn. This option is not recommended.]:passwd'
    '-H[Specifies the URI of the LDAP server.]:ldapuri'
    '--subtrees[Specifies the list of subtrees containing the principals of a realm.]:subtree_dn_list'
    '--sscope[Specifies the scope for searching the principals under the subtree.]:search_scope'
    '--containerref[Specifies the DN of the container object in which the principals of a realm w…]:container_reference_dn'
    '-k[Specifies the key type of the master key in the database.]:mkeytype'
    '--kv[Specifies the version number of the master key in the database; the default i…]:mkeyVNO'
    '-M[Specifies the principal name for the master key in the database.]:mkeyname'
    '-m[Specifies that the master database password should be read from the TTY rathe…]'
    '-P[Specifies the master database password. This option is not recommended.]:password'
    '--sf[Specifies the stash file of the master database password.]:stashfilename'
    '-s[Specifies that the stash file is to be created.]'
    '--maxtktlife[(getdate string) Specifies maximum ticket life for principals in this realm.]:max_ticket_life'
    '--maxrenewlife[(getdate string) Specifies maximum renewable life of tickets for principals i…]:max_renewable_ticket_life'
    '-f[If specified, will not prompt the user for confirmation.]'
    '--force[Forces the deletion of the policy object.]'
    '*:filename:_files'
)
_arguments -s $arguments
