#compdef rasqal-config

# Autogenerated from man page /usr/share/man/man1/rasqal-config.1.gz

local arguments

arguments=(
    '--version[Print the currently installed version of rasqal on the standard output.]'
    '--version-decimal[Print the currently installed version of rasqal as a decimal integer.]'
    '--libs[Print the linker flags that are necessary to link a program with Rasqal.]'
    '--libtool-libs[Print the path to the libtool file for rasqal.]'
    '--cflags[Print the compiler flags that are necessary to compile a program with the RDF…]'
    '--prefix[If specified, use PREFIX instead of the installation prefix that Rasqal was b…]:PREFIX'
    '*:filename:_files'
)
_arguments -s $arguments
