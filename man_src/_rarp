#compdef rarp

# Autogenerated from man page /usr/share/man/man8/rarp.8.gz

local arguments

arguments=(
    '-V[Display the version of RARP in use.]'
    '-v[Tell the user what is going on by being verbose.]'
    '-t[When setting or reading the RARP table, this optional parameter tells rarp w…]:type'
    '--list[Lists the entries in the RARP table.]'
    '--delete[Remove all RARP entries for the specified host.]:hostname'
    '-a[.]'
    '-d[.]:hostname'
    '-s[.]:hw_addr'
    '--set[Create a RARP address mapping entry for host hostname with hardware address s…]:hw_addr'
    '*:filename:_files'
)
