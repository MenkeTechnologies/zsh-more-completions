local arguments

arguments=(
    {-i,--interface}'[tor control port we should attach to (default is 127. 0. 0. 1:9051).]':ADDRESS:PORT
    {-s,--socket}'[tor control socket we should attach to (default is /var/run/tor/control).]':SOCKET_PATH
    {-c,--config}'[user configuration preferences (default is ~/. nyx/config).]':CONFIG_PATH
    {-d,--debug}'[writes all nyx logs to the given location.]':LOG_PATH
    {-l,--log}'[comma separated list of events to log.]':EVENTS
    {-v,--version}'[provides version information.]'
    '(- * :)'{-h,--help}'[provides usage information.]'
    '*:filename:_files'
)
_arguments -s $arguments
