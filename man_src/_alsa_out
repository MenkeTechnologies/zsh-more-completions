#compdef alsa_in

# Autogenerated from man page /usr/share/man/man1/alsa_in.1.gz

local arguments

arguments=(
    '-j[br Set Client Name.]:jack_client_name'
    '-d[br Use this Soundcard.]:alsa_device'
    '-v[br Verbose, prints out resample coefficient and other parameters useful for d…]'
    '-i[br Instrumentation.]'
    '-c[br Set Number of channels.]:channels'
    '-r[br Set sample_rate. The program resamples as necessary.]:sample_rate'
    '-p[br Set the period size. It is not related to the jackd period_size.]:period_size'
    '-n[br Set number of periods. See note for period_size.]:num_period'
    '-q[br Set the quality of the resampler from 0 to 4.]:quality'
    '-m[br The value when a soft xrun occurs.]:max_diff'
    '-t[br The delay alsa_io should try to approach. Same as for max_diff.]:target_delay'
    '-s[br This parameter controls the size of the array used for smoothing the delay…]:smooth_array_size'
    '-C[.]'
    '*:filename:_files'
)
