#compdef a2ps

local arguments

arguments=(
  '--version[display version]'
  '--help[display this help]'
  '--guess[report guessed types of FILES]'
  '--which[report the full path of library files named FILES]'
  '--glob[report the full path of library files matching FILES]'
  '--list[display default settings and parameters]'
  '--list[detailed list on TOPIC (delegations, encodings, features]'
  {-q,--quiet,--silent}'[be really quiet]'
  {-v,--verbose}'[set verbosity on, or to LEVEL]'
  '--debug[enable debugging features]'
  {-D,--define}'[unset variable KEY or set to VALUE]'
  {-M,--medium}'[use output medium NAME]'
  {-r,--landscape}'[print in landscape mode]'
  {-R,--portrait}'[print in portrait mode]'
  '--columns[number of columns per sheet]'
  '--rows[number of rows per sheet]'
  '--major[first fill (DIRECTION=) rows, or columns]'
  {-1,-2}'[..., -9 predefined font sizes and layouts for 1.. 9 virtuals]'
  {-A,--file-align}'[align separate files according to MODE (fill, rank]'
  {-j,--borders}'[* print borders around columns]'
  '--margin[define an interior margin of size NUM]'
  '--line-numbers[precede each NUM lines with its line number]'
  '-C[alias for --line-numbers=5]'
  {-f,--font-size}'[use font SIZE (float) for the body text]'
  {-L,--lines-per-page}'[scale the font to print NUM lines per virtual]'
  {-l,--chars-per-line}'[scale the font to print NUM columns per virtual]'
  {-m,--catman}'[process FILE as a man page (same as -L66)]'
  {-T,--tabsize}'[set tabulator size to NUM]'
  '--non-printable-format[specify how non-printable chars are printed]'
  {-B,--no-header}'[no page headers at all]'
  {-b,--header}'[set page header]'
  {-u,--underlay}'[print TEXT under every page]'
  '--center-title[set page title to TITLE]'
  '--left-title[set left and right page title to TEXT]'
  '--right-title[]'
  '--left-footer[set sheet footers to TEXT]'
  '--footer[]'
  '--right-footer[]'
  {-a,--pages}'[select the pages to print]'
  {-c,--truncate-lines}'[* cut long lines]'
  {-i,--interpret}'[* interpret tab, bs and ff chars]'
  '--end-of-line[specify the eol char (TYPE: r, n, nr, rn, any)]'
  {-X,--encoding}'[use input encoding NAME]'
  {-t,--title}'[set the name of the job]'
  '--stdin[set the name of the input file stdin]'
  '--print-anyway[* force binary printing]'
  {-Z,--delegate}'[* delegate files to another application]'
  '--toc[generate a table of content]'
  {-E,--pretty-print}'[enable pretty-printing (set style to LANG)]'
  '--highlight-level[set pretty printing highlight LEVEL]'
  '-g[alias for --highlight-level=heavy]'
  '--strip-level[level of comments stripping]'
  {-o,--output}'[leave output to file FILE. If FILE is `-]'
  '--version-control[override the usual version control]'
  '--suffix[override the usual backup suffix]'
  {-P,--printer}'[send output to printer NAME]'
  '-d[send output to the default printer]'
  '--prologue[include FILE.pro as PostScript prologue]'
  '--ppd[automatic PPD selection or set to KEY]'
  {-n,--copies}'[print NUM copies of each page]'
  {-s,--sides}'[set the duplex MODE (`1 or `simplex]'
  {-S,--setpagedevice}'[pass a page device definition to output]'
  '--statusdict[pass a statusdict definition to the output]'
  {-k,--page-prefeed}'[enable page prefeed]'
  {-K,--no-page-prefeed}'[disable page prefeed]'
  '*:filename:_files'
)

_arguments -s $arguments
