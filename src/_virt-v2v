#compdef virt-v2v

local arguments

arguments=(
  {-b,--bridge}'[map bridge in to out]'
  {--color,--colors,--colour,--colours}'[use ANSI colour sequences even if not tty]'
  '--compressed[compress output file (-of qcow2 only)]'
  {--dcpath,--dcPath}'[override dcPath (for vCenter)]'
  {--debug-overlay,--debug-overlays}'[save overlay files]'
  '--help[display brief help]'
  '-i[set input mode (default: libvirt)]'
  '-ic[libvirt URI]'
  '-if[input format (for -i disk)]'
  '--in-place[only tune the guest in the input VM]'
  '--machine-readable[make output machine readable]'
  {-n,--network}'[map network in to out]'
  '--no-copy[just write the metadata]'
  '--no-trim[ignored for backwards compatibility]'
  '-o[set output mode (default: libvirt)]'
  '-oa[set output allocation mode]'
  '-oc[libvirt URI]'
  '-of[2>                     Set output format]'
  '-on[rename guest when converting]'
  '-os[set output storage location]'
  '--password-file[use password from file]'
  '--print-source[print source and stop]'
  {-q,--quiet}'[dont print progress messages]'
  '--qemu-boot[boot in qemu (-o qemu only)]'
  '--root[... >                   How to choose root filesystem]'
  {-V,--version}'[display version and exit]'
  {-v,--verbose}'[enable libguestfs debugging messages]'
  '--vdsm-compat[0.10|1.1>            Write qcow2 with compat=0.10|1.1]'
  '--vdsm-image-uuid[output image UUID(s)]'
  '--vdsm-ovf-output[output OVF file]'
  '--vdsm-vm-uuid[output VM UUID]'
  '--vdsm-vol-uuid[output vol UUID(s)]'
  '--vmtype[ignored for backwards compatibility]'
  '-x[enable tracing of libguestfs calls]'
  '*:filename:_files'
)

_arguments -s $arguments
