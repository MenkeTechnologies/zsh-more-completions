#compdef cups-genppdupdate

local arguments

arguments=(
  '-d[enable debugging]'
  '-h[display this help text]'
  '-n[no-action. Dont overwrite any PPD files.]'
  '-q[quiet mode. No messages except errors.]'
  '-s[use ppd_dir as the source PPD directory.]'
  '-p[update PPD files in ppd_dir.]'
  '-P[use the specified driver binary to generate PPD files.]'
  '-v[verbose messages.]'
  '-N[reset options to defaults.]'
  '-o[output PPD files to out_dir.]'
  '-r[use PPD files for Gutenprint major.minor version.]'
  '-f[ignore new PPD file safety checks.]'
  '-i[prompt (interactively) for each PPD file.]'
  '-x[allow update across major Gutenprint releases.]'
  '-l[language choice (Gutenprint 5.1 or below).]'
  '*:filename:_files'
)

_arguments -s $arguments
