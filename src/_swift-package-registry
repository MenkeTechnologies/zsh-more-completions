#compdef swift-package-registry

local arguments

arguments=(
  '--package-path[specify the package path to operate on (default]'
  '--cache-path[specify the shared cache directory path]'
  '--config-path[specify the shared configuration directory path]'
  '--security-path[specify the shared security directory path]'
  '--scratch-path[specify a custom scratch directory path (default]'
  '--pkg-config-path[specify alternative path to search for pkg-config]'
  '--enable-dependency-cache[/--disable-dependency-cache]'
  '--enable-build-manifest-caching[/--disable-build-manifest-caching]'
  '--manifest-cache[caching mode of Package.swift manifests (shared:]'
  {-v,--verbose}'[increase verbosity to include informational output]'
  {--very-verbose,--vv}'[increase verbosity to include debug output]'
  '--disable-sandbox[disable using the sandbox when executing subprocesses]'
  '--netrc[use netrc file even in cases where other credential]'
  '--enable-netrc[/--disable-netrc]'
  '--netrc-file[specify the netrc file path]'
  '--enable-keychain[/--disable-keychain]'
  '--resolver-fingerprint-checking[(default: strict)]'
  '--enable-prefetching[/--disable-prefetching]'
  {--force-resolved-versions,--disable-automatic-resolution,--only-use-versions-from-resolved-file}'[only use versions from the Package.resolved file and]'
  '--skip-update[skip updating dependencies from their remote during a]'
  '--disable-scm-to-registry-transformation[disable source control to registry transformation]'
  '--use-registry-identity-for-scm[look up source control dependencies in the registry]'
  '--replace-scm-with-registry[look up source control dependencies in the registry]'
  {-c,--configuration}'[build with configuration (default: debug)]'
  '-Xcc[pass flag through to all C compiler invocations]'
  '-Xswiftc[pass flag through to all Swift compiler invocations]'
  '-Xlinker[pass flag through to all linker invocations]'
  '-Xcxx[pass flag through to all C++ compiler invocations]'
  '--triple[]'
  '--sdk[]'
  '--toolchain[]'
  '--sanitize[turn on runtime checks for erroneous behavior]'
  '--auto-index-store[/--enable-index-store/--disable-index-store]'
  '--enable-parseable-module-interfaces[]'
  {-j,--jobs}'[the number of jobs to spawn in parallel during the]'
  '--emit-swift-module-separately[]'
  '--use-integrated-swift-driver[]'
  '--explicit-target-dependency-import-check[(default: none)]'
  '--experimental-explicit-module-build[]'
  '--build-system[(default: native)]'
  '--enable-dead-strip[/--disable-dead-strip]'
  '--static-swift-stdlib[/--no-static-swift-stdlib]'
  '--version[show the version.]'
  {-h,-help,--help}'[show help information.]'
  '*:filename:_files'
)

_arguments -s $arguments
