#compdef y2racc2.7

local arguments

arguments=(
  {-o,--output}'[output file name \[<input>.racc\]]'
  {-c,--classname}'[name of the parser class. \[MyParser\]]'
  {-A,--without-action}'[does not include actions.]'
  {-h,--with-header}'[includes header (%{...%}).]'
  {-u,--with-user-code}'[includes user code.]'
  '--version[prints version and quit.]'
  '--copyright[prints copyright and quit.]'
  '--help[prints this message and quit.]'
  '*:filename:_files'
)

_arguments -s $arguments
