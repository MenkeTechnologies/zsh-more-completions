#compdef dm_date

local arguments

arguments=(
  '-h[/--help        : Print help.]'
  '-d[/--date=STRING : Print information about that date.]'
  '-f[/--file=FILE   : Print out each date (1/line) in FILE]'
  '-r[/--reference=FILE]'
  '-R[/--rfc-2822    : Print the date in RFC 2822 format]'
  '-u[/--utc/--universal:]'
  '*:filename:_files'
)

_arguments -s $arguments
