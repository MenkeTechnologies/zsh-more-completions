#compdef dnssec-revoke

local arguments

arguments=(
  '-E[specify OpenSSL engine]'
  '-f[force overwrite]'
  '-K[use directory for key files]'
  '-h[help]'
  '-r[remove old keyfiles after creating revoked version]'
  '-v[set level of verbosity]'
  '-V[print version information]'
  '*:filename:_files'
)

_arguments -s $arguments
