#compdef smap

local arguments

arguments=(
  {-c,--commandline}'[output written with straight to the]'
  {-D,--display}'[set which display mode to use]'
  {-h,--noheader}'[no headers on output]'
  {-H,--show}'[_hidden display hidden partitions and their jobs]'
  {-i,--iterate}'[specify an interation period]'
  {-I,--ionodes}'[only show objects with these ionodes]'
  {-M,--cluster}'[cluster to issue commands to. Default is]'
  {-n,--nodes}'[only show objects with these nodes.]'
  {-R,--resolve}'[resolve an XYZ coord from a Rack/Midplane id]'
  {-V,--version}'[output version information and exit]'
  '--help[show this help message]'
  '--usage[display brief usage message]'
  '*:filename:_files'
)

_arguments -s $arguments
