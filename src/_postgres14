#compdef postgres14

local arguments

arguments=(
  '-B[number of shared buffers]'
  '-c[VALUE      set run-time parameter]'
  '-C[print value of run-time parameter, then exit]'
  '-d[5             debugging level]'
  '-D[database directory]'
  '-e[use European date input format (DMY)]'
  '-F[turn fsync off]'
  '-h[host name or IP address to listen on]'
  '-i[enable TCP/IP connections]'
  '-k[unix-domain socket location]'
  '-l[enable SSL connections]'
  '-N[maximum number of allowed connections]'
  '-p[port number to listen on]'
  '-s[show statistics after each query]'
  '-S[set amount of memory for sorts (in kB)]'
  {-V,--version}'[output version information, then exit]'
  '--NAME[set run-time parameter]'
  '--describe-config[describe configuration parameters, then exit]'
  '-f[forbid use of some plan types]'
  '-n[do not reinitialize shared memory after abnormal exit]'
  '-O[allow system table structure changes]'
  '-P[disable system indexes]'
  '-t[show timings after each query]'
  '-T[send SIGSTOP to all backend processes if one dies]'
  '-W[wait NUM seconds to allow attach from a debugger]'
  '--single[selects single-user mode (must be first argument)]'
  '-d[5             override debugging level]'
  '-E[echo statement before execution]'
  '-j[do not use newline as interactive query delimiter]'
  '-r[send stdout and stderr to given file]'
  '--boot[selects bootstrapping mode (must be first argument)]'
  '-r[send stdout and stderr to given file]'
  '-x[internal use]'
  '*:filename:_files'
)

_arguments -s $arguments
