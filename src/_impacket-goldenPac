#compdef impacket-goldenPac

local arguments

arguments=(
  {-h,--help}'[show this help message and exit]'
  '-ts[adds timestamp to every logging output]'
  '-debug[turn DEBUG output ON]'
  '-c[uploads the filename for later execution, arguments]'
  '-w[writes the golden ticket in CCache format into the]'
  '-dc-ip[address     IP Address of the domain controller (needed to get the]'
  '-target-ip[address]'
  '-hashes[NTLM hashes, format is LMHASH:NTHASH]'
  '*:filename:_files'
)

_arguments -s $arguments
