#compdef uulog

local arguments

arguments=(
  {-n,--lines}'[show given number of lines from end of log]'
  {-s,--system}'[print entries for named system]'
  {-f,--follow}'[follow entries for named system]'
  {-u,--user}'[print entries for named user]'
  {-F,--follow}'[follow entries for any system]'
  {-S,--statslog}'[show statistics file]'
  {-D,--debuglog}'[show debugging file]'
  {-X,--debug}'[set debugging level]'
  {-I,--config}'[set configuration file to use]'
  {-v,--version}'[Print version and exit]'
  '--help[Print help and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
