#compdef btiotest

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show help options]'
  {-c,--channel}'[RFCOMM channel]'
  {-p,--psm}'[L2CAP PSM]'
  {-j,--cid}'[L2CAP CID]'
  {-t,--addr-type}'[address type (0 BR/EDR 1 LE Public 2 LE Random]'
  {-s,--sco}'[use SCO]'
  {-d,--defer}'[use DEFER_SETUP for incoming connections]'
  {-V,--voice}'[voice setting (0x0060 CVSD, 0x0003 Transparent)]'
  {-S,--sec-level}'[security level]'
  {-U,--update-sec-level}'[update security level]'
  {-i,--dev}'[which HCI device to use]'
  {-r,--reject}'[reject connection after N seconds]'
  {-D,--disconnect}'[disconnect connection after N seconds]'
  {-a,--accept}'[accept connection after N seconds]'
  {-m,--master}'[master role switch (incoming connections)]'
  {-P,--priority}'[transmission priority: Setting a priority outside the range 0 to 6 requires theCAP_NET_ADMIN capability.]'
  '*:filename:_files'
)

_arguments -s $arguments
