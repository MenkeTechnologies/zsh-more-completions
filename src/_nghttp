#compdef nghttp

local arguments

arguments=(
  {-v,--verbose}'[print debug information such as reception and]'
  {-n,--null-out}'[discard downloaded data.]'
  {-O,--remote-name}'[save download data in the current directory. The]'
  {-t,--timeout}'[timeout each request after <DURATION>. Set 0 to disable]'
  {-w,--window-bits}'[sets the stream level initial window size to 2**<N>-1.]'
  {-W,--connection-window-bits}'[sets the connection level initial window size to]'
  {-a,--get-assets}'[download assets such as stylesheets, images and script]'
  {-s,--stat}'[print statistics.]'
  {-H,--header}'[add a header to the requests. Example: -H:method: PUT]'
  '--trailer[add a trailer header to the requests. <HEADER> must not]'
  '--cert[use the specified client certificate file. The file]'
  '--key[use the client private key file. The file must be in]'
  {-d,--data}'[post FILE to server. If - is given, data will be read]'
  {-m,--multiply}'[request each URI <N> times. By default, same URI is not]'
  {-u,--upgrade}'[perform HTTP Upgrade for HTTP/2. This option is ignored]'
  {-p,--weight}'[sets weight of given URI. This option can be used]'
  {-M,--peer-max-concurrent-streams}'[use <N> as SETTINGS_MAX_CONCURRENT_STREAMS value of]'
  {-c,--header-table-size}'[specify decoder header table size. If this option is]'
  '--encoder-header-table-size[specify encoder header table size. The decoder (server)]'
  {-b,--padding}'[add at most <N> bytes to a frame payload as padding.]'
  {-r,--har}'[output HTTP transactions <PATH> in HAR format. If -]'
  '--color[force colored log output.]'
  '--continuation[send large header to test CONTINUATION.]'
  '--no-content-length[dont send content-length header field.]'
  '--no-dep[dont send dependency based priority hint to server.]'
  '--hexdump[display the incoming traffic in hexadecimal (Canonical]'
  '--no-push[disable server push.]'
  '--max-concurrent-streams[the number of concurrent pushed streams this client]'
  '--expect-continue[perform an Expect/Continue handshake: wait to send DATA]'
  {-y,--no-verify-peer}'[suppress warning on server certificate verification]'
  '--version[display version information and exit.]'
  '(- * :)'{-h,--help}'[display this help and exit.]'
  '*:filename:_files'
)

_arguments -s $arguments
