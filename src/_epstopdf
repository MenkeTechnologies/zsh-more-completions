#compdef epstopdf

local arguments

arguments=(
  '--help[display this help and exit]'
  '--version[display version information and exit]'
  {-o,--outfile}'[write result to FILE (default based on input name)]'
  '--restricted[use restricted mode (default: false)]'
  '--gscmd[pipe output to VAL (default: gs)]'
  '--gsopt[single option for gs (see below)]'
  '--gsopts[options for gs (see below)]'
  '--autorotate[set AutoRotatePages (default: None)]'
  '--device[use -sDEVICE=DEV (default: pdfwrite)]'
  '--pdfsettings[use -dPDFSETTINGS=/VAL (default is prepress if --embed]'
  '--res[set image resolution (default: \[use gs default\])]'
  '--gsopts[a single string of options, which is split at whitespace]'
  '--gsopt[its argument as a single option to the gs command line.]'
  '*:filename:_files'
)

_arguments -s $arguments
