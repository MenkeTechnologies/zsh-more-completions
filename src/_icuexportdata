#compdef icuexportdata

local arguments

arguments=(
  '-h[-? or --help  this usage text]'
  {-V,--version}'[show a version message]'
  {-m,--mode}'[mode: currently only uprops, but more may be added]'
  '--trie-type[set the trie type (small or fast, default small)]'
  {-d,--destdir}'[destination directory, followed by the path]'
  '--all[write out all properties known to icuexportdata]'
  '--index[write an _index.toml summarizing all data exported]'
  {-c,--copyright}'[include a copyright notice]'
  {-v,--verbose}'[turn on verbose output]'
  {-q,--quiet}'[do not display warnings and progress]'
  '*:filename:_files'
)

_arguments -s $arguments
