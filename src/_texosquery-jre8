#compdef texosquery-jre8

local arguments

arguments=(
  {-h,--help,-help}'[display this help message and exit.]'
  {-v,--version,-version}'[display version information and exit.]'
  '--nodebug[no debugging messages (default)]'
  '--debug[or -debug \[<n>\]]'
  '--compatible[or -compat <n>]'
  '--encoding[or -enc <charset>]'
  {--default-encoding,-defenc}'[use the default file encoding.]'
  '--strip-path-prefix[or -sp <prefix>]'
  '--nostrip-path-prefix[cancel the effect of --strip-path-prefix.]'
  '--replace-path[<replacement> or -rp <regex> <replacement>]'
  '--noreplace-path[cancel the effect of --replace-path.]'
  '--strip-uri-prefix[or -su <prefix>]'
  '--nostrip-uri-prefix[cancel the effect of --strip-uri-prefix.]'
  '--replace-uri[<replacement> or -ru <regex> <replacement>]'
  '--noreplace-uri[cancel the effect of --replace-uri.]'
  {-o,--osname}'[display OS name.]'
  {-r,--osversion}'[display OS version.]'
  {-a,--osarch}'[display OS architecture.]'
  {-n,--pdfnow}'[display current date-time in PDF format.]'
  {-cs,--codeset}'[display the codeset.]'
  {-C,--codeset-lcs}'[lower case codeset with hyphens stripped.]'
  {-M,--date-time}'[display all the current date-time data.]'
  {-L,--locale}'[display POSIX locale information.]'
  {-l,--locale-lcs}'[display POSIX style locale information with lower case codeset.]'
  {-b,--bcp47}'[display locale as BCP47 tag.]'
  '-N[or --numeric \[locale\]]'
  '-D[or --locale-data \[locale\]]'
  '-Z[or --time-zones \[locale\]]'
  {-c,--cwd}'[display current working directory.]'
  {-m,--userhome}'[display users home directory.]'
  {-t,--tmpdir}'[display temporary directory.]'
  '-d[or --pdfdate <file>]'
  '-s[or --filesize <file>]'
  '-i[<dir> \[<sort>\] or --list <sep> <dir> \[<sort>\]]'
  '-f[<regex> <dir> \[<sort>\] or --filterlist <sep> <regex> <dir> \[<sort>\]]'
  '-id[<dir> \[<sort>\] or --list-dir <sep> <dir> \[<sort>\]]'
  '-fd[<regex> <dir> \[<sort>\] or --filterlist-dir <sep> <regex> <dir> \[<sort>\]]'
  '-ir[<dir> \[<sort>\] or --list-regular <sep> <dir> \[<sort>\]]'
  '-fr[<regex> <dir> \[<sort>\] or --filterlist-regular <sep> <regex> <dir> \[<sort>\]]'
  '-w[<regex> <dir> \[<sort>\] or --walk <sep> <regex> <dir> \[<sort>\]]'
  '-u[or --uri <file>]'
  '-p[or --path <file>]'
  '-e[or --dirname <file>]'
  '*:filename:_files'
)

_arguments -s $arguments
