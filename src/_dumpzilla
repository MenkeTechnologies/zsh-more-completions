#compdef dumpzilla

local arguments

arguments=(
  '--All[everything but the DOM data. Doesnt extract thumbnails or HTML 5 offline)]'
  {--Cookies,-domain,-name,-hostcookie,-access,-create,-secure}'[0/1> -httponly <0/1> -range_last -range_create <start> <end>\]]'
  '--Permissions[<string>\]]'
  '--Downloads[<end>\]]:start:'
  '--Forms[\[-value <string> -range_forms <start> <end>\]]'
  '--History[title <string> -date <date> -range_history <start> <end> -frequency\]]:string:'
  '--Bookmarks[<end>\]]:start:'
  '--Cacheoffline[extract <directory>\]]:start::end:'
  '--Thumbnails[<directory>\]]'
  '--Range[date> <end date>]'
  '--Addons[]'
  '--Passwords[only in Unix)]'
  '--Certoverride[]'
  '--Session[]'
  '--Watch[<string>\] (Shows in daemon mode the URLs and text form in real time. -text Option allow filter, support all grep Wildcards. Exit: Ctrl + C. only Unix).]'
  '*:filename:_files'
)

_arguments -s $arguments
