#compdef sip

local arguments

arguments=(
  '-h[display this help message]'
  '-V[display the sip version number]'
  '-a[the name of the QScintilla API file \[default not generated\]]'
  '-b[the name of the build file \[default none generated\]]'
  '-B[add tag to the list of timeline backstops]'
  '-c[the name of the code directory \[default not generated\]]'
  '-d[the name of the documentation file (deprecated) \[default not generated\]]'
  '-D[generate code for a debug build of Python]'
  '-e[enable support for exceptions \[default disabled\]]'
  '-f[warnings are handled as errors]'
  '-g[always release and reacquire the GIL \[default only when specified\]]'
  '-I[look in this directory when including files]'
  '-j[split the generated code into # files \[default 1 per class\]]'
  '-k[support keyword arguments in functions and methods]'
  '-m[the name of the XML export file \[default not generated\]]'
  '-n[the qualified name of the private copy of the sip module]'
  '-o[enable the automatic generation of docstrings \[default disabled\]]'
  '-p[the name of the consolidated module that this is a component of]'
  '-P[enable the protected/public hack]'
  '-r[generate code with tracing enabled \[default disabled\]]'
  '-s[the suffix to use for C or C++ source files \[default ".c" or ".cpp"\]]'
  '-t[the version/platform to generate code for]'
  '-w[enable warning messages]'
  '-x[this feature is disabled]'
  '-X[create the extracts for an id in a file]'
  '-y[the name of the .pyi stub file \[default not generated\]]'
  '-z[the name of a file containing more command line flags]'
  '*:filename:_files'
)

_arguments -s $arguments
