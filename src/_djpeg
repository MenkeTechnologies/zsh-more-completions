#compdef djpeg

local arguments

arguments=(
  '-colors[reduce image to no more than N colors]'
  '-fast[fast, low-quality processing]'
  '-grayscale[force grayscale output]'
  '-rgb[force RGB output]'
  '-scale[/N     Scale output image by fraction M/N, eg, 1/8]'
  '-bmp[select BMP output format (Windows style)]'
  '-gif[select GIF output format]'
  '-os2[select BMP output format (OS/2 style)]'
  '-pnm[select PBMPLUS (PPM/PGM) output format (default)]'
  '-targa[select Targa output format]'
  '-dct[use integer DCT method (default)]'
  '-dct[use fast integer DCT (less accurate)]'
  '-dct[use floating-point DCT method]'
  '-dither[use F-S dithering (default)]'
  '-dither[dont use dithering in quantization]'
  '-dither[use ordered dither (medium speed, quality)]'
  '-map[map to colors used in named image file]'
  '-nosmooth[dont use high-quality upsampling]'
  '-onepass[use 1-pass quantization (fast, low quality)]'
  '-maxmemory[maximum memory to use (in kbytes)]'
  '-outfile[specify name for output file]'
  '-verbose[or  -debug   Emit debug output]'
  '*:filename:_files'
)

_arguments -s $arguments
