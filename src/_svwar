#compdef svwar

local arguments

arguments=(
  '--version[show programs version number and exit]'
  '(- * :)'{-h,--help}'[show this help message and exit]'
  {-v,--verbose}'[increase verbosity]'
  {-q,--quiet}'[quiet mode]'
  {-p,--port}'[destination port or port ranges of the SIP device - eg]'
  '-p5060[5061,8000-8100]'
  {-P,--localport}'[source port for our packets]'
  {-x,--externalip}'[IP Address to use as the external ip. Specify this if]'
  {-b,--bindingip}'[by default we bind to all interfaces. This option]'
  {-t,--timeout}'[this option allows you to trottle the speed at which]'
  {-R,--reportback}'[send the author an exception traceback. Currently]'
  {-A,--autogetip}'[automatically get the current IP address. This is]'
  {-s,--save}'[save the session. Has the benefit of allowing you to]'
  '--resume[resume a previous scan]'
  {-c,--enablecompact}'[enable compact mode. Makes packets smaller but]'
  {-d,--dictionary}'[specify a dictionary file with possible extension]'
  {-m,--method}'[specify a request method. The default is REGISTER.]'
  {-e,--extensions}'[specify an extension or extension range example: -e]'
  {-z,--zeropadding}'[the number of zeros used to padd the username.]'
  '--force[force scan, ignoring initial sanity checks.]'
  {-T,--template}'[A format string which allows us to specify a template]'
  {-D,--enabledefaults}'[scan for default / typical extensions such as]'
  '--maximumtime[maximum time in seconds to keep sending requests]'
  '--domain[force a specific domain name for the SIP message, eg.]'
  '-d[.org]'
  '--debug[print SIP messages received]'
  '*:filename:_files'
)

_arguments -s $arguments
