#compdef qemu-keymap

local arguments

arguments=(
  '-h[print this text]'
  '-f[set output file (default: stdout)]'
  '-m[set kbd model (default: pc105)]'
  '-l[set kbd layout (default: us)]'
  '-v[set kbd variant (default: -)]'
  '-o[set kbd options (default: -)]'
  '*:filename:_files'
)

_arguments -s $arguments
