#compdef camcontrol

local arguments

arguments=(
  '-v[be verbose, print out sense information]'
  '-t[command timeout in seconds, overrides default timeout]'
  '-n[specify device name, e.g. "da", "cd"]'
  '-u[specify unit number, e.g. "0", "5"]'
  '-E[have the kernel attempt to perform SCSI error recovery]'
  '-C[specify the SCSI command retry count (needs -E to work)]'
  '-Q[specify ordered, simple or head tag type for SCSI cmds]'
  '-l[list all available mode pages]'
  '-m[specify the mode page to view or edit]'
  '-e[edit the specified mode page]'
  '-b[force view to binary mode]'
  '-d[disable block descriptors for mode sense]'
  '-P[page control field 0-3]'
  '-f[specify defect list format (block, bfi or phys)]'
  '-G[get the grown defect list]'
  '-P[get the permanent defect list]'
  '-D[get the standard inquiry data]'
  '-S[get the serial number]'
  '-R[get the transfer rate, etc.]'
  '-c[only report a count of available LUNs]'
  '-l[only print out luns, and not a count]'
  '-r[specify "default", "wellknown" or "all"]'
  '-b[only report the blocksize]'
  '-h[human readable device size, base 2]'
  '-H[human readable device size, base 10]'
  '-N[print the number of blocks instead of last block]'
  '-q[quiet, print numbers only]'
  '-s[only report the last block/device size]'
  '-c[\[args\] specify the SCSI CDB]'
  '-i[fmt specify input data and input data format]'
  '-o[fmt \[args\] specify output data and output data fmt]'
  '-r[fmt \[args\] specify the SMP command to be sent]'
  '-R[fmt \[args\] specify SMP response format]'
  '-l[specify the long response format]'
  '-p[specify the PHY to operate on]'
  '-l[specify the long request/response format]'
  '-o[specify the phy control operation]'
  '-d[set the attached device name]'
  '-m[set the minimum physical link rate]'
  '-M[set the maximum physical link rate]'
  '-T[set the partial pathway timeout value]'
  '-a[enable or disable SATA slumber]'
  '-A[enable or disable SATA partial phy power]'
  '-s[enable or disable SAS slumber]'
  '-S[enable or disable SAS partial phy power]'
  '-l[specify the long response format]'
  '-q[only print phys with attached devices]'
  '-l[specify the long response format]'
  '-I[CAM_DEBUG_INFO -- scsi commands, errors, data]'
  '-T[CAM_DEBUG_TRACE -- routine flow tracking]'
  '-S[CAM_DEBUG_SUBTRACE -- internal routine command flow]'
  '-c[CAM_DEBUG_CDB -- print out SCSI CDBs only]'
  '-N[specify the number of tags to use for this device]'
  '-q[be quiet, dont report the number of tags]'
  '-v[report a number of tag-related parameters]'
  '-a[send a test unit ready after negotiation]'
  '-c[report/set current negotiation settings]'
  '-D["enable" or "disable" disconnection]'
  '-M[set ATA mode]'
  '-O[set command delay offset]'
  '-q[be quiet, dont report anything]'
  '-R[synchronization rate in MHz]'
  '-T["enable" or "disable" tagged queueing]'
  '-U[report/set user negotiation settings]'
  '-W[set the bus width in bits (8, 16 or 32)]'
  '-v[also print a Path Inquiry CCB for the controller]'
  '-q[be quiet, dont print status messages]'
  '-r[run in report only mode]'
  '-w[dont send immediate format command]'
  '-y[dont ask any questions]'
  '-a[operation mode: overwrite, block, crypto or exitfailure]'
  '-c[overwrite passes to perform (1 to 31)]'
  '-I[invert overwrite pattern after each pass]'
  '-P[path to overwrite pattern file]'
  '-q[be quiet, dont print status messages]'
  '-r[run in report only mode]'
  '-U[run operation in unrestricted completion exit mode]'
  '-w[dont send immediate sanitize command]'
  '-y[dont ask any questions]'
  '-t[number of seconds before respective state.]'
  '-f[path to firmware image file]'
  '-q[dont print informational messages, only errors]'
  '-s[run in simulation mode]'
  '-v[print info for every firmware segment sent to device]'
  '-y[dont ask any questions]'
  '-d[disable security using the given password for the selected]'
  '-e[erase the device using the given pwd for the selected user]'
  '-f[freeze the security configuration of the specified device]'
  '-h[enhanced erase the device using the given pwd for the]'
  '-k[unlock the device using the given pwd for the selected]'
  '-l[specifies which security level to set: high or maximum]'
  '-q[be quiet, do not print any status messages]'
  '-s[password the device (enable security) using the given]'
  '-T[overrides the timeout (seconds) used for erase operation]'
  '-U[specifies which user to set: user or master]'
  '-y[dont ask any questions]'
  '-f[freeze the HPA configuration of the device]'
  '-l[lock the HPA configuration of the device]'
  '-P[make the HPA max sectors persist]'
  '-p[set the HPA configuration password required for unlock]'
  '-q[be quiet, do not print any status messages]'
  '-s[configures the maximum user accessible sectors of the]'
  '-U[unlock the HPA configuration of the device]'
  '-y[dont ask any questions]'
  '-i[specify read_keys, read_reservation, report_cap, or]'
  '-o[specify register, register_ignore, reserve, release]'
  '-a[set the All Target Ports (ALL_TG_PT) bit]'
  '-I[specify a Transport ID, e.g.: sas,0x1234567812345678]'
  '-k[specify the Reservation Key]'
  '-K[specify the Service Action Reservation Key]'
  '-p[set the Activate Persist Through Power Loss bit]'
  '-R[specify the Relative Target Port]'
  '-s[specify the scope: lun, extent, element or a number]'
  '-S[specify Transport ID for register, requires -I]'
  '-T[specify the reservation type: read_shared, wr_ex, rd_ex]'
  '-U[unregister the current initiator for register_move]'
  '-r[specify attr_values, attr_list, lv_list, part_list, or]'
  '-w[specify an attribute to write, one -w argument per attr]'
  '-a[only display this attribute number]'
  '-c[get cached attributes]'
  '-e[request attributes for the given element in a changer]'
  '-F[1,form2 output format, comma separated list: text_esc, text_raw]'
  '-p[request attributes for the given partition]'
  '-s[request attributes starting at the given number]'
  '-T[specify the element type (used with -e)]'
  '-V[specify the logical volume ID]'
  '-o[specify the individual opcode to list]'
  '-s[specify the service action for the opcode]'
  '-N[do not return SCSI error for unsupported SA]'
  '-T[request nominal and recommended timeout values]'
  '-c[required: rz, open, close, finish, or rwp]'
  '-a[apply the action to all zones]'
  '-l[specify the zone starting LBA]'
  '-o[report zones options: all, empty, imp_open, exp_open]'
  '-P[report zones printing: normal, summary, script]'
  '-c[required: restore, goto, timer, state, enable, disable]'
  '-d[disable power mode (timer, state)]'
  '-D[delayed entry (goto)]'
  '-e[enable power mode (timer, state)]'
  '-H[hold power mode (goto)]'
  '-p[idle_a, Idle_b, Idle_c, Standby_y, Standby_z (timer]'
  '-P[only display power mode (status)]'
  '-r[restore settings from: default, saved (restore)]'
  '-s[save mode (timer, state, restore)]'
  '-S[set power source: battery, nonbattery (source)]'
  '-T[set timer, seconds, .1 sec resolution (timer)]'
  '-r[report the timestamp of the device]'
  '-f[report the timestamp of the device with the given]'
  '-m[report the timestamp of the device as milliseconds since]'
  '-U[report the time with UTC instead of the local time zone]'
  '-s[set the timestamp of the device]'
  '-f[the format of the time string passed into strptime(3)]'
  '-T[the time value passed into strptime(3)]'
  '-U[set the timestamp of the device to UTC time]'
  '*:filename:_files'
)

_arguments -s $arguments
