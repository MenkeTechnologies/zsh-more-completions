#compdef aarch64-linux-gnu-gp-display-src

local arguments

arguments=(
  '--version[print the version number and exit.]'
  '--help[print usage information and exit.]'
  '--verbose[}  enable (on) or disable (off) verbose mode; the default is "off".]'
  '-func[list all the functions from the given object.]'
  '-source[tag    show the source code for item; the tag is used to]'
  '-disasm[tag        show the source code, interleaved with the disassembled]'
  '-outfile[write results to file <filename>; a dash (-) writes to]'
  '*:filename:_files'
)

_arguments -s $arguments
