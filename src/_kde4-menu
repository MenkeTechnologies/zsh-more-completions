#compdef kde4-menu

local arguments

arguments=(
  '--help[show help about options]'
  '--help-qt[show Qt specific options]'
  '--help-kde[show KDE specific options]'
  '--help-all[show all options]'
  '--author[show author information]'
  {-v,--version}'[show version information]'
  '--license[show license information]'
  '--utf8[output data in UTF-8 instead of local encoding]'
  '--print-menu-id[print menu-id of the menu that contains]'
  '--print-menu-name[print menu name (caption) of the menu that]'
  '--highlight[highlight the entry in the menu]'
  '--nocache-update[do not check if sycoca database is up to date]'
  '*:filename:_files'
)

_arguments -s $arguments
