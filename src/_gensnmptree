#compdef gensnmptree

local arguments

arguments=(
  '-d[debug mode]'
  '-E[extract the named enums and bits only]'
  '-e[extract the named oids or enums]'
  '-h[print this info]'
  '-I[add directory to include path]'
  '-i[read from the named file instead of stdin]'
  '-l[generate local include directives]'
  '-p[prepend prefix to file and variable names]'
  '-t[generated a .def file]'
  '*:filename:_files'
)

_arguments -s $arguments
