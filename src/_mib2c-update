#compdef mib2c-update

local arguments

arguments=(
  '-h[this message.]'
  '-c[specifies the configuration file to use]'
  '-I[specifies a path to look for configuration files in]'
  '-f[specifies the output prefix to use. All code]'
  '-d[debugging output (dont do it. trust me.)]'
  '-S[VAL Set $VAR variable to $VAL]'
  '-i[dont run indent on the resulting code]'
  '-s[dont look for mibNode.sed and run sed on the resulting code]'
  '*:filename:_files'
)

_arguments -s $arguments
