#compdef scan-build60

local arguments

arguments=(
  '-analyze-headers[]'
  '--force-analyze-debug-code[]'
  '-o[location>]'
  '-h[]'
  '--help[]'
  '-k[]'
  '--keep-going[]'
  '--html-title[]'
  '--html-title[]'
  '--show-description[]'
  '-plist[]'
  '-plist-html[]'
  '--status-bugs[]'
  '--use-cc[path\]]'
  '--use-cc[path\]]'
  '--analyzer-target[to properly analyze the source code because static]'
  '--use-c[++ \[compiler path\]]'
  '--use-c[++=\[compiler path\]]'
  '--analyzer-target[triple name for analysis\]]'
  '--analyzer-target[triple name for analysis\]]'
  '-v[]'
  '-V[]'
  '--view[]'
  '-no-failure-reports[]'
  '-stats[]'
  '-maxloop[count>]'
  '-internal-stats[]'
  '--use-analyzer[to clang\]]'
  '--use-analyzer[to clang\]]'
  '--keep-empty[]'
  '--override-compiler[always resort to the ccc-analyzer even when better interposition methods]'
  '-analyzer-config[]'
  '-enable-checker[name\]]'
  '-disable-checker[name\]]'
  '-load-plugin[library\]]'
  '*:filename:_files'
)

_arguments -s $arguments
