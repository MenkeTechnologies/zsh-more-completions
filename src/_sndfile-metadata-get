#compdef sndfile-metadata-get

local arguments

arguments=(
  '--bext-description[print the bext description.]'
  '--bext-originator[print the bext originator info.]'
  '--bext-orig-ref[print the bext origination reference.]'
  '--bext-umid[print the bext UMID.]'
  '--bext-orig-date[print the bext origination date.]'
  '--bext-orig-time[print the bext origination time.]'
  '--bext-coding-hist[print the bext coding history.]'
  '--str-title[print the title metadata.]'
  '--str-copyright[print the copyright metadata.]'
  '--str-artist[print the artist metadata.]'
  '--str-comment[print the comment metadata.]'
  '--str-date[print the creation date metadata.]'
  '--str-album[print the album metadata.]'
  '--str-license[print the license metadata.]'
  '*:filename:_files'
)

_arguments -s $arguments
