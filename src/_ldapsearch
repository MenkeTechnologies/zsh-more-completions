#compdef ldapsearch

local arguments

arguments=(
  '-a[one of never (default), always, search, or find]'
  '-A[retrieve attribute names only (no values)]'
  '-b[base dn for search]'
  '-c[continuous operation mode (do not stop on errors)]'
  '-E[!\]<ext>\[=<extparam>\] search extensions (! indicates criticality)]'
  '-f[read operations from `file]'
  '-F[URL prefix for files (default: file:///var/folders/pr/1cyrcdk979dfkrrzyhlllc4c0000gn/T//)]'
  '-l[time limit (in seconds, or "none" or "max") for search]'
  '-L[print responses in LDIFv1 format]'
  '-LL[print responses in LDIF format without comments]'
  '-LLL[print responses in LDIF format without comments]'
  '-M[enable Manage DSA IT control (-MM to make critical)]'
  '-P[protocol version (default: 3)]'
  '-s[one of base, one, sub or children (search scope)]'
  '-S[sort the results by attribute `attr]'
  '-t[write binary values to files in temporary directory]'
  '-tt[write all values to files in temporary directory]'
  '-T[write files to directory specified by path (default: /var/folders/pr/1cyrcdk979dfkrrzyhlllc4c0000gn/T/)]'
  '-u[include User Friendly entry names in the output]'
  '-z[size limit (in entries, or "none" or "max") for search]'
  '-d[set LDAP debugging level to `level]'
  '-D[bind DN]'
  '-e[!\]<ext>\[=<extparam>\] general extensions (! indicates criticality)]'
  '-h[LDAP server]'
  '-H[LDAP Uniform Resource Identifier(s)]'
  '-I[use SASL Interactive mode]'
  '-n[show what would be done but dont actually do it]'
  '-N[do not use reverse DNS to canonicalize SASL host name]'
  '-O[SASL security properties]'
  '-o[<optparam\] general options]'
  '-p[port on LDAP server]'
  '-Q[use SASL Quiet mode]'
  '-R[SASL realm]'
  '-U[SASL authentication identity]'
  '-v[run in verbose mode (diagnostics to standard output)]'
  '-V[print version info (-VV only)]'
  '-w[bind password (for simple authentication)]'
  '-W[prompt for bind password]'
  '-x[simple authentication]'
  '-X[SASL authorization identity ("dn:<dn>" or "u:<user>")]'
  '-y[read password from file]'
  '-Y[SASL mechanism]'
  '-Z[start TLS request (-ZZ to require successful response)]'
  '*:filename:_files'
)

_arguments -s $arguments
