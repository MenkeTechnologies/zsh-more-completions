#compdef cllualatex

local arguments

arguments=(
  {-e,--engine}'[specify which TeX engine to use.]'
  {-o,--output}'[the name of output file.]'
  '--fresh[clean intermediate files before running TeX.]'
  '--max-iterations[maximum number of running TeX to resolve]'
  '--start-with-draft[start with draft mode.]'
  '--watch[watch input files for change.  Requires fswatch]'
  '--tex-option[pass OPTION to TeX as a single option.]'
  '--tex-options[pass OPTIONs to TeX as multiple options.]'
  '--dvipdfmx-option[OPTION\[s\]  Same for dvipdfmx.]'
  '--makeindex[+OPTIONs  Command to generate index, such as]'
  '--bibtex[+OPTIONs  Command for BibTeX, such as]'
  '--biber[+OPTIONs\]  Command for Biber.]'
  '--makeglossaries[+OPTIONs\]  Command for makeglossaries.]'
  '(- * :)'{-h,--help}'[print this message and exit.]'
  {-v,--version}'[print version information and exit.]'
  {-V,--verbose}'[be more verbose.]'
  '--color[make ClutTeXs message colorful. WHEN is one of]'
  '--includeonly[insert \includeonly{NAMEs}.]'
  '--make-depends[write dependencies as a Makefile rule.]'
  '--shell-restricted[]'
  '--synctex[]'
  '--fmt[--\[no-\]file-line-error   \[default: yes\]]'
  '--interaction[\[default: nonstopmode\]]'
  '--jobname[]'
  '--output-directory[\[default: somewhere in the temporary directory\]]'
  '--output-format[FORMAT is `pdf or `dvi.  \[default: pdf\]]'
  '*:filename:_files'
)

_arguments -s $arguments
