#compdef dnssec-settime

local arguments

arguments=(
  '-E[specify OpenSSL engine]'
  '-f[force update of old-style keys]'
  '-K[set key file location]'
  '-L[set default key TTL]'
  '-v[set level of verbosity]'
  '-V[print version information]'
  '-h[help]'
  '-P[/\[+-\]offset/none: set/unset key publication date]'
  '-P[date/\[+-\]offset/none: set/unset CDS and CDNSKEY publication date]'
  '-A[/\[+-\]offset/none: set/unset key activation date]'
  '-R[/\[+-\]offset/none: set/unset key revocation date]'
  '-I[/\[+-\]offset/none: set/unset key inactivation date]'
  '-D[/\[+-\]offset/none: set/unset key deletion date]'
  '-D[date/\[+-\]offset/none: set/unset CDS and CDNSKEY deletion date]'
  '-S[generate a successor to an existing key]'
  '-i[prepublication interval for successor key (default: 30 days)]'
  '-p[/P/Psync/A/R/I/D/Dsync/all: print a particular time value or values]'
  '-u[print times in unix epoch format]'
  '*:filename:_files'
)

_arguments -s $arguments
