#compdef ippfind

local arguments

arguments=(
  '-4[connect using IPv4.]'
  '-6[connect using IPv6.]'
  '-T[set the browse timeout in seconds.]'
  '-V[set default IPP version.]'
  '--help[show this help.]'
  '--version[show program version.]'
  '-P[match port to number or range.]'
  '-d[match domain to regular expression.]'
  '-h[match hostname to regular expression.]'
  '-l[list attributes.]'
  '-n[match service name to regular expression.]'
  '-p[print URI if true.]'
  '-q[quietly report match via exit code.]'
  '-r[true if service is remote.]'
  '-s[print service name if true.]'
  '-t[true if the TXT record contains the key.]'
  '-u[match URI to regular expression.]'
  '-x[\[argument …\] ;]'
  '--domain[match domain to regular expression.]'
  '--exec[\[argument …\] ;]'
  '--host[match hostname to regular expression.]'
  '--ls[list attributes.]'
  '--local[true if service is local.]'
  '--name[match service name to regular expression.]'
  '--path[match resource path to regular expression.]'
  '--port[match port to number or range.]'
  '--print[print URI if true.]'
  '--print-name[print service name if true.]'
  '--quiet[quietly report match via exit code.]'
  '--remote[true if service is remote.]'
  '--txt[true if the TXT record contains the key.]'
  '--txt-[* regex Match TXT record key to regular expression.]'
  '--uri[match URI to regular expression.]'
  '--not[unary NOT of expression.]'
  '--false[always false.]'
  '--true[always true.]'
  '*:filename:_files'
)

_arguments -s $arguments
