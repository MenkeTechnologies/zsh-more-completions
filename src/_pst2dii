#compdef pst2dii

local arguments

arguments=(
  '-f[Set the font file]'
  '-B[Set the bates prefix string]'
  '-O[Set the dii load file output filename]'
  '-V[Version. Display program version]'
  '-b[Set the starting bates sequence number]'
  '-c[Specify the color of the bates stamps as 6 digit hex]'
  '-d[Debug to file.]'
  '-h[Help. This screen]'
  '-o[Output directory to write files to.]'
  '*:filename:_files'
)

_arguments -s $arguments
