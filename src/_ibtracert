#compdef ibtracert

# Autogenerated from man page /usr/share/man/man8/ibtracert.8.gz

local arguments

arguments=(
    {-n,--no_info}'[simple format; don\\(aqt show additional information.]'
    '-m[show the multicast trace of the specified mlid.]'
    {-f,--force}'[force route to destination port.]'
    '-d[raise the IB debugging level. May be used several times (-ddd or -d -d -d).]'
    '-e[show send and receive errors (timeouts and others).]'
    {-v,--verbose}'[increase the application verbosity level.]'
    {-y,--m_key}'[use the specified M_key for requests.]':key
    {-G,--Guid}'[.]'
    {-L,--Lid}'[.]'
    {-s,--sm_port}'[.]'
    '--ports-file[This file contains multiple source and destination lid or guid pairs.]'
    {-C,--Ca}'[.]'
    {-P,--Port}'[Local port Selection.]'
    {-h,--help}'[.]'
    {-V,--version}'[Configuration flags.]'
    {-t,--timeout}'[.]'
    '--node-name-map[This file maps GUIDs to more user friendly names. See FILES section.]'
    {--config,-z}'[Default: /etc/infiniband-diags/ibdiag. conf FILES CONFIG FILE.]'
    '*:filename:_files'
)
_arguments -s $arguments
