#compdef h5unjam

local arguments

arguments=(
  '-i[.h5 Specifies the HDF5 as input. If the input HDF5 file]'
  '-o[.h5 Specifies output HDF5 file without a user block.]'
  '-u[specifies the output file containing the data from the]'
  '--delete[remove the user block from the input HDF5 file. The content]'
  '-h[prints a usage message and exits.]'
  '-V[prints the HDF5 library version and exits.]'
  '*:filename:_files'
)

_arguments -s $arguments
