#compdef pmlogpaste

# Autogenerated from man page /usr/share/man/man1/pmlogpaste.1.gz

local arguments

arguments=(
    {-f,--file}'[File with input text to be archived, or ``-\\ for standard input.]'
    {-h,--hostname}'[Source host name for the archive.]'
    {-l,--label}'[Add context labels to the generated archive.]'
    {-m,--metric}'[Metric name for the archive. The default metric name is paste. value .]'
    {-o,--outfile}'[Archive output file. The default archive name is paste .]'
    {-t,--timezone}'[Source timezone for the archive archive.]'
    {-?,--help}'[Display usage message and exit.]'
    '*:filename:_files'
)
_arguments -s $arguments
