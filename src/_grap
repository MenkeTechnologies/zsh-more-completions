#compdef grap

local arguments

arguments=(
  '-h[print this list and exit (also --help)]'
  '-C[compatibility mode]'
  '-c[default to not clipping lines]'
  '-d[use given defines file]'
  '-D[use no defines file]'
  '-l[use no defines file]'
  '-R[use coarse numeric comparisons (default)]'
  '-r[use fine numeric comparisons]'
  '-v[print version and exit (also --version)]'
  '-V[print parse debugging information]'
  '-u[force graph labels to be unaligned by default]'
  '-M[specify search path for files]'
  '-S[safer mode, no internal user sprintf calls]'
  '*:filename:_files'
)

_arguments -s $arguments
