#compdef sg_write_same

local arguments

arguments=(
  '--10[|-R do WRITE SAME(10) (even if --unmap is given)]'
  '--16[|-S do WRITE SAME(16) (def: 10 unless --unmap given]'
  '--32[|-T do WRITE SAME(32) (def: 10 or 16)]'
  '--anchor[|-a set anchor field in cdb]'
  '--grpnum[GN GN is group number field (def: 0)]'
  '--help[|-h print out usage message]'
  '--in[IF IF is file to fetch one block of data from (use LEN]'
  '--lba[LBA LBA is the logical block address to start (def: 0)]'
  '--lbdata[|-L set LBDATA bit]'
  '--ndob[|-N set no data-out buffer bit]'
  '--num[NUM NUM is number of logical blocks to write (def: 1)]'
  '--pbdata[|-P set PBDATA bit]'
  '--timeout[TO command timeout (unit: seconds) (def: 60)]'
  '--unmap[|-U set UNMAP bit]'
  '--verbose[|-v increase verbosity]'
  '--version[|-V print version string then exit]'
  '--wrprotect[WPR WPR is the WRPROTECT field value (def: 0)]'
  '--xferlen[LEN LEN is number of bytes from IF to send to]'
  '*:filename:_files'
)

_arguments -s $arguments
