#compdef etcd

local arguments

arguments=(
  '--name[]'
  '--data-dir[${name}.etcd]'
  '--wal-dir[]'
  '--snapshot-count[100000]'
  '--heartbeat-interval[100]'
  '--election-timeout[1000]'
  '--initial-election-tick-advance[]'
  '--listen-peer-urls[//localhost:2380]'
  '--listen-client-urls[//localhost:2379]'
  '--max-snapshots[5]'
  '--max-wals[5]'
  '--quota-backend-bytes[0]'
  '--backend-bbolt-freelist-type[]'
  '--backend-batch-interval[]'
  '--backend-batch-limit[0]'
  '--max-txn-ops[128]'
  '--max-request-bytes[1572864]'
  '--grpc-keepalive-min-time[5s]'
  '--grpc-keepalive-interval[2h]'
  '--grpc-keepalive-timeout[20s]'
  '--socket-reuse-port[]'
  '--socket-reuse-address[]'
  '--initial-advertise-peer-urls[//localhost:2380]'
  '--initial-cluster[http://localhost:2380]'
  '--initial-cluster-state[]'
  '--initial-cluster-token[]'
  '--advertise-client-urls[//localhost:2379]'
  '--discovery[]'
  '--discovery-fallback[]'
  '--discovery-proxy[]'
  '--discovery-srv[]'
  '--discovery-srv-name[]'
  '--strict-reconfig-check[]'
  '--pre-vote[]'
  '--auto-compaction-retention[0]'
  '--auto-compaction-mode[]'
  '--enable-v2[]'
  '--v2-deprecation[]'
  '--cert-file[]'
  '--key-file[]'
  '--client-cert-auth[]'
  '--client-crl-file[]'
  '--client-cert-allowed-hostname[]'
  '--trusted-ca-file[]'
  '--auto-tls[]'
  '--peer-cert-file[]'
  '--peer-key-file[]'
  '--peer-client-cert-auth[]'
  '--peer-trusted-ca-file[]'
  '--peer-cert-allowed-cn[]'
  '--peer-cert-allowed-hostname[]'
  '--peer-auto-tls[]'
  '--self-signed-cert-validity[1]'
  '--peer-crl-file[]'
  '--cipher-suites[]'
  '--cors[*]'
  '--host-whitelist[*]'
  '--auth-token[]'
  '--bcrypt-cost[0]'
  '--auth-token-ttl[00]'
  '--enable-pprof[]'
  '--metrics[]'
  '--listen-metrics-urls[]'
  '--logger[]'
  '--log-outputs[]'
  '--log-level[]'
  '--enable-log-rotation[]'
  '--log-rotation-config-json[{"maxsize": 100, "maxage": 0, "maxbackups": 0, "localtime": false, "compress": false}]'
  '--experimental-enable-distributed-tracing[]'
  '--experimental-distributed-tracing-address[4317]'
  '--experimental-distributed-tracing-service-name[]'
  '--experimental-distributed-tracing-instance-id[]'
  '--proxy[]'
  '--proxy-failure-wait[000]'
  '--proxy-refresh-interval[0000]'
  '--proxy-dial-timeout[000]'
  '--proxy-write-timeout[000]'
  '--proxy-read-timeout[time (in milliseconds) for a read to timeout.]'
  '--experimental-initial-corrupt-check[]'
  '--experimental-corrupt-check-time[0s]'
  '--experimental-enable-v2v3[]'
  '--experimental-enable-lease-checkpoint[]'
  '--experimental-compaction-batch-limit[000]'
  '--experimental-peer-skip-client-san-verification[]'
  '--experimental-watch-progress-notify-interval[10m]'
  '--experimental-warning-apply-duration[100ms]'
  '--experimental-txn-mode-write-with-shared-buffer[]'
  '--experimental-bootstrap-defrag-threshold-megabytes[enable the defrag during etcd server bootstrap on condition that it will free at least the provided threshold of disk space. Needs to be set to non-zero value to take effect.]'
  '--force-new-cluster[]'
  '--unsafe-no-fsync[]'
  '*:filename:_files'
)

_arguments -s $arguments
