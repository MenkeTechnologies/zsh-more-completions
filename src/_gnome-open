#compdef gnome-open

local arguments

arguments=(
  '--load-modules[1,MODULE2,... Dynamic modules to load]'
  '--usage[display brief usage message]'
  '--oaf-ior-fd[file descriptor to print IOR on]'
  '--oaf-activate-iid[IID to activate]'
  '--oaf-private[prevent registering of server]'
  '--disable-sound[disable sound server usage]'
  '--enable-sound[enable sound server usage]'
  '--espeaker[host:port on which the sound]'
  '--version[2.32.1]'
  '*:filename:_files'
)

_arguments -s $arguments
