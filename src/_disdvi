#compdef disdvi

local arguments

arguments=(
  '-p[support ASCII pTeX dvi]'
  '-u[support upTeX dvi]'
  '-x[support XeTeX xdv]'
  '-Eenc[output multibyte encoding. The enc argument denotes u:UTF8 e:EUC-JP s:Shift_JIS j:JIS]'
  '-h[this help message.]'
  '*:filename:_files'
)

_arguments -s $arguments
