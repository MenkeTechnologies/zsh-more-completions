#compdef wbinfo

local arguments

arguments=(
  {-u,--domain-users}'[lists all domain users]'
  {-g,--domain-groups}'[lists all domain groups]'
  {-N,--WINS-by-name}'[converts NetBIOS name to]'
  {-I,--WINS-by-ip}'[converts IP address to]'
  {-n,--name-to-sid}'[converts name to sid]'
  {-s,--sid-to-name}'[converts sid to name]'
  '--sid-to-fullname[converts sid to fullname]'
  {-R,--lookup-rids}'[converts RIDs to names]'
  '--lookup-sids[converts SIDs to types]'
  {-U,--uid-to-sid}'[converts uid to sid]'
  {-G,--gid-to-sid}'[converts gid to sid]'
  {-S,--sid-to-uid}'[converts sid to uid]'
  {-Y,--sid-to-gid}'[converts sid to gid]'
  '--allocate-uid[get a new UID out of idmap]'
  '--allocate-gid[get a new GID out of idmap]'
  '--set-uid-mapping[SID Create or modify uid to]'
  '--set-gid-mapping[SID Create or modify gid to]'
  '--remove-uid-mapping[SID Remove uid to sid mapping]'
  '--remove-gid-mapping[SID Remove gid to sid mapping]'
  '--sids-to-unix-ids[translate SIDs to Unix IDs]'
  '--unix-ids-to-sids[(u<num> g<num>) Translate Unix IDs to SIDs]'
  {-t,--check-secret}'[check shared secret]'
  {-c,--change-secret}'[change shared secret]'
  {-P,--ping-dc}'[check the NETLOGON]'
  {-m,--trusted-domains}'[list trusted domains]'
  '--all-domains[list all domains (trusted]'
  '--own-domain[list own domain]'
  '--sequence[deprecated command, see]'
  '--online-status[]'
  '--online-status[show whether domains are]'
  {-D,--domain-info}'[show most of the info we]'
  {-i,--user-info}'[get user info]'
  '--uid-info[get user info from uid]'
  '--group-info[get group info]'
  '--user-sidinfo[get user info from sid]'
  '--gid-info[get group info from gid]'
  {-r,--user-groups}'[get user groups]'
  '--user-domgroups[get user domain groups]'
  '--sid-aliases[get sid aliases]'
  '--user-sids[get user group sids for]'
  {-a,--authenticate}'[%password authenticate user]'
  '--pam-logon[%password do a pam logon equivalent]'
  '--logoff[log off user]'
  '--logoff-user[username to log off]'
  '--logoff-uid[uid to log off]'
  '--set-auth-user[%password Store user and password]'
  '--ccache-save[%password Store user and password]'
  '--getdcname[get a DC name for a]'
  '--dsgetdcname[find a DC for a domain]'
  '--dc-info[find the currently known]'
  '--get-auth-user[retrieve user and]'
  {-p,--ping}'[ping winbindd to see if]'
  '--domain[define to the domain to]'
  {-K,--krb5auth}'[%password authenticate user using]'
  '--krb5ccname[5ccname authenticate user using]'
  '--separator[get the active winbind]'
  '--verbose[print additional]'
  '--change-user-password[change the password for a]'
  '--ntlmv1[use NTLMv1 cryptography]'
  '--ntlmv2[use NTLMv2 cryptography]'
  '--lanman[use lanman cryptography]'
  '--usage[display brief usage]'
  {-V,--version}'[print version]'
  '*:filename:_files'
)

_arguments -s $arguments
