#compdef dav1d

local arguments

arguments=(
  '--input[/-i $file: input file]'
  '--output[/-o $file: output file]'
  '--demuxer[force demuxer type (ivf or annexb; default: detect from extension)]'
  '--muxer[force muxer type (md5, yuv, yuv4mpeg2 or null; default: detect from extension)]'
  '--quiet[/-q: disable status messages]'
  '--frametimes[dump frame times to file]'
  '--limit[/-l $num: stop decoding after $num frames]'
  '--skip[/-s $num: skip decoding of the first $num frames]'
  '--realtime[$fract\]: limit framerate, optional argument to override input framerate]'
  '--realtimecache[set the size of the cache in realtime mode (default: 0)]'
  '--version[/-v: print version and exit]'
  '--framethreads[number of frame threads (default: 1)]'
  '--tilethreads[number of tile threads (default: 1)]'
  '--filmgrain[enable film grain application (default: 1, except if muxer is md5)]'
  '--oppoint[select an operating point of a scalable AV1 bitstream (0 - 32)]'
  '--alllayers[output all spatial layers of a scalable AV1 bitstream (default: 1)]'
  '--verify[5: verify decoded md5. implies --muxer md5, no output]'
  '--cpumask[restrict permitted CPU instruction sets (0, sse2, ssse3, sse41, avx2 or avx512; default: -1)]'
  '*:filename:_files'
)

_arguments -s $arguments
