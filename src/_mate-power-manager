#compdef mate-power-manager

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show help options]'
  '--verbose[show extra debugging information]'
  '--version[show version of installed program and exit]'
  '--timed-exit[exit after a small delay (for debugging)]'
  '--immediate-exit[exit after the manager has loaded (for debugging)]'
  '*:filename:_files'
)

_arguments -s $arguments
