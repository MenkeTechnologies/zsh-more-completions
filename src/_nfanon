#compdef nfanon

local arguments

arguments=(
  '-h[this text you see right here]'
  '-K[anonymize IP addresses using CryptoPAn with key <key>.]'
  '-r[read input from file]'
  '-M[read input from multiple directories.]'
  '-R[read input from sequence of files.]'
  '-w[name of output file. Defaults to input file.]'
  '*:filename:_files'
)

_arguments -s $arguments
