#compdef spuunmux

local arguments

arguments=(
  '-o[base name for script and images \[sub\]]'
  '-v[verbosity level \[0\]]'
  '-f[resize images to full size \[720x576 or 720x480\]]'
  '-F[specify video format, NTSC or PAL]'
  '-s[number of the substream to extract \[0\]]'
  '-p[name of file with dvd palette \[none\]]'
  '-h[print this help]'
  '-V[print version number]'
  '*:filename:_files'
)

_arguments -s $arguments
