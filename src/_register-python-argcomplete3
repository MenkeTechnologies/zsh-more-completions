#compdef register-python-argcomplete3

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this help message and exit]'
  '--no-defaults[when no matches are generated, do not fallback to]'
  '--complete-arguments[..]'
  '-s[tcsh}, --shell {bash,tcsh}]'
  '*:filename:_files'
)

_arguments -s $arguments
