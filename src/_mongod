#compdef mongod

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this usage information]'
  '--version[show version information]'
  {-f,--config}'[arg configuration file specifying]'
  {-v,--verbose}'[\[=arg(=v)\] be more verbose (include multiple times]'
  '--quiet[quieter output]'
  '--port[specify port number - 27017 by default]'
  '--bind[_ip arg comma separated list of ip addresses to]'
  '--bind[_ip_all bind to all ip addresses]'
  '--ipv6[enable IPv6 support (disabled by]'
  '--listenBacklog[(=128) set socket listen backlog size]'
  '--maxConns[max number of simultaneous connections]'
  '--logpath[log file to send write to instead of]'
  '--syslog[log to systems syslog facility instead]'
  '--syslogFacility[syslog facility used for mongodb syslog]'
  '--logappend[append to logpath instead of]'
  '--logRotate[set the log rotation behavior]'
  '--timeStampFormat[desired format for timestamps in log]'
  '--pidfilepath[full path to pidfile (if not set, no]'
  '--timeZoneInfo[full path to time zone info directory]'
  '--keyFile[private key for cluster authentication]'
  '--noauth[run without security]'
  '--setParameter[set a configurable parameter]'
  '--transitionToAuth[for rolling access control upgrade.]'
  '--clusterAuthMode[authentication mode used for cluster]'
  '--nounixsocket[disable listening on unix sockets]'
  '--unixSocketPrefix[alternative directory for UNIX domain]'
  '--filePermissions[permissions to set on UNIX domain]'
  '--fork[fork server process]'
  '--networkMessageCompressors[arg(=disabled)\] (=snappy)]'
  '--auth[run with security]'
  '--clusterIpSourceWhitelist[network CIDR specification of permitted]'
  '--slowms[(=100) value of slow for profile and console]'
  '--slowOpSampleRate[(=1) fraction of slow ops to include in the]'
  '--profile[0=off 1=slow, 2=all]'
  '--cpu[periodically show cpu and iowait]'
  '--sysinfo[print some diagnostic system]'
  '--noIndexBuildRetry[dont retry any index builds that were]'
  '--noscripting[disable scripting engine]'
  '--notablescan[do not allow table scans]'
  '--shutdown[kill a running server (for init]'
  '--oplogSize[size to use (in MB) for replication op]'
  '--master[master mode]'
  '--slave[slave mode]'
  '--source[when slave: specify master as]'
  '--only[when slave: specify a single database]'
  '--slavedelay[specify delay (in seconds) to be used]'
  '--autoresync[automatically resync if slave data is]'
  '--replSet[arg is <setname>\[/<optionalseedhostlist]'
  '--replIndexPrefetch[specify index prefetching behavior (if]'
  '--enableMajorityReadConcern[arg(=1)\] (=1)]'
  '--configsvr[declare this is a config db of a]'
  '--shardsvr[declare this is a shard db of a]'
  '--sslOnNormalPorts[use ssl on configured ports]'
  '--sslMode[set the SSL operation mode]'
  '--sslPEMKeyFile[PEM file for ssl]'
  '--sslPEMKeyPassword[PEM file password]'
  '--sslClusterFile[key file for internal SSL]'
  '--sslClusterPassword[internal authentication key file]'
  '--sslCAFile[certificate Authority file for SSL]'
  '--sslCRLFile[certificate Revocation List file for]'
  '--sslDisabledProtocols[comma separated list of TLS protocols]'
  '--sslWeakCertificateValidation[allow client to connect without]'
  '--sslAllowConnectionsWithoutCertificates[allow client to connect without]'
  '--sslAllowInvalidHostnames[allow server certificates to provide]'
  '--sslAllowInvalidCertificates[allow connections to servers with]'
  '--sslFIPSMode[activate FIPS 140-2 mode at startup]'
  '--storageEngine[what storage engine to use - defaults]'
  '--dbpath[directory for datafiles - defaults to]'
  '--directoryperdb[each database will be stored in a]'
  '--noprealloc[disable data file preallocation - will]'
  '--nssize[(=16) .ns file size (in MB) for new databases]'
  '--quota[limits each database to a certain]'
  '--quotaFiles[number of files allowed per db, implies]'
  '--quota[]'
  '--smallfiles[use a smaller default file size]'
  '--syncdelay[(=60) seconds between disk syncs (0=never]'
  '--upgrade[upgrade db if needed]'
  '--repair[run repair on all dbs]'
  '--repairpath[root directory for repair files -]'
  '--journal[enable journaling]'
  '--nojournal[disable journaling (journaling is on by]'
  '--journalOptions[journal diagnostic options]'
  '--journalCommitInterval[how often to group/batch commit (ms)]'
  '--wiredTigerCacheSizeGB[maximum amount of memory to allocate]'
  '--wiredTigerJournalCompressor[(=snappy)]'
  '--wiredTigerDirectoryForIndexes[put indexes and data in different]'
  '--wiredTigerCollectionBlockCompressor[(=snappy)]'
  '--wiredTigerIndexPrefixCompression[(=1)]'
  '*:filename:_files'
)

_arguments -s $arguments
