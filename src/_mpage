#compdef mpage

local arguments

arguments=(
  {-1,-2,-4,-8}'[pages per sheet (4) -D strftime format for date specs]'
  '-da[ascii input format -dp Force postscript input format]'
  '-a[across/updown layout (u) -l Toggle portrait/landscape (p)]'
  '-f[folding long lines (off) -o Toggle printing outlines (on)]'
  '-r[printing, last->first sheet -v Toggle verbose output, (on)]'
  '-F[font to be used (Courier) -C Character encoding filename]'
  '-E[every second and third page -O Print every first and fourth page]'
  '-s[tabstop width (8) -k kill on %TRailer is PS file]'
  '-V[mpage version.]'
  '-u[of utf-8 check]'
  '-b[(Letter), use l or ? to get a list of sizes]'
  '-R[to across mode, with first page at lower left corner]'
  '-H[page header for each page for text files]'
  '-X[physical page header (filename + physical pagenumber)]'
  '-c[concat pages of different files on same sheet (off)]'
  '-S[t square pages for PS input (default uniform X/Y shrink)]'
  '-B[textbox margin/thickness (no space): \[<num>\[lrtb\]*\]*]'
  '-m[sheetmargin (no space): \[<num>\[lrtb\]*\]*]'
  '-M[pagemargins (no space): \[<num>\[lrtb\]*\]*]'
  '-p[through prprog (no space), pr(1) is default.]'
  '-P[printer queue (no space). -P only uses default queue.]'
  '-z[print command (lpr).]'
  '-Z[print command print queue option (-P).]'
  '-j[specified sheets: first\[-last\]\[%interval\]]'
  '-j[10 does first 10 sheets, -j 1%2 prints odd ones, -j 2%2 even ones.]'
  '-J[the start of the sheet page count]'
  '-t[printing both sides of the paper (Duplex mode, off)]'
  '-T[tumble of every second pages when printing in duplex mode (off)]'
  '*:filename:_files'
)

_arguments -s $arguments
