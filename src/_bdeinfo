#compdef bdeinfo

local arguments

arguments=(
  '-h[:     shows this help]'
  '-k[:     the full volume encryption key and tweak key]'
  '-o[:     specify the volume offset in bytes]'
  '-p[:     specify the password/passphrase]'
  '-r[:     specify the recovery password]'
  '-s[:     specify the file containing the startup key.]'
  '-v[:     verbose output to stderr]'
  '-V[:     print version]'
  '*:filename:_files'
)

_arguments -s $arguments
