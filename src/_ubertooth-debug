#compdef ubertooth-debug

local arguments

arguments=(
  '-h[message]'
  '-r[<reg>\[,...\]\] read the contents of CC2400 register(s)]'
  '-r[read a consecutive set of CC2400 register(s)]'
  '-U[<0-7> set ubertooth device to use]'
  '-v[<0-2> verbosity (default=1)]'
  '*:filename:_files'
)

_arguments -s $arguments
