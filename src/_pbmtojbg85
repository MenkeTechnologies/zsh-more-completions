#compdef pbmtojbg85

local arguments

arguments=(
  '-s[height of a stripe]'
  '-m[maximum adaptive template pixel horizontal offset (default 8)]'
  '-p[options byte value: add TPBON=8, LRLTWO=64]'
  '-C[add the provided string as a comment marker segment]'
  '-Y[yr announce in header initially the larger image height yi]'
  '*:filename:_files'
)

_arguments -s $arguments
