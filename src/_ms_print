local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this message]'
  '--version[show version]'
  '--threshold[.n> significance threshold, in percent \[1\]]'
  '--x[4..1000> graph width, in columns \[72\]]'
  '--y[4..1000> graph height, in rows \[20\]]'
  '*:filename:_files'
)

_arguments -s $arguments
