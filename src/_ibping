local arguments

arguments=(
    '-d[raise the IB debugging level. May be used several times (-ddd or -d -d -d).]'
    '-e[show send and receive errors (timeouts and others).]'
    {-v,--verbose}'[increase the application verbosity level.]'
    {-c,--count}'[stop after count packets.]'
    {-f,--flood}'[flood destination: send packets back to back without delay.]'
    {-o,--oui}'[use specified OUI number to multiplex vendor mads.]'
    {-S,--Server}'[start in server mode (do not return) Addressing Flags.]'
    {-L,--Lid}'[.]'
    {-G,--Guid}'[.]'
    {-s,--sm_port}'[Port Selection flags.]'
    {-C,--Ca}'[.]'
    {-P,--Port}'[Local port Selection.]'
    {--config,-z}'[Default: /etc/infiniband-diags/ibdiag. conf.]'
    {-t,--timeout}'[Debugging flags.]'
    {-h,--help}'[.]'
    {-V,--version}'[FILES CONFIG FILE.]'
    '*:filename:_files'
)
