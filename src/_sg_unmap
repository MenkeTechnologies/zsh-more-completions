#compdef sg_unmap

local arguments

arguments=(
  '--anchor[|-a set anchor field in cdb]'
  '--grpnum[GN GN is group number field (def: 0)]'
  '--help[|-h print out usage message]'
  '--in[FILE read LBA, NUM pairs from FILE (if FILE is -]'
  '--lba[LBA...|-l LBA,LBA... LBA is the logical block address]'
  '--num[NUM...|-n NUM,NUM... NUM is number of logical blocks to]'
  '--timeout[TO command timeout (unit: seconds) (def: 60)]'
  '--verbose[|-v increase verbosity]'
  '--version[|-V print version string and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
