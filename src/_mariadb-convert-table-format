#compdef mariadb-convert-table-format

local arguments

arguments=(
  {-f,--force}'[continue even if there is some error.]'
  {-e,--engine}'[converts tables to the given storage engine (Default: MYISAM)]'
  {-h,--host}'[host name where the database server is located. (Default: localhost)]'
  {-p,--password}'[password for the current user.]'
  {-P,--port}'[TCP/IP port to connect to if host is not "localhost".]'
  {-S,--socket}'[socket to connect with.]'
  {-u,--user}'[user name to log into the SQL server.]'
  {-v,--verbose}'[this is a test specific option that is only used when debugging a test.]'
  {-V,--version}'[shows the version of this program.]'
  '*:filename:_files'
)

_arguments -s $arguments
