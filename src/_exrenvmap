#compdef exrenvmap

local arguments

arguments=(
  '-o[produces a ONE_LEVEL output file (default)]'
  '-m[produces a MIPMAP_LEVELS output file (-m has]'
  '-c[the output file will be a cube-face environment]'
  '-l[the output file will be a latitude-longitude]'
  '-ci[the input file is interpreted as a cube-face]'
  '-li[the input file is interpreted as a latitude-]'
  '-w[sets the width of the output image to x pixels]'
  '-f[n sets the antialiasing filter radius to r]'
  '-b[blurs the environment map image by applying a]'
  '-t[y sets the output files tile size to x by y pixels]'
  '-p[b if the input image is a latitude-longitude map]'
  '-d[sets level size rounding to ROUND_DOWN (default)]'
  '-u[sets level size rounding to ROUND_UP]'
  '-z[sets the data compression method to x]'
  '-v[verbose mode]'
  '-h[prints this message]'
  '*:filename:_files'
)

_arguments -s $arguments
