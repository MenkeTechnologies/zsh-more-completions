#compdef timelineeditor

local arguments

arguments=(
  '--help[display help]'
  '--version[display version information]'
  '--track[<integer>  Specify track number to edit \[1\]]'
  '--timecode[<string>   Specify timecode file to edit timeline]'
  '--media-timescale[specify media timescale to convert]'
  '--media-timebase[<integer>  Specify media timebase to convert]'
  '--skip[<rational> Skip start of media presentation in arbitrary units]'
  '--delay[<rational> Insert blank clip before actual media presentation in arbitrary units]'
  '--dts-compression[eliminate composition delay with DTS hack]'
  '*:filename:_files'
)

_arguments -s $arguments
