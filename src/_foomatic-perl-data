#compdef foomatic-perl-data

local arguments

arguments=(
  '-O[parse overview XML data]'
  '-C[parse printer/driver combo XML data (default)]'
  '-P[parse printer entry XML data]'
  '-D[parse driver entry XML data]'
  '-o[setting]'
  '-l[language in which the texts are returned, default is "en"]'
  '-v[verbose (debug) mode]'
  '-vv[very verbose (debug) mode]'
  '*:filename:_files'
)

_arguments -s $arguments
