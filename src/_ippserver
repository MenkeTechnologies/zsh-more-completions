#compdef ippserver

local arguments

arguments=(
  '-2[supports 2-sided printing (default=1-sided)]'
  '-M[manufacturer name (default=Test)]'
  '-P[PIN printing mode]'
  '-a[load printer attributes from file]'
  '-c[run command for every print job]'
  '-d[spool directory (default=/tmp/ippserver.5433)]'
  '-f[/subtype\[,...\] List of supported types (default=application/pdf,image/jpeg)]'
  '-h[show program help]'
  '-i[PNG icon file (default=printer.png)]'
  '-k[keep job spool files]'
  '-l[location of printer (default=empty string)]'
  '-m[model name (default=Printer)]'
  '-n[hostname for printer]'
  '-p[port number (default=auto)]'
  '-r[bonjour service subtype (default=_print)]'
  '-s[Speed in pages per minute (default=10,0)]'
  '-v[be (very) verbose]'
  '*:filename:_files'
)

_arguments -s $arguments
