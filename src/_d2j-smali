#compdef d2j-smali

local arguments

arguments=(
  {-a,--api-level}'[\[not impl\] The numeric api-level of the file to]'
  '(- * :)'{-h,--help}'[print this help message]'
  {-o,--output}'[the name of the dex file that will be written. T]'
  {-v,--version}'[prints the version then exits]'
  {-x,--allow-odex-instructions}'[\[not impl\] allow odex instructions to be compile]'
  '*:filename:_files'
)

_arguments -s $arguments
