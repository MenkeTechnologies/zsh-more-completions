#compdef easside-ng

local arguments

arguments=(
  '-h[This help screen]'
  '-v[Victim BSSID]:victim mac:'
  '-m[Source MAC address]:src mac:'
  '-i[Source IP address]:ip:'
  '-r[Router IP address]:router ip:'
  '-s[Buddy-ng IP address (mandatory)]:buddy ip:'
  '-f[Interface to use (mandatory)]:iface:'
  '-c[Lock card to this channel]:channel:'
  '-n[Determine Internet IP only]'
  '*:filename:_files'
)

_arguments -s $arguments
