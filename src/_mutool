#compdef mutool

# Autogenerated from man page /usr/share/man/man1/mutool.1.gz

local arguments

arguments=(
    '-p[Use the specified password if the file is encrypted.]:password'
    '-o[The output format is inferred from the output filename.]:output'
    '-F[Enforce a specific output format.]:format'
    '-R[Rotate clockwise by given number of degrees.]:angle'
    '-r[Render the page at the specified resolution.]:resolution'
    '-w[Render the page at the specified width (or, if the -r flag is used, render wi…]:width'
    '-h[Render the page at the specified height (or, if the -r flag is used, render w…]:height'
    '-f[Fit exactly; ignore the aspect ratio when matching specified width/heights.]'
    '-B[Render in banded mode with each band no taller than the given height.]:bandheight'
    '-W[Page width in points for EPUB layout.]:width'
    '-H[Page height in points for EPUB layout.]:height'
    '-S[Font size in points for EPUB layout.]:size'
    '-U[User CSS stylesheet for EPUB layout.]:filename'
    '-c[Render in the specified colorspace.]:colorspace'
    '-G[Apply gamma correction. Some typical values are 0. 7 or 1.]:gamma'
    '-I[Invert colors.]'
    '-s[Show various bits of information: m for glyph cache and total memory usage, f…]:mft5'
    '-A[Specify how many bits of anti-aliasing to use. The default is 8.]:bits'
    '-D[Disable use of display lists.]'
    '-i[Ignore errors.]'
    '-L[Low memory mode (avoid caching objects by clearing cache after each page).]'
    '-P[Run interpretation and rendering at the same time.]'
    '-g[Garbage collect objects that have no references from other objects.]'
    '-d[Decompress streams.]'
    '-l[Linearize output. Create a "Web Optimized" output file.]'
    '-a[ASCII Hex encode binary streams.]'
    '-z[Deflate uncompressed streams.]'
    '-M[List page dimensions.]'
    '-X[List form and postscript XObjects.]'
    '-O[Comma separated list of format specific output options: decompress.]'
    '-x[Split the page into this many horizontal pieces.]:factor'
    '-y[Split the page into this many vertical pieces.]:factor'
    '-b[Print streams as binary data and omit the object header.]'
    '-e[Print streams in their original encoded (or compressed) form.]'
    '*:filename:_files'
)
_arguments -s $arguments
