#compdef eu-unstrip

local arguments

arguments=(
  '--core[find addresses from signatures found in COREFILE]'
  '--debuginfo-path[search path for separate debuginfo files]'
  {-e,--executable}'[find addresses in FILE]'
  {-k,--kernel}'[find addresses in the running kernel]'
  {-K,--offline-kernel}'[kernel with all modules]'
  {-M,--linux-process-map}'[find addresses in files mapped as read from]'
  {-p,--pid}'[find addresses in files mapped into process PID]'
  {-f,--match-file-names}'[match MODULE against file names, not module names]'
  {-i,--ignore-missing}'[silently skip unfindable files]'
  {-a,--all}'[create output for modules that have no separate]'
  {-d,--output-directory}'[create multiple output files under DIRECTORY]'
  {-F,--force}'[force combining files even if some ELF headers]'
  {-m,--module-names}'[use module rather than file names]'
  {-n,--list-only}'[only list module and file names, build IDs]'
  {-o,--output}'[place output into FILE]'
  {-R,--relocate}'[apply relocations to section contents in ET_REL]'
  '--usage[give a short usage message]'
  {-V,--version}'[print program version]'
  '*:filename:_files'
)

_arguments -s $arguments
