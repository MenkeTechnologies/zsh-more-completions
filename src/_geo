#compdef geo

local arguments

arguments=(
  '-w[returns WAN IP]'
  '-l[returns LAN IP(s)]'
  '-r[returns Router IP]'
  '-d[returns DNS Nameserver]'
  '-m[returns MAC address for interface. Ex. eth0]'
  '-g[returns Current IP Geodata]'
  '-o[returns Specific Geodata]'
  '-a[for specific IP in -s]'
  '-v[returns Version]'
  '-h[returns Help Screen]'
  '-u[updates Bash-Snippets]'
  '*:filename:_files'
)

_arguments -s $arguments
