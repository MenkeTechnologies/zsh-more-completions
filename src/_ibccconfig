#compdef ibccconfig

# Autogenerated from man page /usr/share/man/man8/ibccconfig.8.gz

local arguments

arguments=(
    '-d[raise the IB debugging level. May be used several times (-ddd or -d -d -d).]'
    '-e[show send and receive errors (timeouts and others).]'
    {-v,--verbose}'[increase the application verbosity level.]'
    {-y,--m_key}'[use the specified M_key for requests.]':key
    {--cckey,-c}'[Specify a congestion control (CC) key.]':cckey
    {-h,--help}'[.]'
    {-V,--version}'[Addressing Flags.]'
    {-G,--Guid}'[.]'
    {-L,--Lid}'[.]'
    {-s,--sm_port}'[Port Selection flags.]'
    {-C,--Ca}'[.]'
    {-P,--Port}'[Local port Selection.]'
    {--config,-z}'[Default: /etc/infiniband-diags/ibdiag.]'
    '*:filename:_files'
)
_arguments -s $arguments
