#compdef c44

local arguments

arguments=(
  '-slice[+...+n -- select an increasing sequence of data slices]'
  '-bpp[..,n -- select a increasing sequence of bitrates]'
  '-size[..,n -- select an increasing sequence of minimal sizes]'
  '-percent[..,n -- selects the percentage of original file size]'
  '-decibel[..,n -- select an increasing sequence of luminance error]'
  {-dbfrac,--}'[decibel estimation to a fraction of]'
  {-mask,--}'[bitmask specifying image zone to encode]'
  {-dpi,--}'[the image resolution]'
  {-gamma,--}'[the image gamma correction]'
  {-crcbfull,--}'[chrominance with highest quality]'
  {-crcbnormal,--}'[chrominance with normal resolution (default)]'
  {-crcbhalf,--}'[chrominance with half resolution]'
  {-crcbnone,--}'[not encode chrominance at all]'
  {-crcbdelay,--}'[chrominance coding delay (default 10)]'
  '*:filename:_files'
)

_arguments -s $arguments
