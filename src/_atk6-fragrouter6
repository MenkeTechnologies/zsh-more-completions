#compdef atk6-fragrouter6

local arguments

arguments=(
  '-f[size of mini fragments (option 64, default 8)]'
  '-e[hop count to the IDS for evading (option 256 & 512)]'
  '-t[hop count to the target that reaches it]'
  '-v[verbose mode, print all packets processed for evasion]'
  '*:filename:_files'
)

_arguments -s $arguments
