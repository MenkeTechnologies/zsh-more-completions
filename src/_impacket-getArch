#compdef impacket-getArch

local arguments

arguments=(
  {-h,--help}'[show this help message and exit]'
  '-target[<targetName or address>]'
  '-targets[input file with targets system to query Arch from (one per]'
  '-timeout[socket timeout out when connecting to the target (default]'
  '-debug[turn DEBUG output ON]'
  '*:filename:_files'
)

_arguments -s $arguments
