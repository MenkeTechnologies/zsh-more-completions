#compdef smidump

local arguments

arguments=(
  {-V,--version}'[show version and license information]'
  '(- * :)'{-h,--help}'[show usage information]'
  {-q,--quiet}'[do not generate any comments]'
  {-c,--config}'[load a specific configuration file]'
  {-p,--preload}'[preload <module>]'
  {-l,--level}'[set maximum level of errors and warnings]'
  {-m,--error-names}'[print the name of errors in braces]'
  {-s,--severity}'[print the severity of errors in brackets]'
  {-f,--format}'[use <format> when dumping (default smiv2)]'
  {-o,--output}'[use <name> when creating names for output files]'
  {-u,--unified}'[print a single unified output of all modules]'
  {-k,--keep-going}'[continue after serious parse errors]'
  '--cm-explain[explain what the algorithm does]'
  '--identifiers-lines[show line numbers]'
  '--identifiers-path[show file path instead of module name]'
  '--identifiers-ctag[show symbols in \[g\]ctag format]'
  '--jax-package[make classes part of a given package]'
  '--metrics-raw[generate raw statistics (no percentages)]'
  '--netsnmp-no-mgr-stubs[do not generate manager stub code]'
  '--netsnmp-no-agt-stubs[do not generate agent stub code]'
  '--scli-prefix[use prefix instead of module name in stubs]'
  '--scli-include[include stubs for groups matching a regex]'
  '--scli-exclude[exclude stubs for groups matching a regex]'
  '--scli-set[generate set stubs for writable objects]'
  '--scli-create[generate create stubs for tables using RowStatus]'
  '--scli-delete[generate delete stubs for tables using RowStatus]'
  '--svg-width[width of the svg output (default=1100)]'
  '--svg-height[height of the svg output (default=700)]'
  '--svg-show-deprecated[show deprecated items]'
  '--svg-show-depr-obsolete[show deprecated and obsolete items]'
  '--svg-static-output[disable all interactivity (e.g. for printing)]'
  '--tree-no-conformance[do not show conformance nodes]'
  '--tree-no-leafs[do not show leaf nodes]'
  '--tree-full-root[generate the full path to the root]'
  '--tree-compact[generate a more compact representation]'
  '--types-no-implicit[ignore implicit type definitions]'
  '--sizes-variables[show detailed information the sizes of variables]'
  '--xml-no-schema[disable XML Schema spec in the toplevel element]'
  '--xml-no-doctype[disable DOCTYPE spec in the XML prolog]'
  '--xsd-schema-url[URI prefix for schema definitions and namespaces]'
  '--xsd-container[generate a container schema]'
  '--xsd-nest-augments[nest rows of augmented tables in the base tables]'
  '--xsd-nest-subtables[nest subtables in the base tables]'
  '--yang-smi-extensions[generate smi extensions]'
  '--yang-no-notifications[do not generate notifications]'
  '--yang-indent[indentation (default 2)]'
  '*:filename:_files'
)

_arguments -s $arguments
