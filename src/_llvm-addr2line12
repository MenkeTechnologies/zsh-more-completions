#compdef llvm-addr2line12

local arguments

arguments=(
  '--addresses[show address before line information]'
  '--adjust-vma[add specified offset to object file addresses]'
  '-a[alias for --addresses]'
  '--basenames[strip directory names from paths]'
  '-C[alias for --demangle]'
  '--debug-file-directory[path to directory where to look for debug files]'
  '--default-arch[default architecture (for multi-arch objects)]'
  '-demangle[alias for --no-demangle]'
  '-demangle[alias for --demangle]'
  '--demangle[demangle function names]'
  '--dia[use the DIA library to access symbols (Windows only)]'
  '--dsym-hint[path to .dSYM bundles to search for debug info for the object files]'
  '--dwp[path to DWP file to be use for any split CUs]'
  '-e[alias for --obj]'
  '--exe[alias for --obj]'
  '--exe[alias for --obj]'
  '-e[alias for --obj]'
  '-f[alias for --functions]'
  '--fallback-debug-path[fallback path for debug binaries]'
  '--functions[print function name for a given address]'
  '--functions[print function name for a given address]'
  '-f[alias for --functions]'
  '--help[display this help]'
  '--inlines[print all inlined frames for a given address]'
  '--inlining[alias for --no-inlines]'
  '--inlining[alias for --inlines]'
  '--inlining[alias for --inlines]'
  '-i[alias for --inlines]'
  '--no-demangle[dont demangle function names]'
  '--no-inlines[do not print inlined frames]'
  '--no-untag-addresses[remove memory tags from addresses before symbolization]'
  '--obj[path to object file to be symbolized (if not provided, object file should be specified for each input line)]'
  '--output-style[specify print style. Supported styles: LLVM, GNU]'
  '--pretty-print[make the output more human friendly]'
  '--print-address[alias for --addresses]'
  '--print-source-context-lines[print N lines of source file context]'
  '-p[alias for --pretty-print]'
  '--relative-address[interpret addresses as addresses relative to the image base]'
  '--relativenames[strip the compilation directory from paths]'
  '-s[alias for --basenames]'
  '--untag-addresses[]'
  '--verbose[print verbose line info]'
  '--version[display the version]'
  '-v[alias for --version]'
  '*:filename:_files'
)

_arguments -s $arguments
