#compdef mongoimport

local arguments

arguments=(
  '--help[print usage]'
  '--version[print the tool version and]'
  {-v,--verbose}'[more detailed log output]'
  '--quiet[hide all log output]'
  {-h,--host}'[mongodb host to connect to]'
  '--port[server port (can also use]'
  '--host[)]'
  '--gssapiServiceName[service name to use when]'
  '--gssapiHostName[hostname to use when]'
  '--ssl[connect to a mongod or mongos]'
  '--sslCAFile[the .pem file containing the]'
  '--sslPEMKeyFile[the .pem file containing the]'
  '--sslPEMKeyPassword[the password to decrypt the]'
  '--sslCRLFile[the .pem file containing the]'
  '--sslAllowInvalidCertificates[bypass the validation for]'
  '--sslAllowInvalidHostnames[bypass the validation for]'
  '--sslFIPSMode[use FIPS mode of the]'
  {-u,--username}'[username for authentication]'
  {-p,--password}'[password for authentication]'
  '--authenticationDatabase[database that holds the]'
  '--authenticationMechanism[authentication mechanism to]'
  {-d,--db}'[database to use]'
  {-c,--collection}'[collection to use]'
  '--uri[mongodb uri connection string]'
  {-f,--fields}'[<field>\]* comma separated list of]'
  '--fieldFile[file with field names - 1 per]'
  '--file[file to import from; if not]'
  '--headerline[use first line in input]'
  '--jsonArray[treat input source as a JSON]'
  '--parseGrace[controls behavior when type]'
  '--type[input format to import: json]'
  '--columnsHaveTypes[indicated that the field list]'
  '--drop[drop collection before]'
  '--ignoreBlanks[ignore fields with empty]'
  '--maintainInsertionOrder[insert documents in the order]'
  {-j,--numInsertionWorkers}'[number of insert operations]'
  '--stopOnError[stop importing at first]'
  '--mode[insert: insert only. upsert:]'
  '--upsertFields[<field>\]* comma-separated fields for]'
  '--writeConcern[write concern options e.g.]'
  '--writeConcern[]'
  '--writeConcern[{w: 3]'
  '--bypassDocumentValidation[bypass document validation]'
  '*:filename:_files'
)

_arguments -s $arguments
