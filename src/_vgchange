#compdef vgchange

local arguments

arguments=(
  '-p[|--maxphysicalvolumes Number]'
  '-u[|--uuid]'
  '-c[|--clustered y|n]'
  '-s[|--physicalextentsize Size\[m|UNIT\]]'
  '-x[|--resizeable y|n]'
  '--addtag[]'
  '--deltag[]'
  '--alloc[]'
  '--pvmetadatacopies[1|2]'
  '--vgmetadatacopies[]'
  '--profile[]'
  '--detachprofile[]'
  '--metadataprofile[)]'
  '*:filename:_files'
)

_arguments -s $arguments
