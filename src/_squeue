#compdef squeue

local arguments

arguments=(
  {-A,--account}'[(s) comma separated list of accounts]'
  {-a,--all}'[display jobs in hidden partitions]'
  '--array-unique[display one unique pending job array]'
  '--federation[report federated information if a member]'
  {-h,--noheader}'[no headers on output]'
  '--hide[do not display jobs in hidden partitions]'
  {-i,--iterate}'[specify an interation period]'
  {-j,--job}'[(s) comma separated list of jobs IDs]'
  '--local[report information only about jobs on the]'
  {-l,--long}'[long report]'
  {-L,--licenses}'[names) comma separated list of license names to view]'
  {-M,--clusters}'[cluster to issue commands to. Default is]'
  {-n,--name}'[(s) comma separated list of job names to view]'
  '--noconvert[dont convert units from their original type]'
  {-o,--format}'[format specification]'
  {-O,--Format}'[format specification]'
  {-p,--partition}'[(s) comma separated list of partitions]'
  {-q,--qos}'[(s) comma separated list of qoss]'
  {-R,--reservation}'[reservation to view, default is all]'
  {-r,--array}'[display one job array element per line]'
  '--sibling[report information about all sibling jobs]'
  {-s,--step}'[(s) comma separated list of job steps]'
  {-S,--sort}'[comma separated list of fields to sort on]'
  '--start[print expected start times of pending jobs]'
  {-t,--states}'[comma separated list of states to view]'
  {-u,--user}'[(s) comma separated list of users to view]'
  '--name[(s) comma separated list of job names to view]'
  {-v,--verbose}'[verbosity level]'
  {-V,--version}'[output version information and exit]'
  {-w,--nodelist}'[list of nodes to view, default is]'
  '--help[show this help message]'
  '--usage[display a brief summary of squeue options]'
  '*:filename:_files'
)

_arguments -s $arguments
