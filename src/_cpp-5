#compdef cpp-5

local arguments

arguments=(
  '-pass-exit-codes[exit with highest error code from a phase]'
  '--help[display this information]'
  '--target-help[display target specific command line options]'
  '--help[^\]{joined|separate|undocumented}}\[,...\]]'
  '--version[display compiler version information]'
  '-dumpspecs[display all of the built in spec strings]'
  '-dumpversion[display the version of the compiler]'
  '-dumpmachine[display the compilers target processor]'
  '-print-search-dirs[display the directories in the compilers search path]'
  '-print-libgcc-file-name[display the name of the compilers companion library]'
  '-print-file-name[display the full path to library <lib>]'
  '-print-prog-name[display the full path to compiler component <prog>]'
  '-print-multiarch[display the targets normalized GNU triplet, used as]'
  '-print-multi-directory[display the root directory for versions of libgcc]'
  '-print-multi-lib[display the mapping between command line options and]'
  '-print-multi-os-directory[the relative path to OS libraries]'
  '-print-sysroot[display the target libraries directory]'
  '-print-sysroot-headers-suffix[the sysroot suffix used to find headers]'
  '-Wa[Pass comma-separated <options> on to the assembler]:options:'
  '-Wp[Pass comma-separated <options> on to the preprocessor]:options:'
  '-Wl[Pass comma-separated <options> on to the linker]:options:'
  '-Xassembler[pass <arg> on to the assembler]'
  '-Xpreprocessor[pass <arg> on to the preprocessor]'
  '-Xlinker[pass <arg> on to the linker]'
  '-save-temps[do not delete intermediate files]'
  '-save-temps[do not delete intermediate files]'
  '-no-canonical-prefixes[do not canonicalize paths when building relative]'
  '-pipe[use pipes rather than intermediate files]'
  '-time[time the execution of each subprocess]'
  '-specs[override built-in specs with the contents of <file>]'
  '-std[assume that the input sources are for <standard>]'
  '--sysroot[use <directory> as the root directory for headers]'
  '-B[add <directory> to the compilers search paths]'
  '-v[display the programs invoked by the compiler]'
  '-E[preprocess only; do not compile, assemble or link]'
  '-S[compile only; do not assemble or link]'
  '-c[compile and assemble, but do not link]'
  '-o[place the output into <file>]'
  '-pie[create a position independent executable]'
  '-shared[create a shared library]'
  '-x[specify the language of the following input files]'
  '*:filename:_files'
)

_arguments -s $arguments
