#compdef netbiosd

local arguments

arguments=(
  '-help[help on all flags \[tip: all flags can have two dashes\])]'
  '-debug[extended debug output) type: bool default: false]'
  '-dump-packets[sent and received NetBIOS packet contents) type: bool]'
  '-max-refresh[time (in seconds) between searches to refresh the]'
  '-min-refresh[time (in seconds) between searches to refresh the]'
  '-stdout[log messages to standard output) type: bool default: false]'
  '*:filename:_files'
)

_arguments -s $arguments
