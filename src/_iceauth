#compdef iceauth

local arguments

arguments=(
  '-f[name of authority file to use]'
  '-v[turn on extra messages]'
  '-q[turn off extra messages]'
  '-i[ignore locks on authority file]'
  '-b[break locks on authority file]'
  '-V[print version and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
