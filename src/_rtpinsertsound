#compdef rtpinsertsound

local arguments

arguments=(
  '-a[RTP IPv4 addr]'
  '-A[RTP port]'
  '-b[RTP IPv4 addr]'
  '-B[RTP port]'
  '-f[factor - amount by which to:]'
  '-i[(e.g. eth0)]'
  '-j[factor - the reception of a legitimate RTP]'
  '-p[to pause between setup and injection]'
  '-h[print this usage]'
  '-v[output mode]'
  '*:filename:_files'
)

_arguments -s $arguments
