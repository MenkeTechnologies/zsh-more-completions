#compdef vmware-vmx-debug

local arguments

arguments=(
  '-P[start paused]'
  '-q[exit at power off]'
  '-s[value set variable NAME to VALUE]'
  '-x[power on when program starts]'
  '-X[as -x, also go to full screen mode]'
  '-v[print program version]'
  '-o[output GPU capabilities]'
  '-e[extract a resource specified by name]'
  '-3[to parse command line options.]'
  '*:filename:_files'
)

_arguments -s $arguments
