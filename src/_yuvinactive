#compdef yuvinactive

local arguments

arguments=(
  {-h,-H}'[- print out this help]'
  '-i[+Y+XOFF+YOFF - the area which will be set inactive, from top left]'
  '-s[cb,Cr - set the filling color in yuv format]'
  '-d[\[1-100\] - set how much darker the area will be, in percent]'
  '-c[- copy num sourrounding lines into the area]'
  '-a[- use num pixles for on square to average the area]'
  '*:filename:_files'
)

_arguments -s $arguments
