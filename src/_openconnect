#compdef openconnect

local arguments

arguments=(
  '--config[read options from config file]'
  {-b,--background}'[continue in background after startup]'
  '--pid-file[write the daemons PID to this file]'
  {-c,--certificate}'[use SSL client certificate CERT]'
  {-e,--cert-expire-warning}'[warn when certificate lifetime < DAYS]'
  {-k,--sslkey}'[use SSL private key file KEY]'
  {-C,--cookie}'[use WebVPN cookie COOKIE]'
  '--cookie-on-stdin[read cookie from standard input]'
  {-d,--deflate}'[enable compression (default)]'
  {-D,--no-deflate}'[disable compression]'
  '--force-dpd[set minimum Dead Peer Detection interval]'
  {-g,--usergroup}'[set login usergroup]'
  '(- * :)'{-h,--help}'[display help text]'
  {-i,--interface}'[use IFNAME for tunnel interface]'
  {-l,--syslog}'[use syslog for progress messages]'
  '--timestamp[prepend timestamp to progress messages]'
  '--passtos[copy TOS / TCLASS when using DTLS]'
  {-U,--setuid}'[drop privileges after connecting]'
  '--csd-user[drop privileges during CSD execution]'
  '--csd-wrapper[run SCRIPT instead of CSD binary]'
  {-m,--mtu}'[request MTU from server (legacy servers only)]'
  '--base-mtu[indicate path MTU to/from server]'
  {-p,--key-password}'[set key passphrase or TPM SRK PIN]'
  '--key-password-from-fsid[key passphrase is fsid of file system]'
  {-P,--proxy}'[set proxy server]'
  '--proxy-auth[set proxy authentication methods]'
  '--no-proxy[disable proxy]'
  '--libproxy[use libproxy to automatically configure proxy]'
  '--pfs[require perfect forward secrecy]'
  {-q,--quiet}'[less output]'
  {-Q,--queue-len}'[set packet queue limit to LEN pkts]'
  {-s,--script}'[shell command line for using a vpnc-compatible config script]'
  {-S,--script-tun}'[pass traffic to script program, not tun]'
  {-u,--user}'[set login username]'
  {-V,--version}'[report version number]'
  {-v,--verbose}'[more output]'
  '--dump-http-traffic[dump HTTP authentication traffic (implies --verbose]'
  {-x,--xmlconfig}'[XML config file]'
  '--authgroup[choose authentication login selection]'
  '--authenticate[authenticate only and print login info]'
  '--cookieonly[fetch webvpn cookie only; dont connect]'
  '--printcookie[print webvpn cookie before connecting]'
  '--cafile[cert file for server verification]'
  '--disable-ipv6[do not ask for IPv6 connectivity]'
  '--dtls-ciphers[openSSL ciphers to support for DTLS]'
  '--no-dtls[disable DTLS]'
  '--no-http-keepalive[disable HTTP connection re-use]'
  '--no-passwd[disable password/SecurID authentication]'
  '--no-cert-check[do not require server SSL cert to be valid]'
  '--no-system-trust[disable default system certificate authorities]'
  '--no-xmlpost[do not attempt XML POST authentication]'
  '--non-inter[do not expect user input; exit if it is required]'
  '--passwd-on-stdin[read password from standard input]'
  '--token-mode[software token type: rsa, totp or hotp]'
  '--token-secret[software token secret]'
  '--reconnect-timeout[connection retry timeout in seconds]'
  '--servercert[servers certificate SHA1 fingerprint]'
  '--useragent[HTTP header User-Agent: field]'
  '--local-hostname[local hostname to advertise to server]'
  '--resolve[use IP when connecting to HOST]'
  '--os[OS type (linux,linux-64,win,...) to report]'
  '--dtls-local-port[set local port for DTLS datagrams]'
  '*:filename:_files'
)

_arguments -s $arguments
