#compdef cvdbset

local arguments

arguments=(
  '-h[this help and exit.]'
  '-c[on continuous tracing to files cvdbout.000000, cvdbout.000001, etc.]'
  '-d[cvdb tracing.]'
  '-g[the log buffer to standard out.]'
  '-l[modules.]'
  '-L[tracepoints.]'
  '-r[resize trace buffer to <mb> megabytes]'
  '-t[one or more tracepoints (default is to enable all).]'
  '*:filename:_files'
)

_arguments -s $arguments
