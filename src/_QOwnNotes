#compdef QOwnNotes

local arguments

arguments=(
  '(- * :)'{-h,--help}'[displays this help.]'
  '--portable[runs the application in portable mode.]'
  '--dump-settings[prints out a dump of the settings and other information]'
  '--clear-settings[clears the settings and runs the application.]'
  '--session[runs the application in a different context for settings]'
  '*:filename:_files'
)

_arguments -s $arguments
