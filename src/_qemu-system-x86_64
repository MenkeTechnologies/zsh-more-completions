#compdef qemu-system-x86_64

local arguments

arguments=(
  {-h,-help}'[display this help and exit]'
  '-version[display version information and exit]'
  '-machine[\]name\[,prop\[=value\]\[,...\]\]]'
  '-cpu[select CPU (-cpu help for list)]'
  '-accel[\]accelerator\[,thread=single|multi\]]'
  '-smp[\]n\[,maxcpus=cpus\]\[,cores=cores\]\[,threads=threads\]\[,sockets=sockets\]]'
  '-numa[mem=size\]\[,cpus=firstcpu\[-lastcpu\]\]\[,nodeid=node\]]'
  '-numa[memdev=id\]\[,cpus=firstcpu\[-lastcpu\]\]\[,nodeid=node\]]'
  '-numa[src=source,dst=destination,val=distance]'
  '-add-fd[fd,set=set\[,opaque=opaque\]]'
  '-set[.id.arg=value]'
  '-global[.property=value]'
  '-global[driver,property=property,value=value]'
  '-boot[drives\]\[,once=drives\]\[,menu=on|off\]]'
  '-m[\]megs\[,slots=n,maxmem=size\]]'
  '-mem-path[provide backing storage for guest RAM]'
  '-mem-prealloc[preallocate guest memory (use with -mem-path)]'
  '-k[use keyboard layout (for example fr for French)]'
  '-audio-help[print list of audio drivers and their options]'
  '-soundhw[1,... enable audio support]'
  '-balloon[disable balloon device]'
  '-balloon[addr=str\]]'
  '-device[prop\[=value\]\[,...\]\]]'
  '-name[1\[,process=string2\]\[,debug-threads=on|off\]]'
  '-uuid[08x-%04x-%04x-%04x-%012x]'
  '-fda[/-fdb file use file as floppy disk 0/1 image]'
  '-hda[/-hdb file use file as IDE hard disk 0/1 image]'
  '-hdc[/-hdd file use file as IDE hard disk 2/3 image]'
  '-cdrom[use file as IDE cdrom image (cdrom is ide1 master)]'
  '-blockdev[\]driver\[,node-name=N\]\[,discard=ignore|unmap\]]'
  '-drive[file\]\[,if=type\]\[,bus=n\]\[,unit=m\]\[,media=d\]\[,index=i\]]'
  '-mtdblock[use file as on-board Flash memory image]'
  '-sd[use file as SecureDigital card image]'
  '-pflash[use file as a parallel flash image]'
  '-snapshot[write to temporary files instead of disk image files]'
  '-hdachs[h,s\[,t\]]'
  '-fsdev[id=id\[,path=path,\]\[security_model={mapped-xattr|mapped-file|passthrough|none}\]]'
  '-virtfs[path=path,mount_tag=tag,security_model=\[mapped-xattr|mapped-file|passthrough|none\]]'
  '-virtfs[_synth Create synthetic file system image]'
  '-usb[enable the USB driver (if it is not used by default yet)]'
  '-usbdevice[add the host or guest USB device name]'
  '-display[frame=on|off\]\[,alt_grab=on|off\]\[,ctrl_grab=on|off\]]'
  '-display[grab_on_hover=on|off\]\[,gl=on|off\]|]'
  '-display[<display>\[,<optargs>\]]'
  '-display[]'
  '-display[select display type]'
  '-nographic[disable graphical output and redirect serial I/Os to console]'
  '-curses[shorthand for -display curses]'
  '-no-frame[open SDL window without a frame and window decorations]'
  '-alt-grab[use Ctrl-Alt-Shift to grab mouse (instead of Ctrl-Alt)]'
  '-ctrl-grab[use Right-Ctrl to grab mouse (instead of Ctrl-Alt)]'
  '-no-quit[disable SDL window close capability]'
  '-sdl[shorthand for -display sdl]'
  '-spice[port\]\[,tls-port=secured-port\]\[,x509-dir=<dir>\]]'
  '-portrait[rotate graphical output 90 deg left (only PXA LCD)]'
  '-rotate[rotate graphical output some deg left (only PXA LCD)]'
  '-vga[3|virtio|none\]]'
  '-full-screen[start in full screen]'
  '-vnc[shorthand for -display vnc=<display>]'
  '-win2k-hack[use it when installing Windows 2000 to avoid a disk full bug]'
  '-no-fd-bootchk[disable boot signature checking for floppy disks]'
  '-no-acpi[disable ACPI]'
  '-no-hpet[disable HPET]'
  '-acpitable[str\]\[,rev=n\]\[,oem_id=str\]\[,oem_table_id=str\]\[,oem_rev=n\]\[,asl_compiler_id=str\]\[,asl_compiler_rev=n\]\[,{data|file}=file1\[:file2\]...\]]'
  '-smbios[binary]'
  '-smbios[0\[,vendor=str\]\[,version=str\]\[,date=str\]\[,release=%d.%d\]]'
  '-smbios[1\[,manufacturer=str\]\[,product=str\]\[,version=str\]\[,serial=str\]]'
  '-smbios[2\[,manufacturer=str\]\[,product=str\]\[,version=str\]\[,serial=str\]]'
  '-smbios[3\[,manufacturer=str\]\[,version=str\]\[,serial=str\]\[,asset=str\]]'
  '-smbios[4\[,sock_pfx=str\]\[,manufacturer=str\]\[,version=str\]\[,serial=str\]]'
  '-smbios[17\[,loc_pfx=str\]\[,bank=str\]\[,manufacturer=str\]\[,serial=str\]]'
  '-netdev[id=str\[,ipv4\[=on|off\]\]\[,net=addr\[/mask\]\]\[,host=addr\]]'
  '-netdev[id=str\[,fd=h\]\[,fds=x:y:...:z\]\[,ifname=name\]\[,script=file\]\[,downscript=dfile\]]'
  '-netdev[id=str\[,br=bridge\]\[,helper=helper\]]'
  '-netdev[2tpv3,id=str,src=srcaddr,dst=dstaddr\[,srcport=srcport\]\[,dstport=dstport\]]'
  '-netdev[id=str\[,fd=h\]\[,listen=\[host\]:port\]\[,connect=host:port\]]'
  '-netdev[id=str\[,fd=h\]\[,mcast=maddr:port\[,localaddr=addr\]\]]'
  '-netdev[id=str\[,fd=h\]\[,udp=host:port\]\[,localaddr=host:port\]]'
  '-netdev[id=str,chardev=dev\[,vhostforce=on|off\]]'
  '-netdev[id=str,hubid=n]'
  '-net[vlan=n\]\[,macaddr=mac\]\[,model=type\]\[,name=str\]\[,addr=str\]\[,vectors=v\]]'
  '-net[vlan=n\]\[,file=f\]\[,len=n\]]'
  '-net[use it alone to have zero network devices. If no -net option]'
  '-net[vlan=n\]\[,option\]\[,option\]\[,...\]]'
  '-chardev[]'
  '-chardev[id=id\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id\[,host=host\],port=port\[,to=to\]\[,ipv4\]\[,ipv6\]\[,nodelay\]\[,reconnect=seconds\]]'
  '-chardev[id=id,path=path\[,server\]\[,nowait\]\[,telnet\]\[,reconnect=seconds\]]'
  '-chardev[id=id\[,host=host\],port=port\[,localaddr=localaddr\]]'
  '-chardev[id=id\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id\[\[,width=width\]\[,height=height\]\]\[\[,cols=cols\]\[,rows=rows\]\]]'
  '-chardev[id=id\[,size=size\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id,path=path\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id,path=path\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id\[,mux=on|off\]\[,signal=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id,path=path\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id,path=path\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id,path=path\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id,path=path\[,mux=on|off\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id,name=name\[,debug=debug\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-chardev[id=id,name=name\[,debug=debug\]\[,logfile=PATH\]\[,logappend=on|off\]]'
  '-iscsi[user\]\[,password=password\]]'
  '-bt[null dumb bluetooth HCI - doesnt respond to commands]'
  '-bt[host\[:id\]]'
  '-bt[vlan=n\]]'
  '-bt[vlan=n\]]'
  '-bt[vlan=n\]]'
  '-tpmdev[id=id\[,path=path\]\[,cancel-path=path\]]'
  '-tpmdev[id=id,chardev=dev]'
  '-kernel[use bzImage as kernel image]'
  '-append[use cmdline as kernel command line]'
  '-initrd[use file as initial ram disk]'
  '-dtb[file use file as device tree image]'
  '-fw[_cfg \[name=\]<name>,file=<file>]'
  '-fw[_cfg \[name=\]<name>,string=<str>]'
  '-serial[redirect the serial port to char device dev]'
  '-parallel[redirect the parallel port to char device dev]'
  '-monitor[redirect the monitor to char device dev]'
  '-qmp[like -monitor but opens in control mode]'
  '-qmp-pretty[like -qmp but uses pretty JSON formatting]'
  '-mon[\]name\[,mode=readline|control\]]'
  '-debugcon[redirect the debug console to char device dev]'
  '-pidfile[write PID to file]'
  '-singlestep[always run in singlestep mode]'
  '-S[freeze CPU at startup (use c to start execution)]'
  '-realtime[on|off\]]'
  '-gdb[wait for gdb connection on dev]'
  '-s[shorthand for -gdb tcp::1234]'
  '-d[1,... enable logging of specified items (use -d help for a list of log items)]'
  '-D[output log to logfile (default stderr)]'
  '-dfilter[.. filter debug output to range of addresses (useful for -d cpu,exec,etc..)]'
  '-L[set the directory for the BIOS, VGA BIOS and keymaps]'
  '-bios[set the filename for the BIOS]'
  '-enable-kvm[enable KVM full virtualization support]'
  '-enable-hax[enable HAX virtualization support]'
  '-xen-domid[specify xen guest domain id]'
  '-xen-create[create domain using xen hypercalls, bypassing xend]'
  '-xen-attach[attach to existing xen domain]'
  '-xen-domid-restrict[restrict set of available xen operations]'
  '-no-reboot[exit instead of rebooting]'
  '-no-shutdown[stop before shutdown]'
  '-loadvm[start right away with a saved state (loadvm in monitor)]'
  '-daemonize[daemonize QEMU after initializing]'
  '-option-rom[load a file, rom, into the option ROM space]'
  '-rtc[utc|localtime|date\]\[,clock=host|rt|vm\]\[,driftfix=none|slew\]]'
  '-icount[N|auto\]\[,align=on|off\]\[,sleep=on|off,rr=record|replay,rrfile=<filename>,rrsnapshot=<snapshot>\]]'
  '-watchdog[enable virtual hardware watchdog \[default=none\]]'
  '-watchdog-action[action when watchdog fires \[default=reset\]]'
  '-echr[set terminal escape character instead of ctrl-a]'
  '-virtioconsole[set virtio console]'
  '-show-cursor[show cursor]'
  '-tb-size[set TB size]'
  '-incoming[to=maxport\]\[,ipv4\]\[,ipv6\]]'
  '-incoming[ipv4\]\[,ipv6\]]'
  '-incoming[prepare for incoming migration, listen on]'
  '-incoming[]'
  '-incoming[accept incoming migration on given file descriptor]'
  '-incoming[wait for the URI to be specified via migrate_incoming]'
  '-only-migratable[allow only migratable devices]'
  '-nodefaults[dont create default devices]'
  '-chroot[chroot to dir just before starting the VM]'
  '-runas[change to user id user just before starting the VM]'
  '-sandbox[obsolete=allow|deny\]\[,elevateprivileges=allow|deny|children\]]'
  '-readconfig[]'
  '-writeconfig[read/write config file]'
  '-no-user-config[do not load default user-provided config files at startup]'
  '-trace[\]<pattern>\]\[,events=<file>\]\[,file=<file>\]]'
  '-enable-fips[enable FIPS 140-2 compliance]'
  '-msg[on|off\]]'
  '-dump-vmstate[output vmstate information in JSON format to file.]'
  '-object[PROP1=VALUE1,...\]]'
  '*:filename:_files'
)

_arguments -s $arguments
