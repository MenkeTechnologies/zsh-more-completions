#compdef gtkdoc-mkhtml

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this help message and exit]'
  '--version[show programs version number and exit]'
  '--verbose[print extra output while processing]'
  '--path[extra source directories]'
  '--uninstalled[???]'
  '*:filename:_files'
)

_arguments -s $arguments
