#compdef net-snmp-cert

local arguments

arguments=(
  {-V,--version,--}'[version string and exit]'
  {-D,--debug,--}'[debug level (-D -D for higher level)]'
  {-F,--force,--}'[overwrite of existing output files]'
  {-I,--nointeractive,--}'[run (default interactive)]'
  {-Q,--noverbose,--}'[output (default verbose)]'
  {-C,--cfgdir}'[configuration dir (see man(5) snmp_config)]:dir:'
  {-T,--tlsdir}'[root for cert storage (default <cfgdir>/tls)]:dir:'
  {-f,--cfgfile}'[config (default <cfgdir>/net-snmp-cert.conf)]:file:'
  {-i,--identity,--}'[to use from config]'
  {-t,--tag}'[application tag (default snmp)]:tag:'
  {-a,--with-ca,--}'[certificate used to sign request]'
  {-A,--ca,--}'[output CA certificate]'
  {-r,--csr,--}'[signing request]'
  {-x,--from-crt,--}'[certificate for current operation]'
  {-X,--crt,--}'[output certificate]'
  {-y,--install,--}'[result in local repository]'
  {-e,--email,--}'[name]'
  {-h,--host,--}'[host name, or IP address]'
  {-d,--days,--}'[of days certificate is valid]'
  {-n,--cn,--}'[name (CN)]'
  {-o,--org,--}'[name]'
  {-u,--unit,--}'[unit name]'
  {-c,--country,--}'[code (e.g., US)]'
  {-p,--province,--}'[name (synomynous w/ state)]'
  {-p,--state,--}'[name (synomynous w/ province)]'
  {-l,--locality,--}'[name (e.g, town)]'
  {-s,--san,--}'[repeat for each <san>]'
  {--brief,--}'[output (values only where applicable)]'
  {--text,--}'[text description]'
  {--subject,--}'[description]'
  '--subject[_hash -- hash of subject for indexing]'
  {--issuer,--}'[description]'
  '--issuer[_hash -- hash of issuer for indexing]'
  {--fingerprint,--}'[1 digest of DER]'
  {--serial,--}'[number]'
  {--modulus,--}'[of the public key]'
  {--dates,--}'[and end dates]'
  {--purpose,--}'[allowed uses]'
  {--C,--}'[code description]'
  '*:filename:_files'
)

_arguments -s $arguments
