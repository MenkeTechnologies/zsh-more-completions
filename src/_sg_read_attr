#compdef sg_read_attr

local arguments

arguments=(
  '--cache[|-c set CACHE bit in cdn (def: clear)]'
  '--enumerate[|-e enumerate known attributes and service actions]'
  '--element[EA EA is placed in element address field in]'
  '--filter[FL FL is parameter code to match (def: -1 -> all)]'
  '--first[FAI FAI is placed in first attribute identifier]'
  '--help[|-h print out usage message]'
  '--hex[|-H output response in hexadecimal; used twice]'
  '--in[FN FN is a filename containing attribute values in]'
  '--lvn[LVN logical volume number (LVN) (def:0)]'
  '--maxlen[LEN max response length (allocation length in cdb)]'
  '--partition[PN partition number (PN) (def:0)]'
  '--quiet[|-q reduce the amount of output, can use more than once]'
  '--raw[|-r output response in binary]'
  '--readonly[|-R open DEVICE read-only (def: read-write)]'
  '--sa[SA SA is service action (def: 0)]'
  '--verbose[|-v increase verbosity]'
  '--version[|-V print version string and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
