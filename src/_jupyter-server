#compdef jupyter-server

local arguments

arguments=(
  '--debug[set debug level for the extension and underlying server applications.]'
  '--show-config[show the applications configuration (human-readable format)]'
  '--show-config-json[show the applications configuration (json format)]'
  '--generate-config[generate default config file]'
  '-y[answer yes to any questions instead of prompting.]'
  '--allow-root[allow the server to be run from root user.]'
  '--no-browser[prevent the opening of the default url in the browser.]'
  '--autoreload[autoreload the webapp]'
  '--script[DEPRECATED, IGNORED]'
  '--no-script[DEPRECATED, IGNORED]'
  '--log-level[set the log level by value or name.]'
  '--config[full path of a config file.]'
  '--ip[the IP address the Jupyter server will listen on.]'
  '--port[the port the Jupyter server will listen on.]'
  '--port-retries[the number of additional ports to try if the specified port is not]'
  '--transport[choices: any of \[tcp, ipc\] (case-insensitive)]'
  '--keyfile[the full path to a private key file for usage with SSL/TLS.]'
  '--certfile[the full path to an SSL/TLS certificate file.]'
  '--client-ca[the full path to a certificate authority certificate for SSL/TLS client]'
  '--notebook-dir[the directory to use for notebooks and kernels.]'
  '--browser[specify what command to use to invoke a web]'
  '--pylab[DISABLED: use %pylab or %matplotlib in the notebook to enable matplotlib.]'
  '--gateway-url[the url of the Kernel or Enterprise Gateway server where]'
  '*:filename:_files'
)

_arguments -s $arguments
