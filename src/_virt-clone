    '-n[Name of the new guest virtual machine instance.]:NAME'
    {-u,--uuid}'[UUID for the guest; if none is given a random UUID will be generated.]':UUID
    {-f,--file}'[Path to the file, disk partition, or logical volume to use as the backing sto…]':PATH
    '--nvram[Optional path to the new nvram VARS file, if no path is specified and the gue…]:NVRAMFILE'
    '--force-copy[Force cloning the passed disk target (\hdc\, \sda\, etc. ).]:TARGET'
    '--skip-copy[Skip cloning the passed disk target (\hdc\, \sda\, etc. ).]:TARGET'
    '--nonsparse[Fully allocate the new storage if the path being cloned is a sparse file.]'
    '--preserve-data[No storage is cloned: disk images specific by --file are preserved as is, and…]'
    '--reflink[When --reflink is specified, perform a lightweight copy.]'
    {-m,--mac}'[Fixed MAC address for the guest; If this parameter is omitted, or the value R…]':MAC
    '--print-xml[Print the generated clone XML and exit without cloning.]'
    '--replace[Shutdown and remove any existing guest with the passed --name before cloning …]'
    {-h,--help}'[Show the help message and exit.]'
    '--version[Show program\s version number and exit.]'
    '--check[Enable or disable some validation checks.]'
    {-q,--quiet}'[Suppress non-error output.]'
    {-d,--debug}'[Print debugging information to the terminal when running the install process.]'
    '*:filename:_files'
)
