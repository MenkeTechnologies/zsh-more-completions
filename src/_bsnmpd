#compdef bsnmpd

local arguments

arguments=(
  '-d[dont daemonize]'
  '-h[print this info]'
  '-c[specify configuration file]'
  '-D[debugging options]'
  '-e[specify engine id file]'
  '-I[system include path]'
  '-l[default basename for pid and config file]'
  '-m[val define variable]'
  '-p[specify pid file]'
  '*:filename:_files'
)

_arguments -s $arguments
