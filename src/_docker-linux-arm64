#compdef docker-linux-arm64

local arguments

arguments=(
  '--config[location of client config files (default]'
  {-D,--debug}'[enable debug mode]'
  {-H,--host}'[daemon socket(s) to connect to]'
  {-l,--log-level}'[set the logging level]'
  '--tls[use TLS; implied by --tlsverify]'
  '--tlscacert[trust certs signed only by this CA (default]'
  '--tlscert[path to TLS certificate file (default]'
  '--tlskey[path to TLS key file (default]'
  '--tlsverify[use TLS and verify the remote]'
  {-v,--version}'[print version information and quit]'
  '*:filename:_files'
)

_arguments -s $arguments
