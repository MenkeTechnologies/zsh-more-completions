#compdef enc265

local arguments

arguments=(
  '--help[show help]'
  {-v,--verbose}'[increase verbosity level (up to 3 times)]'
  {-i,--input}'[(string), default=paris_cif.yuv]'
  {-o,--output}'[(string), default=out.bin]'
  '--first-frame[(int) 0 <= x, default=0]'
  {-f,--frames}'[(int) 1 <= x]'
  {-w,--width}'[(int) 1 <= x, default=352]'
  {-h,--height}'[(int) 1 <= x, default=288]'
  '--min-cb-size[(int) {8,16,32,64}, default=8]'
  '--max-cb-size[(int) {8,16,32,64}, default=32]'
  '--min-tb-size[(int) {4,8,16,32}, default=4]'
  '--max-tb-size[(int) {8,16,32}, default=32]'
  '--max-transform-hierarchy-depth-intra[) 0 <= x <= 4, default=3]'
  '--max-transform-hierarchy-depth-inter[) 0 <= x <= 4, default=3]'
  '--sop-structure[low-delay}, default=low-delay]'
  '--TB-IntraPredMode[brute-force,fast-brute}, default=fast-brute]'
  '--TB-IntraPredMode-subset[HV+,DC,planar}, default=all]'
  '--CB-IntraPartMode[brute-force}, default=brute-force]'
  '--MEMode[{test,search}, default=test]'
  '--TB-RateEstimation[exact}, default=exact]'
  '--sop-lowDelay-intraPeriod[) 1 <= x, default=250]'
  {-q,--qp}'[(int) 1 <= x <= 51, default=27]'
  '--CB-IntraPartMode-Fixed-partMode[2Nx2N}, default=2Nx2N]'
  '--CB-InterPartMode-Fixed-partMode[2Nx2N,NxN,Nx2N,2NxN,2NxnU,2NxnD,nLx2N,nRx2N}, default=2Nx2N]'
  '--PB-MV-TestMode[random,horiz,verti}, default=horiz]'
  '--PB-MV-Range[(int), default=4]'
  '--PB-MV-Search-Algo[full,diamond,pmvfast}, default=full]'
  '--PB-MV-Search-HRange[), default=8]'
  '--PB-MV-Search-VRange[), default=8]'
  '--IntraPredMode-FastBrute-keepNBest[) 0 <= x <= 32, default=5]'
  '--IntraPredMode-FastBrute-estimator[sad,satd-dct,satd}, default=satd]'
  '--IntraPredMode-MinResidual-estimator[sad,satd-dct,satd}, default=satd]'
  '--TB-Split-BruteForce-ZeroBlockPrune[8x8,8-16,all}, default=all]'
  '*:filename:_files'
)

_arguments -s $arguments
