#compdef newfs_msdos

local arguments

arguments=(
  '-N[t create file system: just print out parameters]'
  '-B[bootstrap from file]'
  '-F[type (12, 16, or 32)]'
  '-I[ID]'
  '-O[string]'
  '-S[/sector]'
  '-P[bytes/sector]'
  '-a[/FAT]'
  '-b[size]'
  '-c[/cluster]'
  '-e[directory entries]'
  '-f[format]'
  '-h[heads]'
  '-i[system info sector]'
  '-k[boot sector]'
  '-m[descriptor]'
  '-n[of FATs]'
  '-o[sectors]'
  '-r[sectors]'
  '-s[system size (in sectors)]'
  '-u[/track]'
  '-v[/volume name]'
  '*:filename:_files'
)

_arguments -s $arguments
