#compdef bulk_extractor

local arguments

arguments=(
  '-o[specifies output directory. Must not exist.]'
  '-i[INFO mode. Do a quick random sample and print a report.]'
  '-b[.txt- Add banner.txt contents to the top of every output file.]'
  '-r[.txt - a file containing the alert list of features to alert]'
  '-w[.txt - a file containing the stop list of features (white list]'
  '-F[Read a list of regular expressions from <rfile> to find]'
  '-f[find occurrences of <regex>; may be repeated.]'
  '-q[Quiet Rate; only print every nn status reports. Default 0; -1 for no status at all]'
  '-s[Set random sampling parameters]'
  '-C[specifies the size of the context window (default 16)]'
  '-S[NN specifies context window for recorder to NN]'
  '-S[NN specifies context window before to NN for recorder]'
  '-S[NN specifies context window after to NN for recorder]'
  '-G[specify the page size (default 16777216)]'
  '-g[specify margin (default 4194304)]'
  '-j[Number of analysis threads to run (default 2)]'
  '-M[sets max recursion depth (default 7)]'
  '-m[maximum number of minutes to wait after all data read]'
  '-p[/f - print the value of <path> with a given format.]'
  '-Y[1> - Start processing at o1 (o1 may be 1, 1K, 1M or 1G)]'
  '-Y[1>-<o2> - Process o1-o2]'
  '-A[Add <off> to all reported feature offsets]'
  '-h[print this message]'
  '-H[print detailed info on the scanners]'
  '-V[print version number]'
  '-z[start on page nn]'
  '-dN[debug mode (see source code)]'
  '-Z[zap (erase) output directory]'
  '-P[Specifies a plugin directory]'
  '-e[enables <scanner> -- -e all enables all]'
  '-x[disable <scanner> -- -x all disables all]'
  '-E[turn off all scanners except <scanner>]'
  '-S[value - sets a bulk extractor option name to be value]'
  '-S[YES Record work start and end of each scanner in report.xml file ()]'
  '-S[YES Disable generation of histograms ()]'
  '-S[0 Set >0 to make histogram maker fail with memory allocations ()]'
  '-S[md5 Specifies hash algorithm to be used for all hash calculations ()]'
  '-S[NO Notify when duplicate data is not processed ()]'
  '-S[YES Write features to flat files ()]'
  '-S[3=NO Write feature files to report.sqlite3 ()]'
  '-S[YES Report read errors ()]'
  '-S[NO Carve network memory structures (net)]'
  '-S[6 Minimum word size (wordlist)]'
  '-S[14 Maximum word size (wordlist)]'
  '-S[100000000 Maximum size of the words output file (wordlist)]'
  '-S[YES Override SQL settings and use flatfiles for wordlist (wordlist)]'
  '-S[0 0=Normal; 1=No `SSN required; 2=No dashes required (accts)]'
  '-S[7 Min. digits required in a phone (accts)]'
  '-S[0 debug exif decoder (exif)]'
  '-S[1 0=carve none; 1=carve encoded; 2=carve all (exif)]'
  '-S[1000 Smallest JPEG stream that will be carved (exif)]'
  '-S[6 Minimum size of a ZIP uncompressed object (zip)]'
  '-S[268435456 Maximum size of a ZIP uncompressed object (zip)]'
  '-S[1024 Maximum name of a ZIP component filename (zip)]'
  '-S[1 0=carve none; 1=carve encoded; 2=carve all (zip)]'
  '-S[YES Search for RAR components (rar)]'
  '-S[YES Search for RAR volumes (rar)]'
  '-S[1 0=carve none; 1=carve encoded; 2=carve all (rar)]'
  '-S[268435456 maximum size for decompressing GZIP objects (gzip)]'
  '-S[NO Dump the contents of PDF buffers (pdf)]'
  '-S[NO Dump the contents of PDF buffers (msxml)]'
  '-S[1 0=carve none; 1=carve encoded; 2=carve all (winpe)]'
  '-S[157286400 Threshold for FAT32 scanner (windirs)]'
  '-S[2=536870912 Threshold for FAT32 scanner (windirs)]'
  '-S[67108864 Threshold for FAT32 scanner (windirs)]'
  '-S[2=268435456 Threshold for FAT32 scanner (windirs)]'
  '-S[3 Ignore FAT32 entries with more attributes set than this (windirs)]'
  '-S[2 Number of weird counts to ignore a FAT32 entry (windirs)]'
  '-S[2023 Ignore FAT32 entries with a later year than this (windirs)]'
  '-S[255 XOR mask value, in decimal (xor)]'
  '-S[2 0=carve none; 1=carve encoded; 2=carve all (sqlite)]'
  '-e[16 - enable scanner base16]'
  '-e[enable scanner facebook]'
  '-e[enable scanner outlook]'
  '-e[enable scanner sceadan]'
  '-e[enable scanner wordlist]'
  '-e[enable scanner xor]'
  '-x[disable scanner accts]'
  '-x[disable scanner aes]'
  '-x[64 - disable scanner base64]'
  '-x[disable scanner elf]'
  '-x[disable scanner email]'
  '-x[disable scanner exif]'
  '-x[disable scanner find]'
  '-x[disable scanner gps]'
  '-x[disable scanner gzip]'
  '-x[disable scanner hiberfile]'
  '-x[disable scanner httplogs]'
  '-x[disable scanner json]'
  '-x[disable scanner kml]'
  '-x[disable scanner msxml]'
  '-x[disable scanner net]'
  '-x[disable scanner pdf]'
  '-x[disable scanner rar]'
  '-x[disable scanner sqlite]'
  '-x[disable scanner vcard]'
  '-x[disable scanner windirs]'
  '-x[disable scanner winlnk]'
  '-x[disable scanner winpe]'
  '-x[disable scanner winprefetch]'
  '-x[disable scanner zip]'
  '*:filename:_files'
)

_arguments -s $arguments
