#compdef gpgcompose

local arguments

arguments=(
  '--user-id[create a user id packet.]'
  '--public-key[create a public key packet.]'
  '--private-key[create a private key packet. \[1m(Unimplemented.)\[0m]'
  '--public-subkey[create a subkey packet.]'
  '--private-subkey[create a private subkey packet. \[1m(Unimplemented.)\[0m]'
  '--sk-esk[create a symmetric-key encrypted session key packet.]'
  '--pk-esk[create a public-key encrypted session key packet.]'
  '--encrypted[create a symmetrically encrypted data packet.]'
  '--encrypted-mdc[create a symmetrically encrypted and integrity protected]'
  '--encrypted-pop[pop the most recent encryption container started by]'
  '--compressed[create a compressed data packet. \[1m(Unimplemented.)\[0m]'
  '--literal[create a literal (plaintext) data packet.]'
  '--signature[create a signature packet.]'
  '--onepass-sig[create a one-pass signature packet.]'
  '--copy[copy the specified file.]'
  '*:filename:_files'
)

_arguments -s $arguments
