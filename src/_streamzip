#compdef streamzip

local arguments

arguments=(
  '-zipfile[write zip container to the filename F]'
  '-member-name[member name \[Default -\]]'
  '-zip64[create a Zip64-compliant zip file \[Default: No\]]'
  '-stream[write a streamed zip file]'
  '-method[compress using method "M".]'
  '-version[display version number \[1.0\]]'
  '-zipfile[write zip container to the filename F]'
  '-member-name[member name \[Default -\]]'
  '-zip64[create a Zip64-compliant zip file \[Default: No\]]'
  '-stream[write a streamed zip file]'
  '-method[compress using method "M".]'
  '-version[display version number \[1.0\]]'
  '*:filename:_files'
)

_arguments -s $arguments
