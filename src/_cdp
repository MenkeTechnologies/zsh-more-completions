#compdef cdp

local arguments

arguments=(
  '-n[number of packets]'
  '-l[length of the device id]'
  '-c[character to fill in device id]'
  '-r[randomize device id string]'
  '-D[device id]'
  '-P[port id]'
  '-L[platform]'
  '-S[software]'
  '-F[IP address]'
  '-C[these are:]'
  '*:filename:_files'
)

_arguments -s $arguments
