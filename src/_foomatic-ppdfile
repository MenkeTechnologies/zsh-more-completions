#compdef foomatic-ppdfile

local arguments

arguments=(
  '-A[show all Printer IDs and compatible drivers]'
  '-P[show all Printer IDs whose names and model]'
  '-P[will match all names with HP in them]'
  '-p[Printer ID]'
  '-d[Driver name]'
  '-w[Generate PPD which is compatible with the CUPS PostScript]'
  '-h[show help information]'
  '*:filename:_files'
)

_arguments -s $arguments
