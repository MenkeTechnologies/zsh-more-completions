#compdef encode_keychange

local arguments

arguments=(
  '-E[0x\]<engineID> EngineID used for kul generation.]'
  '-f[force passphrases to be read from stdin.]'
  '-h[help.]'
  '-N[" Passphrase used to generate new Ku.]'
  '-O[" Passphrase used to generate old Ku.]'
  '-P[turn off prompt indicators.]'
  '-t[5 | sha1 HMAC hash transform type.]'
  '-v[verbose.]'
  '-V[visible. Echo passphrases to terminal.]'
  '-f[require reading from the stdin/terminal, ignoring a) and b).]'
  '-P[prevent prompts for passphrases to stdout from being printed.]'
  '*:filename:_files'
)

_arguments -s $arguments
