#compdef psset

local arguments

arguments=(
  '(- * :)'{-h,--help}'[display this help and exit]'
  {-v,--version}'[display version information and exit]'
  {-q,--quiet}'[dont print informational messages]'
  {-o,--output}'[save result in FILE. If FILE is `-, send to stdout]'
  {-n,--no-fix}'[dont run `fixps to fix PS problems in FILE]'
  {-S,--setpagedevice}'[pass a page device definition to output.]'
  {-a,--at}'[insert the page device definitions at PAGE. Default]'
  {-m,--manualfeed}'[alias for -SManualFeed:true]'
  {-s,--simplex}'[alias for -SDuplex:false]'
  {-d,--duplex}'[alias for -SDuplex:true -STumble:false]'
  {-t,--tumble}'[alias for -SDuplex:true -STumble:true]'
  '*:filename:_files'
)

_arguments -s $arguments
