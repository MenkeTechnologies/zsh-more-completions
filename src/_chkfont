#compdef chkfont

local arguments

arguments=(
  '--help[ERROR- Filename does not end with .flf.]'
  '--help[ERROR- Incorrect magic number.]'
  '--help[Warning- Sub-version character is not a.]'
  '--help[ERROR (fatal)- First line improperly formatted.]'
  '--help[Errors: 3, Warnings: 1]'
  '*:filename:_files'
)

_arguments -s $arguments
