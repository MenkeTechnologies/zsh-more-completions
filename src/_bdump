#compdef bdump

local arguments

arguments=(
  '-d[file containing words to add to the dictionary, one per line]'
  '-h[location of the dictionary used for unhashing.]'
  '-w[1,word2,...]'
  '-x[do not load nor update the dictionary used for name]'
  '-help[display this list of options]'
  '--help[display this list of options]'
  '*:filename:_files'
)

_arguments -s $arguments
