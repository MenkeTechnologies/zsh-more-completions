#compdef massagevendor

local arguments

arguments=(
  {-e,--expression,-f}'[or --file option is given, then the first]'
  '--help[display this help and exit]'
  '--line-length[llow-symlinks]'
  '--null-data[operate in sandbox mode (disable e/r/w commands).]'
  {--quiet,--silent}'[]'
  '--sandbox[sed \[OPTION\]... {script-only-if-no-other-script} \[input-file\]...]'
  '--separate[separate lines by NUL characters]'
  '--unbuffered[use extended regular expressions in the script]'
  '--version[output version information and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
