#compdef srch_strings

local arguments

arguments=(
  '-a[ Scan the entire file, not just the data section]'
  '-f[print the name of the file before each string]'
  '-n[locate & print any NUL-terminated sequence of at]'
  '-t[x,d} Print the location of the string in base 8, 10 or 16]'
  '-o[an alias for --radix=o]'
  '-e[S,b,l,B,L} Select character size and endianness:]'
  '-h[display this information]'
  '-v[print the programs version number]'
  '*:filename:_files'
)

_arguments -s $arguments
