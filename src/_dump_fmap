#compdef dump_fmap

local arguments

arguments=(
  '-x[extract the named sections from the file]'
  '-h[use a human-readable format]'
  '-H[with -h, display any gaps]'
  '-p[use a format easy to parse by scripts]'
  '-F[use the format expected by flashrom]'
  '*:filename:_files'
)

_arguments -s $arguments
