#compdef xsser

local arguments

arguments=(
  '--version[show programs version number and exit]'
  '(- * :)'{-h,--help}'[show this help message and exit]'
  {-s,--statistics}'[show advanced statistics output results]'
  {-v,--verbose}'[active verbose mode output results]'
  '--gtk[launch XSSer GTK Interface]'
  '--wizard[start Wizard Helper!]'
  '--imx[IMX - Create an image with XSS (--imx image.png)]'
  '--fla[FLA - Create a flash movie with XSS (--fla movie.swf)]'
  '--xst[XST - Cross Site Tracing (--xst http(s)://host.com)]'
  '--all[automatically audit an entire target]'
  {-u,--url}'[enter target to audit]'
  '-i[read target(s) urls from file]'
  '-d[search target(s) using a query (ex: news.php?id=)]'
  '-l[search from a list of dorks]'
  '--De[use this search engine (default: yahoo)]'
  '--Da[search massively using all search engines]'
  '-g[send payload using GET (ex: /menu.php?id=3&q=XSS)]'
  '-p[send payload using POST (ex: foo=1&bar=XSS)]'
  '-c[number of urls to crawl on target(s): 1-99999]'
  '--Cw[deeping level of crawler: 1-5 (default 3)]'
  '--Cl[crawl only local target(s) urls (default TRUE)]'
  '--cookie[change your HTTP Cookie header]'
  '--drop-cookie[ignore Set-Cookie header from response]'
  '--user-agent[change your HTTP User-Agent header (default SPOOFED)]'
  '--referer[use another HTTP Referer header (default NONE)]'
  '--xforw[set your HTTP X-Forwarded-For with random IP values]'
  '--xclient[set your HTTP X-Client-IP with random IP values]'
  '--headers[extra HTTP headers newline separated]'
  '--auth-type[HTTP Authentication type (Basic, Digest, GSS or NTLM)]'
  '--auth-cred[HTTP Authentication credentials (name:password)]'
  '--proxy[use proxy server (tor: http://localhost:8118)]'
  '--ignore-proxy[ignore system default HTTP proxy]'
  '--timeout[select your timeout (default 30)]'
  '--retries[retries when the connection timeouts (default 1)]'
  '--threads[maximum number of concurrent HTTP requests (default 5)]'
  '--delay[delay in seconds between each HTTP request (default 0)]'
  '--tcp-nodelay[use the TCP_NODELAY option]'
  '--follow-redirects[follow server redirection responses (302)]'
  '--follow-limit[set limit for redirection requests (default 50)]'
  '--hash[send a hash to check if target is repeating content]'
  '--heuristic[discover parameters filtered by using heuristics]'
  '--discode[set code on reply to discard an injection]'
  '--checkaturl[check reply using: alternative url -> Blind XSS]'
  '--checkmethod[check reply using: GET or POST (default: GET)]'
  '--checkatdata[check reply using: alternative payload]'
  '--reverse-check[establish a reverse connection from target to XSSer to]'
  '--payload[OWN - Inject your own code]'
  '--auto[AUTO - Inject a list of vectors provided by XSSer]'
  '--Phpids0[.6.5 PHPIDS (0.6.5) \[ALL\]]'
  '--Phpids0[.7 PHPIDS (0.7) \[ALL\]]'
  '--Imperva[imperva Incapsula \[ALL\]]'
  '--Webknight[webKnight (4.1) \[Chrome\]]'
  '--F5bigip[F5 Big IP \[Chrome + FF + Opera\]]'
  '--Barracuda[barracuda WAF \[ALL\]]'
  '--Modsec[mod-Security \[ALL\]]'
  '--Quickdefense[quickDefense \[Chrome\]]'
  '--Str[use method String.FromCharCode()]'
  '--Une[use Unescape() function]'
  '--Mix[mix String.FromCharCode() and Unescape()]'
  '--Dec[use Decimal encoding]'
  '--Hex[use Hexadecimal encoding]'
  '--Hes[use Hexadecimal encoding with semicolons]'
  '--Dwo[encode IP addresses with DWORD]'
  '--Doo[encode IP addresses with Octal]'
  '--Cem[set different Character Encoding Mutations]'
  '--Coo[COO - Cross Site Scripting Cookie injection]'
  '--Xsa[XSA - Cross Site Agent Scripting]'
  '--Xsr[XSR - Cross Site Referer Scripting]'
  '--Dcp[DCP - Data Control Protocol injections]'
  '--Dom[DOM - Document Object Model injections]'
  '--Ind[IND - HTTP Response Splitting Induced code]'
  '--Anchor[ANC - Use Anchor Stealth payloader (DOM shadows!)]'
  '--Fp[OWN - Exploit your own code]'
  '--Fr[REMOTE - Exploit a script -remotely-]'
  '--Doss[DOSs - XSS (server) Denial of Service]'
  '--Dos[DOS - XSS (client) Denial of Service]'
  '--B64[B64 - Base64 code encoding in META tag (rfc2397)]'
  '--Onm[ONM - Use onMouseMove() event]'
  '--Ifr[IFR - Use <iframe> source tag]'
  '--save[export to file (XSSreport.raw)]'
  '--xml[export to XML (--xml file.xml)]'
  '--silent[inhibit console output results]'
  '--no-head[NOT send a HEAD request before start a test]'
  '--alive[set limit of errors before check if target is alive]'
  '*:filename:_files'
)

_arguments -s $arguments
