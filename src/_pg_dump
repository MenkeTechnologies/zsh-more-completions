#compdef pg_dump

local arguments

arguments=(
  {-f,--file}'[output file or directory name]'
  {-F,--format}'[output file format (custom, directory, tar]'
  {-j,--jobs}'[use this many parallel jobs to dump]'
  {-v,--verbose}'[verbose mode]'
  {-V,--version}'[output version information, then exit]'
  {-Z,--compress}'[9 compression level for compressed formats]'
  '--lock-wait-timeout[fail after waiting TIMEOUT for a table lock]'
  '--no-sync[do not wait for changes to be written safely to disk]'
  {-a,--data-only}'[dump only the data, not the schema]'
  {-b,--blobs}'[include large objects in dump]'
  {-B,--no-blobs}'[exclude large objects in dump]'
  {-c,--clean}'[clean (drop) database objects before recreating]'
  {-C,--create}'[include commands to create database in dump]'
  {-E,--encoding}'[dump the data in encoding ENCODING]'
  {-n,--schema}'[dump the named schema(s) only]'
  {-N,--exclude-schema}'[do NOT dump the named schema(s)]'
  {-o,--oids}'[include OIDs in dump]'
  {-O,--no-owner}'[skip restoration of object ownership in]'
  {-s,--schema-only}'[dump only the schema, no data]'
  {-S,--superuser}'[superuser user name to use in plain-text format]'
  {-t,--table}'[dump the named table(s) only]'
  {-T,--exclude-table}'[do NOT dump the named table(s)]'
  {-x,--no-privileges}'[do not dump privileges (grant/revoke)]'
  '--binary-upgrade[for use by upgrade utilities only]'
  '--column-inserts[dump data as INSERT commands with column names]'
  '--disable-dollar-quoting[disable dollar quoting, use SQL standard quoting]'
  '--disable-triggers[disable triggers during data-only restore]'
  '--enable-row-security[enable row security (dump only content user has]'
  '--exclude-table-data[do NOT dump data for the named table(s)]'
  '--if-exists[use IF EXISTS when dropping objects]'
  '--inserts[dump data as INSERT commands, rather than COPY]'
  '--no-publications[do not dump publications]'
  '--no-security-labels[do not dump security label assignments]'
  '--no-subscriptions[do not dump subscriptions]'
  '--no-synchronized-snapshots[do not use synchronized snapshots in parallel jobs]'
  '--no-tablespaces[do not dump tablespace assignments]'
  '--no-unlogged-table-data[do not dump unlogged table data]'
  '--quote-all-identifiers[quote all identifiers, even if not key words]'
  '--section[dump named section (pre-data, data, or post-data)]'
  '--serializable-deferrable[wait until the dump can run without anomalies]'
  '--snapshot[use given snapshot for the dump]'
  '--strict-names[require table and/or schema include patterns to]'
  '--use-set-session-authorization[use SET SESSION AUTHORIZATION commands instead of]'
  {-d,--dbname}'[database to dump]'
  {-h,--host}'[database server host or socket directory]'
  {-p,--port}'[database server port number]'
  {-U,--username}'[connect as specified database user]'
  {-w,--no-password}'[never prompt for password]'
  {-W,--password}'[force password prompt (should happen automatically)]'
  '--role[do SET ROLE before dump]'
  '*:filename:_files'
)

_arguments -s $arguments
