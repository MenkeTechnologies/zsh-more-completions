#compdef sip5

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this help message and exit]'
  {-V,--version}'[show programs version number and exit]'
  '-a[the name of the QScintilla API file \[default not]'
  '--abi-version[the ABI version]'
  '-B[add <TAG> to the list of timeline backstops]'
  '-c[the name of the code directory \[default not generated\]]'
  '-D[generate code for a debug build of Python]'
  '-e[enable support for exceptions \[default disabled\]]'
  '-f[warnings are handled as errors]'
  '-g[always release and reacquire the GIL \[default only]'
  '-I[add <DIR> to the list of directories to search when]'
  '-j[split the generated code into <FILES> files \[default 1]'
  '-n[the fully qualified name of the sip module]'
  '-o[enable the automatic generation of docstrings \[default]'
  '-P[enable the protected/public hack \[default disabled\]]'
  '-r[generate code with tracing enabled \[default disabled\]]'
  '-s[the suffix to use for C or C++ source files \[default]'
  '-t[add <TAG> to the list of versions/platforms to]'
  '-w[enable warning messages \[default disabled\]]'
  '-x[add <FEATURE> to the list of disabled features]'
  '-X[add <ID:FILE> to the list of extracts to generate]'
  '-y[the name of the .pyi stub file \[default not generated\]]'
  '*:filename:_files'
)

_arguments -s $arguments
