#compdef fvdeinfo

local arguments

arguments=(
  '-e[:     specify the name of the EncryptedRoot.plist.wipekey file]'
  '-h[:     shows this help]'
  '-k[:     the volume master key formatted in base16]'
  '-o[:     specify the volume offset]'
  '-p[:     specify the password]'
  '-r[:     specify the recovery password]'
  '-v[:     verbose output to stderr]'
  '-V[:     print version]'
  '*:filename:_files'
)

_arguments -s $arguments
