  '-setfacl[_r acl_text iso_rr_path \[***\]]'
  '-setfacl[_list disk_path]'
  '-setfattr[value iso_rr_path \[***\]]'
  '-setfattr[_r \[-\]name value iso_rr_path \[***\]]'
  '-setfattr[_list disk_path]'
  '-alter[_date type timestring iso_rr_path \[***\]]'
  '-alter[_date_r type timestring iso_rr_path \[***\]]'
  '-hide[iso_rr_path \[***\]]'
  '-find[\[test \[op\] \[test ...\]\] \[-exec action \[params\]\]]'
  {-name,-wholename,-disk}'[_name pattern]'
  {-type,-pending}'[_data, -hidden]'
  '-lba[_range start count, -damaged, -has_acl, -has_xattr]'
  '-has[_aaip, -has_filter, -has_md5, -has_any_xattr]'
  '-has[_hfs_crtp, -has_hfs_bless, -bad_outname]'
  '-name[_limit_blocker]'
  {-prune,-decision,-true,-false}'[operators: -not, -or, -and, -sub, (, -subend, )]'
  {-if,-then,-elseif,-else,-endif}'[action may be one of: echo, chown, chown_r, chgrp, chgrp_r]'
  '-mkdir[\[...\]]'
  '-lns[iso_rr_path]'
  '-rmdir[\[***\]]'
  '-clone[iso_rr_path_copy]'
  '-cp[_clone iso_rr_path_original \[***\] iso_rr_path_dest]'
  '-not[_paths disk_path \[***\]]'
  '-not[_leaf pattern]'
  '-not[_list disk_path]'
  '-quoted[_not_list disk_path]'
  '-not[_mgt "reset"|"on"|"off"|"param_on"|"subtree_on"|"ignore_on"]'
  '-follow["|"pattern:param:link:concat:mount:limit=#"|"default"|"off"]'
  '-overwrite["|"nondir"|"off"]'
  '-split[_size number\["k"|"m"\]]'
  '-reassure["|"tree"|"off"]'
  '-external[_filter name option\[:option\] program_path \[arguments\] --]'
  '-unregister[_filter name]'
  '-close[_filter_list]'
  '-set[_filter name iso_rr_path \[***\]]'
  '-set[_filter_r name iso_rr_path \[***\]]'
  '-zisofs[set global zisofs parameters:]'
  '-rollback[discard the manipulated ISO image and reload it.]'
  '-changes[_pending "no"|"yes"|"mkisofs_printed"|"show_status"]'
  '-commit[perform the write operation if changes are pending.]'
  '-commit[_eject "in"|"out"|"all"|"none"]'
  '-write[_type "auto"|"tao"|"sao/dao"]'
  '-close["|"off"|"as_needed"]'
  '-padding["k"|"m"\]|"included"|"appended"]'
  '-dummy["|"off"]'
  '-speed["k/s"|"m/s"|"\[x\]CD"|"\[x\]DVD"|"\[x\]BD"\]]'
  '-stream[_recording "on"|"off"]'
  '-modesty[_on_drive \["on"|"off"|min_percent_number\]]'
  '-dvd[_obs "default"|"32k"|"64k"]'
  '-use[_immed_bit "on"|"off"|"default"]'
  '-stdio[_sync "on"|"off"|"end"|number]'
  '-fs["k"|"m"\]]'
  '-eject["|"out"|"all"]'
  '-cd[change working directory in the ISO image. iso_rr_paths]'
  '-cdi[same as -cd disk_path]'
  '-cdx[change the current working directory in the local]'
  '-lsx[will eventually be looked up at this path.]'
  '-pwd[tells the current working directory in the ISO image.]'
  '-pwdi[same as -pwd.]'
  '-pwdx[tells the current working directory in the local filesystem.]'
  '-iso[_rr_pattern "on"|"ls"|"off"]'
  '-disk[_pattern "on"|"ls"|"off"]'
  '-ls[\[***\] lists files of the ISO image which match one of the]'
  '-lsd[\[***\] like -ls but listing directories as single items.]'
  '-lsl[\[***\] like -ls but also telling some file attributes.]'
  '-lsdl[\[***\] like -lsd but also telling some file attributes.]'
  '-lsx[\[***\] lists files of the local filesystem which match one]'
  '-lsdx[\[***\] like -lsx but listing directories as single items.]'
  '-lslx[\[***\] like -lsx but also telling some file attributes.]'
  '-lsdlx[\[***\] like -lsdx but also telling some file attributes.]'
  '-getfacl[\[***\] list eventual ACLs of the given files.]'
  '-getfacl[_r pattern \[***\] like -getfacl but listing whole file trees.]'
  '-getfattr[\[***\] list eventual xattr of the given files.]'
  '-getfxattr[_r pattern \[***\] like -getfxattr but listing whole file trees.]'
  '-du[\[***\] recursively lists sizes of files or directories in the]'
  '-dux[\[***\] recursively lists sizes of files or directories in the]'
  '-dus[\[***\] like -du but summing up subdirectories without]'
  '-dusx[\[***\] like -dux but summing up subdirectories without]'
  '-findx[\[-name pattern\] \[-type t\] \[-exec action \[params\]\]]'
  '-compare[iso_rr_path]'
  '-compare[_r disk_path iso_rr_path]'
  '-compare[_l disk_prefix iso_rr_prefix disk_path \[***\]]'
  '-show[_stream iso_rr_path \[***\]]'
  '-show[_stream_r iso_rr_path \[***\]]'
  '-osirrox["|"device_files"|"off"|"blocked"|"unblock"|"banned"]'
  '-extract[disk_path]'
  '-extract[_l iso_rr_prefix disk_prefix iso_rr_path \[***\]]'
  '-extract[_single iso_rr_path disk_path]'
  '-extract[_cut iso_rr_path byte_offset byte_count disk_path]'
  '-cpx[\[***\] disk_path]'
  '-cpax[\[***\] disk_path]'
  '-cp[_rx iso_rr_path \[***\] disk_path]'
  '-cp[_rax iso_rr_path \[***\] disk_path]'
  '-paste[_in iso_rr_path disk_path byte_offset byte_count]'
  '-concat["|"overwrite" target iso_rr_path \[***\]]'
  '-concat[" lim prog \[args \[...\]\] lim iso_rr_path \[***\]]'
  '-mount[entity id path]'
  '-check[_media \[options\] --]'
  '-check[_media_defaults \[options\] --]'
  '-as[\[-help|-version|-o|-R|-r|-J|-V|-P|-f|-m|-exclude-list|]'
  '-no-pad[|-M|-C|-graft-points|-path-list|pathspecs|-z|]'
  '-no-emul-boot[|-b|-c|-boot-info-table|-boot-load-size|-G|...\]]'
  '-read[_mkisofsrc]'
  '-as[\[-help|-v|dev=|speed=|blank=|fs=|-eject|-atip|padsize=|-multi\]]'
  '-pacifier["|"cdrecord"|"mkisofs"]'
  '-help[print this text]'
  '-abort[_on severity Set the threshold for events to abort the program.]'
  '-return[_with severity exit_value Set the threshold for events to return]'
  '-report[_about severity Set the threshold for events to be reported.]'
  '-signal[_handling "on"|"off"|"sig_dfl"|"sig_ign"]'
  '-error[_behavior "image_loading"|"file_extraction" behavior]'
  '-dialog["|"off"|"single_line"]'
  '-page[width Prompt user after len output lines (0=no prompt).]'
  '-use[_readline "on"|"off"]'
  '-history[copy text into libreadline history. This command]'
  '-sh[_style_result "on"|"off"]'
  {-pwd,-pwdx,-ls,-lsd,-lsl,-lsdl,-lsx,-lsdx,-lslx,-lsdlx}'[]'
  {-du,-dus,-dux,-dusx,-findx,-find}'[]'
  '-backslash[_codes "on"|"off"|]'
  '-pkt[_output "on"|"off" Direct output to stdout and prefix each line]'
  '-pkt[_output:on is intended for use by frontend programs.]'
  '-msg[_op "start_sieve"|"read_sieve"|"clear_sieve"|"end_sieve"|]'
  '-named[_pipes_loop mode\[:mode\]]'
  '-launch[_frontend program \[args ...\] --]'
  '-logfile[fileaddress Copy output of a channel to the given file.]'
  '-mark[if text is not empty it will get put out each time a command]'
  '-temp[_mem_limit number\["k"|"m"\]]'
  '-prog[use text as this programs name in subsequent messages]'
  '-prog[_help text Use text as this programs name and perform -help]'
  '-status[report the current settings of persistent commands.]'
  '-status[_history_max number Maximum number of history lines to be reported]'
  '-options[_from_file fileaddress]'
  '-no[_rc Only if used as first program argument, this command]'
  '-print[print a text to result channel.]'
  '-print[_info text]'
  '-print[_mark text]'
  '-prompt[wait for Enter key or for a line of input at stdin.]'
  '-sleep[do nothing during the given number of seconds.]'
  '-errfile[_log mode path|channel]'
  '-session[_log path]'
  '-scsi[_log "on"|"off"]'
  '-list[_extras code]'
  '-list[_arg_sorting]'
  '-version[tell program and version number]'
  '-end[end program. Commit eventual pending changes.]'
  '-rollback[_end]'
  '*:filename:_files'
)

_arguments -s $arguments
