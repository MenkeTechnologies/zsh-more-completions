#compdef osirrox

local arguments

arguments=(
  '-x[only in effect if given as program argument:]'
  '-dev[set input and output drive and load eventual ISO image.]'
  '-indev[set input drive and load eventual ISO image. Use expansion]'
  '-outdev[set output drive and use modifying or blind growing.]'
  '-drive[_class "harmless"|"banned"|"risky"|"clear_list" disk_pattern]'
  '-grow[_blindly "off"|predicted_nwa]'
  '-scsi[_dev_family "default"|"sr"|"scd"|"sg"]'
  '-read[_speed number\["k/s"|"m/s"|"\[x\]CD"|"\[x\]DVD"|"\[x\]BD"\]]'
  '-load["|"track"|"lba"|"sbsector"|"volid"|"auto" id]'
  '-indev[hosts more than one session.]'
  '-displacement[when loading ISO tree or reading data files compensate a]'
  '-read[_fs "any"|"norock"|"nojoliet"|"ecma119"]'
  '-rom[_toc_scan "on"|"force"|"off"\[:"emul_on"|"emul_off"\]]'
  '-calm[_drive "in"|"out"|"all"|"on"|"off"]'
  '-assert[_volid pattern severity]'
  '-charset[set the character set name to be used for file name]'
  '-in[_charset name]'
  '-auto[_charset "on"|"off"]'
  '-out[_charset name]'
  '-local[_charset name]'
  '-hardlinks[...\]]'
  '-acl["|"off"]'
  '-xattr["|"off"]'
  '-md5["|"all"|"off"]'
  '-for[_backup]'
  '-ecma119[_map "unmapped"|"stripped"|"uppercase"|"lowercase"]'
  '-disk[_dev_ino "on"|"ino_only"|"off"]'
  '-scdbackup[_tag list_path record_name]'
  '-ban[_stdio_write]'
  '-early[_stdio_test "on"|"appendable_wo"|"off"]'
  '-data[_cache_size number_of_tiles blocks_per_tile]'
  '-blank["force:"\]"fast"|"all"|"deformat"|"deformat_quickest"]'
  '-close[_damaged "as_needed"|"force"]'
  '-format["|"full"|"fast"|"by_index_#"|"by_size_#"]'
  '-volid[specifies the volume ID text. (32 chars out of \[A-Z0-9_\])]'
  '-volset[_id name]'
  '-publisher[specifies the publisher name. (128 chars)]'
  '-application[_id name]'
  '-system[_id name]'
  '-volume[_date type timestring]'
  '-copyright[_file name]'
  '-biblio[_file name]'
  '-abstract[_file name]'
  '-application[_use character|0xXY|disk_path]'
  '-joliet["|"off"]'
  '-hfsplus["|"off"]'
  '-rockridge["|"off"]'
  '-jigdo["|"template_path"|"jigdo_path"|"md5_path"]'
  '-compliance[...\]]'
  '-rr[_reloc_dir name]'
  '-boot[_image "any"|"isolinux"|"grub"]'
  '-append[_partition partition_number type_code disk_path]'
  '-uid[user id to be used for the whole multi-session ISO image.]'
  '-gid[group id for the same purpose.]'
  '-devices[show list of available optical drives and their addresses.]'
  '-device[_links Like devices, but showing link paths which are hopefully]'
  '-toc[show media specific tables of content (sessions).]'
  '-toc[_of "in"|"out"|"all"\[":short"\]]'
  '-mount[_cmd drive entity id path]'
  '-mount[_opts "exclusive"|"shared"]'
  '-session[_string drive entity id "linux:"path|"freebsd:"path|form]'
  '-list[_formats Show media specific list of format descriptors.]'
  '-list[_speeds Show media specific list of write speed descriptors.]'
  '-list[_profiles "in"|"out"|"all"]'
  '-print[_size Print the foreseeable consumption by next -commit.]'
  '-tell[_media_space]'
  '-pvd[_info Print various id strings of the loaded ISO image.]'
  '-report[_el_torito "plain"|"help"|"cmd"|"as_mkisofs"]'
  '-report[_system_area "plain"|"help"|"gpt_crc_of:"disk_path]'
  '-list[_delimiter text Set the list delimiter to be used instead of "--"]'
  '-pathspecs["|"off"|"as_mkisofs"]'
  '-file[_name_limit number]'
  '-file[_size_limit value \[...\]]'
  '-add[\[...\] | disk_path \[***\]]'
  '-add[_plainly "none"|"unknown"|"dashed"|"any"]'
  '-path[_list disk_path]'
  '-quoted[_path_list disk_path]'
  '-map[iso_rr_path]'
  '-map[_single disk_path iso_rr_path]'
  '-map[_l disk_prefix iso_rr_prefix disk_path \[***\]]'
  '-update[iso_rr_path]'
  '-update[_r disk_path iso_rr_path]'
  '-update[_l disk_prefix iso_rr_prefix disk_path \[***\]]'
  '-cut[_out disk_path byte_offset byte_count iso_rr_path]'
  '-cpr[\[***\] iso_rr_path]'
  '-rm[\[***\]]'
  '-rm[_r iso_rr_path \[***\]]'
  '-move[iso_rr_path]'
  '-mv[\[***\] iso_rr_path]'
  '-chown[iso_rr_path \[***\]]'
  '-chown[_r uid iso_rr_path \[***\]]'
  '-chgrp[iso_rr_path \[***\]]'
  '-chgrp[_r gid iso_rr_path \[***\]]'
