#compdef mdb-queries

# Autogenerated from man page /usr/share/man/man1/mdb-queries.1.gz

local arguments

arguments=(
    '-L[List queries in the database (default if no query name is passed).]'
    '-1[Use newline as the delimiter (used in conjunction with listing).]'
    '-d[Specify delimiter to use NOTES .]:delim'
    '*:filename:_files'
)
