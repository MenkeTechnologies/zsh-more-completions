#compdef wmf2gd

local arguments

arguments=(
  '-t[where <type> is one of: png,jpeg.]'
  '--maxwidth[where <w> is maximum width image may have.]'
  '--maxheight[where <h> is maximum height image may have.]'
  '--maxpect[scale image to maximum size keeping aspect.]'
  '--maxsize[scale image to maximum size.]'
  '--version[display version info and exit.]'
  '--help[display this help and exit.]'
  '--wmf-help[display wmf-related help and exit.]'
  '*:filename:_files'
)

_arguments -s $arguments
