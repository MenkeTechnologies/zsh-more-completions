#compdef timerfires

local arguments

arguments=(
  '-t[length of time to run in seconds]'
  '-p[filter to process with pid]'
  '-n[filter to process with name]'
  '-s[show stacks for non-callout timers]'
  '-v[show verbose output where applicable]'
  '-g[aggregate output]'
  '*:filename:_files'
)

_arguments -s $arguments
