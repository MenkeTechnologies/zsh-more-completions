    '-i[Install a package.]'
    '--gi[Install a package in the system directory.]'
    '-t[Show last build + testsuite run from travis, greps for errors.]'
    '-u[Uninstall a package.]'
    '--gu[Uninstall a package from the system directory.]'
    '-l[List installed packages.]'
    '-s[Search in database for packages matching keyword.]'
    '-r[Run command with RZPM_BINDIR in PATH.]'
    '-v[Show version information.]'
    '-h[Show usage help message.]'
    '-c[Clean the source cache.]'
    '-w[Show when a package was installed or exit 1 if pkg does not exist El EXAMPLES…]'
    '*:filename:_files'
)
