#compdef ike-scan

local arguments

arguments=(
  {--help,-h}'[display this usage message and exit.]'
  '--file[or -f <fn> Read hostnames or addresses from the specified file]'
  '--sport[or -s <p> Set UDP source port to <p>, default=500, 0=random.]'
  '--dport[or -d <p> Set UDP destination port to <p>, default=500.]'
  '--retry[or -r <n> Set total number of attempts per host to <n>]'
  '--timeout[or -t <n> Set initial per host timeout to <n> ms, default=500.]'
  '--bandwidth[or -B <n> Set desired outbound bandwidth to <n>, default=56000]'
  '--interval[or -i <n> Set minimum packet interval to <n> ms.]'
  '--backoff[or -b <b> Set timeout backoff factor to <b>, default=1.50.]'
  {--verbose,-v}'[display verbose progress messages.]'
  {--quiet,-q}'[dont decode the returned packet.]'
  {--multiline,-M}'[split the payload decode across multiple lines.]'
  '--lifetime[or -l <s> Set IKE lifetime to <s> seconds, default=28800.]'
  '--lifesize[or -z <s> Set IKE lifesize to <s> Kilobytes, default=0.]'
  '--auth[or -m <n> Set auth. method to <n>, default=1 (PSK).]'
  {--version,-V}'[display program version and exit.]'
  '--vendor[or -e <v> Set vendor id string to hex value <v>.]'
  '--trans[or -a <t> Use custom transform <t> instead of default set.]'
  '--trans[(1=1,2=2,3=3,4=4)". For example]'
  '--trans[1=1,2=2,3=1,4=2) specifies]'
  '--trans[1=7,14=128,2=1,3=3,4=5) specifies]'
  '--trans[1=5,2=1,3=1,4=1,11=1,12=0x00007080) specifies]'
  '--showbackoff[or -o\[<n>\] Display the backoff fingerprint table.]'
  '--fuzz[or -u <n> Set pattern matching fuzz to <n> ms, default=500.]'
  '--patterns[or -p <f> Use IKE backoff patterns file <f>]'
  '--showbackoff[specified.]'
  '--vidpatterns[or -I <f> Use Vendor ID patterns file <f>]'
  {--aggressive,-A}'[use IKE Aggressive Mode (The default is Main Mode)]'
  '--id[or -n <id> Use <id> as the identification value.]'
  '--idtype[or -y <n> Use identification type <n>. Default 3 (ID_USER_FQDN).]'
  '--dhgroup[or -g <n> Use Diffie Hellman Group <n>. Default 2.]'
  '--gssid[or -G <n> Use GSS ID <n> where <n> is a hex string.]'
  {--random,-R}'[randomise the host list.]'
  '--tcp[or -T\[<n>\] Use TCP transport instead of UDP.]'
  '--tcptimeout[or -O <n> Set TCP connect timeout to <n> seconds (default=10).]'
  '--pskcrack[or -P\[<f>\] Crack aggressive mode pre-shared keys.]'
  {--nodns,-N}'[do not use DNS to resolve names.]'
  '--noncelen[or -c <n> Set the nonce length to <n> bytes. Default=20]'
  '--headerlen[or -L <n> Set the length in the ISAKMP header to <n> bytes.]'
  '--mbz[or -Z <n> Use the value <n> for reserved (MBZ) fields, default=0.]'
  '--headerver[or -E <n> Specify the ISAKMP header version.]'
  '--certreq[or -C <c> Add the CertificateRequest payload <c>.]'
  '--doi[or -D <d> Set the SA DOI to <d>, default 1 (IPsec).]'
  '--situation[or -S <s> Set the SA Situation to <d>, default 1.]'
  '--protocol[or -j <p> Set the Proposal protocol ID to <p>, default 1.]'
  '--transid[or -k <t> Set the Transform ID to <t>, default 1.]'
  '--spisize[set the proposal SPI size to <n>. Default=0]'
  '--hdrflags[set the ISAKMP header flags to <n>. Default=0]'
  '--hdrmsgid[set the ISAKMP header message ID to <n>. Default=0]'
  '--cookie[set the ISAKMP initiator cookie to <n>]'
  '--exchange[set the exchange type to <n>]'
  '--nextpayload[set the next payload in the ISAKMP header to <n>]'
  '--randomseed[use <n> to seed the pseudo random number generator.]'
  '--timestamp[display timestamps for received packets.]'
  '--sourceip[set source IP address for outgoing packets to <s>.]'
  '--shownum[display the host number for received packets.]'
  '--nat-t[use RFC 3947 NAT-Traversal encapsulation.]'
  '--dport[providing they are used after the]'
  '--nat-t[.]'
  '--rcookie[set the ISAKMP responder cookie to <n>.]'
  {--ikev2,-2}'[use IKE version 2]'
  '*:filename:_files'
)

_arguments -s $arguments
