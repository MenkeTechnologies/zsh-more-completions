#compdef mwaw2csv

local arguments

arguments=(
  '-h[:          Shows this help message]'
  '-dc[:         Sets the decimal commas to character c: default .]'
  '-fc[:         Sets the field separator to character c: default]'
  '-tc[:         Sets the text separator to character c: default "]'
  '-F[:          Sets to output the formula which exists in the file]'
  '-Dformat[:    Sets the date format: default "%m/%d/%y"]'
  '-Tformat[:    Sets the time format: default "%H:%M:%S"]'
  '-N[:          Output the number of sheets]'
  '-n[sets the choose the sheet to convert (1: means first sheet)]'
  '-o[.csv: Defines the ouput file]'
  '-v[:          Output mwaw2csv version]'
  '*:filename:_files'
)

_arguments -s $arguments
