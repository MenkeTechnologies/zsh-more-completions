#compdef yuvcorrect_tune

local arguments

arguments=(
  '-I[]'
  '-F[frame> (optional)]'
  '-M[keyword concerning the correction MODE of yuvcorrect_tune]'
  '-Y[keyword concerning color corrections in the YUV space]'
  '-R[keyword concerning color corrections in the RGB space]'
  '-v[specifies the degree of verbosity: 0=quiet, 1=normal, 2=verbose/debug]'
  '-h[print this lot!]'
  '*:filename:_files'
)

_arguments -s $arguments
