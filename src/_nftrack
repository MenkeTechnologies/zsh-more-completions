#compdef nftrack

local arguments

arguments=(
  '-h[this text you see right here]'
  '-l[last update of Ports DB]'
  '-V[print version and exit.]'
  '-I[initialize Ports DB files.]'
  '-d[ports DB directory.]'
  '-r[read from file. default: stdin]'
  '-p[online output mode.]'
  '-s[create port statistics for timeslot -t]'
  '-t[timeslot for statistics]'
  '-S[create port statistics for last day]'
  '-w[write output to file]'
  '-f[filter syntaxfile]'
  '*:filename:_files'
)

_arguments -s $arguments
