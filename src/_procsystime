#compdef procsystime

local arguments

arguments=(
  '-p[# examine this PID]'
  '-n[# examine this process name]'
  '-a[# print all details]'
  '-e[# print elapsed times]'
  '-c[# print syscall counts]'
  '-o[# print CPU times]'
  '-T[# print totals]'
  '*:filename:_files'
)

_arguments -s $arguments
