#compdef androiddump

local arguments

arguments=(
  '--extcap-interfaces[list the extcap Interfaces]'
  '--extcap-dlts[list the DLTs]'
  '--extcap-interface[specify the extcap interface]'
  '--extcap-config[list the additional configuration for an interface]'
  '--capture[run the capture]'
  '--extcap-capture-filter[the capture filter]'
  '--fifo[dump data to file or fifo]'
  '--extcap-version[print tool version]'
  '--debug[print additional messages]'
  '--debug-file[print debug messages to file]'
  '--help[print this help]'
  '--adb-server-ip[the IP address of the ADB server]'
  '--adb-server-tcp-port[the TCP port of the ADB server]'
  '--logcat-text[use logcat text format]'
  '--logcat-ignore-log-buffer[ignore log buffer]'
  '--logcat-custom-options[use custom logcat parameters]'
  '--bt-server-tcp-port[bluetooth server TCP port]'
  '--bt-forward-socket[bluetooth forward socket]'
  '--bt-local-ip[the bluetooth local IP]'
  '--bt-local-tcp-port[the bluetooth local TCP port]'
  '*:filename:_files'
)

_arguments -s $arguments
