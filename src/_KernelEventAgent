#compdef KernelEventAgent

local arguments

arguments=(
  '-d[debug mode: do not daemonize and write syslog messages to]'
  '-v[verbose mode: level LOG_INFO and LOG_DEBUG system messages are]'
  '-f[the file system identified by fsname is also passed the timeout]'
  '-t[the timeout value is the number of seconds to wait before the user]'
  '*:filename:_files'
)

_arguments -s $arguments
