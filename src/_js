#compdef js

local arguments

arguments=(
  {-v,--version}'[print Node.js version]'
  {-e,--eval}'[evaluate script]'
  {-p,--print}'[evaluate script and print result]'
  {-c,--check}'[syntax check script without executing]'
  {-i,--interactive}'[always enter the REPL even if stdin]'
  {-r,--require}'[module to preload (option can be repeated)]'
  '--inspect[activate inspector on host:port]'
  '--inspect-brk[activate inspector on host:port]'
  '--inspect-port[set host:port for inspector]'
  '--no-deprecation[silence deprecation warnings]'
  '--trace-deprecation[show stack traces on deprecations]'
  '--throw-deprecation[throw an exception on deprecations]'
  '--pending-deprecation[emit pending deprecation warnings]'
  '--no-warnings[silence all process warnings]'
  '--napi-modules[load N-API modules (no-op - option]'
  '--abort-on-uncaught-exception[aborting instead of exiting causes a]'
  '--trace-warnings[show stack traces on process warnings]'
  '--redirect-warnings[write warnings to file instead of]'
  '--trace-sync-io[show stack trace when use of sync IO]'
  '--force-async-hooks-checks[enables checks for async_hooks]'
  '--trace-events-enabled[track trace events]'
  '--trace-event-categories[comma separated list of trace event]'
  '--track-heap-objects[track heap object allocations for heap snapshots]'
  '--prof-process[process v8 profiler output generated]'
  '--zero-fill-buffers[automatically zero-fill all newly allocated]'
  '--v8-options[print v8 command line options]'
  '--v8-pool-size[set v8s thread pool size]'
  '--tls-cipher-list[use an alternative default TLS cipher list]'
  '--use-bundled-ca[use bundled CA store]'
  '--use-openssl-ca[use OpenSSLs default CA store (default)]'
  '--openssl-config[load OpenSSL configuration from the]'
  '--icu-data-dir[set ICU data load path to dir]'
  '--preserve-symlinks[preserve symbolic links when resolving]'
  '--experimental-modules[experimental ES Module support]'
  '*:filename:_files'
)

_arguments -s $arguments
