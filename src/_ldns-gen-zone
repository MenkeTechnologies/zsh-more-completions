#compdef ldns-gen-zone

local arguments

arguments=(
  '-a[add NUM artifical delegations (NS RRSets) to output.]'
  '-p[add NUM percent of DS RRsets to the NS RRsets (1-4 RRs per DS RRset).]'
  '-o[sets an $ORIGIN, which can be handy if the one in the zonefile is set to @.]'
  '-s[input zone file is already sorted and canonicalized (ie all lowercase)]'
  '-h[this text.]'
  '-v[the version and exits.]'
  '*:filename:_files'
)

_arguments -s $arguments
