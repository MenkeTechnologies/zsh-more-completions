#compdef gpgme-config

local arguments

arguments=(
  '--prefix[]'
  '--exec-prefix[]'
  '--version[]'
  '--api-version[]'
  '--host[]'
  '--libs[]'
  '--cflags[]'
  '--print-lang[print available language bindings]'
  '--have-lang[return success if LANG is available]'
  '*:filename:_files'
)

_arguments -s $arguments
