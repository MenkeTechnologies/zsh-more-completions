#compdef regfmount

local arguments

arguments=(
  '-c[:          codepage of ASCII strings, options: ascii, windows-874, windows-932]'
  '-h[:          shows this help]'
  '-v[:          verbose output to stderr, while regfmount will remain running in the]'
  '-V[:          print version]'
  '-X[:          extended options to pass to sub system]'
  '*:filename:_files'
)

_arguments -s $arguments
