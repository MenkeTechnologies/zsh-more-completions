#compdef eu-ar

local arguments

arguments=(
  '-d[delete files from archive.]'
  '-m[move files in archive.]'
  '-p[print files in archive.]'
  '-q[quick append files to archive.]'
  '-r[replace existing or insert new file into archive.]'
  '-t[display content of archive.]'
  '-x[extract files from archive.]'
  '-a[insert file after \[MEMBER\].]'
  '-b[insert file before \[MEMBER\].]'
  '-c[suppress message when library has to be created.]'
  '-C[do not replace existing files with extracted]'
  '-i[same as -b.]'
  '-N[use instance \[COUNT\] of name.]'
  '-o[preserve original dates.]'
  '-P[use full path for file matching.]'
  '-s[force regeneration of symbol table.]'
  '-T[allow filename to be truncated if necessary.]'
  '-u[update only older files in archive.]'
  '-v[provide verbose output.]'
  '-D[use zero for uid, gid, and date in archive]'
  '-U[use actual uid, gid, and date in archive members.]'
  '--usage[give a short usage message]'
  {-V,--version}'[print program version]'
  '*:filename:_files'
)

_arguments -s $arguments
