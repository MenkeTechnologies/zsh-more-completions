#compdef pdfroff

local arguments

arguments=(
  '-h[]'
  '--help[display this usage summary, and exit.]'
  '-v[]'
  '--version[display a version identification message and exit.]'
  '--report-progress[enable console messages, indicating the progress of the]'
  '--emit-ps[emit PostScript output instead of PDF; this may be useful]'
  '--pdf-output[write the PDF, (or PostScript), output stream to file]'
  '--no-pdf-output[suppress the generation of PDF, (or PostScript), output]'
  '--no-reference-dictionary[suppress the generation of a pdfroff reference dictionary]'
  '--reference-dictionary[save the document reference dictionary in file name.]'
  '--stylesheet[use the file name as a groff style sheet, to control]'
  '--no-toc-relocation[suppress the multiple pass groff processing, which is]'
  '--no-kill-null-pages[suppress the null page elimination filter, which is used]'
  '--keep-temporary-files[suppress the normal clean up of temporary files, which is]'
  '*:filename:_files'
)

_arguments -s $arguments
