#compdef psicc

local arguments

arguments=(
  '-i[<profile> - Input profile: Generates Color Space Array (CSA)]'
  '-o[<profile> - Output profile: Generates Color Rendering Dictionary(CRD)]'
  '-t[<0,1,2,3> - Intent (0=Perceptual, 1=Colorimetric, 2=Saturation, 3=Absolute)]'
  '-b[Black point compensation (CRD only)]'
  '-u[Do NOT generate resource name on CRD]'
  '-c[<0,1,2> - Precision (0=LowRes, 1=Normal (default), 2=Hi-res) (CRD only)]'
  '-n[<gridpoints> - Alternate way to set precission, number of CLUT points (CRD only)]'
  '*:filename:_files'
)

_arguments -s $arguments
