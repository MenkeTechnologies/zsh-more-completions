#compdef rz-bin

# Autogenerated from man page /usr/share/man/man1/rz-bin.1.gz

local arguments

arguments=(
    '-@[Show information (symbol, section, import) of the given address.]'
    '-A[List sub-binaries and their associated arch-bits pairs.]'
    '-a[Set arch (x86, arm, . accepts underscore for bits x86_32).]'
    '-b[Set bits (32, 64, . ).]'
    '-B[Override baddr.]'
    '-c[List classes.]'
    '--cc[List classes in header format.]'
    '-C[Create \[elf,mach0,pe\] for arm and x86-32/64 tiny binaries where \C\ is an hex…]'
    '-d[Show debug/dwarf information.]'
    '-D[Demangle symbol name (or - to read from stdin) for lang (cxx, swift, java, cx…]'
    '-e[Show entrypoints for disk and on-memory.]'
    '--ee[Show constructor/destructors (extended entrypoints).]'
    '-f[Select sub-binary architecture. Useful for fat-mach0 binaries.]'
    '-F[Force to use that bin plugin (ignore header check).]'
    '-g[Show all possible information.]'
    '-G[Load address . offset to header.]'
    '-h[Show usage help message.]'
    '-H[Show header fields (see ih command in rizin).]'
    '-I[Show binary info (iI in rizin).]'
    '-i[Show imports (symbols imported from libraries) (ii).]'
    '-j[Output in json.]'
    '-k[Perform SDB query on loaded file.]'
    '-K[Select a rz-hash checksum algorithm to be performed on sections listing (and …]'
    '-l[List linked libraries to the binary.]'
    '-L[List supported bin plugins.]'
    '-M[Show address of \main\ symbol.]'
    '-m[Show source line reference from a given address.]'
    '-N[Force minimum and maximum number of chars per string (see -z and -zz).]'
    '-n[Show information (symbol, section, import) at string offset.]'
    '-o[Output file/folder for write operations (out by default).]'
    '-O[Perform binary operation on target binary (dump, resize, change sections, .]'
    '-p[Disable VA. Show physical addresses.]'
    '-P[Show debug/pdb information.]'
    '--PP[Download pdb file for binary.]'
    '-q[Be quiet, just show fewer data.]'
    '--qq[Show less info (no offset/size for -z for ex. ).]'
    '-Q[Show load address used by dlopen (non-aslr libs).]'
    '-r[Show output in rizin format.]'
    '-R[Show relocations.]'
    '-s[Show exported symbols.]'
    '-S[Show sections.]'
    '--SS[Show segments.]'
    '-t[Show file hashes.]'
    '-T[Show Certificates.]'
    '-u[Unfiltered (no rename duplicated symbols/sections).]'
    '-U[Show Resources.]'
    '-v[Show version information.]'
    '-V[Show binary version information.]'
    '-w[Show try/catch blocks.]'
    '-x[Extract all sub binaries from a fat binary (f. ex: fatmach0).]'
    '-X[Package a fat or zip containing all the files passed (fat, zip).]'
    '-z[Show strings inside . data section (like gnu strings does).]'
    '-Z[Guess size of binary program.]'
    '--zz[Shows strings from raw bins.]'
    '--zzz[Dump raw strings to stdout (for huge files) El ENVIRONMENT Pp RZ_BIN_LANG sam…]'
    '*:filename:_files'
)
_arguments -s $arguments
