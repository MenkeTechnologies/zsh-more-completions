#compdef js-yaml

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this help message and exit.]'
  {-v,--version}'[show programs version number and exit.]'
  {-c,--compact}'[display errors in compact mode]'
  {-t,--trace}'[show stack trace on error]'
  '*:filename:_files'
)

_arguments -s $arguments
