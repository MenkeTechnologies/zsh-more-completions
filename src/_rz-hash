#compdef rz-hash

# Autogenerated from man page /usr/share/man/man1/rz-hash.1.gz

local arguments

arguments=(
    '-a[Select an algorithm for the hashing. Valid values are listed in: rz-hash -L.]'
    '-b[Define the block size.]'
    '-c[Compare the computed hash with this one.]'
    '-D[Decrypt instead of hash using the given algorithm (base64, base91, rc4, aes, …]'
    '-e[Use little endian to display checksums.]'
    '-E[Encrypt instead of hash using the given algorithm (base64, base91, rc4, aes, …]'
    '-i[Apply the hash Iters times to itself+seed.]'
    '-I[Set initialization vector (IV) for the cryptographic functions.]'
    '-j[Show output in JSON (see -r).]'
    '-B[Show per-block hash.]'
    '-k[Show result using OpenSSH\s VisualHostKey randomart algorithm.]'
    '-n[Amount of blocks to hash.]'
    '-s[Hash this string instead of using the \source\ and \hash-file\ arguments.]'
    '-S[Set seed to hash with, use ^to prefix seed, otherwise its suffixed.]'
    '-f[Start hashing at given address.]'
    '-t[Stop hashing at given address.]'
    '-p[Show vertical entropy/statistical entropy graphs.]'
    '-q[Quiet mode (-qq for even quieter!).]'
    '-x[Hash the given hexpair string instead of using the \source\ and \hash-file\ a…]'
    '-v[Show version information.]'
    '-h[Show usage help message.]'
    '*:filename:_files'
)
_arguments -s $arguments
