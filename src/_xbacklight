#compdef xbacklight

local arguments

arguments=(
  '-display[or -d <display>]'
  '-help[]'
  '-set[or = <percentage>]'
  '-inc[or + <percentage>]'
  '-dec[or - <percentage>]'
  '-get[]'
  '-time[time in milliseconds>]'
  '-steps[of steps in fade>]'
  '*:filename:_files'
)

_arguments -s $arguments
