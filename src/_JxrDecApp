#compdef JxrDecApp

local arguments

arguments=(
  '-i[.jxr/wdp Input JPEG XR/HD Photo file name]'
  '-o[.bmp/tif/jxr Output image file name]'
  '-c[specifies the uncompressed output format]'
  '-r[left height width Specifies the rectangle for region decode]'
  '-T[reduced resolution (mipmap) decode]'
  '-O[0: No transformation (default)]'
  '-s[subbands Used for compressed domain transcoding]'
  '-a[decode 0: Decode without alpha channel]'
  '-p[post processing filter strength]'
  '-C[suppress overlapping boundary macro blocks]'
  '-t[display timing information]'
  '-v[display verbose decoder information]'
  '*:filename:_files'
)

_arguments -s $arguments
