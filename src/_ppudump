#compdef ppudump

local arguments

arguments=(
  '-F[<format>  Set output format to <format>]'
  '-M[with ExitCode=2 if more information is available]'
  '-S[PPU version check. May lead to reading errors]'
  '-V[<verbose>  Set verbosity to <verbose>]'
  '-h[-?       This helpscreen]'
  '*:filename:_files'
)

_arguments -s $arguments
