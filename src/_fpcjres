#compdef fpcjres

local arguments

arguments=(
  {--help,-h}'[-\? Show this screen.]'
  {--version,-V}'[show program version.]'
  {--verbose,-v}'[be verbose.]'
  {--input,-i}'[ignored for compatibility.]'
  {--output,-o}'[set the output file name.]'
  '*:filename:_files'
)

_arguments -s $arguments
