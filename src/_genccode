#compdef genccode

local arguments

arguments=(
  '-h[-\? or --help this usage text]'
  {-d,--destdir}'[destination directory, followed by the path]'
  {-n,--name}'[symbol prefix, followed by the prefix]'
  {-e,--entrypoint}'[entry point name, followed by the name (_dat will be appended)]'
  {-r,--revision}'[specify a version]'
  {-o,--object}'[write a .obj file instead of .c]'
  {-m,--match-arch}'[.o match the architecture (CPU, 32/64 bits) of the specified .o]'
  {-f,--filename}'[specify an alternate base filename. (default: symbolname_typ)]'
  {-a,--assembly}'[create assembly file. (possible values are: gcc, gcc-darwin, gcc-cygwin, gcc-mingw64, sun, sun-x86, xlc, aCC-ia64, aCC-parisc, masm)]'
  '*:filename:_files'
)

_arguments -s $arguments
