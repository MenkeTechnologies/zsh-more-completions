#compdef aria_read_log

local arguments

arguments=(
  {-a,--apply}'[apply log to tables: modifies tables! you should make a]'
  '--character-sets-dir[directory where character sets are.]'
  {-c,--check}'[if --display-only, check if record is fully readable (for]'
  {-d,--display-only}'[display brief info read from records header]'
  {-e,--end-lsn}'[stop applying at this lsn. If end-lsn is used, UNDO:s]'
  {-h,--aria-log-dir-path}'[path to the directory where to store transactional log]'
  {-P,--page-buffer-size}'[the size of the buffer used for index blocks for Aria]'
  {-o,--start-from-lsn}'[start reading log from this lsn]'
  {-C,--start-from-checkpoint}'[start applying from last checkpoint]'
  {-s,--silent}'[print less information during apply/undo phase]'
  {-T,--tables-to-redo}'[list of tables sepearated with , that we should apply]'
  {-t,--tmpdir}'[path for temporary files. Multiple paths can be]'
  '--translog-buffer-size[the size of the buffer used for transaction log for Aria]'
  {-u,--undo}'[apply UNDO records to tables. (disable with]'
  '--disable-undo[)]'
  {-v,--verbose}'[print more information during apply/undo phase]'
  {-V,--version}'[print version and exit.]'
  '--print-defaults[print the program argument list and exit.]'
  '--no-defaults[dont read default options from any option file.]'
  '--defaults-file[only read default options from the given file #.]'
  '--defaults-extra-file[read this file after the global files are read.]'
  '--defaults-group-suffix[additionally read default groups with # appended as a suffix.]'
  '*:filename:_files'
)

_arguments -s $arguments
