#compdef as10k1

local arguments

arguments=(
  '-l[specify a listing file, default is none.]'
  '-o[10k1 file\] Specify an output file, default is based input]'
  '-d[...\] Turn on debug messages.]'
  '-h[prints this message]'
  '-v[prints version number.]'
  '*:filename:_files'
)

_arguments -s $arguments
