#compdef httprint

local arguments

arguments=(
  '-h[host can be either an IP address, a symbolic name]'
  '-i[text file> file containing list of hosts as described above]'
  '-x[xml file> Nmap -oX option generated xml file as input file.]'
  '-s[file containing http fingerprint signatures.]'
  '-o[file> output in html format.]'
  '-oc[file> output in csv format.]'
  '-ox[file> output in xml format.]'
  '-noautossl[disable automatic detection of SSL.]'
  '-tp[timeout> Ping timeout in milliseconds.]'
  '-ct[1-100> Default is 75. Do not change.]'
  '-ua[agent> Default is Mozilla/4.0 (compatible; MSIE 5.01;]'
  '-t[connection/read timeout in milliseconds.]'
  '-r[number of retries. Default is 3. Maximum 30.]'
  '-P0[turn ICMP ping off.]'
  '-nr[no redirection. Do not automatically follow 301]'
  '-th[number of threads. Default is 8. Maximum 64.]'
  '*:filename:_files'
)

_arguments -s $arguments
