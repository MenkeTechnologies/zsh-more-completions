#compdef jupyter-execute

local arguments

arguments=(
  '--allow-errors[errors are ignored and execution is continued until the end of the notebook.]'
  '--timeout[the time to wait (in seconds) for output from executions. If a cell]'
  '--startup[_timeout=<Int>]'
  '--kernel[_name=<Unicode>]'
  '*:filename:_files'
)

_arguments -s $arguments
