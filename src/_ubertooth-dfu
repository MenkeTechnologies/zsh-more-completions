#compdef ubertooth-dfu

local arguments

arguments=(
  '-u[upload - read firmware from device]'
  '-d[download - write DFU file to device]'
  '-r[ubertooth after other operations complete]'
  '-s[add DFU suffix to binary firmware file]'
  '-U[0-7> set ubertooth device to use]'
  '*:filename:_files'
)

_arguments -s $arguments
