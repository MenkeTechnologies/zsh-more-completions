#compdef foo2slx

local arguments

arguments=(
  {-sPAPERSIZE,-r1200x600,-sDEVICE}'[\]'
  '-sOutputFile[testpage.ps]'
  {-sPAPERSIZE,-g10200x6600,-r1200x600,-sDEVICE}'[\]'
  '-sOutputFile[testpage.ps]'
  '-c[force color mode if autodetect doesnt work]'
  '-g[set page dimensions in pixels \[10200x6600\]]'
  '-m[media code to send to printer \[0\]]'
  '-p[paper code to send to printer \[6\]]'
  '-n[number of copies \[1\]]'
  '-r[set device resolution in pixels/inch \[1200x600\]]'
  '-s[source code to send to printer \[0\]]'
  '-u[set offset of upper left printable in pixels \[0x0\]]'
  '-l[set offset of lower right printable in pixels \[0x0\]]'
  '-L[send logical clipping values from -u/-l in ZjStream \[3\]]'
  '-A[allIsBlack: convert C=1,M=1,Y=1 to just K=1]'
  '-B[blackClears: K=1 forces C,M,Y to 0]'
  {-A,-B}'[with bitcmyk input only]'
  '-S[output just a single color plane from a color print \[all\]]'
  '-D[set Debug level \[0\]]'
  '-V[version $Id: foo2slx.c,v 1.24 2011/06/09 13:02:24 rick Exp $]'
  '*:filename:_files'
)

_arguments -s $arguments
