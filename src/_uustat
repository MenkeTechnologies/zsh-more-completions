#compdef uustat

local arguments

arguments=(
  {-a,--all}'[list all UUCP jobs]'
  {-B,--mail-lines}'[number of lines to return in -M or -N mail message]'
  {-c,--command}'[list requests for named command]'
  {-C,--not-command}'[list requests for other than named command]'
  {-e,--executions}'[list queued executions rather than job requests]'
  {-i,--prompt}'[prompt for whether to kill each listed job]'
  {-k,--kill}'[kill specified UUCP job]'
  {-K,--kill-all}'[kill each listed job]'
  {-m,--status}'[report status for all remote machines]'
  {-M,--mail}'[mail report on each listed job to UUCP administrator]'
  {-N,--notify}'[mail report on each listed job to requestor]'
  {-o,--older-than}'[list all jobs older than given number of hours]'
  {-p,--ps}'[show status of all processes holding UUCP locks]'
  {-q,--list}'[list number of jobs for each system]'
  {-Q,--no-list}'[dont list jobs, just take actions (-i, -K, -M, -N)]'
  {-r,--rejuvenate}'[rejuvenate specified UUCP job]'
  {-R,--rejuvenate-all}'[rejuvenate each listed job]'
  {-s,--system}'[list all jobs for specified system]'
  {-S,--not-system}'[list all jobs for other than specified system]'
  {-u,--user}'[list all jobs for specified user]'
  {-U,--not-user}'[list all jobs for other than specified user]'
  {-W,--comment}'[comment to include in mail messages]'
  {-y,--younger-than}'[list all jobs younger than given number of hours]'
  {-x,--debug}'[set debugging level]'
  {-I,--config}'[set configuration file to use]'
  {-v,--version}'[Print version and exit]'
  '--help[Print help and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
