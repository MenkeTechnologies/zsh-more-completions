#compdef resolveLinks

local arguments

arguments=(
  '-b[path to strip from output seed file> (default /)]'
  '-d[flags> (default 0)]'
  '-i[path where the HTML will end up> (default is -b value)]'
  '-r[to apple_ref> (can be used multiple times)]'
  '-s[file> (can be used multiple times)]'
  '-S[path for previous seed file> (can be used multiple times)]'
  '-t[of threads> (default 2)]'
  '-x[seed file>]'
  '*:filename:_files'
)

_arguments -s $arguments
