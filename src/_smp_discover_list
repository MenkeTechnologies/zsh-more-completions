#compdef smp_discover_list

local arguments

arguments=(
  '--adn[|-A output attached device name in one line per]'
  '--brief[|-b brief: less output, can be used multiple times]'
  '--cap[|-c decode phy capabilities bits]'
  '--descriptor[TY descriptor type:]'
  '--filter[FI phy filter: 0 -> all (def); 1 -> expander]'
  '--help[|-h print out usage message]'
  '--hex[|-H print response in hexadecimal]'
  '--ignore[|-i sets the Ignore Zone Group bit; will show]'
  '--interface[PARAMS specify or override interface]'
  '--num[NUM maximum number of descriptors to fetch (def: 1)]'
  '--one[|-o one line output per response descriptor (phy)]'
  '--phy[ID phy identifier \[or starting phy id\]]'
  '--raw[|-r output response in binary]'
  '--sa[SAS_ADDR SAS address of SMP target (use leading]'
  '--summary[|-S output 1 line per active phy; typically]'
  '--verbose[|-v increase verbosity]'
  '--version[|-V print version string and exit]'
  '--zpi[FN FN is file that zone phy information will be]'
  '*:filename:_files'
)

_arguments -s $arguments
