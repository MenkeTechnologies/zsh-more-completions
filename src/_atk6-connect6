#compdef atk6-connect6

local arguments

arguments=(
  '-a[send Hop-by-Hop Router Alert option]'
  '-A[like -a but lets you define the alarmtype (number)]'
  '-i[interactive mode (like telnet)]'
  '-I[end a linefeed, wait for a string, print it]'
  '-O[try TCP Fast Open connection]'
  '-w[wait time for connect in ms (default: 1000)]'
  '-p[ping mode]'
  '*:filename:_files'
)

_arguments -s $arguments
