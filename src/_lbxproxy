#compdef lbxproxy

local arguments

arguments=(
  '-help[prints message with these options]'
  '-display[specify address of LBX server]'
  '-motion[allow # motion events in flight]'
  '-I[ignore all remaining arguments]'
  '-reconnect[reset if server connection is broken]'
  '-nolbx[disable LBX reencoding of X requests]'
  '-nocomp[disable stream compression]'
  '-nodelta[disable request deltas]'
  '-notags[disable tags]'
  '-nogfx[disable graphics enhancements]'
  '-noimage[disable image compression]'
  '-nosquish[disable event squishing]'
  '-nointernsc[disable InternAtom short circuiting]'
  '-noatomsfile[disable atom control file]'
  '-atomsfile[override AtomControl file]'
  '-nowinattr[disable GetWindowAttributes/GetGeometry]'
  '-nograbcmap[disable colormap grabbing]'
  '-norgbfile[disables color name to RGB resolution]'
  '-rgbfile[path specifies an alternate RGB database]'
  '-tagcachesize[set tag cache size]'
  '-maxservers[maximum number of servers to use]'
  '-zlevel[zlib compression level (1-9)]'
  '-compstats[report stream compression statistics]'
  '-nozeropad[dont zero out pad bytes in X requests]'
  '-cheaterrors[cheat on X protocol errors for better performance]'
  '-cheatevents[cheat on events and errors for better performance]'
  '*:filename:_files'
)

_arguments -s $arguments
