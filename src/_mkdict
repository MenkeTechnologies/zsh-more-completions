#compdef mkdict

local arguments

arguments=(
  '--rsyncablemakersync-friendlyarchive[]'
  {-1,--fastcompressfaster}'[]'
  {-9,--bestcompressbetter}'[]'
  {-c,--stdoutwriteonstandardoutput}'[keeporiginalfilesunchanged]'
  {-d,--decompressdecompress}'[]'
  {-f,--forceforceoverwriteofoutputfileandcompresslinks}'[]'
  {-h,--helpgivethishelp}'[]'
  {-l,--licensedisplaysoftwarelicense}'[]'
  {-l,--listlistcompressedfilecontents}'[]'
  {-n,--namesaveorrestoretheoriginalnameandtimestamp}'[]'
  {-n,--no-namedonotsaveorrestoretheoriginalnameandtimestamp}'[]'
  {-q,--quietsuppressallwarnings}'[]'
  {-r,--recursiveoperaterecursivelyondirectories}'[]'
  {-s,--suffix}'[]'
  {-t,--testtestcompressedfileintegrity}'[]'
  {-v,--verboseverbosemode}'[]'
  {-v,--versiondisplayversionnumber}'[compressoruncompressfiles(bydefault,compressfilesin-place).]'
  '*:filename:_files'
)

_arguments -s $arguments
