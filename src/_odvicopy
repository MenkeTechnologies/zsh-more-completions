#compdef odvicopy

local arguments

arguments=(
  '-magnification[override existing magnification with NUMBER]'
  '-max-pages[process NUMBER pages; default one million]'
  '-page-start[start at PAGE-SPEC, for example `2 or `5.*.-2]'
  '-help[display this help and exit]'
  '-version[output version information and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
