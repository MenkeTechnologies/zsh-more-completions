#compdef dosepsbin

local arguments

arguments=(
  '--eps-file[write PS section to <file>.]'
  '--wmf-file[write WMF section to <file>.]'
  '--tiff-file[write TIFF section to <file>.]'
  '--inputfile[the name of the input file.]'
  '--verbose[verbose output.]'
  '--quiet[only errors and warnings are printed.]'
  '--help[brief help message.]'
  '--man[full documentation.]'
  '--version[print version identification.]'
  '--eps-file[the PS section is written to <file>. The output file must be]'
  '--wmf-file[the WMF section is written to <file> if present. The output file]'
  '--tiff-file[the TIFF section is written to <file> if present. The output file]'
  '--inputfile[the input file can also be given directly on the command line. If]'
  '--verbose[verbose messages.]'
  '--quiet[no messages are printed except for errors and warnings.]'
  '--help[display help screen.]'
  '--man[prints manual page.]'
  '--version[print version identification and exit.]'
  '*:filename:_files'
)

_arguments -s $arguments
