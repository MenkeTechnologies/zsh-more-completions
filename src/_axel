#compdef axel

local arguments

arguments=(
  {--max-speed,-s}'[specify maximum speed (bytes per second)]'
  {--num-connections,-n}'[specify maximum number of connections]'
  '--max-redirect[specify maximum number of redirections]'
  {--output,-o}'[specify local output file]'
  {--search,-S}'[search for mirrors and download from n servers]'
  {--ipv4,-4}'[use the IPv4 protocol]'
  {--ipv6,-6}'[use the IPv6 protocol]'
  {--header,-H}'[add HTTP header string]'
  {--user-agent,-U}'[set user agent]'
  {--no-proxy,-N}'[just dont use any proxy server]'
  {--insecure,-k}'[dont verify the SSL certificate]'
  {--no-clobber,-c}'[skip download if file already exists]'
  {--quiet,-q}'[leave stdout alone]'
  {--verbose,-v}'[more status information]'
  {--alternate,-a}'[alternate progress indicator]'
  {--help,-h}'[this information]'
  {--timeout,-T}'[set I/O and connection timeout]'
  {--version,-V}'[version information]'
  '*:filename:_files'
)

_arguments -s $arguments
