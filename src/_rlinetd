#compdef rlinetd

local arguments

arguments=(
  '-p[|--parser file alternative parser module (default /usr/lib/x86_64-linux-gnu/rlinetd/libparse.so)]'
  '-f[|--config file alternative config file (default /etc/rlinetd.conf)]'
  '-d[|--debug enable debugging]'
  '-h[|--help this message]'
  '*:filename:_files'
)

_arguments -s $arguments
