#compdef FileCheck60

local arguments

arguments=(
  '-D[VALUE> - Define a variable to be used in capture patterns.]'
  '-allow-empty[Allow the input file to be empty. This is useful when making]'
  '-check-prefix[Prefix to use from check file (defaults to CHECK)]'
  '-check-prefixes[Alias for -check-prefix permitting multiple comma separated values]'
  '-enable-var-scope[Enables scope for regex variables. Variables with names that]'
  '-implicit-check-not[Add an implicit negative check with this pattern to every]'
  '-input-file[File to check (defaults to stdin)]'
  '-match-full-lines[Require all positive matches to cover an entire input line.]'
  '-strict-whitespace[Do not treat all horizontal whitespace as equivalent]'
  '-help[Display available options (-help-hidden for more)]'
  '-help-list[Display list of available options (-help-list-hidden for more)]'
  '-version[Display the version of this program]'
  '*:filename:_files'
)

_arguments -s $arguments
