#compdef ipptool

local arguments

arguments=(
  '--help[show help.]'
  '--stop-after-include-error[stop tests after a failed INCLUDE.]'
  '--version[show version.]'
  '-4[connect using IPv4.]'
  '-6[connect using IPv6.]'
  '-C[send requests using chunking (default).]'
  '-E[test with encryption using HTTP Upgrade to TLS.]'
  '-I[ignore errors.]'
  '-L[send requests using content-length.]'
  '-P[.plist Produce XML plist to a file and test report to standard output.]'
  '-S[test with encryption using HTTPS.]'
  '-T[set the receive/send timeout in seconds.]'
  '-V[set default IPP version.]'
  '-X[produce XML plist instead of plain text.]'
  '-c[produce CSV output.]'
  '-d[value Set named variable to value.]'
  '-f[set default request filename.]'
  '-h[validate HTTP response headers.]'
  '-i[repeat the last file with the given time interval.]'
  '-l[produce plain text output.]'
  '-n[repeat the last file the given number of times.]'
  '-q[run silently.]'
  '-t[produce a test report.]'
  '-v[be verbose.]'
  '*:filename:_files'
)

_arguments -s $arguments
