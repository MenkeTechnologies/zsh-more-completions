#compdef testparm

local arguments

arguments=(
    {-s,--suppress-prompt}'[Suppress prompt for enter]'
    {-v,--verbose}'[Show default options too]'
    {-l,--skip-logic-checks}'[Skip the global checks]'
    '--show-all-parameters[Show the parameters, type, possible values]'
    '--parameter-name[Limit testparm to a named parameter]'
    '--section-name[Limit testparm to a named section]'
    '--usage[Display brief usage message]'
    {-V,--version}'[Print version]'
    {-d,--debuglevel}'[Set debug level]'
    '--option[value Set smb.conf option from command line]'
    '*:filename:_files'
)

_arguments -s $arguments
