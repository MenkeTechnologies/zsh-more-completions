#compdef asleap

local arguments

arguments=(
  '-r[from a libpcap file]'
  '-i[to capture on]'
  '-f[file with NT hashes]'
  '-n[file for NT hashes]'
  '-s[the check to make sure authentication was successful]'
  '-h[this help information and exit]'
  '-v[verbose information (more -v for more verbosity)]'
  '-V[program version and exit]'
  '-C[value in colon-delimited bytes]'
  '-R[value in colon-delimited bytes]'
  '-W[dictionary file (special purpose)]'
  '*:filename:_files'
)

_arguments -s $arguments
