#compdef sdiag

local arguments

arguments=(
  '-a[all statistics]'
  '-r[reset statistics]'
  '--help[show this help message]'
  '--sort-by-id[sort RPCs by id]'
  '--sort-by-time[sort RPCs by total run time]'
  '--sort-by-time2[RPCs by average run time]'
  '--usage[display brief usage message]'
  '--version[display current version number]'
  '*:filename:_files'
)

_arguments -s $arguments
