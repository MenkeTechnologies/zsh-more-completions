#compdef valadoc-0.54

local arguments

arguments=(
  {-h,--help}'[show help options]'
  {-o,--directory}'[output directory]'
  {-b,--basedir}'[base source directory]'
  {-D,--define}'[...                      Define SYMBOL]'
  '--profile[minimum runtime dependency: gobject (default) or posix (minimal libc)]'
  '--enable-experimental[enable experimental features]'
  '--enable-experimental-non-null[enable experimental enhancements for non-null types]'
  '--metadatadir[...                  Look for GIR .metadata files in DIRECTORY]'
  '--girdir[...                       Look for .gir files in DIRECTORY]'
  '--vapidir[...                      Look for package bindings in DIRECTORY]'
  '--pkg[...                            Include binding for PACKAGE]'
  '--driver[name of an driver or path to a custom driver (DEPRECATED AND IGNORED)]'
  '--importdir[...                    Look for external documentation in DIRECTORY]'
  '--import[...                         Include binding for PACKAGE]'
  '--alternative-resource-dir[...     Alternative resource directories]'
  '--wiki[wiki directory]'
  '--deps[adds packages to the documentation]'
  '--doclet[name of an included doclet or path to custom doclet]'
  {-X,--doclet-arg}'[pass arguments to the doclet]'
  '--no-protected[removes protected elements from documentation]'
  '--internal[adds internal elements to documentation]'
  '--private[adds private elements to documentation]'
  '--use-svg-images[generate SVG image charts instead of PNG]'
  '--package-name[package name]'
  '--package-version[package version]'
  '--gir[.gir                      GObject-Introspection repository file name]'
  '--version[display version number]'
  '--force[force]'
  '--fatal-warnings[treat warnings as fatal]'
  '--verbose[show all warnings]'
  '--no-color[disable colored output]'
  '--target-glib[.MINOR, or auto      Target version of glib for code generation]'
  '*:filename:_files'
)

_arguments -s $arguments
