#compdef rizin

# Autogenerated from man page /usr/share/man/man1/rizin.1.gz

local arguments

arguments=(
    '--Fl[Open rizin on an empty file.]'
    '-0[Print \\\\x00 after initialization and after every command executed.]'
    '-2[Close stderr before starting RzCore.]'
    '-a[force asm. arch (x86, ppc, arm, mips, bf, java, . ).]'
    '-A[run \aaa\ command before prompt or patch to analyze all referenced code.]'
    '-b[force asm. bits (16, 32, 64).]'
    '-B[Specify the base address to be used when loading a new binary. See \e?bin.]'
    '-c[Execute the given command before giving prompt.]'
    '-d[Start in debugger mode.]'
    '-D[Enable debug mode. Set cfg. debug=true.]'
    '-e[Set configuration eval variable key=value. For example \\-e scr. color=false.]'
    '-f[Blocksize = file size.]'
    '-i[Run script file. After the file is loaded.]'
    '-I[Run script file. Before the file is loaded.]'
    '-k[Select kernel (asm. os) for syscall resolution.]'
    '-l[Load given plugin file.]'
    '-L[List supported IO plugins.]'
    '-m[map file at given address.]'
    '-M[Disable demangling.]'
    '-n[Do not perform any analysis (r_bin). Just load the raw file.]'
    '--nn[Only load the rbin structures (elf, mach0, . ).]'
    '-N[Do not load user settings/projects from ~/. rizinrc, ~/.]'
    '--NN[Same as -N but also disables the automatic loading of plugins on startup time.]'
    '-q[Quiet mode (no prompt) and quit after running the commands specified with -i …]'
    '--qq[Quit before showing the prompt.]'
    '-Q[Same as q, but exiting without freeing RzCore, this produces leaks at exit ti…]'
    '-p[Set project file.]'
    '-r[Specify dbg.]'
    '-R[Specify custom rz-run directives without having to create a rz-run profile.]'
    '-s[Start seeking at this address.]'
    '-t[Get binary information using a thread.]'
    '-T[Avoid computing the file hashes.]'
    '-u[Set bin. filter=false to load rbin info without filtering names.]'
    '-v[Show version information and exit (Use -qv to get just the version number).]'
    '-V[Show rizin library versions.]'
    '-w[Open in write mode.]'
    '-h[Show help message.]'
    '-H[Show files and environment help.]'
    '-x[Open the file map without executable permissions.]'
    '-X[Same as -e bin.]'
    '*:filename:_files'
)
_arguments -s $arguments
