#compdef prlcc

local arguments

arguments=(
  '-h[This help]'
  '-r[Service poll period (in milliseconds) \[100, 1000\], default: 500]'
  '-t[DynRes confirmation timeout (in milliseconds) \[1000, 10000\], default: 2000]'
  '-m[DynRes use Randr12 API if available, default: 1]'
  '-l[Logging level \[0, 3\], default 2]'
  '-v[Verbose logging, set log level to 3]'
  '*:filename:_files'
)

_arguments -s $arguments
