#compdef godoc

local arguments

arguments=(
    '-analysis[comma-separated list of analyses to perform (supported: type, pointer). See http://golang.org/lib/godoc/analysis/help.html]'
    '-ex[show examples in command line mode]'
    '-goroot[Go root directory (default "/usr/local/Cellar/go/1.10/libexec")]'
    '-html[print HTML in command-line mode]'
    '-http[HTTP service address (e.g., :6060)]'
    '-httptest[.serve string]'
    '-index[enable search index]'
    '-index[_files string]'
    '-index[_interval duration]'
    '-index[_throttle float]'
    '-links[link identifiers to their declarations (default true)]'
    '-maxresults[maximum number of full text search results shown (default 10000)]'
    '-notes[regular expression matching note markers to show (default "BUG")]'
    '-play[enable playground in web interface]'
    '-q[arguments are considered search queries]'
    '-server[webserver address for command line searches]'
    '-src[print (exported) source in command-line mode]'
    '-tabwidth[tab width (default 4)]'
    '-templates[load templates/JS/CSS from disk in this directory]'
    '-timestamps[show timestamps with directory listings]'
    '-url[print HTML for named URL]'
    '-v[verbose mode]'
    '-write[_index]'
    '-zip[zip file providing the file system to serve; disabled if empty]'
    '*:filename:_files'
)

_arguments -s $arguments
