#compdef cjpeg

local arguments

arguments=(
  '-quality[...\] Compression quality (0..100; 5-95 is most useful range]'
  '-grayscale[create monochrome JPEG file]'
  '-rgb[create RGB JPEG file]'
  '-optimize[optimize Huffman table (smaller file, but slow compression)]'
  '-progressive[create progressive JPEG file]'
  '-targa[input file is Targa format (usually not needed)]'
  '-arithmetic[use arithmetic coding]'
  '-dct[use integer DCT method (default)]'
  '-dct[use fast integer DCT (less accurate)]'
  '-dct[use floating-point DCT method]'
  '-restart[set restart interval in rows, or in blocks with B]'
  '-smooth[smooth dithered input (N=1..100 is strength)]'
  '-maxmemory[maximum memory to use (in kbytes)]'
  '-outfile[specify name for output file]'
  '-memdst[compress to memory instead of file (useful for benchmarking)]'
  '-verbose[or -debug Emit debug output]'
  '-version[print version information and exit]'
  '-baseline[force baseline quantization tables]'
  '-qtables[use quantization tables given in FILE]'
  '-qslots[...\] Set component quantization tables]'
  '-sample[...\] Set component sampling factors]'
  '-scans[create multi-scan JPEG per script FILE]'
  '*:filename:_files'
)

_arguments -s $arguments
