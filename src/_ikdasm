#compdef ikdasm

local arguments

arguments=(
  '-out[name> Direct output to file rather than stdout]'
  '-help[print this help]'
  '-assembly[dumps the contents of the Assembly table]'
  '-assemblyref[dumps the contents of the AssemblyRef table]'
  '-moduleref[dumps the contents of the ModuleRef table]'
  '-exported[dumps the contents of the ExportedType table]'
  '-customattr[dumps the contents of the CustomAttribute table]'
  '*:filename:_files'
)

_arguments -s $arguments
