#compdef modutil

local arguments

arguments=(
  '-add[add the named module to the module database]'
  '-libfile[the name of the file (.so or .dll)]'
  '-changepw[change the password on the named token]'
  '-chkfips[true | false \] If true, verify FIPS mode. If false]'
  '-create[create a new set of security databases]'
  '-default[make the given module a default provider]'
  '-mechanisms[of the given mechanisms]'
  '-delete[remove the named module from the module]'
  '-disable[disable the named module]'
  '-enable[enable the named module]'
  '-fips[true | false \] If true, enable FIPS mode. If false]'
  '-force[do not run interactively]'
  '-jar[install a PKCS #11 module from the given]'
  '-installdir[use DIR as the root directory of the]'
  '-list[lists information about the specified module]'
  '-rawadd[add module spec string to secmod DB]'
  '-rawlist[display module spec(s) for one or all]'
  '-undefault[the given module is NOT a default provider]'
  '-mechanisms[of the listed mechanisms]'
  '-dbdir[directory DIR contains the security databases]'
  '-dbprefix[prefix for the security databases]'
  '-nocertdb[do not load certificate or key databases. No]'
  '-secmod[name of the security modules file]'
  '*:filename:_files'
)

_arguments -s $arguments
