#compdef xslt-config

local arguments

arguments=(
  '--prefix[change XSLT prefix \[default /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.14.sdk/usr\]]'
  '--exec-prefix[change XSLT executable prefix \[default /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.14.sdk/usr\]]'
  '--libs[print library linking information]'
  '--cflags[print pre-processor and compiler flags]'
  '--plugins[print plugin directory]'
  '--help[display this help and exit]'
  '--version[output version information]'
  '*:filename:_files'
)

_arguments -s $arguments
