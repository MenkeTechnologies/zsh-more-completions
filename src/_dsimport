#compdef dsimport

local arguments

arguments=(
  '--crypt[all passwords to be imported are crypt-based]'
  '--force[value]'
  '--groupid[GID used for any records missing primary GID]'
  '--grouppreset[designate a preset record to be applied to group records]'
  '--loglevel[increases the log level for the log file]'
  '--outputfile[outputs plist with names of imported records]'
  '--password[is the admins password for import operations]'
  '--recordformat[template used when no header line is available]'
  '--remotehost[connects to a remote machine at the network address]'
  '--remoteusername[specifies user name to use for the remote connection]'
  '--remotepassword[specifies password to use for the remote connection]'
  '--startuid[indicates the UID to start with when importing]'
  '--template[used for delimited import of files lacking descriptor]'
  '--username[the admin user to use when importing]'
  '--userpreset[designate a preset record to be applied to user records]'
  '*:filename:_files'
)

_arguments -s $arguments
