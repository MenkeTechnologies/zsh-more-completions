#compdef radtest

local arguments

arguments=(
  '-d[set radius directory]'
  '-t[set authentication method]'
  '-P[select udp (default) or tcp]'
  '-x[enable debug output]'
  '-4[use IPv4 for the NAS address (default)]'
  '-6[use IPv6 for the NAS address]'
  '*:filename:_files'
)

_arguments -s $arguments
