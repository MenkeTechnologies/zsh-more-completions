#compdef dvihp

local arguments

arguments=(
  '-A[print odd pages]'
  '-B[print even pages]'
  '-d[set debug bits to # (see documentation)]'
  '-D[set resolution to #]'
  '-f[run as filter]'
  '-l[dont print pages after #]'
  '-m[manual feed]'
  '-n[print # pages]'
  '-O[# set/change paper offset to #,# mm]'
  '-o[output to s instead of spooling]'
  '-p[dont print pages before #]'
  '-Ps[pass directly to lpr]'
  '-v[verbose operation]'
  '-x[set magnification to #]'
  '*:filename:_files'
)

_arguments -s $arguments
