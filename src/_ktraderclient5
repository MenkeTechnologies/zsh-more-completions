#compdef ktraderclient5

local arguments

arguments=(
  {-v,--version}'[displays version information.]'
  '(- * :)'{-h,--help}'[displays this help.]'
  '--author[show author information.]'
  '--license[show license information.]'
  '--desktopfile[name> The base file name of the desktop entry for this]'
  '--mimetype[A MIME type]'
  '--servicetype[A servicetype, like KParts/ReadOnlyPart or]'
  '--constraint[A constraint expressed in the trader query]'
  '--short[output only paths to desktop files]'
  '*:filename:_files'
)

_arguments -s $arguments
