#compdef gvfs-info

local arguments

arguments=(
  {-w,--query-writable}'[list writable attributes]'
  {-f,--filesystem}'[get file system info]'
  {-a,--attributes}'[the attributes to get]'
  {-n,--nofollow-symlinks}'[donâ€™t follow symbolic links]'
  '*:filename:_files'
)

_arguments -s $arguments
