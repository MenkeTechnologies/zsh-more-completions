#compdef clang-scan-deps12

local arguments

arguments=(
  '--help[- Display available options (--help-hidden for more)]'
  '--help-list[- Display list of available options (--help-list-hidden for more)]'
  '--version[- Display the version of this program]'
  '--compilation-database[- Compilation database]'
  '--format[- The output format for the dependencies]'
  '--full-command-line[- Include the full command lines to use to build modules]'
  '-j[- Number of worker threads to use (default: use all concurrent threads)]'
  '--mode[- The preprocessing mode used to compute the dependencies]'
  '--reuse-filemanager[- Reuse the file manager and its cache between invocations.]'
  '--skip-excluded-pp-ranges[- Use the preprocessor optimization that skips excluded conditionals by bumping the buffer pointer in the lexer instead of lexing the tokens  until reaching the end directive.]'
  '-v[- Use verbose output.]'
  '*:filename:_files'
)

_arguments -s $arguments
