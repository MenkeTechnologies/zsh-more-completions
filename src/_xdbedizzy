#compdef xdbedizzy

local arguments

arguments=(
  '-display[X server connection to use.]'
  '-delta[rotate <dlt> per frame (video) or page (printer).]'
  '-class[class of visual to use.]'
  '-depth[depth of visual to use.]'
  '-visid[0xnn\] Visual ID to use (ignore -class, -depth).]'
  '-list[list double buffer capable visuals.]'
  '-nodb[single buffer (ignore -class, -depth, -visid).]'
  '-help[print this message.]'
  '-speed[floating-point value to set the speed.]'
  '-sync[use synchronous X connection.]'
  '-spokes[specify number of spokes to draw.]'
  '-verbose[produce chatty messages while running.]'
  '*:filename:_files'
)

_arguments -s $arguments
