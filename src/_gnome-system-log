#compdef gnome-system-log

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show help options]'
  '--help-all[show all help options]'
  '--help-gtk[show GTK+ Options]'
  '--version[show the version of the program.]'
  '--display[X display to use]'
  '*:filename:_files'
)

_arguments -s $arguments
