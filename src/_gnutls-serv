#compdef gnutls-serv

local arguments

arguments=(
  {-d,--debug}'[enable debugging]'
  '--noticket[dont accept session tickets]'
  {-g,--generate}'[generate Diffie-Hellman and RSA-export parameters]'
  {-q,--quiet}'[suppress some messages]'
  '--nodb[do not use a resumption database]'
  '--http[act as an HTTP server]'
  '--echo[act as an Echo server]'
  {-u,--udp}'[use DTLS (datagram TLS) over UDP]'
  '--mtu[set MTU for datagram TLS]'
  '--srtp-profiles[offer SRTP profiles]'
  {-a,--disable-client-cert}'[do not request a client certificate]'
  {-r,--require-client-cert}'[require a client certificate]'
  '--verify-client-cert[if a client certificate is sent then verify it.]'
  {-b,--heartbeat}'[activate heartbeat support]'
  '--x509fmtder[use DER format for certificates to read from]'
  '--priority[priorities string]'
  '--dhparams[DH params file to use]'
  '--x509cafile[certificate file or PKCS #11 URL to use]'
  '--x509crlfile[CRL file to use]'
  '--pgpkeyfile[PGP Key file to use]'
  '--pgpkeyring[PGP Key ring file to use]'
  '--pgpcertfile[PGP Public Key (certificate) file to use]'
  '--x509keyfile[X.509 key file or PKCS #11 URL to use]'
  '--x509certfile[X.509 Certificate file or PKCS #11 URL to use]'
  '--x509dsakeyfile[alternative X.509 key file or PKCS #11 URL to use]'
  '--x509dsacertfile[alternative X.509 Certificate file or PKCS #11 URL to use]'
  '--x509ecckeyfile[alternative X.509 key file or PKCS #11 URL to use]'
  '--x509ecccertfile[alternative X.509 Certificate file or PKCS #11 URL to use]'
  '--pgpsubkey[PGP subkey to use (hex or auto)]'
  '--srppasswd[SRP password file to use]'
  '--srppasswdconf[SRP password configuration file to use]'
  '--pskpasswd[PSK password file to use]'
  '--pskhint[PSK identity hint to use]'
  '--ocsp-response[the OCSP response to send to client]'
  {-p,--port}'[the port to connect to]'
  {-l,--list}'[print a list of the supported algorithms and modes]'
  '--provider[specify the PKCS #11 provider library]'
  {-v,--version}'[output version information and exit]'
  '(- * :)'{-h,--help}'[display extended usage information and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
