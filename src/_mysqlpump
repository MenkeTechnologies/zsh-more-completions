#compdef mysqlpump

local arguments

arguments=(
  '--print-defaults[print the program argument list and exit.]'
  '--no-defaults[dont read default options from any option file]'
  '--defaults-file[only read default options from the given file #.]'
  '--defaults-extra-file[read this file after the global files are read.]'
  '--defaults-group-suffix[also read groups with concat(group, suffix)]'
  '--login-path[read this path from the login file.]'
  '--add-drop-database[a DROP DATABASE before each CREATE DATABASE.]'
  '--add-drop-table[add a DROP TABLE before each CREATE TABLE.]'
  '--add-drop-user[add a DROP USER before each CREATE USER.]'
  '--add-locks[wrap data inserts on table with write lock on that table]'
  {-A,--all-databases}'[all databases. This is default behaviour if no]'
  '--bind-address[IP address to bind to.]'
  '--character-sets-dir[directory for character set files.]'
  '--column-statistics[an ANALYZE TABLE statement to regenerate any existing]'
  '--complete-insert[use complete insert statements, include column names.]'
  {-C,--compress}'[use compression in server/client protocol.]'
  '--compress-output[compresses all output files with LZ4 or ZLIB compression]'
  {-B,--databases}'[dump selected databases, specified in positional options.]'
  '--all-databases[.]'
  '--debug-check[this is a non-debug version. Catch this and exit.]'
  '--debug-info[this is a non-debug version. Catch this and exit.]'
  '--default-character-set[set the default character set.]'
  '--default-parallelism[specifies number of threads to process each parallel]'
  '--default-auth[default authentication client-side plugin to use.]'
  '--defer-table-indexes[defer addition of indexes of table to be added after all]'
  '--events[dump event scheduler events.]'
  '--exclude-databases[specifies comma-separated list of databases to exclude.]'
  '--exclude-events[specifies comma-separated list of events to exclude.]'
  '--exclude-routines[specifies comma-separated list of stored procedures or]'
  '--exclude-tables[specifies comma-separated list of tables to exclude.]'
  '--exclude-triggers[specifies comma-separated list of triggers to exclude.]'
  '--exclude-users[specifies comma-separated list of users to exclude.]'
  '--extended-insert[allow usage of multiple-row INSERT syntax that include]'
  '--get-server-public-key[get public key from server]'
  '--hex-blob[dump binary strings (in fields of type BINARY, VARBINARY]'
  {-h,--host}'[connect to host.]'
  '--include-databases[specifies comma-separated list of databases and all of]'
  '--include-events[specifies comma-separated list of events to include. If]'
  '--include-routines[specifies comma-separated list of stored procedures or]'
  '--include-tables[specifies comma-separated list of tables to include. If]'
  '--include-triggers[specifies comma-separated list of triggers to include. If]'
  '--include-users[specifies comma-separated list of users to include. If]'
  '--insert-ignore[use INSERT IGNORE INTO for dumped rows instead of INSERT]'
  '--log-error-file[append warnings and errors to specified file.]'
  '--max-allowed-packet[the maximum packet length to send to or receive from]'
  '--net-buffer-length[the buffer size for TCP/IP and socket communication.]'
  '--no-create-db[suppress CREATE DATABASE statements.]'
  {-t,--no-create-info}'[suppress CREATE TABLE statements.]'
  '--parallel-schemas[\[N:\]<list of: schema_name separated with ,>. Process]'
  {-p,--password}'[password to use when connecting to server. If password is]'
  '--plugin-dir[directory for client-side plugins.]'
  {-P,--port}'[port number to use for connection.]'
  '--protocol[the protocol to use for connection (tcp, socket, pipe]'
  '--replace[use REPLACE INTO for dumped rows instead of INSERT INTO.]'
  '--result-file[direct all output generated for all objects to a given]'
  '--routines[dump stored procedures and functions.]'
  '--server-public-key-path[path to file containing server public key]'
  '--set-charset[add SET NAMES default_character_set to the output to]'
  '--set-gtid-purged[add SET @@GLOBAL.GTID_PURGED to the output. Possible]'
  '--single-transaction[creates a consistent snapshot by dumping all tables in a]'
  '--single-transaction[is in process, to ensure a]'
  '--add-locks[.]'
  '--skip-definer[skip DEFINER and SQL SECURITY clauses for Views and]'
  {-d,--skip-dump-rows}'[skip dumping rows of all tables to output.]'
  {-S,--socket}'[the socket file to use for connection.]'
  '--ssl-ca[CA file in PEM format.]'
  '--ssl-capath[CA directory.]'
  '--ssl-cert[X509 cert in PEM format.]'
  '--ssl-cipher[SSL cipher to use.]'
  '--ssl-crl[certificate revocation list.]'
  '--ssl-crlpath[certificate revocation list path.]'
  '--ssl-fips-mode[SSL fips mode to use.]'
  '--ssl-key[X509 key in PEM format.]'
  '--ssl-mode[SSL connection mode.]'
  '--tls-version[TLS version to use.]'
  '--triggers[dump triggers.]'
  '--tz-utc[SET TIME_ZONE=+00:00 at top of dump to allow dumping of]'
  {-u,--user}'[user for login if not current user.]'
  '--users[dump users with their privileges in GRANT format.]'
  {-V,--version}'[output version information and exit.]'
  '--watch-progress[shows periodically dump process progress information on]'
  '*:filename:_files'
)

_arguments -s $arguments
