#compdef traceroute6.iputils

local arguments

arguments=(
  '-d[use SO_DEBUG socket option]'
  '-i[bind to <device>]'
  '-m[use maximum <hops>]'
  '-n[no dns name resolution]'
  '-p[use destination <port>]'
  '-q[number of probes]'
  '-r[use SO_DONTROUTE socket option]'
  '-s[use source <address>]'
  '-v[verbose output]'
  '-w[time to wait for response]'
  '*:filename:_files'
)

_arguments -s $arguments
