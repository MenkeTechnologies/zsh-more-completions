#compdef blkiomon

local arguments

arguments=(
  '-I[sample interval.]'
  '-h[human-readable output file.]'
  '-b[binary output file.]'
  '-d[output file for data emitted by low level device driver.]'
  '-D[output file for debugging data.]'
  '-Qqm[to message queue using given ID for messages.]'
  '-V[print program version.]'
  '*:filename:_files'
)

_arguments -s $arguments
