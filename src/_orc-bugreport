#compdef orc-bugreport

local arguments

arguments=(
  '--help[show help options]'
  '--verbose[increase debugging messages]'
  '-sse2[disable SSE2]'
  '-sse3[disable SSE3]'
  '-ssse3[disable SSEE3]'
  '-sse41[disable SSE4.1]'
  '-sse42[disable SSE4.2]'
  '-sse4a[disable SSE4a]'
  '-sse5[disable SSE5]'
  '*:filename:_files'
)

_arguments -s $arguments
