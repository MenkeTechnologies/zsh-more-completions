#compdef aumvdown

local arguments

arguments=(
  {-b,--lower-branch-id}'[]'
  {-B,--upper-branch-id}'[]'
  {-i,--interactive}'[]'
  {-k,--keep-upper}'[]'
  {-o,--overwrite-lower}'[]'
  {-r,--allow-ro-lower}'[]'
  {-R,--allow-ro-upper}'[]'
  {-v,--verbose}'[]'
  {-V,--version}'[aufs-util for aufs4.9 20170918]'
  '*:filename:_files'
)

_arguments -s $arguments
