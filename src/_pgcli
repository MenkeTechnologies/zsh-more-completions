#compdef pgcli

# zsh completions for 'pgcli'
# automatically generated with http://github.com/RobSis/zsh-completion-generator
local arguments

arguments=(
  {-h,--host}'[host address of the postgres database.]'
  {-p,--port}'[port number at which the postgres instance is]'
  {-U,--username}'[username to connect to the postgres database.]'
  '--user[username to connect to the postgres database.]'
  {-W,--password}'[force password prompt.]'
  {-w,--no-password}'[never prompt for password.]'
  '--single-connection[do not use a separate connection for completions.]'
  {-v,--version}'[version of pgcli.]'
  {-d,--dbname}'[database name to connect to.]'
  '--pgclirc[location of pgclirc file.]'
  {-D,--dsn}'[use DSN configured into the \[alias_dsn\] section of]'
  '--list-dsn[list of DSN configured into the \[alias_dsn\] section]'
  '--row-limit[set threshold for row limit prompt. Use 0 to disable]'
  '--less-chatty[skip intro on startup and goodbye on exit.]'
  '--prompt[prompt format (Default: "\u@\h:\d> ").]'
  '--prompt-dsn[prompt format for connections using DSN aliases]'
  {-l,--list}'[list available databases, then exit.]'
  '--auto-vertical-output[automatically switch to vertical output mode if the]'
  {--warn,--no-warn}'[warn before running a destructive query.]'
  '--help[show this message and exit.]'
  '*:filename:_files'
)

_arguments -s $arguments
