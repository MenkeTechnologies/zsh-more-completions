#compdef nenscript

local arguments

arguments=(
  '-1[same as --columns=1]'
  '-2[same as --columns=2]'
  '--columns[specify the number of columns per page]'
  {-a,--pages}'[specify which pages are printed]'
  {-A,--file-align}'[align separate input files to ALIGN]'
  {-b,--header}'[set page header]'
  {-B,--no-header}'[no page headers]'
  {-c,--truncate-lines}'[cut long lines (default is to wrap)]'
  {-C,--line-numbers}'[precede each line with its line number]'
  '-d[an alias for option --printer]'
  {-D,--setpagedevice}'[pass a page device definition to output]'
  {-e,--escapes}'[enable special escape interpretation]'
  {-E,--highlight}'[highlight source code]'
  {-f,--font}'[use font NAME for body text]'
  {-F,--header-font}'[use font NAME for header texts]'
  {-g,--print-anyway}'[nothing (compatibility option)]'
  '-G[same as --fancy-header]'
  '--fancy-header[select fancy page header]'
  {-h,--no-job-header}'[suppress the job header page]'
  {-H,--highlight-bars}'[specify how high highlight bars are]'
  {-i,--indent}'[set line indent to NUM characters]'
  {-I,--filter}'[read input files through input filter CMD]'
  {-j,--borders}'[print borders around columns]'
  '-J[an alias for option --title]'
  {-k,--page-prefeed}'[enable page prefeed]'
  {-K,--no-page-prefeed}'[disable page prefeed]'
  {-l,--lineprinter}'[simulate lineprinter, this is an alias for:]'
  '--lines-per-page[6, --no-header, --portrait]'
  '--columns[]'
  {-L,--lines-per-page}'[specify how many lines are printed on each page]'
  {-m,--mail}'[send mail upon completion]'
  {-M,--media}'[use output media NAME]'
  {-n,--copies}'[print NUM copies of each page]'
  {-N,--newline}'[select the newline character. Possible]'
  '-o[an alias for option --output]'
  {-O,--missing-characters}'[list missing characters]'
  {-p,--output}'[leave output to file FILE. If FILE is `-]'
  {-P,--printer}'[print output to printer NAME]'
  {-q,--quiet,--silent}'[be really quiet]'
  {-r,--landscape}'[print in landscape mode]'
  {-R,--portrait}'[print in portrait mode]'
  {-s,--baselineskip}'[set baselineskip to NUM]'
  {-S,--statusdict}'[pass a statusdict definition to the output]'
  {-t,--title}'[set banner pages job title to TITLE. Option]'
  {-T,--tabsize}'[set tabulator size to NUM]'
  {-u,--underlay}'[print TEXT under every page]'
  {-U,--nup}'[print NUM logical pages on each output page]'
  {-v,--verbose}'[tell what we are doing]'
  {-V,--version}'[print version number]'
  {-w,--language}'[set output language to LANG]'
  {-W,--option}'[OPTION pass option OPTION to helper application APP]'
  {-X,--encoding}'[use input encoding NAME]'
  {-z,--no-formfeed}'[do not interpret form feed characters]'
  {-Z,--pass-through}'[pass through PostScript and PCL files]'
  '--color[create color outputs with states]'
  '--continuous-page-numbers[count page numbers across input files. Dont]'
  '--download-font[download font NAME]'
  '--extended-return-values[enable extended return values]'
  '--filter-stdin[specify how stdin is shown to the input filter]'
  '--footer[set page footer]'
  '--h-column-height[set the horizontal column height to HEIGHT]'
  '--help[print this help and exit]'
  '--help-highlight[describe all supported --highlight languages]'
  '--highlight-bar-gray[print highlight bars with gray NUM (0 - 1)]'
  '--list-media[list names of all known media]'
  '--margins[adjust page marginals]'
  '--mark-wrapped-lines[mark wrapped lines in the output with STYLE]'
  '--non-printable-format[specify how non-printable chars are printed]'
  '--nup-columnwise[layout pages in the N-up printing columnwise]'
  '--nup-xpad[set the page x-padding of N-up printing to NUM]'
  '--nup-ypad[set the page y-padding of N-up printing to NUM]'
  '--page-label-format[set page label format to FMT]'
  '--ps-level[set the PostScript language level that enscript]'
  '--printer-options[pass extra options to the printer command]'
  '--rotate-even-pages[rotate even-numbered pages 180 degrees]'
  '--slice[print vertical slice NUM]'
  '--style[use highlight style STYLE]'
  '--swap-even-page-margins[swap left and right side margins for each even]'
  '--toc[print table of contents]'
  '--ul-angle[set underlay texts angle to ANGLE]'
  '--ul-font[print underlays with font NAME]'
  '--ul-gray[print underlays with gray value NUM]'
  '--ul-position[set underlays starting position to POS]'
  '--ul-style[print underlays with style STYLE]'
  '--word-wrap[wrap long lines from word boundaries]'
  '*:filename:_files'
)

_arguments -s $arguments
