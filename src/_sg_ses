#compdef sg_ses

local arguments

arguments=(
  '--byte1[1|-b B1 byte 1 (2nd byte) of control page set to B1]'
  '--clear[STR clear field by acronym or position]'
  '--control[|-c send control information (def: fetch status)]'
  '--data[H...|-d H,H... string of ASCII hex bytes for control pages]'
  '--data[| -d - fetch string of ASCII hex bytes from stdin]'
  '--descriptor[DN descriptor name, alternative to --index=...]'
  '--enumerate[|-e enumerate page names + element types (ignore]'
  '--filter[|-f filter out enclosure status flags that are clear]'
  '--get[STR get value of field by acronym or position]'
  '--help[|-h print out usage message]'
  '--hex[|-H print page response (or field) in hex]'
  '--index[IIA individual index (-1 for overall) or element]'
  '--index[II|-I TIA,II comma separated pair: TIA is type header]'
  '--inner-hex[|-i print innermost level of a status page in hex]'
  '--join[|-j group Enclosure Status, Element Descriptor]'
  '--list[|-l same as --enumerate option]'
  '--nickname[SEN SEN is new subenclosure nickname]'
  '--nickid[SEID SEID is subenclosure identifier (def: 0)]'
  '--page[PG diagnostic page code (abbreviation or number)]'
  '--raw[|-r print status page in ASCII hex suitable for -d;]'
  '--set[STR set value of field by acronym or position]'
  '--status[|-s fetch status information (default action)]'
  '--verbose[|-v increase verbosity]'
  '--version[|-V print version string and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
