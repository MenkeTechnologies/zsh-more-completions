#compdef dartaotruntime

local arguments

arguments=(
  '--enable-asserts[enable assert statements.]'
  {--help,-h}'[display this message (add -v or --verbose for information about]'
  '--package-root[or -p<path>]'
  '--packages[where to find a package spec file.]'
  '--observe[/<bind-address>\]\]]'
  '--enable-vm-service[/<bind-address>\]\]]'
  '--pause-isolates-on-exit[]'
  '--pause-isolates-on-unhandled-exceptions[]'
  '--warn-on-pause-with-no-debugger[this set is subject to change.]'
  '--snapshot-kind[]'
  '--snapshot[these snapshot options are used to generate a snapshot of the loaded]'
  '--version[print the VM version.]'
  '*:filename:_files'
)

_arguments -s $arguments
