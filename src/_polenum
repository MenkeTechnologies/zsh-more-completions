#compdef polenum

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this help message and exit]'
  {--username,-u}'[the specified username]'
  {--password,-p}'[the password of the user]'
  {--domain,-d}'[the domain or IP]'
  '--protocols[\[PROTOCOLS ...\]\]]'
  '*:filename:_files'
)

_arguments -s $arguments
