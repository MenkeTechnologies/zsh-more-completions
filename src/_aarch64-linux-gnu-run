#compdef aarch64-linux-gnu-run

local arguments

arguments=(
  '--watch-cycles-ACTION[+\]COUNT]'
  '--watch-pc-ACTION[!\]ADDRESS]'
  '--watch-clock-ACTION[+\]MILLISECONDS]'
  '--watch-delete[delete a watchpoint]'
  '--watch-info[list scheduled watchpoints]'
  {--memory-delete,--delete-memory}'[delete memory at ADDRESS (all addresses)]'
  '--memory-region[SIZE\[,MODULO\]]'
  '--memory-alias[SIZE{,ADDRESS}]'
  '--memory-size[bytes, Kb (k suffix), Mb (m suffix) or Gb (g suffix)\]]'
  '--memory-fill[fill subsequently added memory regions]'
  '--memory-clear[clear subsequently added memory regions]'
  '--memory-mapfile[memory-map next memory region from file]'
  {--memory-info,--info-memory}'[list configurable memory regions]'
  '--map-info[list mapped regions]'
  {-p,--profile}'[perform profiling]'
  '--profile-insn[perform instruction profiling]'
  '--profile-memory[perform memory profiling]'
  '--profile-core[perform CORE profiling]'
  '--profile-model[perform model profiling]'
  '--profile-cpu-frequency[FREQUENCY]'
  '--profile-file[NAME    Specify profile output file]'
  '--profile-pc[perform PC profiling]'
  '-F[PROFILE FREQUENCY, --profile-pc-frequency PC PROFILE FREQUENCY]'
  '-S[PROFILE SIZE, --profile-pc-size PC PROFILE SIZE]'
  '--profile-pc-granularity[PROFILE GRANULARITY]'
  '--profile-pc-range[BOUND]'
  {-t,--trace}'[trace useful things]'
  '--trace-insn[perform instruction tracing]'
  '--trace-disasm[disassemble instructions (slower, but more]'
  '--trace-decode[trace instruction decoding]'
  '--trace-extract[trace instruction extraction]'
  '--trace-linenum[perform line number tracing (implies]'
  '--trace-insn[)]'
  '--trace-memory[trace memory operations]'
  '--trace-alu[trace ALU (Arithmetic Logic Unit) operations]'
  '--trace-fpu[trace FPU (Floating Point Unit) operations]'
  '--trace-vpu[trace VPU (Vector Processing Unit) operations]'
  '--trace-branch[trace branching]'
  '--trace-semantics[perform ALU, FPU, VPU, MEMORY, and BRANCH tracing]'
  '--trace-model[include model performance data]'
  '--trace-core[trace core operations]'
  '--trace-events[trace events]'
  '--trace-syscall[trace system calls]'
  '--trace-register[trace cpu register accesses]'
  '--trace-debug[add information useful for debugging the]'
  '--trace-file[NAME      Specify tracing output file]'
  '--model[specify model to simulate]'
  {--model-info,--info-model}'[list selectable models]'
  {-v,--verbose}'[verbose output]'
  {-E,--endian}'[set endianness]'
  '--environment[set running environment]'
  '--alignment[set memory access alignment]'
  {-D,--debug}'[print debugging messages]'
  '--debug-insn[print instruction debugging messages]'
  '--debug-file[NAME      Specify debugging output file]'
  '--do-command[]'
  {-H,--help}'[print help information]'
  '--version[print version information]'
  '--architecture[specify the architecture to use]'
  {--architecture-info,--info-architecture}'[list supported architectures]'
  '--target[specify the object-code format for the object]'
  '--load-[{lma,vma}            Use VMA or LMA addresses when loading image]'
  '--sysroot[root for system calls with absolute file-names]'
  '*:filename:_files'
)

_arguments -s $arguments
