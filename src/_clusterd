#compdef clusterd

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this help message and exit]'
  '-i[address\] Server address]'
  '-iL[server list]'
  '-p[server port]'
  '--proxy[//server:port\]]'
  '--proxy-auth[proxy credentials]'
  '--timeout[connection timeout \[5s\]]'
  '--random-agent[use a random User-Agent for requests]'
  '--ssl[force SSL]'
  '-a[2|glassfish\]]'
  '-o[hint at remote host OS]'
  '-v[specific version to test]'
  '--usr-auth[login credentials for service]'
  '--fingerprint[fingerprint the remote system]'
  '--arch[86|x64\] Specify remote OS architecture]'
  '--delay[delay N seconds between each attempt]'
  '--deploy[deploy to the discovered service]'
  '--undeploy[undeploy file from server]'
  '--deployer[specify a deployer to use]'
  '--invoke[invoke payload after deployment]'
  '--rand-payload[use a random name for the deployed file]'
  '-b[brute force credentials for user \[admin\]]'
  '--wordlist[wordlist for brute forcing passwords]'
  '--deployer-list[list all available deployers]'
  '--aux-list[list all available exploits]'
  '--gen-payload[for reverse connection]'
  '--discover[attempt to discover application servers using the]'
  '--listen[adapter to listen on when needed]'
  '-d[enable debug output]'
  '-l[log output to file \[$time$_log.log\]]'
  '*:filename:_files'
)

_arguments -s $arguments
