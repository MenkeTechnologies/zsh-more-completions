#compdef uvicorn

local arguments

arguments=(
  '--host[bind socket to this host.  \[default:]'
  '--port[bind socket to this port.  \[default: 8000\]]'
  '--uds[bind to a UNIX domain socket.]'
  '--fd[bind to socket from this file descriptor.]'
  '--reload[enable auto-reload.]'
  '--reload-dir[set reload directories explicitly, instead]'
  '--reload-include[set glob patterns to include while watching]'
  '--reload-exclude[set glob patterns to exclude while watching]'
  '--reload-delay[delay between previous and next check if]'
  '--workers[number of worker processes. Defaults to the]'
  '--loop[event loop implementation.  \[default: auto\]]'
  '--http[11|httptools\]     HTTP protocol implementation.  \[default:]'
  '--ws[webSocket protocol implementation.]'
  '--ws-max-size[webSocket max size message in bytes]'
  '--ws-ping-interval[webSocket ping interval  \[default: 20.0\]]'
  '--ws-ping-timeout[webSocket ping timeout  \[default: 20.0\]]'
  '--ws-per-message-deflate[webSocket per-message-deflate compression]'
  '--lifespan[lifespan implementation.  \[default: auto\]]'
  '--interface[3|asgi2|wsgi\]]'
  '--env-file[environment configuration file.]'
  '--log-config[logging configuration file. Supported]'
  '--log-level[log level. \[default: info\]]'
  {--access-log,--no-access-log}'[enable/Disable access log.]'
  {--use-colors,--no-use-colors}'[enable/Disable colorized logging.]'
  {--proxy-headers,--no-proxy-headers}'[enable/Disable X-Forwarded-Proto]'
  {--server-header,--no-server-header}'[enable/Disable default Server header.]'
  {--date-header,--no-date-header}'[enable/Disable default Date header.]'
  '--forwarded-allow-ips[comma seperated list of IPs to trust with]'
  '--root-path[set the ASGI root_path for applications]'
  '--limit-concurrency[maximum number of concurrent connections or]'
  '--backlog[maximum number of connections to hold in]'
  '--limit-max-requests[maximum number of requests to service before]'
  '--timeout-keep-alive[close Keep-Alive connections if no new data]'
  '--ssl-keyfile[SSL key file]'
  '--ssl-certfile[SSL certificate file]'
  '--ssl-keyfile-password[SSL keyfile password]'
  '--ssl-version[SSL version to use (see stdlib ssl modules)]'
  '--ssl-cert-reqs[whether client certificate is required (see]'
  '--ssl-ca-certs[CA certificates file]'
  '--ssl-ciphers[ciphers to use (see stdlib ssl modules)]'
  '--header[specify custom default HTTP response headers]'
  '--version[display the uvicorn version and exit.]'
  '--app-dir[look for APP in the specified directory, by]'
  '--factory[treat APP as an application factory, i.e. a]'
  '--help[show this message and exit.]'
  '*:filename:_files'
)

_arguments -s $arguments
