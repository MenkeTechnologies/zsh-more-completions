#compdef platex-dev

local arguments

arguments=(
  '-cnf-line[parse STRING as a configuration file line]'
  '-etex[enable e-TeX extensions]'
  '-fmt[use NAME instead of program name or %&format.]'
  '-halt-on-error[stop processing at the first error]'
  '-ini[be iniptex.]'
  '-interaction[set interaction mode (STRING=batchmode|nonstopmode|]'
  '-ipc[send DVI output to a socket as well as the usual]'
  '-ipc-start[as -ipc, and also start the server at the other end]'
  '-jobname[set the job name to STRING]'
  '-kanji[set Japanese encoding (STRING=euc|jis|sjis|utf8)]'
  '-kanji-internal[set Japanese internal encoding (STRING=euc|sjis)]'
  '-kpathsea-debug[set path searching debugging flags according to]'
  '-mltex[enable MLTeX extensions such as \charsubdef]'
  '-output-comment[use STRING for DVI file comment instead of date]'
  '-output-directory[use existing DIR as the directory to write files in]'
  '-progname[set program (and fmt) name to STRING]'
  '-recorder[enable filename recorder]'
  '-shell-restricted[enable restricted \write18]'
  '-src-specials[insert source specials into the DVI file]'
  '-src-specials[insert source specials in certain places of]'
  '-synctex[generate SyncTeX data for previewers according to]'
  '-translate-file[use the TCX file TCXNAME]'
  '-help[print this message and exit.]'
  '-version[print version information and exit.]'
  '*:filename:_files'
)

_arguments -s $arguments
