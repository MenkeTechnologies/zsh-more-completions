#compdef rejarForAnalysis

local arguments

arguments=(
  '-analyzeOnly[only read the jars files and analyze them; dont produce new jar files]'
  '-maxAge[maximum age in days (ignore jar files older than this)]'
  '-inputFileList[text file containing names of jar files]'
  '-auxFileList[text file containing names of jar files for aux class path]'
  '-maxClasses[maximum number of classes per analysis*.jar file]'
  '-outputDir[directory for the generated jar files]'
  '-ignoreTimestamps[ignore timestamps on zip entries; use first version found]'
  '-prefix[name prefix> comma separated list of class name prefixes that should be analyzed (e.g., edu.umd.cs.)]'
  '-exclude[name prefix> comma separated list of class name prefixes that should be excluded from both analyze and auxilary jar files (e.g., java.)]'
  '-excludePattern[name pattern(s)> comma separated list of regular expressions; all classes matching them are excluded]'
  '*:filename:_files'
)

_arguments -s $arguments
