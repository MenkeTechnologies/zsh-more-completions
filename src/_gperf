#compdef gperf

local arguments

arguments=(
  '--output-file[write output to specified file.]'
  {-e,--delimiters}'[allow user to provide a string containing delimiters]'
  {-t,--struct-type}'[allows the user to include a structured type]'
  '--ignore-case[consider upper and lower case ASCII characters as]'
  {-L,--language}'[generates code in the specified language. Languages]'
  {-K,--slot-name}'[select name of the keyword component in the keyword]'
  {-F,--initializer-suffix}'[initializers for additional components in the keyword]'
  {-H,--hash-function-name}'[specify name of generated hash function. Default is]'
  {-N,--lookup-function-name}'[specify name of generated lookup function. Default]'
  {-Z,--class-name}'[specify name of generated C++ class. Default name is]'
  {-7,--seven-bit}'[assume 7-bit characters.]'
  {-l,--compare-lengths}'[compare key lengths before trying a string]'
  {-c,--compare-strncmp}'[generate comparison code using strncmp rather than]'
  {-C,--readonly-tables}'[make the contents of generated lookup tables]'
  {-E,--enum}'[define constant values using an enum local to the]'
  {-I,--includes}'[include the necessary system include file <string.h>]'
  {-G,--global-table}'[generate the static table of keywords as a static]'
  {-P,--pic}'[optimize the generated table for inclusion in shared]'
  {-Q,--string-pool-name}'[specify name of string pool generated by option --pic.]'
  '--null-strings[use NULL strings instead of empty strings for empty]'
  {-W,--word-array-name}'[specify name of word list array. Default name is]'
  '--length-table-name[specify name of length table array. Default name is]'
  {-S,--switch}'[causes the generated C code to use a switch]'
  {-T,--omit-struct-type}'[prevents the transfer of the type declaration to the]'
  '--size-type[specify the type for length parameters. Default type is]'
  {-k,--key-positions}'[select the key positions used in the hash function.]'
  {-D,--duplicates}'[handle keywords that hash to duplicate values. This]'
  {-m,--multiple-iterations}'[perform multiple choices of the -i and -j values]'
  {-i,--initial-asso}'[provide an initial value for the associate values]'
  {-j,--jump}'[affects the "jump value", i.e., how far to advance]'
  {-n,--no-strlen}'[do not include the length of the keyword when]'
  {-r,--random}'[utilizes randomness to initialize the associated]'
  {-s,--size-multiple}'[affects the size of the generated hash table. The]'
  '(- * :)'{-h,--help}'[print this message.]'
  {-v,--version}'[print the gperf version number.]'
  {-d,--debug}'[enables the debugging option (produces verbose]'
  '*:filename:_files'
)

_arguments -s $arguments
