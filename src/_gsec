#compdef gsec

local arguments

arguments=(
  '-user[administrator name>]'
  '-password[administrator password>]'
  '-fetch[_password <file to fetch password from>]'
  '-role[administrator SQL role name>]'
  '-trusted[trusted authentication)]'
  '-database[to manage>]'
  '-z[]'
  '-pw[]'
  '-uid[]'
  '-gid[]'
  '-fname[]'
  '-mname[]'
  '-lname[]'
  '-admin[}]'
  '*:filename:_files'
)

_arguments -s $arguments
