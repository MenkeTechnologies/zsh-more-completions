#compdef fax2ps

local arguments

arguments=(
  '-w[suppress warning messages]'
  '-l[set maximum output line length for generated PostScript]'
  '-p[select page to print (can use multiple times)]'
  '-x[set default horizontal resolution of input data (dpi)]'
  '-y[set default vertical resolution of input data (lpi)]'
  '-S[scale output to page size]'
  '-W[set output page width (inches), default is 8.5]'
  '-H[set output page height (inches), default is 11]'
  '*:filename:_files'
)

_arguments -s $arguments
