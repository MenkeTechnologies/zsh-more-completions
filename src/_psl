#compdef psl

local arguments

arguments=(
  '--version[show library version information]'
  '--use-latest-data[use the latest PSL data available \[default\]]'
  '--use-builtin-data[use the builtin PSL data]'
  '--no-star-rule[do not apply the prevailing star rule]'
  '--load-psl-file[load PSL data from file]'
  '--is-public-suffix[check if domains are public suffixes \[default\]]'
  '--is-cookie-domain-acceptable[check if cookie-domain is acceptable for domains]'
  '--print-unreg-domain[print the longest public suffix part]'
  '--print-reg-domain[print the shortest private suffix part]'
  '--print-info[print info about library builtin data]'
  '*:filename:_files'
)

_arguments -s $arguments
