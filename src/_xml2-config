#compdef xml2-config

local arguments

arguments=(
  '--prefix[change libxml prefix \[default /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.14.sdk/usr\]]'
  '--exec-prefix[change libxml exec prefix \[default /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.14.sdk/usr\]]'
  '--libs[print library linking information]'
  '--cflags[print pre-processor and compiler flags]'
  '--modules[module support enabled]'
  '--help[display this help and exit]'
  '--version[output version information]'
  '*:filename:_files'
)

_arguments -s $arguments
