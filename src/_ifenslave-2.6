#compdef ifenslave-2.6

local arguments

arguments=(
  {-a,--all-interfaces}'[show information about all interfaces.]'
  {-c,--change-active}'[change active slave.]'
  {-d,--detach}'[removes slave interfaces from the bonding device.]'
  {-f,--force}'[force actions to be taken if one of the specified interfaces]'
  '(- * :)'{-h,--help}'[display a help message and exit.]'
  {-u,--usage}'[show usage information and exit.]'
  {-v,--verbose}'[print warning and debug messages.]'
  {-V,--version}'[show version information and exit.]'
  '*:filename:_files'
)

_arguments -s $arguments
