#compdef ipa-getkeytab

local arguments

arguments=(
  {-q,--quiet}'[print as little as]'
  {-s,--server}'[name Contact this]'
  {-p,--principal}'[service Principal Name The principal to]'
  {-k,--keytab}'[file Name File were to store]'
  {-e,--enctypes}'[separated encryption types list Encryption types to]'
  '--permitted-enctypes[show the list of]'
  {-P,--password}'[asks for a]'
  {-D,--binddn}'[to bind as if not using kerberos LDAP DN]'
  {-w,--bindpw}'[to use if not using kerberos LDAP password]'
  '--cacert[CA certificate Path to the IPA CA]'
  {-H,--ldapuri}'[LDAP uri to connect]'
  '--server[]'
  {-Y,--mech}'[LDAP SASL bind]'
  {-r,--retrieve}'[retrieve current]'
  '--usage[display brief usage]'
  '*:filename:_files'
)

_arguments -s $arguments
