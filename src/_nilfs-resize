  '(- * :)'{-h,--help}'[display this help and exit]'
  {-v,--verbose}'[verbose mode]'
  {-y,--yes,--assume-yes}'[assume Yes to all queries and do not prompt]'
  {-V,--version}'[display version and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
