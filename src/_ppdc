#compdef ppdc

local arguments

arguments=(
  '-D[value Set named variable to value.]'
  '-I[add include directory to search path.]'
  '-c[.po Load the specified message catalog.]'
  '-d[specify the output directory.]'
  '-l[lang,â€¦\] Specify the output language(s) (locale).]'
  '-m[use the ModelName value as the filename.]'
  '-t[test PPDs instead of generating them.]'
  '-v[be verbose.]'
  '-z[compress PPD files using GNU zip.]'
  '--cr[end lines with CR (Mac OS 9).]'
  '--crlf[end lines with CR + LF (Windows).]'
  '--lf[end lines with LF (UNIX/Linux/macOS).]'
  '*:filename:_files'
)

_arguments -s $arguments
