#compdef androiddeployqt

local arguments

arguments=(
  '--input[reads <inputfile> for options generated by]'
  '--deployment[supported deployment mechanisms:]'
  '--aab[: Build an Android App Bundle.]'
  '--no-build[: Do not build the package, it is useful to just install]'
  '--install[: Installs apk to device/emulator. By default this step is]'
  '--reinstall[: Installs apk to device/emulator. By default this step]'
  '--device[ID\]: Use specified device for deployment. Default]'
  '--android-platform[builds against the given android]'
  '--release[: Builds a package ready for release. By default, the]'
  '--sign[/to/keystore> <alias>: Signs the package with the]'
  '--release[.]'
  '--storepass[keystore password.]'
  '--storetype[keystore type.]'
  '--keypass[password for private key (if different]'
  '--sigfile[name of .SF/.DSA file.]'
  '--digestalg[name of digest algorithm. Default is]'
  '--sigalg[name of signature algorithm. Default is]'
  '--tsa[location of the Time Stamping Authority.]'
  '--tsacert[public key certificate for TSA.]'
  '--internalsf[: Include the .SF file inside the signature block.]'
  '--sectionsonly[: Dont compute hash of entire manifest.]'
  '--protected[: Keystore has protected authentication path.]'
  '--jarsigner[: Force jarsigner usage, otherwise apksigner will be]'
  '--jdk[/to/jdk>: Used to find the jarsigner tool when used]'
  '--qml-import-paths[: Specify additional search paths for QML]'
  '--verbose[: Prints out information during processing.]'
  '--no-generated-assets-cache[: Do not pregenerate the entry list for]'
  '--aux-mode[: Operate in auxiliary mode. This will only copy the]'
  '--apk[/where/to/copy/the/apk>: Path where to copy the built apk.]'
  '--qml-importscanner-binary[/to/qmlimportscanner>: Override the]'
  '--help[: Displays this information.]'
  '*:filename:_files'
)

_arguments -s $arguments
