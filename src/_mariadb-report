#compdef mariadb-report

# Autogenerated from man page /usr/share/man/man1/mariadb-report.1.gz

local arguments

arguments=(
    '--help[Output help information and exit.]'
    '--user[.]:USER'
    '--password[As of version 2.]'
    '--host[.]:ADDRESS'
    '--port[.]:PORT'
    '--socket[.]:SOCKET'
    '--no-mycnf[.]'
    '--dtq[Print Distribution of Total Queries (DTQ) report (under Total in Questions re…]'
    '--dms[Print Data Manipulation Statements (DMS) report (under DMS in Questions repor…]'
    '--com[Print top N number of non-DMS Com_ status values in descending order (after D…]:N'
    '--sas[Print report for Select_ and Sort_ status values (after Questions report).]'
    '--tab[Print Threads, Aborted, and Bytes status reports (after Created temp report).]'
    '--qcache[Print Query Cache report.]'
    '--all[Equivalent to "--dtq --dms --com 3 --sas --qcache".]'
    '--infile[Instead of getting SHOW STATUS values from MySQL, read values from FILE.]:FILE'
    '--outfile[After printing the report to screen, print the report to FILE too.]'
    '--email[After printing the report to screen, email the report to ADDRESS.]:ADDRESS'
    '--flush-status[Execute a "FLUSH STATUS;" after generating the reports.]'
    '*:filename:_files'
)
_arguments -s $arguments
