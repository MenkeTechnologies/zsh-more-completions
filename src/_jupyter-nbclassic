#compdef jupyter-nbclassic

local arguments

arguments=(
  '--no-browser[dont open the notebook in a browser after startup.]'
  '--no-mathjax[disable MathJax]'
  '--allow-root[allow the notebook to be run from root user.]'
  '--ip[the IP address the Jupyter server will listen on.]'
  '--port[the port the Jupyter server will listen on.]'
  '--port-retries[the number of additional ports to try if the specified port is not]'
  '--keyfile[the full path to a private key file for usage with SSL/TLS.]'
  '--certfile[the full path to an SSL/TLS certificate file.]'
  '--client-ca[the full path to a certificate authority certificate for SSL/TLS client]'
  '--notebook-dir[DEPRECATED, use root_dir.]'
  '--browser[specify what command to use to invoke a web]'
  '*:filename:_files'
)

_arguments -s $arguments
