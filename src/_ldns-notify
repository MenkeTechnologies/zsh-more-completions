#compdef ldns-notify

local arguments

arguments=(
  '-z[the zone]'
  '-s[SOA version number to include]'
  '-y[TSIG sign the query]'
  '-p[port to use to send to]'
  '-v[print version information]'
  '-d[print verbose debug information]'
  '-r[max number of retries (15)]'
  '-h[print this help information]'
  '*:filename:_files'
)

_arguments -s $arguments
