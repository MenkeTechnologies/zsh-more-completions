#compdef eventlogadm

local arguments

arguments=(
  '-o[Writes records to eventlog from STDIN]:Eventlog Name:'
  '-o[<sourcename> <msgfileDLLname> 	Adds the specified source & DLL eventlog registry entry]:EventlogName:'
  '-o[<starting_record>					Dump stored eventlog entries on STDOUT]:Eventlog Name:'
  '-s[use configuration file <filename>.]'
  '-d[turn debug on]'
  '-h[display help]'
  '*:filename:_files'
)

_arguments -s $arguments
