#compdef eventlogadm

local arguments

arguments=(
  '-o[<Eventlog Name> 					Writes records to eventlog from STDIN]'
  '-o[<EventlogName> <sourcename> <msgfileDLLname> 	Adds the specified source & DLL eventlog registry entry]'
  '-o[<Eventlog Name> <starting_record>					Dump stored eventlog entries on STDOUT]'
  '-s[use configuration file <filename>.]'
  '-d[turn debug on]'
  '-h[display help]'
  '*:filename:_files'
)

_arguments -s $arguments
