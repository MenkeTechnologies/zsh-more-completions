#compdef mount.nfs4

local arguments

arguments=(
  '-r[mount file system readonly]'
  '-v[verbose]'
  '-V[print version]'
  '-w[mount file system read-write]'
  '-f[fake mount, do not actually mount]'
  '-n[do not update /etc/mtab]'
  '-s[tolerate sloppy mount options rather than fail]'
  '-h[print this help]'
  '*:filename:_files'
)

_arguments -s $arguments
