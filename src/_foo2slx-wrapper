#compdef foo2slx-wrapper

local arguments

arguments=(
  '-c[print in color (else monochrome)]'
  '-m[media code to send to printer \[0\]]'
  '-p[paper code \[letter\]]'
  '-n[number of copies \[1\]]'
  '-r[set device resolution in pixels/inch \[600x600\]]'
  '-s[source code to send to printer \[0\]]'
  '-2[/-3/-4/-6/-8/-10/-12/-14/-15/-16/-18]'
  '-o[for N-up: -op is portrait, -ol is landscape, -os is seascape.]'
  '-u[set offset of upper left printable in pixels \[varies\]]'
  '-l[set offset of lower right printable in pixels \[varies\]]'
  '-L[send logical clipping values from -u/-l in ZjStream \[3\]]'
  '-g[additional options to pass to Ghostscript, such as]'
  '-dDITHERPPI[etc. May appear more than once. \[\]]'
  '-G[.icm Convert profile.icm to a Postscript CRD using icc2ps and]'
  '-I[select profile intent from ICM file \[0\]]'
  '-G[.ps Prepend gamma-file to the Postscript input to perform]'
  '-S[output just a single color plane from a color print \[all\]]'
  '-D[set Debug level \[0\]]'
  '-V[$Id: foo2slx-wrapper.in,v 1.22 2011/01/14 23:21:39 rick Exp $]'
  '*:filename:_files'
)

_arguments -s $arguments
