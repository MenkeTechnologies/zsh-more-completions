#compdef sctp_status

local arguments

arguments=(
  {-p,-s}'[case \] \[-d level\]]'
  '-c[packets of specifed size.]'
  '-m[(1500-65515, default value 32768)]'
  '-x[of repeats]'
  '-X[of messages]'
  '-o[0 = all unordered(default)]'
  '-M[(default value 0)]'
  '-D[. If in client mode do a read following send.]'
  '-I[after <n> times of send, default value 1.]'
  '*:filename:_files'
)

_arguments -s $arguments
