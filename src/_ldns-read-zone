#compdef ldns-read-zone

local arguments

arguments=(
  '-b[bubble Babble encoding of DSs.]'
  '-0[timestamps and signature in RRSIG records.]'
  '-c[all rrs in the zone.]'
  '-d[show DNSSEC data from the zone]'
  '-h[this text]'
  '-n[not print the SOA record]'
  '-p[SOA serial with spaces so it takes exactly ten characters.]'
  '-s[DNSSEC data from the zone]'
  '-S[+|-\]<number> | YYYYMMDDxx | unixtime \]]'
  '-v[the version and exits]'
  '-z[the zone (implies -c).]'
  '*:filename:_files'
)

_arguments -s $arguments
