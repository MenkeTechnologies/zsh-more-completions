#compdef animate-im6

local arguments

arguments=(
  '-alpha[on, activate, off, deactivate, set, opaque, copy]'
  '-authenticate[decipher image with this password]'
  '-backdrop[display image centered on a backdrop]'
  '-channel[apply option to select image channels]'
  '-colormap[shared or Private]'
  '-colorspace[alternate image colorspace]'
  '-decipher[convert cipher pixels to plain pixels]'
  '-define[define one or more image format options]'
  '-delay[display the next image after pausing]'
  '-density[horizontal and vertical density of the image]'
  '-depth[image depth]'
  '-display[display image to this X server]'
  '-dispose[layer disposal method]'
  '-dither[apply error diffusion to image]'
  '-filter[use this filter when resizing an image]'
  '-format[" output formatted image characteristics]'
  '-gamma[level of gamma correction]'
  '-geometry[preferred size and location of the Image window]'
  '-gravity[horizontal and vertical backdrop placement]'
  '-identify[identify the format and characteristics of the image]'
  '-immutable[displayed image cannot be modified]'
  '-interlace[type of image interlacing scheme]'
  '-interpolate[pixel color interpolation method]'
  '-limit[value pixel cache resource limit]'
  '-loop[loop images then exit]'
  '-matte[store matte channel if the image has one]'
  '-map[display image using this Standard Colormap]'
  '-monitor[monitor progress]'
  '-pause[seconds to pause before reanimating]'
  '-page[size and location of an image canvas (setting)]'
  '-quantize[reduce colors in this colorspace]'
  '-quiet[suppress all warning messages]'
  '-regard-warnings[pay attention to warning messages]'
  '-remote[execute a command in an remote display process]'
  '-respect-parentheses[remain in effect until parenthesis boundary]'
  '-sampling-factor[horizontal and vertical sampling factor]'
  '-scenes[image scene range]'
  '-seed[seed a new sequence of pseudo-random numbers]'
  '-set[value set an image attribute]'
  '-size[width and height of image]'
  '-support[resize support: > 1.0 is blurry, < 1.0 is sharp]'
  '-transparent-color[transparent color]'
  '-treedepth[color tree depth]'
  '-verbose[print detailed information about the image]'
  '-visual[display image using this visual type]'
  '-virtual-pixel[virtual pixel access method]'
  '-window[display image to background of this window]'
  '-colors[preferred number of colors in the image]'
  '-crop[preferred size and location of the cropped image]'
  '-extract[extract area from image]'
  '-monochrome[transform image to black and white]'
  '-repage[size and location of an image canvas (operator)]'
  '-resample[change the resolution of an image]'
  '-resize[resize the image]'
  '-rotate[apply Paeth rotation to the image]'
  '-strip[strip image of all profiles and comments]'
  '-thumbnail[create a thumbnail of the image]'
  '-trim[trim image edges]'
  '-coalesce[merge a sequence of images]'
  '-flatten[flatten a sequence of images]'
  '-debug[display copious debugging information]'
  '-help[print program options]'
  '-list[print a list of supported option arguments]'
  '-log[format of debugging information]'
  '-version[print version information]'
  {-borderwidth,-font,-foreground,-iconGeometry,-iconic,-name}'[]'
  {-mattecolor,-shared-memory}'[or -title.]'
  '*:filename:_files'
)

_arguments -s $arguments
