#compdef m2300w

local arguments

arguments=(
  '-i[filename to read from]'
  '-o[filename to write into file]'
  '-c[B/W or Color Mode \[no default\]]'
  '-m[media code: \[0\]]'
  '-p[paper code: \[4\]]'
  '-r[resolution mode: \[1\]]'
  '-s[save Toner]'
  '-v[verbose level \[0\]]'
  '*:filename:_files'
)

_arguments -s $arguments
