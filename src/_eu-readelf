#compdef eu-readelf

local arguments

arguments=(
  '--elf-section[use the named SECTION (default .gnu_debugdata)]'
  {-a,--all}'[all these plus -p .strtab -p .dynstr -p .comment]'
  {-A,--arch-specific}'[display architecture specific information, if any]'
  {-d,--dynamic}'[display the dynamic segment]'
  {-e,--exception}'[display sections for exception handling]'
  {-h,--file-header}'[display the ELF file header]'
  {-I,--histogram}'[display histogram of bucket list lengths]'
  {-l,--program-headers}'[display the program headers]'
  {-n,--notes}'[display the ELF notes]'
  {-r,--relocs}'[display relocations]'
  {-s,--symbols}'[display the symbol table sections]'
  {-S,--section-headers}'[display the sections headers]'
  {-V,--version-info}'[display versioning information]'
  {-c,--archive-index}'[display the symbol index of an archive]'
  {-p,--strings}'[print string contents of sections]'
  {-w,--debug-dump}'[display DWARF section content. SECTION can be one]'
  {-x,--hex-dump}'[dump the uninterpreted contents of SECTION, by]'
  {-N,--numeric-addresses}'[do not find symbol names for addresses in DWARF]'
  {-U,--unresolved-address-offsets}'[display just offsets instead of resolving values]'
  {-W,--wide}'[ignored for compatibility (lines always wide)]'
  {-z,--decompress}'[show compression information for compressed]'
  '--usage[give a short usage message]'
  '--version[print program version]'
  '*:filename:_files'
)

_arguments -s $arguments
