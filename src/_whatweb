#compdef whatweb

local arguments

arguments=(
  {--input-file,-i}'[read targets from a file. You can pipe]'
  '--url-prefix[add a prefix to target URLs.]'
  '--url-suffix[add a suffix to target URLs.]'
  '--url-pattern[insert the targets into a URL.]'
  {--aggression,-a}'[set the aggression level. Default: 1.]'
  {--user-agent,-U}'[identify as AGENT instead of WhatWeb/0.4.9.]'
  {--header,-H}'[add an HTTP header. eg "Foo:Bar". Specifying a]'
  '--follow-redirect[control when to follow redirects. WHEN may be]'
  '--max-redirects[maximum number of redirects. Default: 10.]'
  {--user,-u}'[HTTP basic authentication.]'
  {--cookie,-c}'[use cookies, e.g. name=value; name2=value2.]'
  '--proxy[Set proxy hostname and port.]:hostname\[:port\]:'
  '--proxy-user[Set proxy user and password.]:username:password:'
  {--list-plugins,-l}'[list all plugins.]'
  {--info-plugins,-I}'[list all plugins with detailed information.]'
  '--search-plugins[search plugins for a keyword.]'
  {--plugins,-p}'[select plugins. LIST is a comma delimited set]'
  '-p[is a shortcut for -p +plugins-disabled.]'
  {--grep,-g}'[search for STRING in HTTP responses. Reports]'
  '--custom-plugin[define a custom plugin named Custom-Plugin]'
  '--dorks[list Google dorks for the selected plugin.]'
  {--verbose,-v}'[verbose output includes plugin descriptions.]'
  {--colour,--color}'[control whether colour is used. WHEN may be]'
  {--quiet,-q}'[do not display brief logging to STDOUT.]'
  '--no-errors[suppress error messages.]'
  '--log-brief[log brief, one-line output.]'
  '--log-verbose[log verbose output.]'
  '--log-errors[log errors.]'
  '--log-xml[log XML format.]'
  '--log-json[log JSON format.]'
  '--log-sql[log SQL INSERT statements.]'
  '--log-sql-create[create SQL database tables.]'
  '--log-json-verbose[log JSON Verbose format.]'
  '--log-magictree[log MagicTree XML format.]'
  '--log-object[log Ruby object inspection format.]'
  '--log-mongo-database[name of the MongoDB database.]'
  '--log-mongo-collection[name of the MongoDB collection.]'
  '--log-mongo-host[mongoDB hostname or IP address.]'
  '--log-mongo-username[mongoDB username. Default: nil.]'
  '--log-mongo-password[mongoDB password. Default: nil.]'
  '--log-elastic-index[name of the index to store results. Default: whatweb]'
  '--log-elastic-host[host:port of the elastic http interface. Default: 127.0.0.1:9200]'
  {--max-threads,-t}'[number of simultaneous threads. Default: 25.]'
  '--open-timeout[time in seconds. Default: 15.]'
  '--read-timeout[time in seconds. Default: 30.]'
  '--wait[wait SECONDS between connections.]'
  '--short-help[short usage help.]'
  {--help,-h}'[complete usage help.]'
  '--debug[raise errors in plugins.]'
  '--version[display version information.]'
  '--url-suffix[.xml -p crossdomain_xml]'
  '*:filename:_files'
)

_arguments -s $arguments
