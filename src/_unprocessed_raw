#compdef unprocessed_raw

local arguments

arguments=(
  '-q[- be quiet]'
  '-s[- select Nth image in file (default=0)]'
  '-g[- use gamma correction with gamma 2.2 (not precise,use for visual inspection only)]'
  '-A[- autoscaling (by integer factor)]'
  '-T[- write tiff instead of pgm]'
  '*:filename:_files'
)

_arguments -s $arguments
