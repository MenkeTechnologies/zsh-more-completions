#compdef regfexport

local arguments

arguments=(
  '-c[:     codepage of ASCII strings, options: ascii, windows-874]'
  '-h[:     shows this help]'
  '-K[:     show information about a specific key path.]'
  '-l[:     logs information about the exported items]'
  '-v[:     verbose output to stderr]'
  '-V[:     print version]'
  '*:filename:_files'
)

_arguments -s $arguments
