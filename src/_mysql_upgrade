#compdef mysql_upgrade

local arguments

arguments=(
  '--print-defaults[print the program argument list and exit.]'
  '--no-defaults[dont read default options from any option file]'
  '--defaults-file[only read default options from the given file #.]'
  '--defaults-extra-file[read this file after the global files are read.]'
  '--defaults-group-suffix[also read groups with concat(group, suffix)]'
  '--login-path[read this path from the login file.]'
  '--bind-address[IP address to bind to.]'
  '--character-sets-dir[directory for character set files.]'
  {-C,--compress}'[use compression in server/client protocol.]'
  '--debug-check[this is a non-debug version. Catch this and exit.]'
  '--debug-info[this is a non-debug version. Catch this and exit.]'
  '--default-character-set[set the default character set.]'
  '--default-auth[default authentication client-side plugin to use.]'
  {-f,--force}'[force execution of SQL statements even if mysql_upgrade]'
  '--get-server-public-key[get public key from server]'
  {-h,--host}'[connect to host.]'
  '--max-allowed-packet[the maximum packet length to send to or receive from]'
  '--net-buffer-length[the buffer size for TCP/IP and socket communication.]'
  {-p,--password}'[password to use when connecting to server. If password is]'
  '--plugin-dir[directory for client-side plugins.]'
  {-P,--port}'[port number to use for connection.]'
  '--protocol[the protocol to use for connection (tcp, socket, pipe]'
  '--server-public-key-path[path to file containing server public key]'
  '--skip-sys-schema[do not upgrade/install the sys schema.]'
  {-S,--socket}'[the socket file to use for connection.]'
  '--ssl-ca[CA file in PEM format.]'
  '--ssl-capath[CA directory.]'
  '--ssl-cert[X509 cert in PEM format.]'
  '--ssl-cipher[SSL cipher to use.]'
  '--ssl-crl[certificate revocation list.]'
  '--ssl-crlpath[certificate revocation list path.]'
  '--ssl-fips-mode[SSL fips mode to use.]'
  '--ssl-key[X509 key in PEM format.]'
  '--ssl-mode[SSL connection mode.]'
  '--tls-version[TLS version to use.]'
  {-s,--upgrade-system-tables}'[only upgrade the system tables, do not try to upgrade the]'
  {-u,--user}'[user for login if not current user.]'
  {-v,--verbose}'[display more output about the process.]'
  {-V,--version}'[output version information and exit.]'
  {-k,--version-check}'[this program only if its server version matches the]'
  '--write-binlog[write all executed SQL statements to binary log. Disabled]'
  '*:filename:_files'
)

_arguments -s $arguments
