local arguments

arguments=(
    {-i,--in}'[JSON formatted input infile.]'
    {-m,--minimal}'[Produce JSON output with all superfluous whitespace removed from the resultin…]'
    {-o,--out}'[Formatted output is written to the named outfile .]'
    {-p,--pretty}'[Produce JSON output in a human-readable format.]'
    {-q,--quiet}'[Verify the input as valid JSON only, no output is produced.]'
    {-V,--version}'[Display version number and exit.]'
    {-y,--yaml}'[Produce YAML-like output, a human-readable format with less syntactic sugar t…]'
    {-?,--help}'[Display usage message and exit.]'
    '*:filename:_files'
)
