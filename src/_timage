local arguments

arguments=(
  '-t[generate rectangular gamut boundary test chart]'
  '-p[generate a colorspace step chart with L* steps^2]'
  '-r[resolution in DPI (default 200)]'
  '-s[smooth blend]'
  '-x[16 bit output]'
  '-4[CMYK output]'
  '-g[percentage towards grey (default 0%)]'
  '-P[save as PNG file (deffault TIFF)]'
  '*:filename:_files'
)

_arguments -s $arguments
