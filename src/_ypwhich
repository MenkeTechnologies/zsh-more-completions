#compdef ypwhich

local arguments

arguments=(
  '-d[use domain instead of the default domain]'
  '-m[find the master NIS server for the map mname]'
  '-t[inhibits map nickname translation]'
  '-V[version of ypbind, V3 is default]'
  '-x[display the map nickname translation table]'
  '--verbose[verbose output of result]'
  '--usage[give a short usage message]'
  '--version[print program version]'
  '*:filename:_files'
)

_arguments -s $arguments
