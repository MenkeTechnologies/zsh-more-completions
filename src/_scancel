#compdef scancel

local arguments

arguments=(
  {-A,--account}'[act only on jobs charging this account]'
  {-b,--batch}'[signal batch shell for specified job]'
  {-f,--full}'[signal batch shell and all steps for specified job]'
  {-H,--hurry}'[avoid burst buffer stage out]'
  {-i,--interactive}'[require response from user for each job]'
  {-M,--clusters}'[clusters to issue commands to.]'
  {-n,--name}'[act only on jobs with this name]'
  {-p,--partition}'[act only on jobs in this partition]'
  {-Q,--quiet}'[disable warnings]'
  {-q,--qos}'[act only on jobs with this quality of service]'
  {-R,--reservation}'[act only on jobs with this reservation]'
  '--sibling[remove an active sibling job from a federated job]'
  {-s,--signal}'[| integer signal to send to job, default is SIGKILL]'
  {-t,--state}'[act only on jobs in this state. Valid job]'
  {-u,--user}'[act only on jobs of this user]'
  {-V,--version}'[output version information and exit]'
  {-v,--verbose}'[verbosity level]'
  {-w,--nodelist}'[act only on jobs on these nodes]'
  '--wckey[act only on jobs with this workload]'
  '--help[show this help message]'
  '--usage[display brief usage message]'
  '*:filename:_files'
)

_arguments -s $arguments
