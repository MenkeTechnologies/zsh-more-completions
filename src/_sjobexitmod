#compdef sjobexitmod

local arguments

arguments=(
  '-e[code> Modify the derived exit code to new value.]'
  '-r[string> Modify the jobs comment field to new value.]'
  '-c[name of cluster (optional).]'
  '-l[list information for a completed job.]'
  '-h[show usage.]'
  '-man[show man page.]'
  '*:filename:_files'
)

_arguments -s $arguments
