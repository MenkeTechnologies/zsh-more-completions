#compdef java2html

local arguments

arguments=(
  '(- * :)'{-h,--help}'[print help and exit]'
  '--detailed-help[print help, including all details and hidden]'
  {-V,--version}'[print version and exit]'
  {-i,--input}'[input file (default=stdin)]'
  {-o,--output}'[output file (default=stdout, when the third]'
  {-s,--src-lang}'[source language (use --lang-list to get the]'
  '--lang-list[list all the supported language and associated]'
  '--outlang-list[list all the supported output language and]'
  {-f,--out-format}'[output format (use --outlang-list to get the]'
  {-d,--doc}'[create an output file that can be used as a]'
  '--no-doc[cancel the --doc option even if it is implied]'
  {-c,--css}'[the external style sheet filename.  Implies]'
  '--doc[]'
  {-T,--title}'[give a title to the output document.  Implies]'
  '--doc[]'
  {-t,--tab}'[specify tab length.  (default=`8)]'
  {-H,--header}'[file to insert as header]'
  {-F,--footer}'[file to insert as footer]'
  '--style-file[specify the file containing format options]'
  '--style-css-file[specify the file containing format options (in]'
  '--style-defaults[specify the file containing defaults for format]'
  '--outlang-def[output language definition file]'
  '--outlang-map[output language map file]'
  '--data-dir[directory where language definition files and]'
  '--output-dir[output directory]'
  '--lang-def[language definition file]'
  '--lang-map[language map file  (default=`lang.map)]'
  '--show-lang-elements[prints the language elements that are defined]'
  '--infer-lang[force to infer source script language]'
  {-n,--line-number}'[number all output lines, using the specified]'
  '--line-number-ref[number all output lines and generate an anchor]'
  '--line-range[generate only the lines in the specified]'
  '--range-separator[the optional separator to be printed among]'
  '--range-context[number of (context) lines generated even if not]'
  '--regex-range[generate only the lines within the specified]'
  '--gen-references[generate references  (possible]'
  '--ctags-file[specify the file generated by ctags that will]'
  '--ctags[how to run the ctags command.  If this option]'
  '--tag-relative[)]'
  {-v,--verbose}'[verbose mode on]'
  {-q,--quiet}'[print no progress information]'
  '--binary-output[write output files in binary mode]'
  '--statistics[print some statistics (i.e., elapsed time)]'
  '--gen-version[put source-highlight version in the generated]'
  '--check-lang[only check the correctness of a language]'
  '--check-outlang[only check the correctness of an output]'
  '--failsafe[if no language definition is found for the]'
  {-g,--debug-langdef}'[debug a language definition.  In dump mode just]'
  '--show-regex[show the regular expression automaton]'
  '*:filename:_files'
)

_arguments -s $arguments
