#compdef tagmedia

local arguments

arguments=(
  '--show[list tags]'
  '--digest[add DIGEST (md5, sha1, sha224, sha256, sha384, sha512)]'
  '--pad[ignore N 2k-sectors of padding]'
  '--check[force yast check]'
  '--add-tag[bar add foo with value bar]'
  '--remove-tag[remove foo]'
  '--clean[remove all tags]'
  '*:filename:_files'
)

_arguments -s $arguments
