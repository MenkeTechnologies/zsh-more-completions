#compdef img_stat

local arguments

arguments=(
  '-t[display type only]'
  '-i[the format of the image file (use -i list for list of supported types)]'
  '-b[the size (in bytes) of the device sectors]'
  '-v[verbose output to stderr]'
  '-V[Print version]'
  '*:filename:_files'
)

_arguments -s $arguments
