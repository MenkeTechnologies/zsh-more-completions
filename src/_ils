#compdef ils

local arguments

arguments=(
  '-e[Display all inodes]'
  '-m[Display output in the mactime format]'
  '-O[Display inodes that are unallocated, but were sill open (UFS/ExtX only)]'
  '-p[Display orphan inodes (unallocated with no file name)]'
  '-s[time skew of original machine (in seconds)]'
  '-a[Allocated inodes]'
  '-A[Unallocated inodes]'
  '-l[Linked inodes]'
  '-L[Unlinked inodes]'
  '-z[Unused inodes]'
  '-Z[Used inodes]'
  '-i[the format of the image file (use -i list for supported types)]'
  '-b[the size (in bytes) of the device sectors]'
  '-f[file system type (use -f list for supported types)]'
  '-o[the offset of the file system in the image (in sectors)]'
  '-v[verbose output to stderr]'
  '-V[Display version number]'
  '*:filename:_files'
)

_arguments -s $arguments
