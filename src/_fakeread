#compdef fakeread

local arguments

arguments=(
  '-v[verbose mode \[level\]]'
  '-e[video encode device input to sepration as:]'
  '-p[.icc Use device link separation profile on input]'
  '-E[video decode separation device output. See -e above]'
  '-Z[quantize test values to fit in nbits]'
  '-k[.cal Apply calibration (include in .ti3 output)]'
  '-i[.cal Include calibration in .ti3 output, but dont apply it]'
  '-K[.cal Apply inverse calibration]'
  '-r[add average random deviation of <level>% to device values (after sep. & cal.)]'
  '-0[apply power to device chanel 0-9]'
  '-B[.icc Use BT.1886 source EOTF with technical gamma 2.4]'
  '-b[.g:display.icc Use BT.1886-like source EOTF with effective gamma g.g]'
  '-b[.p:g.g:display.icc Use effective gamma g.g source EOTF with p.p prop. output black point offset]'
  '-g[.g:display.icc Use effective gamma g.g source EOTF with all output black point offset]'
  '-I[r = relative colorimetric, a = absolute (default)]'
  '-A[a,b Scale black point to target Lab value]'
  '-l[output Lab rather than XYZ]'
  '-s[lookup MPP spectral values]'
  '-R[add average random deviation of <level>% to output PCS values]'
  '-u[make random deviations have uniform distributions rather than normal]'
  '-S[set random seed]'
  '-U[reverse convert PCS to device, output_r.ti3]'
  '*:filename:_files'
)

_arguments -s $arguments
