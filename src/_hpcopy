#compdef hpcopy

local arguments

arguments=(
  '-m[II: A popular format for binary file transfer.]'
  '-b[an alternative format for ASCII file transfer.]'
  '-t[performs end-of-line translation.]'
  '-r[data: Performs no translation.]'
  '-a[A mode will be chosen automatically for each file]'
  '*:filename:_files'
)

_arguments -s $arguments
