#compdef eu-strings

local arguments

arguments=(
  {-a,--all}'[scan entire file, not only loaded sections]'
  {-e,--encoding}'[select character size and endianess: s = 7-bit, S]'
  {-f,--print-file-name}'[print name of the file before each string.]'
  {-n,--bytes}'[only NUL-terminated sequences of MIN-LEN]'
  '-o[alias for --radix=o]'
  {-t,--radix}'[Print location of the string in base 8, 10, or 16]'
  '--usage[give a short usage message]'
  {-V,--version}'[print program version]'
  '*:filename:_files'
)

_arguments -s $arguments
