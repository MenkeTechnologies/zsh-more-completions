#compdef liquibase

local arguments

arguments=(
  '--always-override-stored-logic-schema[]'
  '--auto-reorg[should Liquibase automatically include REORG TABLE]'
  '--changelog-lock-poll-rate[]'
  '--changelog-lock-wait-time-in-minutes[]'
  '--classpath[additional classpath entries to use]'
  '--convert-data-types[]'
  '--database-changelog-lock-table-name[]'
  '--database-changelog-table-name[]'
  '--database-class[class to use for Database implementation]'
  '--defaultlogger-level[]'
  '--defaults-file[file with default Liquibase properties]'
  '--diff-column-order[]'
  '--driver[database driver class]'
  '--driver-properties-file[]'
  '--filter-log-messages[]'
  '--generate-changeset-created-values[]'
  '--generated-changeset-ids-contains-description[]'
  '--generatedChangesetIdsContainsDescription[\]]'
  {-h,--help}'[show this help message and exit]'
  '--headless[force liquibase think it has no access to a]'
  '--hub-api-key[liquibase Hub API key for operations]'
  '--hub-log-level[log level for filtering log messages to send to]'
  '--hub-mode[content to send to Liquibase Hub during]'
  '--hub-url[liquibase Hub URL for operations]'
  '--include-catalog-in-specification[]'
  '--include-system-classpath[]'
  '--liquibase-catalog-name[]'
  '--liquibase-schema-name[]'
  '--liquibase-tablespace-name[]'
  '--log-file[(liquibase.logFile)]'
  '--log-level[DEFAULT: Controls which logs get set to stderr AND]'
  '--output-file[(liquibase.outputFile)]'
  '--output-file-encoding[]'
  '--output-line-separator[]'
  '--pro-license-key[]'
  '--pro-mark-unused-not-drop[]'
  '--pro-sql-inline[if true, generate changeSets with SQL-based]'
  '--pro-synonyms-drop-public[]'
  '--prompt-for-non-local-database[]'
  '--property-provider-class[]'
  '--should-run[should Liquibase commands execute]'
  '--should-snapshot-data[]'
  '--sqlplus-args[extra arguments which will be passed to the]'
  '--sqlplus-keep-temp[]'
  '--sqlplus-keep-temp-name[]'
  '--sqlplus-keep-temp-overwrite[]'
  '--sqlplus-keep-temp-path[]'
  '--sqlplus-path[path to sqlplus executable]'
  '--sqlplus-timeout[]'
  '--support-property-escaping[]'
  '--use-procedure-schema[]'
  {-v,--version}'[print version information and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
