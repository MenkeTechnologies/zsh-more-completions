#compdef hashcat

local arguments

arguments=(
  {-m,--hash-type}'[Hash-type, see references below -m 1000]:Num'
  {-a,--attack-mode}'[Attack-mode, see references below -a 3]:Num'
    '(- * :)'{-V,--version}'[Print version]'
    '(- * :)'{-h,--help}'[Print help]'
  '--quiet[Suppress output]'
  '--hex-charset[Assume charset is given in hex]'
  '--hex-salt[Assume salt is given in hex]'
  '--hex-wordlist[Assume words in wordlist are given in hex]'
  '--force[Ignore warnings]'
  '--status[Enable automatic update of the status screen]'
  '--status-timer=[Sets seconds between status screen updates to X --status-timer=1]:Num'
  '--machine-readable[Display the status view in a machine-readable format]'
  '--keep-guessing[Keep guessing the hash after it has been cracked]'
  '--self-test-disable[Disable self-test functionality on startup]'
  '--loopback[Add new plains to induct directory]'
  '--markov-hcstat=[Specify hcstat file to use --markov-hc=my.hcstat]:File:_files'
  '--markov-disable[Disables markov-chains, emulates classic brute-force]'
  '--markov-classic[Enables classic markov-chains, no per-position]'
  {-t,--markov-threshold}'[Threshold X when to stop accepting new markov-chains -t 50]:Num'
  '--runtime=[Abort session after X seconds of runtime --runtime=10]:Num'
  '--session=[Define specific session name --session=mysession]:Str '
  '--restore[Restore session from --session]'
  '--restore-disable[Do not write restore file]'
  '--restore-file-path=[Specific path to restore file --restore-file-path=x.restore]:File'
  {-o,--outfile}'[Define outfile for recovered hash -o outfile.txt]:File:_file'
  '--outfile-format=[Define outfile-format X for recovered hash --outfile-format=7]:Num'
  '--outfile-autohex-disable[Disable the use of $HEX\[\] in output plains]'
  '--outfile-check-timer[Sets seconds between outfile checks to X --outfile-check=30]:Num'
  '--wordlist-autohex-disable[Disable the conversion of $HEX\[\] from the wordlist]'
  {-p,--separator}'[Separator char for hashlists and outfile -p :]:Char'
  '--stdout[Do not crack a hash, instead print candidates only]'
  '--show[Compare hashlist with potfile; show cracked hashes]'
  '--left[Compare hashlist with potfile; show uncracked hashes]'
  '--username[Enable ignoring of usernames in hashfile]'
  '--remove[Enable removal of hashes once they are cracked]'
  '--remove-timer[Update input hash file each X seconds --remove-timer=30]:Num'
  '--potfile-disable[Do not write potfile]'
  '--potfile-path[Specific path to potfile --potfile-path=my.pot]:Dir'
  '--encoding-from[Force internal wordlist encoding from X --encoding-from=iso-8859-15]:Code'
  '--encoding-to[Force internal wordlist encoding to X --encoding-to=utf-32le]:Code'
  '--debug-mode[Defines the debug mode (hybrid only by using rules) --debug-mode=4]:Num'
  '--debug-file[Output file for debugging rules --debug-file=good.log]:File'
  '--induction-dir[Specify the induction directory to use for loopback --induction=inducts]:Dir'
  '--outfile-check-dir=[Specify the outfile directory to monitor for plains --outfile-check-dir=x]:Dir:_files -/'
  '--logfile-disable[Disable the logfile]'
  '--hccapx-message-pair[Load only message pairs from hccapx matching X --hccapx-message-pair=2]:Num'
  '--nonce-error-corrections[The BF size range to replace APs nonce last bytes --nonce-error-corrections=16]:Num'
  '--truecrypt-keyfiles=[Keyfiles to use, separated with commas --truecrypt-key=x.png]:File'
  '--veracrypt-keyfiles=[Keyfiles to use, separated with commas --veracrypt-key=x.txt]:File'
  '--veracrypt-pim[VeraCrypt personal iterations multiplier --veracrypt-pim=1000]:Num'
  {-b,--benchmark}'[Run benchmark of selected hash-modes]'
  '--benchmark-all[Run benchmark of all hash-modes (requires -b)]'
  '--speed-only[Return expected speed of the attack, then quit]'
  '--progress-only[Return ideal progress step size and time to process]'
  {-c,--segment-size}'[Sets size in MB to cache from the wordfile to X -c 32]:Num'
  '--bitmap-min[Sets minimum bits allowed for bitmaps to X --bitmap-min=24]:Num'
  '--bitmap-max[Sets maximum bits allowed for bitmaps to X --bitmap-max=24]:Num'
  '--cpu-affinity[Locks to CPU devices, separated with commas --cpu-affinity=1,2,3]:Str'
  '--example-hashes[Show an example hash for each hash-mode]'
  {-I,--opencl-info}'[Show info about detected OpenCL platforms/devices -I]'
  '--opencl-platforms[OpenCL platforms to use, separated with commas --opencl-platforms=2]:Str'
  {-d,--opencl-devices=}'[OpenCL devices to use, separated with commas -d 1]:Str'
  {-D,--opencl-device-types=}'[OpenCL device-types to use, separated with commas -D 1]:Str'
  '--opencl-vector-width=[Manually override OpenCL vector-width to X --opencl-vector=4]:Num'
  {-O,--optimized-kernel-enable}'[Enable optimized kernels (limits password length)]'
  {-w,--workload-profile}'[Enable a specific workload profile, see pool below -w 3]:Num'
  {-n,--kernel-accel}'[Manual workload tuning, set outerloop step size to X -n 64]:Num'
  {-u,--kernel-loops}'[Manual workload tuning, set innerloop step size to X -u 256]:Num'
  '--nvidia-spin-damp[Workaround NVIDIAs CPU burning loop bug, in percent --nvidia-spin-damp=50]:Num'
  '--gpu-temp-disable[Disable temperature and fanspeed reads and triggers]'
  '--gpu-temp-abort[Abort if GPU temperature reaches X degrees Celsius --gpu-temp-abort=100]:Num'
  '--scrypt-tmto[Manually override TMTO value for scrypt to X --scrypt-tmto=3]:Num'
  {-s,--skip}'[Skip X words from the start -s 1000000]:Num'
  {-l,--limit}'[Limit X words from the start + skipped words -l 1000000]:Num'
  '--keyspace[Show keyspace base:mod values and quit]'
  {-j,--rule-left}'[Single rule applied to each word from left wordlist -j c]:Rule'
  {-k,--rule-right}'[Single rule applied to each word from right wordlist -k -]:Rule'
  {-r,--rules-file}'[Multiple rules applied to each word from wordlists -r rules/best64.rule]:File'
  {-g,--generate-rules}'[Generate X random rules -g 10000]:Num'
  '--generate-rules-func-min[Force min X functions per rule]:Num'
  '--generate-rules-func-max[Force max X functions per rule]:Num'
  '--generate-rules-seed[Force RNG seed set to X]:Num'
  {-1,--custom-charset1}'[User-defined charset ?1 -1 ?l?d?u]:CS'
  {-2,--custom-charset2}'[User-defined charset ?2 -2 ?l?d?s]:CS'
  {-3,--custom-charset3}'[User-defined charset ?3]:CS'
  {-4,--custom-charset4}'[User-defined charset ?4]:CS'
  {-i,--increment}'[Enable mask increment mode]'
  '--increment-min=[Start mask incrementing at X --increment-min=4]:Num'
  '--increment-man=[Stop mask incrementing at X --increment-max=8]:Num'
  '*:filename:_files'
)

_arguments -s $arguments
