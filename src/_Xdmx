#compdef Xdmx

local arguments

arguments=(
  '-a[default pointer acceleration (factor)]'
  '-ac[disable access control restrictions]'
  '-audit[set audit trail level]'
  '-auth[select authorization file]'
  '-br[create root window with black background]'
  '-bs[disable any backing store support]'
  '-c[turns off key-click]'
  '-cc[default color visual class]'
  '-nocursor[disable the cursor]'
  '-core[generate core dump on fatal error]'
  '-displayfd[file descriptor to write display number to when ready to connect]'
  '-dpi[screen resolution in dots per inch]'
  '-dpms[disables VESA DPMS monitor control]'
  '-deferglyphs[16\] defer loading of \[no|all|16-bit\] glyphs]'
  '-f[bell base (0-100)]'
  '-fc[cursor font]'
  '-fn[default font name]'
  '-fp[default font path]'
  '-help[prints message with these options]'
  '-iglx[prohibit creating indirect GLX contexts (default)]'
  '-I[ignore all remaining arguments]'
  '-ld[limit data space to N Kb]'
  '-lf[limit number of open files to N]'
  '-ls[limit stack space to N Kb]'
  '-nolock[disable the locking mechanism]'
  '-maxclients[set maximum number of clients (power of two)]'
  '-nolisten[dont listen on protocol]'
  '-listen[listen on protocol]'
  '-noreset[dont reset after last client exists]'
  '-background[create root window with no background]'
  '-reset[reset after last client exists]'
  '-p[screen-saver pattern duration (minutes)]'
  '-pn[accept failure to listen on all ports]'
  '-nopn[reject failure to listen on all ports]'
  '-r[turns off auto-repeat]'
  '-render[set render color alloc policy]'
  '-retro[start with classic stipple and cursor]'
  '-s[screen-saver timeout (minutes)]'
  '-seat[seat to run on]'
  '-t[default pointer threshold (pixels/t)]'
  '-terminate[terminate at server reset]'
  '-to[connection time out]'
  '-tst[disable testing extensions]'
  '-v[screen-saver without video blanking]'
  '-wm[whenMapped default backing-store]'
  '-wr[create root window with white background]'
  '-maxbigreqsize[set maximal bigrequest size]'
  '-xinerama[disable XINERAMA extension]'
  '-dumbSched[disable smart scheduling and threaded input, enable old behavior]'
  '-schedInterval[set scheduler interval in msec]'
  '-sigstop[enable SIGSTOP based startup]'
  '-extension[disable extension]'
  '-query[contact named host for XDMCP]'
  '-broadcast[broadcast for XDMCP]'
  '-multicast[\[hops\]\] IPv6 multicast for XDMCP]'
  '-indirect[contact named host for indirect XDMCP]'
  '-port[UDP port number to send messages to]'
  '-from[specify the local address to connect from]'
  '-once[terminate server after one session]'
  '-class[specify display class to send in manage]'
  '-cookie[specify the magic cookie for XDMCP]'
  '-displayID[manufacturer display ID for request]'
  '-ardelay[set XKB autorepeat delay]'
  '-arinterval[set XKB autorepeat interval]'
  '-display[specify the back-end display(s)]'
  '-input[specify input source for core device]'
  '-xinput[specify input source for XInput device]'
  '-shadowfb[enable shadow frame buffer]'
  '-configfile[read from a configuration file]'
  '-config[select a specific configuration]'
  '-nomulticursor[turn of multiple cursor support]'
  '-fontpath[sets the default font path]'
  '-stat[scrns Print out performance statistics]'
  '-syncbatch[set interval for XSync batching]'
  '-nooffscreenopt[disable offscreen optimization]'
  '-nosubdivprims[disable primitive subdivision]'
  '-nowindowopt[disable lazy window creation optimization]'
  '-noxkb[disable use of the XKB extension with]'
  '-depth[specify the default root window depth]'
  '-norender[disable RENDER extension support]'
  '-noglxproxy[disable GLX Proxy]'
  '-noglxswapgroup[disable swap group and swap barrier]'
  '-glxsyncswap[force XSync after swap buffers]'
  '-glxfinishswap[force glFinish after swap buffers]'
  '-ignorebadfontpaths[ignore bad font paths during initialization]'
  '-addremovescreens[enable dynamic screen addition/removal]'
  '-param[Specify configuration parameters (e.g.]'
  {-input,-xinput}'[specifies no input.]'
)

_arguments -s $arguments
