#compdef systemstats

local arguments

arguments=(
  '--create-archive[creates an archive of all stats files found in the local system store or path specified with -f]'
  '--list-files[print the paths of all stats files found in the local system store or path specified with -f]'
  '--list-files-with-null[like --list-files, but separates file paths with NULL characters instead of newlines.]'
  '--show-events[similar to --dump but pretty prints the records in a single line where possible and makes some unit conversions.]'
  '--dump[dump all records from stats files in the sample range to standard output.]'
  '--dashboard[show a selection of statistics about the systems power usage over the sample range.]'
  '--concise-dashboard[less verbose version of the dashboard intended to make bulk processing easier.]'
  '--analytics-sharing-status[show history and current status of sharing SystemStats analytics with Apple.]'
  '(- * :)'{-h,--help}'[show this summary of command-line options and usage.]'
  '--checkpoint-immediate[checkpoint all sampled systemstats values to the database.]'
  '--checkpoint-wait[notify and exit when systemstats autonomously checkpoints all sampled values to the database.]'
  '--logger-helper[/path/to/archive>]'
  {-f,--from}'[/path/to/archive>]'
  {-d,--day}'[(Deprecated)]'
  {-s,--start-time}'[hh:mm:ss>]'
  {-e,--end-time}'[hh:mm:ss>]'
  {-A,--ago}'[set the sample range from the given time ago to the present.]'
  '--file-size-limit[when running the commands list-files, list-files-with-null, and create-archive]'
  '--exclude-private-records[prevents systemstats from ingesting records that can only be viewed on device.]'
  {-b,--build}'[only read stats files from given system version (e.g. 13A500).]'
  {-B,--boot}'[only read stats files from the given boot, specified by UUID.]'
  '--all-process-data[show all available systemstats intervals and all available processes for eligible summarizers.]'
  '--ranked-summary-count[for eligible summarizers, show the top N most resource-hungry intervals in dashboard mode.]'
  '--ranked-process-count[for eligible summarizers and intervals, show the top N most resource-hungry processes in dashboard mode.]'
  '*:filename:_files'
)

_arguments -s $arguments
