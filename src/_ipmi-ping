#compdef ipmi-ping

local arguments

arguments=(
  '-c[count]'
  '-i[interval in seconds]'
  '-I[interface address or device name]'
  '-t[timeout in seconds]'
  '-v[verbose output]'
  '-r[protocol version]'
  '-s[starting sequence number]'
  '-d[turn on debugging]'
  '*:filename:_files'
)

_arguments -s $arguments
