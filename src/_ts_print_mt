#compdef ts_print_mt

local arguments

arguments=(
  {-r,--raw}'[dont apply filter modules. use what module_raw]'
  {-n,--non-blocking}'[tests non-blocking read and loops]'
  {-i,--idev}'[explicitly choose the touch input device]'
  {-s,--samples}'[number of samples to request ts_read_mt() to]'
  {-j,--slots}'[set the number of concurrently available touch]'
  '(- * :)'{-h,--help}'[print this help text]'
  {-v,--version}'[print version information only]'
  '*:filename:_files'
)

_arguments -s $arguments
