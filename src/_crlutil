#compdef crlutil

local arguments

arguments=(
  '-L[list CRL]'
  '-n[specify the nickname of the CA certificate]'
  '-d[key database directory (default is ~/.netscape)]'
  '-P[cert & Key database prefix (default is "")]'
  '-D[delete a CRL from the cert database]'
  '-n[specify the nickname for the CA certificate]'
  '-t[specify the crl type.]'
  '-d[key database directory (default is ~/.netscape)]'
  '-P[cert & Key database prefix (default is "")]'
  '-E[erase all CRLs of specified type from hte cert database]'
  '-t[specify the crl type.]'
  '-d[key database directory (default is ~/.netscape)]'
  '-P[cert & Key database prefix (default is "")]'
  '-S[show contents of a CRL file (without database)]'
  '-i[specify the file which contains the CRL to show]'
  '-I[import a CRL to the cert database]'
  '-i[specify the file which contains the CRL to import]'
  '-u[specify the url.]'
  '-t[specify the crl type.]'
  '-d[key database directory (default is ~/.netscape)]'
  '-P[cert & Key database prefix (default is "")]'
  '-B[bypass CA certificate checks.]'
  '-p[partial decode for faster operation.]'
  '-r[repeat the operation.]'
  '-G[create CRL]'
  '-M[modify CRL]'
  '-c[specify crl initialization file]'
  '-n[specify the nickname of the CA certificate]'
  '-i[specify the file which contains the CRL to import]'
  '-o[specify a CRL output file]'
  '-a[specify to use base64 encoded CRL output format]'
  '-d[key database directory (default is ~/.netscape)]'
  '-f[provide path to a default pwd file]'
  '-w[provide db password in command line]'
  '-P[cert & Key database prefix (default is "")]'
  '-u[specify the url.]'
  '-B[bypass CA certificate checks.]'
  '*:filename:_files'
)

_arguments -s $arguments
