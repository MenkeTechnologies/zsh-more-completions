#compdef llvm-ifs-13

local arguments

arguments=(
  '--color[- Use colors in output (default=autodetect)]'
  '--help[- Display available options (--help-hidden for more)]'
  '--help-list[- Display list of available options (--help-list-hidden for more)]'
  '--version[- Display the version of this program]'
  '--arch[- Specify the architecture, e.g. x86_64]'
  '--bitwidth[- Specify the bit width]'
  '--endianness[- Specify the endianness]'
  '--hint-ifs-target[- When --output-format is IFS, this flag will hint the expected target triple for IFS output]'
  '--input-format[- Specify the input file format]'
  '--output[- Output file]'
  '--output-format[- Specify the output file format]'
  '--soname[- Manually set the DT_SONAME entry of any emitted files]'
  '--strip-ifs-arch[- Strip target architecture information away from IFS output]'
  '--strip-ifs-bitwidth[- Strip target bit width information away from IFS output]'
  '--strip-ifs-endianness[- Strip target endianness information away from IFS output]'
  '--strip-ifs-target[- Strip all target information away from IFS output]'
  '--target[- Specify the target triple, e.g. x86_64-linux-gnu]'
  '--write-if-changed[- Write the output file only if it is new or has changed.]'
  '*:filename:_files'
)

_arguments -s $arguments
