#compdef yersinia

local arguments

arguments=(
  '-V[program version.]'
  '-h[this help screen.]'
  '-G[graphical mode (GTK).]'
  '-I[interactive mode (ncurses).]'
  '-D[daemon mode.]'
  '-d[debug.]'
  '-l[select logfile.]'
  '-c[select config file.]'
  '*:filename:_files'
)

_arguments -s $arguments
