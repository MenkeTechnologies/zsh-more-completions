#compdef benchmark_xl

local arguments

arguments=(
  '--input[file or file pattern matching input files.]'
  '--codec[comma separated list of image codec descriptions to benchmark.]'
  '--print[_details]'
  '--noprint[_details]'
  '--print[_details_csv]'
  '--noprint[_details_csv]'
  '--extra[_metrics=<string>]'
  '--print[_more_stats]'
  '--noprint[_more_stats]'
  '--print[_distance_percentiles]'
  '--noprint[_distance_percentiles]'
  '--silent[_errors]'
  '--nosilent[_errors]'
  '--save[_compressed]'
  '--nosave[_compressed]'
  '--save[_decompressed]'
  '--nosave[_decompressed]'
  '--output[_extension=<string>]'
  '--output[_description=<string>]'
  '--intensity[_target=<scalar>]'
  '--dec-hints[color encoding hints for the input images to encoder. Comma separated key=value pairs. The key color_space indicates ColorEncoding (see ParseDescription; e.g. RGB_D65_SRG_Rel_709) for input images without color encoding (such as PNM)]'
  '--override[_bitdepth=<unsigned>]'
  '--mul[_output=<scalar>]'
  '--heatmap[_good=<scalar>]'
  '--heatmap[_bad=<scalar>]'
  '--write[_html_report]'
  '--nowrite[_html_report]'
  '--html[_report_self_contained]'
  '--nohtml[_report_self_contained]'
  '--markdown[]'
  '--nomarkdown[adds formatting around ASCII table to render correctly in Markdown based interfaces]'
  '--more[_columns]'
  '--nomore[_columns]'
  '--originals[_url=<string>]'
  '--output[_dir=<string>]'
  '--num[_threads=<integer>]'
  '--inner[_threads=<integer>]'
  '--encode[_reps=<unsigned>]'
  '--decode[_reps=<unsigned>]'
  '--sample[_tmp_dir=<string>]'
  '--num[_samples=<integer>]'
  '--sample[_dimensions=<integer>]'
  '--error[_pnorm=<scalar>]'
  '--hf[_asymmetry=<scalar>]'
  '--profiler[]'
  '--noprofiler[if true, print profiler results.]'
  '--show[_progress]'
  '--noshow[_progress]'
  '--skip[_butteraugli]'
  '--noskip[_butteraugli]'
  '--decode[_only]'
  '--nodecode[_only]'
  '--xmul[multiplier for the difference in X channel in Butteraugli.]'
  '--quant[_bias=<scalar>]'
  '--use[_ac_strategy]'
  '--nouse[_ac_strategy]'
  '--qprogressive[]'
  '--noqprogressive[enable quantized progressive mode for AC.]'
  '--progressive[]'
  '--noprogressive[enable progressive mode for AC.]'
  '--progressive[_dc=<integer>]'
  '--noise[1]'
  '--dots[1]'
  '--patches[1]'
  '--debug[_image_dir=<string>]'
  '--chroma[_subsampling=444/422/420/411]'
  {-h,--help}'[prints this help message.]'
  '*:filename:_files'
)

_arguments -s $arguments
