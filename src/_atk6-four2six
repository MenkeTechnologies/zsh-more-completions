#compdef atk6-four2six

local arguments

arguments=(
  '-F[insert atomic fragment header (can be set multiple times)]'
  '-H[insert and empty hop-by-hop header]'
  '-D[insert a large destination header that fragments the packet]'
  '-p[set a specific UDP source port or Ping ID]'
  '-s[set a specific IPv6 source address]'
  '-m[set a specific MAC source address]'
  '*:filename:_files'
)

_arguments -s $arguments
