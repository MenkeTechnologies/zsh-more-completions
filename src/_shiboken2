#compdef shiboken2

local arguments

arguments=(
  '--api-version["package mask">,<"version">]'
  '--debug-level[set the debug level]'
  '--documentation-only[do not generates any code, just the documentation]'
  '--drop-type-entries[0>\[;TypeEntry1;...\]"]'
  '-F[<path>, --framework-include-paths=<path>\[:<path>:...\]]'
  '-isystem[<path>, --system-include-paths=<path>\[:<path>:...\]]'
  '--generator-set["generator module">]'
  '--skip-deprecated[skip deprecated functions]'
  '--diff[print a diff of wrapper files]'
  '--dry-run[dry run, do not generate wrapper files]'
  '(- * :)'{-h,--help}'[display this help and exit]'
  '-I[<path>, --include-paths=<path>\[:<path>:...\]]'
  {--language-level,-std}'[C++ Language level (c++11..c++17, default=c++14)]'
  '--license-file[file used for copyright headers of generated files]'
  '--no-suppress-warnings[show all warnings]'
  '--output-directory[the directory where the generated files will be written]'
  '--project-file[text file containing a description of the binding project.]'
  '--silent[avoid printing any message]'
  '-T[<path>, --typesystem-paths=<path>\[:<path>:...\]]'
  '--version[output version information and exit]'
  '--avoid-protected-hack[avoid the use of the #define protected public hack.]'
  '--disable-verbose-error-messages[disable verbose error messages. Turn the python code hard to debug]'
  '--enable-parent-ctor-heuristic[enable heuristics to detect parent relationship on constructors.]'
  '--enable-pyside-extensions[enable PySide extensions, such as support for signal/slots]'
  '--enable-return-value-heuristic[enable heuristics to detect parent relationship on return values]'
  '--use-isnull-as-nb[_nonzero]'
  '--doc-parser[the documentation parser used to interpret the documentation]'
  '--documentation-code-snippets-dir[directory used to search code snippets used by the documentation]'
  '--documentation-data-dir[directory with XML files generated by documentation tool]'
  '--documentation-extra-sections-dir[directory used to search for extra documentation sections]'
  '--library-source-dir[directory where library source code is located]'
  '--additional-documentation[list of additional XML files to be converted to .rst files]'
  '*:filename:_files'
)

_arguments -s $arguments
