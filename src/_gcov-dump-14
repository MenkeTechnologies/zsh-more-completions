#compdef gcov-dump-14

local arguments

arguments=(
  {-h,--help}'[print this help]'
  {-l,--long}'[dump record contents too]'
  {-p,--positions}'[dump record positions]'
  {-r,--raw}'[print content records in raw format]'
  {-s,--stable}'[print content in stable format usable for comparison]'
  {-v,--version}'[print version number]'
  '*:filename:_files'
)

_arguments -s $arguments
