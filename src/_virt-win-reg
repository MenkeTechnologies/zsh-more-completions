#compdef virt-win-reg

local arguments

arguments=(
  '--help[display brief help.]'
  '--version[display version number and exit.]'
  '--debug[enable debugging messages.]'
  '-c[]'
  '--connect[if using libvirt, connect to the given URI. If omitted, then we]'
  '--format[specify the format of disk images given on the command line. If this]'
  '--merge[in merge mode, this merges a textual regedit file into the Windows]'
  '--encoding[16LE|ASCII]'
  '--unsafe-printable-strings[when exporting (only), assume strings are UTF-16LE and print them as]'
  '*:filename:_files'
)

_arguments -s $arguments
