#compdef zshdb

local arguments

arguments=(
  '(- * :)'{-h,--help}'[print this help.]'
  {-q,--quiet}'[do not print introductory and quiet messages.]'
  {-A,--annotate}'[Set the annotation level.]':level
  {-B,--basename}'[show basename only on source file listings.]'
  '--highlight[Use dark or light background ANSI terminal sequence]:highlight:(dark light)'
  '--no-highlight[Do not use ANSI terminal sequences for syntax highlight]'
  '--init-file[source script file FILE. Similar to bashs corresponding option]':file
  {-L,--library}'[set the directory location of library helper file]':directory
  {-c,--command}'[run STRING instead of a script file]':string
  {-n,--nx,--no-init}'[dont run initialization files.]'
  {-S,--style}'[run use pygments STYLE for formatting source code]:style'
  {-t,--tty}'[run using device for your programs standard input and output]'
  {--tty_in,--terminal_in}'[Set to...]:dev'
  {-T,--tempdir}'[use DIRECTORY to store temporary files in]:directory'
  {-V,--version}'[print the debugger version number.]'
  {-X,--trace}'[set line tracing similar to set -x]'
  {-x,--eval-command}'[execute debugger commands from CMDFILE.]'
  '*:filename:_files'
)

_arguments -s $arguments
