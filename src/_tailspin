#compdef tailspin

local arguments

arguments=(
  {-r,--reason}'[adds a reason string to the tailspin file]'
  {-l,--for-last-seconds}'[attempts to limit trace to the last n seconds]'
  {-n,--no-symbolicate}'[prevents symbolication of the tailspin file]'
  {-d,--dsymForUUID}'[use dsymForUUID to find better symbols. Cannot be used with -n]'
  {-c,--continue-tracing}'[continue tracing for the next n seconds after saving out current buffer]'
  {-s,--gather-os-signposts}'[adds os_signposts covering the duration of the trace to the tailspin file]'
  {-o,--gather-os-logs}'[adds os_logs covering the duration of the trace to the tailspin file]'
  {-s,--with-symbols}'[add symbol data to the file.]'
  {-d,--dsymForUUID}'[use dsymForUUID to find better symbols. This may take a while.]'
  {-l,--with-os-signposts-logs}'[adds os_signposts and os_logs covering the duration of trace to tailspin file.]'
  {-L,--log-archive-path}'[Uses the specified log archive when appending os_signposts and os_logs.]:path:'
  '*:filename:_files'
)

_arguments -s $arguments
