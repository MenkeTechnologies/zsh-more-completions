#compdef xelatex

local arguments

arguments=(
  '-etex[enable e-TeX extensions]'
  '-fmt[use FMTNAME instead of program name or a %& line]'
  '-halt-on-error[stop processing at the first error]'
  '-ini[be xeinitex, for dumping formats; this is implicitly]'
  '-interaction[set interaction mode (STRING=batchmode/nonstopmode/]'
  '-jobname[set the job name to STRING]'
  '-kpathsea-debug[set path searching debugging flags according to]'
  '-mltex[enable MLTeX extensions such as \charsubdef]'
  '-output-comment[use STRING for XDV file comment instead of date]'
  '-output-directory[use existing DIR as the directory to write files in]'
  '-output-driver[use CMD as the XDV-to-PDF driver instead of xdvipdfmx]'
  '-no-pdf[generate XDV (extended DVI) output rather than PDF]'
  '-papersize[set PDF media size to STRING]'
  '-progname[set program (and fmt) name to STRING]'
  '-recorder[enable filename recorder]'
  '-shell-restricted[enable restricted \write18]'
  '-src-specials[insert source specials into the XDV file]'
  '-src-specials[insert source specials in certain places of]'
  '-synctex[generate SyncTeX data for previewers according to]'
  '-translate-file[(ignored)]'
  '-8bit[make all characters printable, dont use ^^X sequences]'
  '-help[display this help and exit]'
  '-version[output version information and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
