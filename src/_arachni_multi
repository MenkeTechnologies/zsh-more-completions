#compdef arachni_multi

local arguments

arguments=(
  '(- * :)'{-h,--help}'[output this message.]'
  '--version[show version information.]'
  '--authorized-by[E-mail address of the person who authorized the scan.]'
  '--scope-include-pattern[only include resources whose path/action matches PATTERN.]'
  '--scope-include-subdomains[follow links to subdomains.]'
  '--scope-exclude-pattern[exclude resources whose path/action matches PATTERN.]'
  '--scope-exclude-file-extensions[EXTENSION2,..]'
  '--scope-exclude-content-pattern[exclude pages whose content matches PATTERN.]'
  '--scope-exclude-binaries[exclude non text-based pages.]'
  '--scope-redundant-path-pattern[limit crawl on redundant pages like galleries or catalogs.]'
  '--scope-auto-redundant[only follow URLs with identical query parameter names LIMIT amount of times.]'
  '--scope-directory-depth-limit[directory depth limit.]'
  '--scope-page-limit[how many pages to crawl and audit.]'
  '--scope-extend-paths[add the paths in FILE to the ones discovered by the crawler.]'
  '--scope-restrict-paths[use the paths in FILE instead of crawling.]'
  '--scope-url-rewrite[rewrite URLs based on the given PATTERN and SUBSTITUTION.]'
  '--scope-dom-depth-limit[how deep to go into the DOM tree of each page, for pages with JavaScript code.]'
  '--scope-dom-event-limit[how many DOM events to trigger for each DOM depth, for pages with JavaScript code.]'
  '--scope-https-only[forces the system to only follow HTTPS URLs.]'
  '--audit-links[audit links.]'
  '--audit-forms[audit forms.]'
  '--audit-cookies[audit cookies.]'
  '--audit-cookies-extensively[submit all links and forms of the page along with the cookie permutations.]'
  '--audit-headers[audit headers.]'
  '--audit-link-template[regular expression with named captures to use to extract input information from generic paths.]'
  '--audit-jsons[audit JSON request inputs.]'
  '--audit-xmls[audit XML request inputs.]'
  '--audit-ui-inputs[audit orphan <input> elements with events.]'
  '--audit-ui-forms[audit UI Forms.]'
  '--audit-parameter-names[payloads into parameter names.]'
  '--audit-with-raw-payloads[inject payloads with and without HTTP encoding.]'
  '--audit-with-extra-parameter[inject payloads into extra element parameters.]'
  '--audit-with-both-methods[audit elements with both GET and POST requests.]'
  '--audit-exclude-vector[exclude input vectors whose name matches PATTERN.]'
  '--audit-include-vector[include only input vectors whose name matches PATTERN.]'
  '--input-value[PATTERN to match against input names and VALUE to use for them.]'
  '--input-values-file[YAML file containing a Hash object with regular expressions, to match against input names, as keys and input values as values.]'
  '--input-without-defaults[do not use the system default input values.]'
  '--input-force[fill-in even non-empty inputs.]'
  '--http-user-agent[value for the User-Agent HTTP request header.]'
  '--http-request-concurrency[maximum HTTP request concurrency.]'
  '--http-request-timeout[HTTP request timeout in milliseconds.]'
  '--http-request-redirect-limit[maximum amount of redirects to follow for each HTTP request.]'
  '--http-request-queue-size[maximum amount of requests to keep in the queue.]'
  '--http-request-header[VALUE]'
  '--http-response-max-size[do not download response bodies larger than the specified LIMIT, in bytes.]'
  '--http-cookie-jar[netscape-styled HTTP cookiejar file.]'
  '--http-cookie-string[cookie representation as a Set-Cookie HTTP response header.]'
  '--http-authentication-username[username for HTTP authentication.]'
  '--http-authentication-password[password for HTTP authentication.]'
  '--http-authentication-type[basic,digest,digest_ie,negotiate,ntlm]'
  '--http-proxy[proxy to use.]'
  '--http-proxy-authentication[proxy authentication credentials.]'
  '--http-proxy-type[http_1_0,socks4,socks4a,socks5,socks5h]'
  '--http-ssl-verify-peer[verify SSL peer.]'
  '--http-ssl-verify-host[verify SSL host.]'
  '--http-ssl-certificate[SSL certificate to use.]'
  '--http-ssl-certificate-type[der]'
  '--http-ssl-key[SSL private key to use.]'
  '--http-ssl-key-type[der]'
  '--http-ssl-key-password[password for the SSL private key.]'
  '--http-ssl-ca[file holding one or more certificates with which to verify the peer.]'
  '--http-ssl-ca-directory[directory holding multiple certificate files with which to verify the peer.]'
  '--http-ssl-version[1,TLSv1_0,TLSv1_1,TLSv1_2,SSLv2,SSLv3]'
  '--checks-list[list available checks based on the provided glob.]'
  '--checks[CHECK2,...]'
  {--checks,-backup}'[_files,-xss]'
  '-xss[* to unload all XSS checks.]'
  '--plugins-list[list available plugins based on the provided glob.]'
  '--plugin[VALUE,OPTION2=VALUE2]'
  '--platforms-list[list available platforms.]'
  '--platforms-no-fingerprinting[disable platform fingerprinting.]'
  '--platforms[PLATFORM2,...]'
  '--session-check-url[URL to use to verify that the scanner is still logged in to the web application.]'
  '--session-check-pattern[pattern used against the body of the session-check-url to verify that the scanner is still logged in to the web application.]'
  '--profile-save-filepath[save the current configuration profile/options to FILEPATH.]'
  '--profile-load-filepath[load a configuration profile from FILEPATH.]'
  '--browser-cluster-local-storage[sets the browsers local storage using the JSON data in FILE.]'
  '--browser-cluster-wait-for-element[wait for element matching CSS to appear when visiting a page whose URL matches the PATTERN.]'
  '--browser-cluster-pool-size[amount of browser workers to keep in the pool and put to work.]'
  '--browser-cluster-job-timeout[maximum allowed time for each job.]'
  '--browser-cluster-worker-time-to-live[re-spawn the browser of each worker every LIMIT jobs.]'
  '--browser-cluster-ignore-images[do not load images.]'
  '--browser-cluster-screen-width[browser screen width.]'
  '--browser-cluster-screen-height[browser screen height.]'
  '--instance-spawns[how many slaves to spawn for a high-performance mult-Instance scan.]'
  '--report-save-path[directory or file path where to store the scan report.]'
  '--timeout[stop the scan after the given duration is exceeded.]'
  '*:filename:_files'
)

_arguments -s $arguments
