#compdef y4mcolorbars

local arguments

arguments=(
  '-n[frame count (output n frames) \[1\]]'
  '-W[frame width \[720\]]'
  '-H[frame height \[480\]]'
  '-F[framerate (as ratio) \[30000:1001\]]'
  '-A[pixel aspect ratio \[10:11\]]'
  '-I[interlacing \[p\]]'
  '-Q[content of -I/Q areas: \[0\]]'
  '-S[chroma subsampling mode \[444\]]'
  '-v[verbosity (0,1,2) \[1\]]'
  '*:filename:_files'
)

_arguments -s $arguments
