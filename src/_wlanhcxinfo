#compdef wlanhcxinfo

local arguments

arguments=(
  '-i[input hccapx file]'
  '-j[input john file (doesnt support all list options)]'
  '-o[output info file (default stdout)]'
  '-a[list access points]'
  '-A[list anonce]'
  '-s[list stations]'
  '-S[list snonce]'
  '-M[list key mic]'
  '-R[list replay count]'
  '-w[list wpa version]'
  '-P[list key key number]'
  '-p[list messagepair]'
  '-l[list essid len]'
  '-e[list essid]'
  '-h[this help]'
  '*:filename:_files'
)

_arguments -s $arguments
