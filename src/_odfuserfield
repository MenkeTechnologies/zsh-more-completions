#compdef odfuserfield

# Autogenerated from man page /usr/share/man/man1/odfuserfield.1.gz

local arguments

arguments=(
    '-l[List (extract) all known user-fields.]'
    '-L[List (extract) all known user-fields with type and value.]'
    '-x[Extract the contents of this field from the file.]:IfieldR'
    '-X[Same as -x, but also preserves/includes the field name and type.]:IfieldR'
    '-s[Set the value of an existing user field. The field type will be the same.]:Ifield:valueR'
    '-o[Filename to write modified ODT file to.]:IpathR'
    '*:filename:_files'
)
_arguments -s $arguments
