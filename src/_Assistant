#compdef Assistant

local arguments

arguments=(
  '-collectionFile[uses the specified collection]'
  '-showUrl[shows the document with the]'
  '-enableRemoteControl[enables Assistant to be]'
  '-show[shows the specified dockwidget]'
  '-activate[activates the specified dockwidget]'
  '-hide[hides the specified dockwidget]'
  '-register[registers the specified help file]'
  '-unregister[unregisters the specified help file]'
  '-setCurrentFilter[set the filter as the active filter.]'
  '-remove-search-index[removes the full text search index.]'
  '-rebuild-search-index[obsolete. Use -remove-search-index instead.]'
  '-quiet[does not display any error or]'
  '-help[displays this help.]'
  '*:filename:_files'
)

_arguments -s $arguments
