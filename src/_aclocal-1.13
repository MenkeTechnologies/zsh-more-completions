#compdef aclocal-1.13

local arguments

arguments=(
  '--automake-acdir[directory holding automake-provided m4 files]'
  '--system-acdir[directory holding third-party system-wide files]'
  '--diff[run COMMAND \[diff -u\] on M4 files that would be]'
  '--dry-run[pretend to, but do not actually update any file]'
  '--force[always update output file]'
  '--help[print this help, then exit]'
  '-I[add directory to search list for .m4 files]'
  '--install[copy third-party files to the first -I directory]'
  '--output[put output in FILE (default aclocal.m4)]'
  '--print-ac-dir[print name of directory holding system-wide]'
  '--verbose[dont be silent]'
  '--version[print version number, then exit]'
  {-W,--warnings}'[report the warnings falling in CATEGORY]'
  '*:filename:_files'
)

_arguments -s $arguments
