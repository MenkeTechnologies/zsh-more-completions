#compdef tqdm

local arguments

arguments=(
  '(- * :)'{-h,--help}'[print this help and exit]'
  {-v,--version}'[print version and exit]'
  '--desc[str, optional]'
  '--total[int, optional]'
  '--leave[bool, optional]'
  '--ncols[int, optional]'
  '--mininterval[float, optional]'
  '--maxinterval[float, optional]'
  '--miniters[int, optional]'
  '--ascii[bool or str, optional]'
  '--disable[bool, optional]'
  '--unit[str, optional]'
  '--unit[_scale=<unit_scale> : bool or int or float, optional]'
  '--dynamic[_ncols=<dynamic_ncols> : bool, optional]'
  '--smoothing[float, optional]'
  '--bar[_format=<bar_format> : str, optional]'
  '--initial[int, optional]'
  '--position[int, optional]'
  '--postfix[dict or *, optional]'
  '--unit[_divisor=<unit_divisor> : float, optional]'
  '--write[_bytes=<write_bytes> : bool, optional]'
  '--delim[chr, optional]'
  '--buf[_size=<buf_size> : int, optional]'
  '--bytes[bool, optional]'
  '--manpath[str, optional]'
  '--log[str, optional]'
  '*:filename:_files'
)

_arguments -s $arguments
