#compdef xorrisofs

local arguments

arguments=(
  {-f,-follow-links}'[follow symbolic links]'
  '-graft-points[allow to use graft points for filenames]'
  '-help[print option help]'
  '-hfsplus[generate HFS+ filesystem]'
  '-hfsplus-file-creator-type[TYPE iso_rr_path]'
  '-hfs-bless[name of Folder to be blessed]'
  '-hfs-bless-by[ISO_RR_PATH]'
  '-hfsplus-serial-no[HFS serial number: 16 characters \[0-9a-fA-F\]]'
  '-hfsplus-block-size[set HFS+ block size]'
  '-apm-block-size[set Apple Partition Map block size]'
  '-hide[hide ISO9660/RR file]'
  '-hide-list[file with list of ISO9660/RR files to hide]'
  '-hide-joliet[hide Joliet file]'
  '-hide-joliet-list[file with list of Joliet files to hide]'
  '-hide-hfsplus[hide HFS+ file]'
  '-hide-hfsplus-list[file with list of HFS+ files to hide]'
  '-input-charset[local input charset for file name conversion]'
  '-output-charset[output charset for file name conversion]'
  '-iso-level[set ISO9660 conformance level (1..3) or 4 for ISO9660 version 2]'
  '-disallow[_dir_id_ext Do not allow dot in ISO directory names]'
  {-J,-joliet}'[generate Joliet directory information]'
  '-joliet-long[allow Joliet file names to be 103 Unicode characters]'
  '-joliet-utf16[encode Joliet names in UTF-16BE rather than UCS-2]'
  {-U,-untranslated-filenames}'[untranslated filenames (for HPUX & AIX - violates ISO9660).]'
  '-untranslated[_name_len LEN Allow up to LEN (1..96) name characters (heavily violates ISO9660).]'
  '-allow-lowercase[allow lower case characters in addition to the current character set (violates ISO9660)]'
  '-relaxed-filenames[allow 7 bit ASCII except lower case characters (violates ISO9660)]'
  {-d,-omit-period}'[omit trailing periods from filenames (violates ISO9660)]'
  {-l,-full-iso9660-filenames}'[full 31 character filenames for ISO9660 names]'
  '-max-iso9660-filenames[allow 37 character filenames for ISO9660 names (violates ISO9660)]'
  {-N,-omit-version-number}'[omit version number from ISO9660 filename (violates ISO9660)]'
  {-D,-disable-deep-relocation}'[disable deep directory relocation (violates ISO9660)]'
  '-hide-rr-moved[relocate deep directories to /.rr_moved]'
  '-rr[_reloc_dir NAME Set deep directory relocation target in root]'
  {-o,-output}'[set output file name]'
  {-m,-exclude}'[exclude file name]'
  {-x,-old-exclude}'[exclude file name]'
  '-exclude-list[file with list of file names to exclude]'
  '-pad[pad output by 300k (default)]'
  '-no-pad[do not pad output]'
  {-M,-prev-session}'[set path to previous session to merge]'
  {-C,-cdrecord-params}'[magic paramters from cdrecord]'
  '-dir-mode[make the mode of all directories this mode.]'
  '-file-mode[make the mode of all plain files this mode.]'
  '-path-list[file with list of pathnames to process]'
  '--quoted[_path_list FILE File with list of quoted pathnames to process]'
  '-print-size[print estimated filesystem size and exit]'
  '-quiet[run quietly]'
  '-gui[switch behaviour for GUI]'
  {-R,-rock}'[generate Rock Ridge directory information]'
  {-r,-rational-rock}'[generate rationalized Rock Ridge directory information]'
  '--norock[disable Rock Ridge. (Strongly discouraged !)]'
  '-file[_name_limit LEN Set truncation limit for Rock Ridge names]'
  '--hardlinks[record eventual hard link relations of files]'
  '--acl[record eventual ACLs of files]'
  '--xattr[record eventual user space xattr of files]'
  '--md5[compute and record MD5 checksums of data files]'
  '--scdbackup[_tag PATH NAME With --md5 record a scdbackup checksum tag]'
  '--for[_backup Use all options which improve backup fidelity]'
  {-V,-volid}'[set Volume ID]'
  '-volset[set Volume set ID]'
  '-publisher[set Volume publisher]'
  {-A,-appid}'[set Application ID]'
  '-sysid[set System ID]'
  {-p,-preparer}'[set Volume preparer]'
  '-abstract[set Abstract filename]'
  '-biblio[set Bibliographic filename]'
  '-copyright[set Copyright filename]'
  '--application[_use CHAR|PATH Set content of Application Use field]'
  '-jigdo-jigdo[produce a jigdo .jigdo file as well as the .iso]'
  '-jigdo-template[produce a jigdo .template file as well as the .iso]'
  '-jigdo-min-file-size[minimum size for a file to be listed in the jigdo file]'
  '-jigdo-force-md5[pattern(s) where files MUST match an externally-supplied MD5sum]'
  '-jigdo-exclude[pattern(s) to exclude from the jigdo file]'
  '-jigdo-map[1=PATTERN2]'
  '-md5-list[file containing MD5 sums of the files that should be checked]'
  '-jigdo-template-compress[choose to use gzip or bzip2 compression for template data; default is gzip]'
  '-checksum[_algorithm_iso alg1,alg2,...]'
  '-checksum[_algorithm_template alg1,alg2,...]'
  {-b,-eltorito-boot}'[set El Torito boot image name]'
  '-eltorito-alt-boot[start specifying alternative El Torito boot parameters]'
  '--efi-boot[set El Torito EFI boot image name and type]'
  '-e[set EFI boot image name (more rawly)]'
  {-c,-eltorito-catalog}'[set El Torito boot catalog name]'
  '--boot-catalog-hide[hide boot catalog from ISO9660/RR and Joliet]'
  '-boot-load-size[set numbers of load sectors]'
  '-hard-disk-boot[boot image is a hard disk image]'
  '-no-emul-boot[boot image is no emulation image]'
  '-boot-info-table[patch boot image with info table]'
  '--grub2-boot-info[patch boot image at byte 2548]'
  '-eltorito-id[set El Torito Id String]'
  '-eltorito-selcrit[set El Torito Selection Criteria]'
  '-isohybrid-gpt-basdat[mark El Torito boot image as Basic Data in GPT]'
  '-isohybrid-gpt-hfsplus[mark El Torito boot image as HFS+ in GPT]'
  '-isohybrid-apm-hfsplus[mark El Torito boot image as HFS+ in APM]'
  '-part[_like_isohybrid Mark in MBR, GPT, APM without -isohybrid-mbr]'
  '--gpt[_disk_guid GUID Set GPT disk GUID or choose automatic GUID]'
  {-G,-generic-boot}'[set generic boot image name]'
  '--embedded-boot[alias of -G]'
  '--protective-msdos-label[patch System Area by partition table]'
  '--mbr-force-bootable[enforce existence of bootable flag in MBR]'
  '-partition[_offset LBA Make image mountable by first partition, too]'
  '-partition[_sec_hd NUMBER Define number of sectors per head]'
  '-partition[_hd_cyl NUMBER Define number of heads per cylinder]'
  '-partition[_cyl_align MODE Control cylinder alignment: off, on, auto, all]'
  '-mips-boot[set mips boot image name (relative to image root)]'
  '-mipsel-boot[set mipsel boot image name (relative to image root)]'
  {-B,-sparc-boot}'[set sparc boot image names]'
  '-sparc-label[text Set sparc boot disk label]'
  '-hppa-cmdline[set hppa boot command line]'
  '-hppa-kernel-32[set hppa 32-bit image name (relative to image root)]'
  '-hppa-kernel-64[set hppa 64-bit image name (relative to image root)]'
  '-hppa-bootloader[set hppa boot loader file name (relative to image root)]'
  '-hppa-ramdisk[set hppa ramdisk file name (relative to image root)]'
  '-hppa-hdrversion[set hppa PALO header version to 4 or 5]'
  '-alpha-boot[set alpha boot image name (relative to image root)]'
  '--grub2-sparc-core[set path of core file for disk label patching]'
  '-efi-boot-part[set data source for EFI System Partition]'
  '-chrp-boot-part[mark ISO image size by MBR partition type 0x96]'
  '-chrp-boot[alias of -chrp-boot-part]'
  '-prep-boot-part[set data source for MBR partition type 0x41]'
  '-append[_partition NUMBER TYPE FILE]'
  '-appended[_part_as_gpt mark appended partitions in GPT instead of MBR.]'
  '-appended[_part_as_apm mark appended partitions in APM.]'
  '--modification-date[override date of creation and modification]'
  '--set[_all_file_dates TIME Override mtime, atime, ctime in all files]'
  '-isohybrid-mbr[set SYSLINUX mbr/isohdp\[fp\]x*.bin for isohybrid]'
  '--grub2-mbr[set GRUB2 MBR for boot image address patching]'
  '--sort-weight[FILE Set LBA weight number to file or file tree]'
  '--sort-weight-list[read list of NUMBER FILE pairs for --sort-weight]'
  {--sort-weight-patterns,--sort-weight-list}'[pattern expansion]'
  '--stdio[_sync on|off|number Control forced output to disk files]'
  '--no-emul-toc[save 64 kB size on random access output files]'
  '--emul-toc[multi-session history on such output files]'
  '--old-empty[use old style block addresses for empty files]'
  {-z,-transparent-compression}'[enable transparent compression of files]'
  '-root[set root directory for all new files and directories]'
  '-old-root[set root directory in previous session that is searched for files]'
  '--old-root-no-md5[do not record and use MD5 with -old-root]'
  '--old-root-no-ino[do not use disk inode numbers with -old-root]'
  '--old-root-devno[use disk device numbers with -old-root]'
  '-log-file[re-direct messages to LOG_FILE]'
  '--no[_rc Do not execute startup files]'
  {-v,-verbose}'[verbose]'
  '-version[print the current version]'
  '*:filename:_files'
)

_arguments -s $arguments
