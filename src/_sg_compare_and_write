#compdef sg_compare_and_write

local arguments

arguments=(
  '--in[\[--inw=WF\] --lba=LBA \[--num=NUM\]]'
  '--dpo[|-d set the dpo bit in cdb (def: clear)]'
  '--fua[|-f set the fua bit in cdb (def: clear)]'
  '--fua[_nv|-F set the fua_nv bit in cdb (def: clear)]'
  '--group[GN GN is GROUP NUMBER to set in cdb (def: 0)]'
  '--help[|-h print out usage message]'
  '--in[IF IF is a file containing a compare buffer and]'
  '--inw[WF WF is a file containing a write buffer]'
  '--lba[LBA LBA of the first block of the compare and write]'
  '--num[NUM number of blocks to compare/write (def: 1)]'
  '--quiet[|-q suppress MISCOMPARE report to stderr]'
  '--timeout[TO timeout for the command (def: 60 secs)]'
  '--verbose[|-v increase verbosity (use -vv for more)]'
  '--version[|-V print version string then exit]'
  '--wrprotect[WP write protect information (def: 0)]'
  '--xferlen[LEN number of bytes to transfer. Default is]'
  '*:filename:_files'
)

_arguments -s $arguments
