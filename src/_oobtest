#compdef oobtest

local arguments

arguments=(
  {-B,--bredr}'[use BR/EDR transport]'
  {-L,--le}'[use LE transport]'
  {-S,--sc}'[use Secure Connections]'
  {-O,--sconly}'[use Secure Connections Only]'
  {-P,--legacy}'[use Legacy Pairing]'
  {-R,--random}'[use Static random address]'
  {-Y,--privacy}'[use LE privacy feature]'
  {-D,--debug}'[use Pairing debug keys]'
  {-C,--cross}'[use cross-transport pairing]'
  {-0,--tk}'[provide LE legacy OOB data]'
  {-1,--p192}'[provide P-192 OOB data]'
  {-2,--p256}'[provide P-256 OOB data]'
  {-I,--initiator}'[initiator provides OOB data]'
  {-A,--acceptor}'[acceptor provides OOB data]'
  '(- * :)'{-h,--help}'[show help options]'
  '*:filename:_files'
)

_arguments -s $arguments
