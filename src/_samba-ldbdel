#compdef samba-ldbdel

local arguments

arguments=(
  {-H,--url}'[database URL]'
  {-b,--basedn}'[base DN]'
  {-e,--editor}'[external editor]'
  {-s,--scope}'[search scope]'
  {-v,--verbose}'[increase verbosity]'
  '--trace[enable tracing]'
  {-i,--interactive}'[input from stdin]'
  {-r,--recursive}'[recursive delete]'
  '--modules-path[modules path]'
  '--num-searches[number of test searches]'
  '--num-records[number of test records]'
  {-a,--all}'[(|(objectClass=*)(distinguishedName=*))]'
  '--nosync[non-synchronous transactions]'
  {-S,--sorted}'[sort attributes]'
  '-o[ldb_connect option]'
  '--controls[controls]'
  '--show-binary[display binary LDIF]'
  '--paged[use a paged search]'
  '--show-deleted[show deleted objects]'
  '--show-recycled[show recycled objects]'
  '--show-deactivated-link[show deactivated links]'
  '--reveal[reveal ldb internals]'
  '--relax[pass relax control]'
  '--cross-ncs[search across NC boundaries]'
  '--extended-dn[show extended DNs]'
  '--usage[display brief usage message]'
  {-d,--debuglevel}'[set debug level]'
  '--debug-stderr[send debug output to STDERR]'
  {-s,--configfile}'[use alternative configuration]'
  '--option[value                     Set smb.conf option from command]'
  {-l,--log-basename}'[basename for log/debug files]'
  '--leak-report[enable talloc leak reporting on]'
  '--leak-report-full[enable full talloc leak]'
  {-U,--user}'[/\]USERNAME\[%PASSWORD\]     Set the network username]'
  {-N,--no-pass}'[dont ask for a password]'
  '--password[password]'
  {-A,--authentication-file}'[get the credentials from a file]'
  {-P,--machine-pass}'[use stored machine account]'
  '--simple-bind-dn[DN to use for a simple bind]'
  {-k,--kerberos}'[use Kerberos, -k \[yes|no\]]'
  '--krb5-ccache[credentials cache location for]'
  {-S,--sign}'[sign connection to prevent]'
  {-e,--encrypt}'[encrypt connection for privacy]'
  {-R,--name-resolve}'[use these name resolution]'
  {-O,--socket-options}'[socket options to use]'
  {-n,--netbiosname}'[primary netbios name]'
  {-S,--signing}'[set the client signing state]'
  {-W,--workgroup}'[set the workgroup name]'
  '--realm[set the realm name]'
  {-i,--scope}'[use this Netbios scope]'
  {-m,--maxprotocol}'[set max protocol level]'
  {-V,--version}'[print version]'
  '*:filename:_files'
)

_arguments -s $arguments
