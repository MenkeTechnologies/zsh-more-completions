#compdef appstream-util

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show help options]'
  '--nonet[do not use network access]'
  {-v,--verbose}'[show extra debugging information]'
  '--version[show version]'
  '--profile[enable profiling]'
  '*:filename:_files'
)

_arguments -s $arguments
