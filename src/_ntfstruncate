#compdef ntfstruncate

local arguments

arguments=(
  '-n[do not write to disk]'
  '-f[force execution despite errors]'
  '-q[quiet execution]'
  '-v[verbose execution]'
  '-vv[very verbose execution]'
  '-V[display version information]'
  '-l[display licensing information]'
  '-h[display this help]'
  '*:filename:_files'
)

_arguments -s $arguments
