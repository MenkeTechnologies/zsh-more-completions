#compdef xpr

local arguments

arguments=(
  {-append,-noff,-output}'[]'
  '-compact[]'
  '-device[03 | la100 | ps | lw | pp | ljet | pjet | pjetxl}]'
  '-dump[]'
  '-gamma[]'
  '-gray[2 | 3 | 4}]'
  {-height,-width}'[]'
  {-header,-trailer}'[]'
  {-landscape,-portrait}'[]'
  {-left,-top}'[]'
  '-noposition[]'
  '-nosixopt[]'
  '-plane[]'
  '-psfig[]'
  '-render[]'
  '-report[]'
  '-rv[]'
  '-scale[]'
  '-slide[]'
  '-split[]'
  '*:filename:_files'
)

_arguments -s $arguments
