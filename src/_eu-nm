#compdef eu-nm

local arguments

arguments=(
  {-a,--debug-syms}'[display debugger-only symbols]'
  '--defined-only[display only defined symbols]'
  {-D,--dynamic}'[display dynamic symbols instead of normal symbols]'
  {-g,--extern-only}'[display only external symbols]'
  {-s,--print-armap}'[include index for symbols from archive members]'
  {-u,--undefined-only}'[display only undefined symbols]'
  {-A,--print-file-name}'[print name of the input file before every symbol]'
  '-B[same as --format=bsd]'
  {-f,--format}'[use the output format FORMAT. FORMAT can be]'
  '--mark-special[mark special symbols]'
  {-P,--portability}'[same as --format=posix]'
  {-S,--print-size}'[print size of defined symbols]'
  {-t,--radix}'[use RADIX for printing symbol values]'
  '--color[colorize the output. WHEN defaults to always or]'
  {-C,--demangle}'[decode low-level symbol names into source code]'
  {-n,--numeric-sort}'[sort symbols numerically by address]'
  {-p,--no-sort}'[do not sort the symbols]'
  {-r,--reverse-sort}'[reverse the sense of the sort]'
  '--usage[give a short usage message]'
  {-V,--version}'[print program version]'
  '*:filename:_files'
)

_arguments -s $arguments
