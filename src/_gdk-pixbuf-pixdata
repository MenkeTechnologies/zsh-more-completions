#compdef gdk-pixbuf-pixdata

local arguments

arguments=(
  {-r,--rle}'[compress the image data using RLE]'
  '(- * :)'{-h,--help}'[show this help message]'
  {-v,--version}'[print version informations]'
  '--g-fatal-warnings[make warnings fatal (abort)]'
  '*:filename:_files'
)

_arguments -s $arguments
