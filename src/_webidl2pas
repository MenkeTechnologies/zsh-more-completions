#compdef webidl2pas

local arguments

arguments=(
  '(- * :)'{-h,--help}'[this help text]'
  {-c,--constexternal}'[write consts as external const (no value)]'
  {-e,--expandunionargs}'[add overloads for all Union typed function arguments]'
  {-d,--dicttoclass}'[write dictionaries as classes]'
  {-i,--input}'[input webidl file]'
  {-m,--implementation}'[include file as implementation]'
  {-n,--include}'[include file at end of interface]'
  {-o,--output}'[output file. Defaults to unit name with .pas extension appended.]'
  {-p,--optionsinheader}'[add options to header of generated file]'
  {-t,--typealiases}'[A comma separated list of type aliases in Alias=Name form]'
  {-u,--unitname}'[name for unit. Defaults to input file without extension.]'
  {-v,--verbose}'[output some diagnostic information]'
  {-w,--webidlversion}'[set web IDL version. Allowed values: v1 or v2]'
  {-x,--extra}'[extra units to put in uses clause (comma separated list)]'
  '*:filename:_files'
)

_arguments -s $arguments
