#compdef hilatex

local arguments

arguments=(
  '-help[display this help and exit]'
  '-version[output version information and exit]'
  '-etex[enable e-TeX extensions]'
  '-ltx[enable LaTeX extensions, implies -etex]'
  '-ini[be initex for dumping formats; this is]'
  '-progname[set program (and fmt) name to STRING]'
  '-fmt[use FMTNAME instead of program name or a %& line]'
  '-output-directory[use existing DIR as the directory to write files to]'
  '-jobname[set the TeX \jobname to STRING]'
  '-interaction[set interaction mode (STRING=batchmode/]'
  '-kpathsea-debug[set path searching debugging flags according]'
  '-recorder[enable filename recorder]'
  '-cnf-line[process STRING like a line in texmf.cnf]'
  '-compress[enable compression of section 1 and 2]'
  '-hyphenate-first-word[hyphenate the first word of a paragraph]'
  '-no-hyphenate-first-word[dont hyphenate the first word of a paragraph]'
  '-resolution[set the resolution to NUMBER dpi]'
  '-mfmode[set the METAFONT mode to MODE]'
  '*:filename:_files'
)

_arguments -s $arguments
