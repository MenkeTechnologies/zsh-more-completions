#compdef xkill

local arguments

arguments=(
  '-display[X server to contact]'
  '-id[resource whose client is to be killed]'
  '-frame[dont ignore window manager frames]'
  '-button[specific button to be pressed to select window]'
  '-all[kill all clients with top level windows]'
  '-version[print version and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
