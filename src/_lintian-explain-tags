#compdef lintian-explain-tags

# Autogenerated from man page /usr/share/man/man1/lintian-explain-tags.1.gz

local arguments

arguments=(
    {-l,--list-tags}'[List all tags Lintian knows about in sorted order.]'
    {-h,--help}'[Display usage information and exit.]'
    '--include-dir[Use dir as an additional \\*(L"Lintian root\\*(R".]:dir'
    '--profile[Use the severities from the vendor profile prof when displaying tags.]:prof'
    {-t,--tag,--tags}'[This option has no effect. It exists for historical reasons.]'
    '*:filename:_files'
)
_arguments -s $arguments
