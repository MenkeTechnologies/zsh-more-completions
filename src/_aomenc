#compdef aomenc

local arguments

arguments=(
  '--help[show usage options and exit]'
  {-c,--cfg}'[config file to use]'
  {-D,--debug}'[debug mode (makes output deterministic)]'
  {-o,--output}'[output filename]'
  '--codec[codec to use]'
  {-p,--passes}'[number of passes (1/2)]'
  '--pass[pass to execute (1/2)]'
  '--fpf[first pass statistics file name]'
  '--limit[stop encoding after n input frames]'
  '--skip[skip the first n input frames]'
  '--good[use Good Quality Deadline]'
  {-q,--quiet}'[do not print encode progress]'
  {-v,--verbose}'[show encoder parameters]'
  '--psnr[show PSNR in status line]'
  '--webm[output WebM (default when WebM IO is enabled)]'
  '--ivf[output IVF]'
  '--obu[output OBU]'
  {-P,--output-partitions}'[makes encoder output partitions. Requires IVF output!]'
  '--q-hist[show quantizer histogram (n-buckets)]'
  '--rate-hist[show rate histogram (n-buckets)]'
  '--disable-warnings[disable warnings about potentially incorrect encode settings.]'
  {-y,--disable-warning-prompt}'[display warnings, but do not prompt user to continue.]'
  '--test-decode[test encode/decode mismatch]'
  '--yv12[input file is YV12]'
  '--i420[input file is I420 (default)]'
  '--i422[input file is I422]'
  '--i444[input file is I444]'
  {-u,--usage}'[usage profile number to use]'
  {-t,--threads}'[max number of threads to use]'
  '--profile[bitstream profile number to use]'
  {-w,--width}'[frame width]'
  {-h,--height}'[frame height]'
  '--forced[_max_frame_width Maximum frame width value to force]'
  '--forced[_max_frame_height Maximum frame height value to force]'
  '--stereo-mode[stereo 3D video format]'
  '--timebase[output timestamp precision (fractional seconds)]'
  '--fps[stream frame rate (rate/scale)]'
  '--global-error-resilient[enable global error resiliency features]'
  {-b,--bit-depth}'[bit depth for codec (8 for version <=1, 10 or 12 for version 2)]'
  '--lag-in-frames[max number of frames to lag]'
  '--large-scale-tile[large scale tile coding (0: off (default), 1: on)]'
  '--monochrome[monochrome video (no chroma planes)]'
  '--full-still-picture-hdr[use full header for still picture]'
  '--drop-frame[temporal resampling threshold (buf %)]'
  '--resize-mode[frame resize mode]'
  '--resize-denominator[frame resize denominator]'
  '--resize-kf-denominator[frame resize keyframe denominator]'
  '--superres-mode[frame super-resolution mode]'
  '--superres-denominator[frame super-resolution denominator]'
  '--superres-kf-denominator[frame super-resolution keyframe denominator]'
  '--superres-qthresh[frame super-resolution qindex threshold]'
  '--superres-kf-qthresh[frame super-resolution keyframe qindex threshold]'
  '--end-usage[rate control mode]'
  '--target-bitrate[bitrate (kbps)]'
  '--min-q[minimum (best) quantizer]'
  '--max-q[maximum (worst) quantizer]'
  '--undershoot-pct[datarate undershoot (min) target (%)]'
  '--overshoot-pct[datarate overshoot (max) target (%)]'
  '--buf-sz[client buffer size (ms)]'
  '--buf-initial-sz[client initial buffer size (ms)]'
  '--buf-optimal-sz[client optimal buffer size (ms)]'
  '--bias-pct[CBR/VBR bias (0=CBR, 100=VBR)]'
  '--minsection-pct[GOP min bitrate (% of target)]'
  '--maxsection-pct[GOP max bitrate (% of target)]'
  '--enable-fwd-kf[enable forward reference keyframes]'
  '--kf-min-dist[minimum keyframe interval (frames)]'
  '--kf-max-dist[maximum keyframe interval (frames)]'
  '--disable-kf[disable keyframe placement]'
  '--cpu-used[CPU Used (0..8)]'
  '--dev-sf[dev Speed (0..255)]'
  '--auto-alt-ref[enable automatic alt reference frames]'
  '--sharpness[loop filter sharpness (0..7)]'
  '--static-thresh[motion detection threshold]'
  '--single-tile-decoding[single tile decoding (0: off (default), 1: on)]'
  '--tile-columns[number of tile columns to use, log2]'
  '--tile-rows[number of tile rows to use, log2 (set to 0 while threads > 1)]'
  '--arnr-maxframes[altRef max frames (0..15)]'
  '--arnr-strength[altRef filter strength (0..6)]'
  '--tune[distortion metric tuned with]'
  '--cq-level[constant/Constrained Quality level]'
  '--max-intra-rate[max I-frame bitrate (pct)]'
  '--max-inter-rate[max P-frame bitrate (pct)]'
  '--gf-cbr-boost[boost for Golden Frame in CBR mode (pct)]'
  '--lossless[lossless mode (0: false (default), 1: true)]'
  '--enable-cdef[enable the constrained directional enhancement filter (0: false, 1: true (default))]'
  '--enable-restoration[enable the loop restoration filter (0: false, 1: true (default))]'
  '--disable-trellis-quant[disable trellis optimization of quantized coefficients (0: false (default) 1: true)]'
  '--enable-qm[enable quantisation matrices (0: false (default), 1: true)]'
  '--qm-min[min quant matrix flatness (0..15), default is 8]'
  '--qm-max[max quant matrix flatness (0..15), default is 15]'
  '--enable-dist-8x8[enable dist-8x8 (0: false (default), 1: true)]'
  '--frame-parallel[enable frame parallel decodability features (0: false (default), 1: true)]'
  '--error-resilient[enable error resilient features (0: false (default), 1: true)]'
  '--aq-mode[adaptive quantization mode (0: off (default), 1: variance 2: complexity, 3: cyclic refresh)]'
  '--deltaq-mode[delta qindex mode (0: off (default), 1: deltaq 2: deltaq + deltalf)]'
  '--frame-boost[enable frame periodic boost (0: off (default), 1: on)]'
  '--noise-sensitivity[noise sensitivity (frames to blur)]'
  '--tune-content[tune content type]'
  '--cdf-update-mode[CDF update mode for entropy coding (0: no CDF update; 1: update CDF on all frames(default); 2: selectively update CDF on some frames]'
  '--color-primaries[color primaries (CICP) of input content:]'
  '--transfer-characteristics[characteristics (CICP) of input content:]'
  '--matrix-coefficients[matrix coefficients (CICP) of input content:]'
  '--chroma-sample-position[the chroma sample position when chroma 4:2:0 is signaled:]'
  '--min-gf-interval[min gf/arf frame interval (default 0, indicating in-built behavior)]'
  '--max-gf-interval[max gf/arf frame interval (default 0, indicating in-built behavior)]'
  '--sb-size[superblock size to use]'
  '--num-tile-groups[maximum number of tile groups, default is 1]'
  '--mtu-size[MTU size for a tile group, default is 0 (no MTU targeting), overrides maximum number of tile groups]'
  '--timing-info[signal timing info in the bitstream (model unly works for no hidden frames, no super-res yet):]'
  '--film-grain-test[film grain test vectors (0: none (default), 1: test-1 2: test-2, ... 16: test-16)]'
  '--film-grain-table[path to file containing film grain parameters]'
  '--enable-ref-frame-mvs[enable temporal mv prediction (default is 1)]'
  {-b,--bit-depth}'[bit depth for codec (8 for version <=1, 10 or 12 for version 2)]'
  '--input-bit-depth[bit depth of input]'
  '--sframe-dist[S-Frame interval (frames)]'
  '--sframe-mode[S-Frame insertion mode (1..2)]'
  '--annexb[save as Annex-B]'
  '*:filename:_files'
)

_arguments -s $arguments
