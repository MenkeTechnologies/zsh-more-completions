#compdef dnssec-coverage

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this help message and exit]'
  '-K[a directory containing keys to process]'
  '-f[zone master file]'
  '-m[the longest TTL in the zone(s)]'
  '-d[the DNSKEY TTL]'
  '-r[the RRSIG refresh interval in seconds \[default: 22.5 days\]]'
  '-c[path to named-compilezone]'
  '-l[length of time to check for DNSSEC coverage \[default: 0]'
  '-z[only check zone-signing keys (ZSKs)]'
  '-k[only check key-signing keys (KSKs)]'
  {-D,--debug}'[turn on debugging output]'
  {-v,--version}'[show programs version number and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
