local arguments

arguments=(
  '-q[only output resultant string. No extra junk text]'
  '-e[on buffer overwrite error checking logic]'
  '-l[a length test]'
  '-r[process whole file ifname (this is the input file)]'
  '-w[the output filename for whole file processing]'
  '*:filename:_files'
)

_arguments -s $arguments
