#compdef aarch64-linux-gnu-gp-collect-app

local arguments

arguments=(
  '--version[print the version number and exit.]'
  '--help[print usage information and exit.]'
  '--verbose[}  enable (on) or disable (off) verbose mode; the default is "off".]'
  '-p[}  disable (off) or enable (on) clock-profiling using a default]'
  '-h[...,<ctr_n_def>}  enable hardware event counter profiling and select]'
  '-o[specify the name for (and path to) the experiment directory; the]'
  '-O[the same as -o, but unlike the -o option, silently overwrite an]'
  '-C[add up to 10 comment labels to the experiment; comments appear in]'
  '-j[} enable (on), or disable (off) Java profiling when the target]'
  '-J[specify arguments to the JVM.]'
  '-t[specify the duration over which to record data; the default unit]'
  '-n[dry run; display several run-time settings, but do not run the]'
  '-y[r\]     specify delayed initialization and a pause/resume signal; by default]'
  '-F[<regex>}  control to follow descendant processes; disable (off), enable (on)]'
  '-a[}  specify archiving of binaries and other files;]'
  '-S[}  disable (off) or enable (on) periodic sampling of process-wide resource]'
  '-l[specify a signal that will trigger a sample of process-wide resource utilization.]'
  '-s[<API>\]  enable synchronization wait tracing; <option> is used to define the specifics]'
  '-H[}        disable (off), or enable (on) heap tracing; the default is "-H off".]'
  '-i[}        disable (off), or enable (on) I/O tracing; the default is "-i off".]'
  '*:filename:_files'
)

_arguments -s $arguments
