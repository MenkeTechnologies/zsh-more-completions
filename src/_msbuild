#compdef msbuild

local arguments

arguments=(
  '-target[:<targets>  Build these targets in this project. Use a semicolon or a]'
  '-target[:Resources;Compile]'
  '-property[:<n>=<v>  Set or override these project-level properties. <n> is]'
  '-property[:WarningLevel=2;OutDir=bin\Debug\]'
  '-maxCpuCount[specifies the maximum number of concurrent processes to]'
  '-toolsVersion[:<version>]'
  '-tv[)]'
  '-toolsVersion[:3.5]'
  '-verbosity[:<level> Display this amount of information in the event log.]'
  '-verbosity[:quiet]'
  '-consoleLoggerParameters[:<parameters>]'
  '-consoleLoggerParameters[:PerformanceSummary;NoSummary;]'
  '-noConsoleLogger[disable the default console logger and do not log events]'
  '-fileLogger[logs the build output to a file. By default]'
  '-fileLoggerParameters[provides any extra parameters for file loggers.]'
  '-fileLoggerParameters[also used by any distributed]'
  '-fileLoggerParameters[:LogFile=MyLog.log;Append;]'
  '-flp[:Summary;Verbosity=minimal;LogFile=msbuild.sum]'
  '-flp1[:warningsonly;logfile=msbuild.wrn]'
  '-flp2[:errorsonly;logfile=msbuild.err]'
  '-distributedLogger[:<central logger>*<forwarding logger>]'
  '-dl[:XMLLogger,MyLogger,Version=1.0.2,Culture=neutral]'
  '-dl[:MyLogger,C:\My.dll*ForwardingLogger,C:\Logger.dll]'
  '-distributedFileLogger[logs the build output to multiple log files, one log file]'
  '-logger[:<logger>   Use this logger to log events from MSBuild. To specify]'
  '-logger[:XMLLogger,MyLogger,Version=1.0.2,Culture=neutral]'
  '-logger[:XMLLogger,C:\Loggers\MyLogger.dll;OutputAsHTML]'
  '-binaryLogger[\]output.binlog\[;ProjectImports={None,Embed,ZipFile}\]\]]'
  '-bl[]'
  '-bl[:output.binlog]'
  '-bl[:output.binlog;ProjectImports=None]'
  '-bl[:output.binlog;ProjectImports=ZipFile]'
  '-bl[:..\..\custom.binlog]'
  '-binaryLogger[]'
  '-warnAsError[;code2\]\]]'
  '-warnAsError[:MSB4130]'
  '-warnAsMessage[;code2\]\]]'
  '-warnAsMessage[:MSB3026]'
  '-ignoreProjectExtensions[:<extensions>]'
  '-ignoreProjectExtensions[:.sln]'
  '-nodeReuse[:<parameters>]'
  '-nr[:true]'
  '-preprocess[creates a single, aggregated project file by]'
  '-pp[:out.txt]'
  '-detailedSummary[shows detailed information at the end of the build]'
  '-restore[runs a target named Restore prior to building]'
  '-restore[:True. Use the parameter to override]'
  '-restoreProperty[:<n>=<v>]'
  '-restoreProperty[:IsRestore=true;MyProperty=value]'
  '-profileEvaluation[:<file>]'
  '-interactive[indicates that actions in the build are allowed to]'
  '-interactive[:true.  Use the parameter to override a]'
  '-isolateProjects[causes MSBuild to build each project in isolation.]'
  '-inputResultsCaches[:<cacheFile>...]'
  '-outputResultsCache[:\[cacheFile\]]'
  '-graphBuild[causes MSBuild to construct and build a project graph.]'
  '-noAutoResponse[do not auto-include any MSBuild.rsp files. (Short form:]'
  '-noAutoRsp[)]'
  '-noLogo[do not display the startup banner and copyright message.]'
  '-version[display version information only. (Short form: -ver)]'
  '-help[display this usage message. (Short form: -\? or -h)]'
  '-p[:Configuration=Debug;TargetFrameworkVersion=v3.5]'
  '*:filename:_files'
)

_arguments -s $arguments
