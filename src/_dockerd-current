#compdef dockerd-current

local arguments

arguments=(
  '--add-registry[registry to query before a public one (default \[\])]'
  '--add-runtime[register an additional OCI compatible runtime (default \[\])]'
  '--api-cors-header[set CORS headers in the Engine API]'
  '--authorization-plugin[authorization plugins to load (default \[\])]'
  '--bip[specify network bridge IP]'
  '--block-registry[dont contact given registry (default \[\])]'
  {-b,--bridge}'[attach containers to a network bridge]'
  '--cgroup-parent[set parent cgroup for all containers]'
  '--cluster-advertise[address or interface name to advertise]'
  '--cluster-store[URL of the distributed storage backend]'
  '--cluster-store-opt[set cluster store options (default map\[\])]'
  '--config-file[daemon configuration file (default "/etc/docker/daemon.json")]'
  '--containerd[path to containerd socket]'
  '--cpu-rt-period[limit the CPU real-time period in microseconds]'
  '--cpu-rt-runtime[limit the CPU real-time runtime in microseconds]'
  {-D,--debug}'[enable debug mode]'
  '--default-gateway[container default gateway IPv4 address]'
  '--default-gateway-v6[container default gateway IPv6 address]'
  '--default-runtime[default OCI runtime for containers (default "runc")]'
  '--default-ulimit[default ulimits for containers (default \[\])]'
  '--disable-legacy-registry[disable contacting legacy registries]'
  '--dns[DNS server to use (default \[\])]'
  '--dns-opt[DNS options to use (default \[\])]'
  '--dns-search[DNS search domains to use (default \[\])]'
  '--enable-secrets[enable Secrets (default true)]'
  '--exec-opt[runtime execution options (default \[\])]'
  '--exec-root[root directory for execution state files (default "/var/run/docker")]'
  '--experimental[enable experimental features]'
  '--fixed-cidr[IPv4 subnet for fixed IPs]'
  '--fixed-cidr-v6[IPv6 subnet for fixed IPs]'
  {-g,--graph}'[root of the Docker runtime (default "/var/lib/docker")]'
  {-G,--group}'[group for the unix socket (default "docker")]'
  '--help[print usage]'
  {-H,--host}'[daemon socket(s) to connect to (default \[\])]'
  '--icc[enable inter-container communication (default true)]'
  '--init[run an init in the container to forward signals and reap processes]'
  '--init-path[path to the docker-init binary]'
  '--insecure-registry[enable insecure registry communication (default \[\])]'
  '--ip[default IP when binding container ports (default 0.0.0.0)]'
  '--ip-forward[enable net.ipv4.ip_forward (default true)]'
  '--ip-masq[enable IP masquerading (default true)]'
  '--iptables[enable addition of iptables rules (default true)]'
  '--ipv6[enable IPv6 networking]'
  '--label[set key=value labels to the daemon (default \[\])]'
  '--live-restore[enable live restore of docker when containers are still running]'
  '--log-driver[default driver for container logs (default "json-file")]'
  {-l,--log-level}'[set the logging level ("debug", "info", "warn", "error", "fatal") (default "info")]'
  '--log-opt[default log driver options for containers (default map\[\])]'
  '--max-concurrent-downloads[set the max concurrent downloads for each pull (default 3)]'
  '--max-concurrent-uploads[set the max concurrent uploads for each push (default 5)]'
  '--max-health-check-retries[set the maximum number of health check retries before forced containerd restart (default 3)]'
  '--metrics-addr[set default address and port to serve the metrics api on]'
  '--mtu[set the containers network MTU]'
  '--oom-score-adjust[set the oom_score_adj for the daemon (default -500)]'
  {-p,--pidfile}'[path to use for daemon PID file (default "/var/run/docker.pid")]'
  '--raw-logs[full timestamps without ANSI coloring]'
  '--registry-mirror[preferred Docker registry mirror (default \[\])]'
  '--seccomp-profile[path to seccomp profile]'
  '--selinux-enabled[enable selinux support]'
  '--shutdown-timeout[set the default shutdown timeout (default 15)]'
  '--signature-verification[check images signatures on pull (default true)]'
  '--skip-schema2-push[override push behavior to push only schema1 manifests]'
  {-s,--storage-driver}'[storage driver to use]'
  '--storage-opt[storage driver options (default \[\])]'
  '--swarm-default-advertise-addr[set default address or interface for swarm advertised address]'
  '--tls[use TLS; implied by --tlsverify]'
  '--tlscacert[trust certs signed only by this CA (default "/home/p/.docker/ca.pem")]'
  '--tlscert[path to TLS certificate file (default "/home/p/.docker/cert.pem")]'
  '--tlskey[path to TLS key file (default "/home/p/.docker/key.pem")]'
  '--tlsverify[use TLS and verify the remote]'
  '--userland-proxy[use userland proxy for loopback traffic (default true)]'
  '--userland-proxy-path[path to the userland proxy binary]'
  '--userns-remap[user/Group setting for user namespaces]'
  {-v,--version}'[print version information and quit]'
  '*:filename:_files'
)

_arguments -s $arguments
