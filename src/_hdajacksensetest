#compdef hdajacksensetest

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show help options]'
  {-c,--card}'[card index (as can be seen in /proc/asound/cards)]'
  {-d,--codec}'[codec device index (as can be seen in /proc/asound/cardX/codecY)]'
  {-a,--allpins}'[try all pins, even those who (probably) does not have a physical jack]'
  {-s,--setpinsense}'[execute Set pin sense before the pin sense is measured]'
  '*:filename:_files'
)

_arguments -s $arguments
