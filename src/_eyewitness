#compdef eyewitness

local arguments

arguments=(
  '--web[HTTP Screenshot using Selenium]'
  '-f[line seperated file containing URLs to capture]'
  '-x[.xml Nmap XML or .Nessus file]'
  '--single[URL Single URL/Host to capture]'
  '--no-dns[skip DNS resolution when connecting to websites]'
  '--timeout[maximum number of seconds to wait while requesting a]'
  '--jitter[of Seconds]'
  '--threads[of Threads]'
  '--max-retries[retries on a timeout]'
  '-d[name Directory name for report output]'
  '--results[per Page]'
  '--no-prompt[dont prompt to open the report]'
  '--user-agent[agent]'
  '--cycle[agent Type]'
  '--difference[threshold]'
  '--show-selenium[show display for selenium]'
  '--resolve[resolve IP/Hostname for targets]'
  '--add-http-ports[comma-seperated additional port(s) to assume are http]'
  '--add-https-ports[comma-seperated additional port(s) to assume are https]'
  '--only-ports[comma-seperated list of exclusive ports to use (e.g.]'
  '--prepend-https[prepend http:// and https:// to URLs without either]'
  '--resume[.db Path to db file if you want to resume]'
  '--ocr[use OCR to determine RDP usernames]'
  '*:filename:_files'
)

_arguments -s $arguments
