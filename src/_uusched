#compdef uusched

local arguments

arguments=(
  {-s,-S,--system}'[call system (-S implies -f)]'
  {-f,--force}'[Force call despite system status]'
  '-r[1 for master, 0 for slave (default)]'
  '--master[Act as master]'
  '--slave[Act as slave (default)]'
  {-p,--port}'[specify port]'
  {-l,--prompt}'[Prompt for login name and password]'
  {-e,--loop}'[Endless loop of login prompts and daemon execution]'
  {-w,--wait}'[After calling out, wait for incoming calls]'
  {-q,--nouuxqt}'[Dont start uuxqt when done]'
  {-c,--quiet}'[Dont log bad time or no work warnings]'
  {-C,--ifwork}'[Only call named system if there is work]'
  {-D,--nodetach}'[Dont detach from controlling terminal]'
  {-u,--login}'[Set login name (privileged users only)]'
  {-i,--stdin}'[type of standard input (only TLI supported)]'
  {-z,--try-next}'[If a call fails, try the next alternate]'
  {-x,-X,--debug}'[set debugging level]'
  {-I,--config}'[set configuration file to use]'
  {-v,--version}'[Print version and exit]'
  '--help[Print help and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
