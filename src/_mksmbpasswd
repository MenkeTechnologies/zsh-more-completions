  {-F,--field-separator}'[]'
  '-v[val --assign=var=val]'
  {-b,--characters-as-bytes}'[]'
  {-c,--traditional}'[]'
  {-C,--copyright}'[]'
  {-d,--dump-variables}'[]'
  {-D,--debug}'[]'
  '-e[ --source=program-text]'
  {-E,--exec}'[]'
  {-g,--gen-pot}'[]'
  '(- * :)'{-h,--help}'[]'
  {-i,--include}'[]'
  {-l,--load}'[]'
  {-L,--lint}'[]'
  {-M,--bignum}'[]'
  {-N,--use-lc-numeric}'[]'
  {-n,--non-decimal-data}'[]'
  {-o,--pretty-print}'[]'
  {-O,--optimize}'[]'
  {-p,--profile}'[]'
  {-P,--posix}'[]'
  {-r,--re-interval}'[]'
  {-s,--no-optimize}'[]'
  {-S,--sandbox}'[]'
  {-t,--lint-old}'[]'
