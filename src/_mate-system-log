#compdef mate-system-log

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show help options]'
  '--help-all[show all help options]'
  '--help-gtk[show GTK+ Options]'
  {-V,--version}'[show the applications version]'
  '--display[X display to use]'
  '*:filename:_files'
)

_arguments -s $arguments
