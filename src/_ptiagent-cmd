#compdef ptiagent-cmd

local arguments

arguments=(
  '--help[show this help message.]'
  '--info[start PT download and installation if PTIAgent and host versions differ.]'
  '--install[start PT download and installation without version check.]'
  '--ver[compare versions of PTIAgent and host.]'
  '--download[only download PT installation package.]'
  '--eject[eject PT installation ISO image.]'
  '*:filename:_files'
)

_arguments -s $arguments
