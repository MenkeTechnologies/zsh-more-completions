#compdef ausearch

local arguments

arguments=(
  {-a,--event}'[event id> search based on audit event id]'
  '--arch[search based on the CPU architecture]'
  {-c,--comm}'[search based on command line name]:Comm name:'
  '--checkpoint[file> search from last complete event]'
  '--debug[write malformed events that are skipped to stderr]'
  {-e,--exit}'[search based on syscall exit code]:Exit code or errno:'
  {-f,--file}'[search based on file name]:File name:'
  {-ga,--gid-all}'[group id> search based on All group ids]'
  {-ge,--gid-effective}'[group id> search based on Effective]'
  {-gi,--gid}'[id> search based on group id]'
  '(- * :)'{-h,--help}'[help]'
  {-hn,--host}'[name> search based on remote host name]'
  {-i,--interpret}'[interpret results to be human readable]'
  {-if,--input}'[file name> use this file instead of current logs]'
  '--input-logs[use the logs even if stdin is a pipe]'
  '--just-one[emit just one event]'
  {-k,--key}'[search based on key field]:key string:'
  {-l,--line-buffered}'[flush output on every line]'
  {-m,--message}'[search based on message type]:Message type:'
  {-n,--node}'[search based on machines name]:Node name:'
  {-o,--object}'[search based on context of object]:SE Linux Object context:'
  {-p,--pid}'[search based on process id]:Process id:'
  {-pp,--ppid}'[process id> search based on parent process id]'
  {-r,--raw}'[output is completely unformatted]'
  {-sc,--syscall}'[name> search based on syscall name or number]'
  {-se,--context}'[linux context> search based on either subject or]'
  '--session[session id> search based on login session id]'
  {-su,--subject}'[linux context> search based on context of the Subject]'
  {-sv,--success}'[value> search based on syscall or event]'
  {-te,--end}'[date\] \[end time\] ending date & time for search]'
  {-ts,--start}'[date\] \[start time\] starting data & time for search]'
  {-tm,--terminal}'[search based on terminal]'
  {-ua,--uid-all}'[user id> search based on All user ids]'
  {-ue,--uid-effective}'[user id> search based on Effective]'
  {-ui,--uid}'[id> search based on user id]'
  {-ul,--loginuid}'[id> search based on the Users Login id]'
  {-uu,--uuid}'[UUID> search for events related to the virtual]'
  {-v,--version}'[version]'
  {-vm,--vm-name}'[name> search for events related to the virtual]'
  {-w,--word}'[string matches are whole word]'
  {-x,--executable}'[name> search based on executable name]'
  '*:filename:_files'
)

_arguments -s $arguments
