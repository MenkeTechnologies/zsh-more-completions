#compdef kextlibs

local arguments

arguments=(
  '-arch[resolve for architecture <arch> instead of running kernels]'
  '-xml[print XML fragment suitable for pasting]'
  '-system-extensions[):]'
  '-repository[(-r):]'
  '-all-symbols[:]'
  '-onedef-symbols[:]'
  '-undef-symbols[:]'
  '-multdef-symbols[:]'
  '-compatible-versions[):]'
  '-unsupported[:]'
  '*:filename:_files'
)

_arguments -s $arguments
