#compdef JxrEncApp

local arguments

arguments=(
  '-i[.bmp/tif/hdr Input image file name]'
  '-o[.jxr Output JPEG XR file name]'
  '-q[\[0.0 - 1.0) Default = 1.0, lossless]'
  '-c[required to define uncompressed source pixel format]'
  '-d[sub-sampling 0: Y-only]'
  '-l[0: No overlapping]'
  '-f[turn off frequency order bit stream (to spatial)]'
  '-p[turn off progressive mode (to sequential)]'
  '-t[display timing information]'
  '-v[display verbose encoder information]'
  '-V[0 \[tile_wd1 ... \] Macro block columns per tile ]'
  '-H[0 \[tile_ht1 ... \] Macro block rows per tile]'
  '-U[num_h_tiles Vertical & horizontal tile count for uniform tiling]'
  '-b[/White Applies to 1bpp black/white images]'
  '-a[channel format Required for any pixel format with an alpha channel]'
  '-Q[for alpha \[1 - 255\] Default = 1, lossless]'
  '-F[flexbits \[0 - 15\] 0: no trimming (default)]'
  '-s[subbands 0: All subbands included (default)]'
  '*:filename:_files'
)

_arguments -s $arguments
