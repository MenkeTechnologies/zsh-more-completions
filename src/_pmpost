#compdef pmpost

local arguments

arguments=(
  '-ini[be inipmpost, for dumping mem files]'
  '-interaction[set interaction mode (STRING=batchmode/nonstopmode/]'
  '-numbersystem[set number system mode (STRING=scaled/double/binary/decimal)]'
  '-jobname[set the job name to STRING]'
  '-kanji[set the Japanese encoding to STRING]'
  '-progname[set program (and mem) name to STRING]'
  '-tex[use TEXPROGRAM for text labels]'
  '-debug[print debugging info and leave temporary files in place]'
  '-kpathsea-debug[set path searching debugging flags according to]'
  '-mem[or &MEMNAME use MEMNAME instead of program name or a %& line]'
  '-recorder[enable filename recorder]'
  '-restricted[be secure: disable tex, makempx and editor commands]'
  '-troff[set prologues:=1 and assume TEXPROGRAM is really troff]'
  '-s["STRING" set internal INTERNAL to the string value STRING]'
  '-s[NUMBER set internal INTERNAL to the integer value NUMBER]'
  '-help[display this help and exit]'
  '-version[output version information and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
