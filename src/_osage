#compdef osage

local arguments

arguments=(
  '-V[Print version and exit]'
  '-v[Enable verbose mode]'
  '-Gname[Set graph attribute name to val]'
  '-Nname[Set node attribute name to val]'
  '-Ename[Set edge attribute name to val]'
  '-Tv[Set output format to v]'
  '-Kv[Set layout engine to v (overrides default based on command name)]'
  '-lv[Use external library v]'
  '-ofile[Write output to file]'
  '-O[Automatically generate an output filename based on the input filename with a .format appended. (Causes all -ofile options to be ignored.)]'
  '-P[Internally generate a graph of the current plugins.]'
  '-q[Set level of message suppression (=1)]'
  '-s[Scale input by v (=72)]'
  '-y[Invert y coordinate in output]'
  '-n[No layout mode v (=1)]'
  '-x[Reduce graph]'
  '-Lg[Dont use grid]'
  '-LO[Use old attractive force]'
  '-Ln[Set number of iterations to i]:i:'
  '-LU[Set unscaled factor to i]:i:'
  '-LC[Set overlap expansion factor to v]:v:'
  '-LT[Set temperature (temperature factor) to v]'
  '-m[Memory test (Observe no growth with top. Kill when done.)]'
  '-m[Memory test - v iterations.]'
  '-c[Configure plugins (Writes $prefix/lib/graphviz/config]'
  '*:filename:_files'
)

_arguments -s $arguments
