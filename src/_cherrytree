#compdef cherrytree

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this help message and exit]'
  {-n,--node}'[node Name to Focus]'
  {-x,--export}'[_to_html_dir EXPORT_TO_HTML_DIR]'
  {-t,--export}'[_to_txt_dir EXPORT_TO_TXT_DIR]'
  {-w,--export}'[_overwrite]'
  '*:filename:_files'
)

_arguments -s $arguments
