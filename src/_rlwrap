#compdef rlwrap

local arguments

arguments=(
    {-a,--always-readline}'[Always remain in "readline mode", regardless of command terminal settings.]':'password prompt':
    {-A,--ansi-colour-aware}'[prompts tha use colour will confuse rlwrap]'
    {-b,--break-chars}'[consider Consider the specified characters word-breaking]':'list of characters':
    {-c,--complete-filenames}'[Complete filenames]'
    {-C,--command-name}'[Use command_name instead of command]':'command name':
    {-D,--history-no-dupes}'[How agressively to weed out duplicate entries from the input history]':'n':
    {-e,--extra-char-after-completion}'[char or empty string]':'char':
    {-f,--file}'[Split file into words and add them to the completion word list]':file:_files
    {-g,--forget-matching}'[Forget (i.e. drop from history list) all input lines that match the POSIX 1003.2 regular expression regexp]':regexp:
    '(- * :)'{-h,--help}'[Print a short help message.]'
    {-H,--history-filename}'[Read command history from file (and write it back there if --histsize >= 0)]':file:_files
    {-i,--case-insensitive}'[Ignore case when completing (filename completion remains case-sensitive)]'
    {-I,--pass-sigint-as-sigterm}'[Send a TERM signal to command when an INT is received]'
    {-l,--logfile}'[When in readline mode, append command output (including echoed user input) to file]':file:_files
    {-m,--multi-line}'[Enable multi-line input using a "newline substitute" character sequence]':'newline substitute':
    {-M,--multi-line-ext}'[Call multi-line-editor on temporary files with filename extension .ext]':extension:
    {-n,--no-warnings}'[Do not print warnings.]'
    {-N,--no-children}'[Do not rlwrap command children]'
    {-o,--one-shot}'[Send an EOF to command after accepting the first line of input]'
    {-O,--only-cook}'[Only ever "cook" prompts that match regexp]':regexp:
    {-p,--prompt-colour}'[Use one of the colour names black, red, green, yellow, blue, cyan, purple (=magenta) or white]':'color name':
    {-P,--pre-given}'[Start rlwrap with text in its edit buffer]':text:
    {-q,--quote-characters}'[Assume that the given characters act as quotes, e.g. when matching parentheses]':'list of characters':
    {-r,--remember}'[Put all words seen on in- and output on the completion list]'
    {-R,--renice}'[Make rlwrap nicer than command]'
    {-s,--histsize}'[Limit the history list to N entries, truncating the history file]':N:
    {-S,--substitute-prompt}'[Substitute the specified prompt for command own prompt]':prompt:
    {-t,--set-term-name}'[Set command TERM to <name>]':name:
    {-U,--mirror-arguments}'[Keep track of command arguments as seen by the ps command]'
    {-v,--version}'[Print rlwrap version]'
    {-w,--wait-before-prompt}'[In order to determine if command last output is a prompt, rlwrap waits timeout millisecs after receiving it]':timeout:
    {-W,--polling}'[Wake up every timeout millisecs]'
    {-z,--filter}'[rlwrap -z listing writes a list of installed filters]':filter:
    '*:command:_path_commands'
)

_arguments -s $arguments
