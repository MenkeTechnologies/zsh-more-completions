#compdef econftool

local arguments

arguments=(
  {-f,--full}'[:      copy the original configuration file to /etc instead of]'
  '--use-home[:      only has an effect if the user is root.]'
  {-y,--yes}'[:       assumes yes for all prompts and runs non-interactively.]'
  '*:filename:_files'
)

_arguments -s $arguments
