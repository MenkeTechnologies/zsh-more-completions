#compdef sg_sat_read_gplog

local arguments

arguments=(
  '--ck[_cond | -C set ck_cond field in pass-through (def: 0)]'
  '--count[| -c CO block count (def: 1)]'
  {--dma,-d}'[use READ LOG DMA EXT (def: READ LOG EXT)]'
  {--help,-h}'[output this usage message]'
  {--hex,-H}'[output response in hex bytes, -HH yields hex]'
  '--len[6|12 | -l 16|12 cdb length: 16 or 12 bytes (def: 16)]'
  '--log[| -L LA Log address to be read (def: 0)]'
  '--page[PN Log page number within address (def: 0)]'
  {--readonly,-r}'[open DEVICE read-only (def: read-write)]'
  {--verbose,-v}'[increase verbosity]'
  {--version,-V}'[print version string and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
