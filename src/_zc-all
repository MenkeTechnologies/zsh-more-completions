#compdef zc-all

local arguments

arguments=(
  {-h,--help}'[this message]'
  {-q,--quiet}'[dont output status messages]'
  {-v,--verbose}'[output more status messages]'
  {-a,--ask}'[ask for command (if not provided)]'
  {-f,--force}'[send also to busy sessions; command can time out]'
  '*:filename:_files'
)

_arguments -s $arguments
