#compdef mate-keyboard-properties

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show help options]'
  '--help-all[show all help options]'
  '--help-gtk[show GTK+ Options]'
  '--apply[just apply settings and quit (compatibility only; now handled by daemon)]'
  '--init-session-settings[just apply settings and quit (compatibility only; now handled by daemon)]'
  '--typing-break[start the page with the typing break settings showing]'
  '--a11y[start the page with the accessibility settings showing]'
  '--display[X display to use]'
  '*:filename:_files'
)

_arguments -s $arguments
