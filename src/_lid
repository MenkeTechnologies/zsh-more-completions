#compdef lid

local arguments

arguments=(
  {-i,--interactive}'[prompt for all information]'
  {-g,--group}'[list members of a named group instead of the group]'
  {-n,--onlynames}'[only list membership information by name, and not]'
  '--usage[display brief usage message]'
  '*:filename:_files'
)

_arguments -s $arguments
