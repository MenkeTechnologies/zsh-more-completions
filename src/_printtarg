#compdef printtarg

local arguments

arguments=(
  '-v[verbose mode]'
  '-i[0 | 22 | 41 | 51 | SS | i1 | CM Select target instrument (default DTP41)]'
  '-h[use hexagon patches for SS, double density for CM]'
  '-a[scale patch size and spacers by factor (e.g. 0.857 or 1.5 etc.)]'
  '-A[scale spacers by additional factor (e.g. 0.857 or 1.5 etc.)]'
  '-r[dont randomize patch location]'
  '-s[create a scan image recognition (.cht) file]'
  '-S[same as -s, but dont generate wide orientation strip.]'
  '-c[force colored spacers]'
  '-b[force B&W spacers]'
  '-n[force no spacers]'
  '-f[create PostScript DeviceN Color fallback]'
  '-w[white colorspace encoding: DeviceGray (def), DeviceRGB, Separation or DeviceN]'
  '-k[black colorspace encoding: DeviceGray (def), DeviceCMYK, Separation or DeviceN]'
  '-o[CMY colorspace encoding DeviceCMYK (def), inverted DeviceRGB or DeviceN]'
  '-e[output EPS compatible file]'
  '-t[output 8 bit TIFF raster file, optional res DPI (default 100)]'
  '-T[output 16 bit TIFF raster file, optional res DPI (default 100)]'
  '-C[dont use TIFF compression]'
  '-N[use TIFF alpha N channels more than 4]'
  '-D[dither 8 bit TIFF values down from 16 bit]'
  '-Q[quantize test values to fit in nbits]'
  '-R[use given random start number]'
  '-K[.cal Apply printer calibration to patch values and include in .ti2]'
  '-I[.cal Include calibration in .ti2 (but dont apply it)]'
  '-x[use given strip indexing pattern (Default = "A-Z, A-Z")]'
  '-y[use given patch indexing pattern (Default = "0-9,@-9,@-9;1-999")]'
  '-m[set a page margin in mm (default 6.0 mm)]'
  '-M[set a page margin in mm and include it in TIFF]'
  '-P[dont limit strip length]'
  '-L[suppress any left paper clip border]'
  '-U[suppress CUPS cupsJobTicket: cups-disable-cmm in PS & EPS files]'
  '-p[select page size from:]'
  '-p[custom size, WWW mm wide by HHH mm high]'
  '*:filename:_files'
)

_arguments -s $arguments
