#compdef nvramtool

# Autogenerated from man page /usr/share/man/man8/nvramtool.8.gz

local arguments

arguments=(
    '-n[is specified, show only the value.]'
    '-e[Show all possible values for parameter given by NAME.]:NAME'
    '-a[Show the names and values for all coreboot parameters.]'
    '-w[Assign VALUE to coreboot parameter given by NAME.]:VALUE'
    '-p[Assign values to coreboot parameters according to the contents of INPUT_FILE.]:INPUT_FILE'
    '-i[This is similar to the.]'
    '-c[If VALUE is present then set the CMOS checksum for the coreboot parameters to…]:VALUE'
    '-l[If ARG is present then show information from the coreboot table as specified …]:ARG'
    '-d[Do a low-level dump of the coreboot table.]'
    '-Y[Write CMOS layout information to standard output.]'
    '-b[Write the contents of CMOS memory to the binary file OUTPUT_FILE.]:OUTPUT_FILE'
    '-B[Read binary data from INPUT_FILE and write the data to CMOS memory.]:INPUT_FILE'
    '-x[Show a hex dump of all CMOS data.]'
    '-X[Read binary data from DUMPFILE (presumably a CMOS dumpfile created using the.]:DUMPFILE'
    '-v[Show version information for this program.]'
    '-h[Show a help message for this program.]'
    '*:filename:_files'
)
_arguments -s $arguments
