#compdef themole

local arguments

arguments=(
  '-u[the url which contains a sqli vulnerability.]'
  '-n[the string which is printed on good queries.]'
  '-t[the amount of threads to run. Defaults to 4.]'
  '-e[use ENCODING to decode data retrieved from the server.]'
  '-p[sets the GET vulnerable param(URL must be provided).]'
  '*:filename:_files'
)

_arguments -s $arguments
