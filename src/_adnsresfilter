#compdef adnsresfilter

local arguments

arguments=(
  {-w,--wait}'[always wait for queries to time out or fail]'
  {-b,--brackets}'[require \[...\] around IP addresses]'
  {-a,--address}'[always include \[address\] in output]'
  {-u,--unchecked}'[do not forward map for checking]'
  '--config[use this instead of resolv.conf]'
  '--debug[turn on adns resolver debugging]'
  '*:filename:_files'
)

_arguments -s $arguments
