#compdef sg_write_x

local arguments

arguments=(
  '--16[|-6 send 16 byte cdb variant (this is default action)]'
  '--32[|-3 send 32 byte cdb variant of command (def: 16 byte)]'
  '--app-tag[AT expected application tag field (def: 0xffff)]'
  '--atomic[AB send WRITE ATOMIC command with AB being its]'
  '--bmop[PGP|-p OP,PGP set BMOP field to OP and Previous]'
  '--bs[BS block size (def: use READ CAPACITY), if power of]'
  '--combined[DOF scatter list and data combined for WRITE]'
  '--dld[DLD set duration limit descriptor (dld) bits (def: 0)]'
  '--dpo[|-d set DPO (disable page out) field (def: clear)]'
  '--dry-run[|-x exit just before sending SCSI write command]'
  '--fua[|-f set FUA (force unit access) field (def: clear)]'
  '--generation[NOG set Expected ORWgeneration field to EOG]'
  '--grpnum[GN GN is group number field (def: 0, range: 0 to 31)]'
  '--help[|-h use multiple times for different usage messages]'
  '--in[IF IF is file to fetch NUM blocks of data from.]'
  '--lba[LBA... list of LBAs (Logical Block Addresses) to start]'
  '--normal[|-N send normal WRITE command (default when no other]'
  '--num[NUM... NUM is number of logical blocks to write (def:]'
  '--offset[DLEN\] OFF is byte offset in IF to start reading from]'
  '--or[|-O send ORWRITE command]'
  '--quiet[|-Q suppress some informational messages]'
  '--ref-tag[RT expected reference tag field (def: 0xffffffff)]'
  '--same[NDOB send WRITE SAME command. NDOB (no data out buffer)]'
  '--scat-file[SF file containing LBA, NUM pairs, see manpage]'
  '--scat-raw[|-R read --scat_file=SF as binary (def: ASCII hex)]'
  '--scattered[RD send WRITE SCATTERED command with RD range]'
  '--stream[ID send WRITE STREAM command with its STR_ID]'
  '--strict[|-s exit if read less than requested from IF ;]'
  '--tag-mask[TM tag mask field (def: 0xffff)]'
  '--timeout[TO command timeout (unit: seconds) (def: 120)]'
  '--unmap[U_A 0 clears both UNMAP and ANCHOR bits (default)]'
  '--verbose[|-v increase verbosity]'
  '--version[|-V print version string then exit]'
  '--wrprotect[WPR WPR is the WRPROTECT field value (def: 0)]'
  '*:filename:_files'
)

_arguments -s $arguments
