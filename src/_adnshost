#compdef adnshost

local arguments

arguments=(
  {-f,--pipe}'[read queries on stdin instead of using args]'
  {-a,--asynch}'[allow answers to be reordered]'
  {-Fs,--fmt-simple}'[answers to stdout, errors as messages to stderr (default)]'
  {-Fi,--fmt-inline}'[answers and errors both to stdout in parseable format]'
  {-Fa,--fmt-asynch}'[fully-parseable output format (default for --asynch)]'
  {-Vq,--quiet}'[do not print anything to stderr]'
  {-Vn,--no-quiet}'[report unexpected kinds of problem only (default)]'
  {-Vd,--debug}'[debugging mode]'
  '--config[configuration to use instead of /etc/resolv.conf]'
  '--version[print version number]'
  '--help[print usage information]'
  '-t[/ --type <type> Query type (see below)]:type:'
  '-i[/ --ptr <addr> Do reverse query (address -> name lookup)]:addr:'
  '--reverse[Lookup in in-addr-like `zone (eg MAPS RBL)]:zone:'
  {-s,--search}'[use the search list]'
  {-Qq,--qc-query}'[let query domains contain quote-requiring chars]'
  {-Qa,--qc-anshost}'[let hostnames in answers contain ...]'
  {-u,--tcp}'[force use of a virtual circuit]'
  {-Tt,--ttl-ttl}'[show the TTL as a TTL]'
  {-Ta,--ttl-abs}'[show the TTL as a time_t when the data might expire]'
  {-Tn,--no-ttl}'[do not show the TTL (default)]'
  {-A4,----addr-ipv4-only}'[only return IPv4 addresses]'
  {-A6,----addr-ipv6-only}'[only return IPv6 addresses]'
  {-Aa,----addr-all}'[return all available addresses (default)]'
  {-Am,----addr-ipv6-mapped}'[return IPv4 addresses as IPv6-mapped]'
  {-Cf,--cname-reject}'[call it an error if a CNAME is found]'
  {-Cl,--cname-loose}'[allow references to CNAMEs in other RRs]'
  {-Cs,--cname-ok}'[CNAME ok for query domain, but not in RRs (default)]'
  '--asynch-id[set <id>, default is decimal sequence starting 0]'
  '--cancel-id[cancel the query with id <id> (no error if not found)]'
  '-X[+X. In each case the default is the one not listed.]'
  '*:filename:_files'
)

_arguments -s $arguments
