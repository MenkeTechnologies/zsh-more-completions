#compdef vte-2.91

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show help options]'
  '--help-all[show all help options]'
  '--help-gtk[show GTK+ Options]'
  '--allow-window-ops[allow window operations (resize, move, raise/lower, (de)iconify)]'
  {-a,--audible-bell}'[use audible terminal bell]'
  '--background-color[set default background color]'
  '--background-image[set background image from file]'
  '--background-extend[set background image extend]'
  '--blink[text blink mode (never|focused|unfocused|always)]'
  '--cell-height-scale[.0..2.0 Add extra line spacing]'
  '--cell-width-scale[.0..2.0 Add extra letter spacing]'
  '--cjk-width[specify the cjk ambiguous width to use for UTF-8 encoding]'
  '--cursor-blink[cursor blink mode (system|on|off)]'
  '--cursor-background-color[enable a colored cursor background]'
  '--cursor-foreground-color[enable a colored cursor foreground]'
  '--cursor-shape[set cursor shape (block|underline|ibeam)]'
  {-D,--dingu}'[add regex highlight]'
  {-d,--debug}'[enable various debugging checks]'
  '--encoding[specify the terminal encoding to use]'
  '--env[VALUE Add environment variable to the childs environment]'
  '--extra-margin[add extra margin around the terminal widget]'
  {-f,--font}'[specify a font to use]'
  '--foreground-color[set default foreground color]'
  '--gregex[use GRegex instead of PCRE2]'
  {-g,--geometry}'[set the size (in characters) and position]'
  '--highlight-background-color[enable distinct highlight background color for selection]'
  '--highlight-foreground-color[enable distinct highlight foreground color for selection]'
  {-i,--icon-title}'[enable the setting of the icon title]'
  {-k,--keep}'[live on after the command exits]'
  '--no-argb-visual[dont use an ARGB visual]'
  '--no-bold[disable bold]'
  {-B,--no-bold-is-bright}'[bold does not brightens colors]'
  '--no-builtin-dingus[highlight URLs inside the terminal]'
  '--no-context-menu[disable context menu]'
  {-2,--no-double-buffer}'[disable double-buffering]'
  {-G,--no-geometry-hints}'[allow the terminal to be resized to any dimension, not constrained to fit to an integer multiple of characters]'
  {-H,--no-hyperlink}'[disable hyperlinks]'
  {-R,--no-rewrap}'[disable rewrapping on resize]'
  {-S,--no-shell}'[disable spawning a shell inside the terminal]'
  {-N,--object-notifications}'[print VteTerminal object notifications]'
  '--output-file[save terminal contents to file at exit]'
  '--reverse[reverse foreground/background colors]'
  {-n,--scrollback-lines}'[specify the number of scrollback-lines (-1 for infinite)]'
  {-T,--transparent}'[..100 Enable the use of a transparent background]'
  {-v,--verbose}'[enable verbose debug output]'
  '--version[show version]'
  '--whole-window-transparent[make the whole window transparent]'
  '--word-char-exceptions[specify the word char exceptions]'
  {-w,--working-directory}'[specify the initial working directory of the terminal]'
  '--display[X display to use]'
  '*:filename:_files'
)

_arguments -s $arguments
