#compdef telnetd

local arguments

arguments=(
  {-D,--debug}'[set debugging level]'
  {-E,--exec-login}'[set program to be executed instead of]'
  {-h,--no-hostinfo}'[do not print host information before login has]'
  {-l,--linemode}'[set line mode]'
  {-n,--no-keepalive}'[disable TCP keep-alives]'
  {-U,--reverse-lookup}'[refuse connections from addresses that cannot be]'
  '--usage[give a short usage message]'
  {-V,--version}'[print program version]'
  '*:filename:_files'
)

_arguments -s $arguments
