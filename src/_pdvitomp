#compdef pdvitomp

local arguments

arguments=(
  '-progname[set program name to STRING]'
  '-kanji[set kanji encoding (sjis, jis, euc, utf8)]'
  '-kpathsea-debug[set path searching debugging flags according to]'
  '-help[display this help and exit]'
  '-version[output version information and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
