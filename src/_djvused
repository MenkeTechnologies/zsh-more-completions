#compdef djvused

local arguments

arguments=(
  {-v,--}'[]'
  {-f,--}'[commands from a file]'
  {-e,--}'[commands from the command line]'
  {-s,--}'[after execution]'
  {-u,--}'[utf8 instead of escaping non ascii chars]'
  {-n,--}'[not save anything]'
  '*:filename:_files'
)

_arguments -s $arguments
