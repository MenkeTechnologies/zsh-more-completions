#compdef tbl-dctrl

local arguments

arguments=(
  {-c,--column}'[append the specified column.]'
  {-C,--copying}'[print out the copyright license.]'
  {-d,--delimiter}'[specify a delimiter.]'
  {-H,--no-heading}'[do not print a table heading]'
  {-l,--errorlevel}'[set debugging level to LEVEL.]'
  '--mmap[attempt mmapping input files]'
  '--usage[give a short usage message]'
  {-V,--version}'[print program version]'
  '*:filename:_files'
)

_arguments -s $arguments
