#compdef h5dump

local arguments

arguments=(
  '(- * :)'{-h,--help}'[print a usage message and exit]'
  {-V,--version}'[print version number and exit]'
  {-n,--contents}'[print a list of the file contents and exit]'
  {-B,--superblock}'[print the content of the super block]'
  {-H,--header}'[print the header only; no data is displayed]'
  {-f,--filedriver}'[specify which driver to open the file with]'
  {-o,--output}'[output raw data into file F]'
  {-b,--binary}'[binary file output, of form B]'
  {-O,--ddl}'[output ddl text into file F]'
  {-a,--attribute}'[print the specified attribute]'
  {-d,--dataset}'[print the specified dataset]'
  {-g,--group}'[print the specified group and all members]'
  {-l,--soft-link}'[print the value(s) of the specified soft link]'
  {-t,--datatype}'[print the specified named datatype]'
  {-N,--any}'[_path=P Print any attribute, dataset, group, datatype, or link that matches P]'
  {-A,--onlyattr}'[print the header and value of attributes]'
  {-i,--object-ids}'[print the object ids]'
  {-p,--properties}'[print dataset filters, storage layout and fill value]'
  {-M,--packedbits}'[print packed bits as unsigned integers, using mask]'
  {-R,--region}'[print dataset pointed by region references]'
  {-e,--escape}'[escape non printing characters]'
  {-r,--string}'[print 1-byte integer datasets as ASCII]'
  {-y,--noindex}'[do not print array indices with the data]'
  {-m,--format}'[set the floating point output format]'
  {-q,--sort}'[_by=Q Sort groups and attributes by index Q]'
  {-z,--sort}'[_order=Z Sort groups and attributes by order Z]'
  '--enable-error-stack[messages from the HDF5 error stack as they]'
  '--no-compact-subset[disable compact form of subsetting and allow the use]'
  {-w,--width}'[set the number of columns of output. A value of 0 (zero)]'
  {-x,--xml}'[output in XML using Schema]'
  {-u,--use-dtd}'[output in XML using DTD]'
  {-D,--xml-dtd}'[use the DTD or schema at U]'
  {-X,--xml-ns}'[(XML Schema) Use qualified names n the XML]'
  {-s,--start}'[offset of start of subsetting selection]'
  {-S,--stride}'[hyperslab stride]'
  {-c,--count}'[number of blocks to include in selection]'
  {-k,--block}'[size of block in hyperslab]'
  '-d[) is used. B is an optional argument, defaults to NATIVE]'
  '*:filename:_files'
)

_arguments -s $arguments
