#compdef natd

local arguments

arguments=(
  '-unregistered[_only \[yes|no\]]'
  '-u[\[yes|no\]]'
  '-log[\[yes|no\]]'
  '-l[\[yes|no\]]'
  '-proxy[_only \[yes|no\]]'
  '-reverse[\[yes|no\]]'
  '-deny[_incoming \[yes|no\]]'
  '-d[\[yes|no\]]'
  '-use[_sockets \[yes|no\]]'
  '-s[\[yes|no\]]'
  '-same[_ports \[yes|no\]]'
  '-m[\[yes|no\]]'
  '-verbose[\[yes|no\]]'
  '-v[\[yes|no\]]'
  '-dynamic[\[yes|no\]]'
  '-in[_port number|service_name]'
  '-i[number|service_name]'
  '-out[_port number|service_name]'
  '-o[number|service_name]'
  '-port[number|service_name]'
  '-p[number|service_name]'
  '-globalport[number|service_name]'
  '-alias[_address x.x.x.x]'
  '-a[x.x.x.x]'
  '-target[_address x.x.x.x]'
  '-t[x.x.x.x]'
  '-interface[network_if_name]'
  '-n[network_if_name]'
  '-proxy[_rule \[type encode_ip_hdr|encode_tcp_stream\] port xxxx server a.b.c.d:yyyy]'
  '-redirect[_port tcp|udp local_addr:local_port_range\[,...\] \[public_addr:\]public_port_range \[remote_addr\[:remote_port_range\]\]]'
  '-redirect[_proto proto local_addr \[public_addr\] \[remote_addr\]]'
  '-redirect[_address local_addr\[,...\] public_addr]'
  '-config[file_name]'
  '-f[file_name]'
  '-log[_denied \[yes|no\]]'
  '-log[_facility facility]'
  '-punch[_fw basenumber:count]'
  '-skinny[_port port]'
  '-log[_ipfw_denied \[yes|no\]]'
  '-pid[_file file_name]'
  '-P[file_name]'
  '-instance[instance name]'
  '-exit[_delay ms]'
  '*:filename:_files'
)

_arguments -s $arguments
