#compdef auditreduce

local arguments

arguments=(
  '-A[all records]'
  '-a[after date]'
  '-b[before date]'
  '-c[matching class]'
  '-d[on date]'
  '-e[effective user]'
  '-f[effective group]'
  '-g[real group]'
  '-j[subject id]'
  '-m[matching event]'
  '-o[objectvalue]'
  '-r[real user]'
  '-u[audit user]'
  '-U[unbuffered output]'
  '-v[select non-matching records]'
  '*:filename:_files'
)

_arguments -s $arguments
