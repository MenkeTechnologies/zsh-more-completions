#compdef ohrwurm

local arguments

arguments=(
  '-a[4 address A in dot-decimal notation> SIP phone A]'
  '-b[4 address B in dot-decimal notation> SIP phone B]'
  '-s[randomseed (default: read from /dev/urandom)]'
  '-e[bit error ratio in % (default: 1.230000)]'
  '-i[network interface (default: eth0)]'
  '-t[RTCP packets (default: dont suppress)]'
  '-A[number> of RTP port on IP a (requires -B)]'
  '-B[number> of RTP port on IP b (requires -A)]'
  '*:filename:_files'
)

_arguments -s $arguments
