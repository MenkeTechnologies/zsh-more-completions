#compdef qdarkstyle

local arguments

arguments=(
  {-h,--help}'[show this help message and exit]'
  {-i,--information}'[show information about environment]'
  {-b,--bindings}'[show available bindings for Qt]'
  {-a,--abstractions}'[show available abstraction layers for Qt bindings]'
  {-d,--dependencies}'[show information about dependencies]'
  '--all[show all information options at once]'
  {--version,-v}'[show programs version number and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
