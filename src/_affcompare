#compdef affcompare

local arguments

arguments=(
  {-p,---}'[about the results of preening]'
  {-e,---}'[report about existence (use with -r)]'
  {-s,---}'[see if all of the segments are present, but dont]'
  {-V,---}'[print the version number and exit]'
  {-v,---}'[; each file as it is compared.]'
  {-q,---}'[. No output except for errors]'
  {-a,---}'[whats the same (all)]'
  {-b,---}'[the numbers of differing sectors]'
  {-c,---}'[the contents of differing sectors]'
  {-m,---}'[report about the data (ignore metadata)]'
  {-P,---}'[examine the differences on page ###]'
  '-r[1 dir2 --- recursively compare whats in dir1 with dir2, and]'
  {-s,---}'[to see if named files are on Amazon S3]'
  '*:filename:_files'
)

_arguments -s $arguments
