#compdef cd-drive

local arguments

arguments=(
  {-d,--debug}'[set debugging to LEVEL]'
  {-i,--cdrom-device}'[show only info about CD-ROM device]'
  {-q,--quiet}'[dont produce warning output]'
  {-V,--version}'[display version and copyright information]'
  '--usage[display brief usage message]'
  '*:filename:_files'
)

_arguments -s $arguments
