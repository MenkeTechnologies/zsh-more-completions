#compdef gtkdoc-fixxref

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show this help message and exit]'
  '--version[show programs version number and exit]'
  '--module[name of the doc module being processed.]'
  '--module-dir[the directory which contains the generated HTML.]'
  '--html-dir[the directory where gtk-doc generated documentation]'
  '--extra-dir[directories to recursively scan for indices]'
  '--src-lang[programing language used for syntax highlighting.]'
  '*:filename:_files'
)

_arguments -s $arguments
