#compdef sg_timestamp

local arguments

arguments=(
  '--help[|-h print out usage message]'
  '--milliseconds[MS set timestamp to MS milliseconds since]'
  '--origin[|-o show Report timestamp origin (def: dont)]'
  '--raw[|-r output Report timestamp response to stdout in]'
  '--readonly[|-R open DEVICE read only (def: read/write)]'
  '--seconds[SEC set timestamp to SEC seconds since]'
  '--srep[|-S output Report timestamp in seconds (def:]'
  '--verbose[|-v increase verbosity]'
  '--version[|-V print version string and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
