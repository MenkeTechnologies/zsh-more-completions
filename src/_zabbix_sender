#compdef zabbix_sender

local arguments

arguments=(
  '-o[zabbix_sender \[-v\] -z server \[-p port\] \[-I IP-address\] \[-s host\] \[-T\] \[-r\]]'
  '-i[zabbix_sender \[-v\] -c config-file \[-z server\] \[-p port\] \[-I IP-address\]]'
  {--tls-connect,--tls-ca-file}'[\[--tls-crl-file CRL-file\]]'
  {--tls-cert-file,--tls-key-file,-k}'[]'
  '-o[zabbix_sender \[-v\] -z server \[-p port\] \[-I IP-address\] \[-s host\]]'
  {--tls-connect,--tls-ca-file}'[\[--tls-crl-file CRL-file\]]'
  {--tls-cert-file,--tls-key-file}'[\[-T\] \[-r\]]'
  '-i[zabbix_sender \[-v\] -c config-file \[-z server\] \[-p port\] \[-I IP-address\]]'
  {--tls-cert-file,--tls-key-file,-k}'[]'
  '-o[zabbix_sender \[-v\] -c config-file \[-z server\] \[-p port\] \[-I IP-address\]]'
  {--tls-cert-file,--tls-key-file}'[\[-T\] \[-r\]]'
  '-i[zabbix_sender \[-v\] -z server \[-p port\] \[-I IP-address\] -s host]'
  {--tls-connect,--tls-psk-identity}'[]'
  {--tls-psk-file,-k,-o}'[zabbix_sender \[-v\] -z server \[-p port\] \[-I IP-address\] \[-s host\]]'
  {--tls-connect,--tls-psk-identity}'[]'
  '--tls-psk-file[\[-T\] \[-r\] -i input-file]'
  {--tls-psk-file,-k,-o}'[zabbix_sender \[-v\] -c config-file \[-z server\] \[-p port\] \[-I IP-address\]]'
  '--tls-psk-file[\[-T\] \[-r\] -i input-file]'
  {-c,--config}'[absolute path to Zabbix agentd configuration file]'
  {-z,--zabbix-server}'[hostname or IP address of Zabbix server or proxy]'
  {-p,--port}'[specify port number of trapper process of Zabbix]'
  {-I,--source-address}'[specify source IP address. When used]'
  {-s,--host}'[specify host name the item belongs to (as]'
  {-k,--key}'[specify item key]'
  {-o,--value}'[specify item value]'
  {-i,--input-file}'[load values from input file. Specify - for]'
  {-T,--with-timestamps}'[each line of file contains whitespace delimited:]'
  {-r,--real-time}'[send metrics one by one as soon as they are]'
  {-v,--verbose}'[verbose mode, -vv for more details]'
  '(- * :)'{-h,--help}'[display this help message]'
  {-V,--version}'[display version number]'
  '--tls-connect[how to connect to server or proxy. Values:]'
  '--tls-ca-file[full pathname of a file containing the top-level]'
  '--tls-crl-file[full pathname of a file containing revoked]'
  '--tls-server-cert-issuer[allowed server certificate issuer]'
  '--tls-server-cert-subject[allowed server certificate subject]'
  '--tls-cert-file[full pathname of a file containing the certificate]'
  '--tls-key-file[full pathname of a file containing the private key]'
  '--tls-psk-identity[unique, case sensitive string used to]'
  '--tls-psk-file[full pathname of a file containing the pre-shared]'
  {--tls-connect,--tls-ca-file}'[/zabbix/zabbix_ca_file \]'
  '--tls-server-cert-issuer["CN=Signing CA,OU=IT operations,O=Example Corp,DC=example,DC=com" \]'
  '--tls-server-cert-subject["CN=Zabbix proxy,OU=IT operations,O=Example Corp,DC=example,DC=com" \]'
  '--tls-cert-file[/zabbix/zabbix_agentd.crt \]'
  '--tls-key-file[/zabbix/zabbix_agentd.key]'
  {--tls-connect,--tls-psk-identity}'[ID Zabbix agentd" \]'
  '--tls-psk-file[/zabbix/zabbix_agentd.psk]'
  '*:filename:_files'
)

_arguments -s $arguments
