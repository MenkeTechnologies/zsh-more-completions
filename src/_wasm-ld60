#compdef wasm-ld60

local arguments

arguments=(
  '--allow-undefined-file[allow symbols listed in <file> to be undefined in linked binary]'
  '--allow-undefined[allow undefined symbols in linked binary]'
  '--check-signatures[check function signatures]'
  '--color-diagnostics[use colors in diagnostics]'
  '--color-diagnostics[use colors in diagnostics]'
  '--emit-relocs[generate relocations in output]'
  '--entry[name of entry point symbol]'
  '--error-limit[maximum number of errors to emit before stopping (0 = no limit)]'
  '--global-base[where to start to place global data]'
  '--help[print option help]'
  '--import-memory[import memory from the environment]'
  '--initial-memory[initial size of the linear memory]'
  '-L[add a directory to the library search path]'
  '-l[root name of library to use]'
  '--max-memory[maximum size of the linear memory]'
  '--mllvm[options to pass to LLVM]'
  '--no-check-signatures[dont check function signatures]'
  '--no-color-diagnostics[not use colors in diagnostics]'
  '--no-entry[do not output any entry point]'
  '--no-threads[do not run the linker multi-threaded]'
  '-o[path to file to write output]'
  '--relocatable[create relocatable object file]'
  '--strip-all[strip all symbols]'
  '--strip-debug[strip debugging information]'
  '--threads[run the linker multi-threaded]'
  '--undefined[force undefined symbol during linking]'
  '--undefined[force undefined symbol during linking]'
  '--verbose[verbose mode]'
  '--version[display the version number and exit]'
  '-v[display the version number]'
  '-z[linker option extensions]'
  '*:filename:_files'
)

_arguments -s $arguments
