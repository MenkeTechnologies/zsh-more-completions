#compdef thumbpdf

local arguments

arguments=(
  '--help[print usage]'
  '--version[print version number]'
  '--gscmd[call of ghostscript (gs)]'
  '--antialias[1>\[num2\] anti-aliasing, 0 = disable, 4 = max (4)]'
  '--device[|png \[png\]<dev> Ghostscript device for thumbnails]'
  '--resolution[thumbnail resolution for makepng (9)]'
  '--compress[thumbnail compress level, n = 0..10 (10)]'
  '--modes[mode\] mode=pdftex|pdfmark|dvips|ps2pdf|]'
  '--password[for an encrypted pdf file ()]'
  '*:filename:_files'
)

_arguments -s $arguments
