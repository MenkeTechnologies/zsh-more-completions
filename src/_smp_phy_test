#compdef smp_phy_test

local arguments

arguments=(
  '--control[CO phy test pattern dwords control (def: 0)]'
  '--dwords[DW phy test pattern dwords (def:0)]'
  '--expected[EX set expected expander change count to EX]'
  '--function[FN phy test function (def:0 -> stop)]'
  '--help[|-h print out usage message]'
  '--hex[|-H print response in hexadecimal]'
  '--interface[PARAMS specify or override interface]'
  '--linkrate[LR physical link rate (def: 9 -> 3 Gbps)]'
  '--pattern[PA phy test pattern (def: 2 -> CJTPAT)]'
  '--phy[ID phy identifier (def: 0)]'
  '--raw[|-r output response in binary]'
  '--sa[SAS_ADDR SAS address of SMP target (use leading]'
  '--sata[|-t set phy test function SATA bit (def: 0)]'
  '--spread[SC set phy test function SCC to SC (def: 0)]'
  '--verbose[|-v increase verbosity]'
  '--version[|-V print version string and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
