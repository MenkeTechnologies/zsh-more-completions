#compdef qdbusxml2cpp

local arguments

arguments=(
  '-a[write the adaptor code to <filename>]'
  '-c[use <classname> as the class name for the generated classes]'
  '-h[show this information]'
  '-i[add #include to the output]'
  '-l[when generating an adaptor, use <classname> as the parent class]'
  '-m[generate #include "filename.moc" statements in the .cpp files]'
  '-N[dont use namespaces]'
  '-p[write the proxy code to <filename>]'
  '-v[be verbose.]'
  '-V[show the program version and quit.]'
  '*:filename:_files'
)

_arguments -s $arguments
