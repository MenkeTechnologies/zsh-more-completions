#compdef zabbix_get

local arguments

arguments=(
  {--tls-connect,--tls-ca-file}'[\[--tls-crl-file CRL-file\] \[--tls-agent-cert-issuer cert-issuer\]]'
  {--tls-cert-file,--tls-key-file,-k}'[zabbix_get -s host-name-or-IP \[-p port-number\] \[-I IP-address\]]'
  {--tls-connect,--tls-psk-identity}'[]'
  {--tls-psk-file,-k}'[zabbix_get -h]'
  {-s,--host}'[specify host name or IP address of a host]'
  {-p,--port}'[specify port number of agent running on the host]'
  {-I,--source-address}'[specify source IP address]'
  {-k,--key}'[specify key of the item to retrieve value for]'
  {-h,--help}'[display this help message]'
  {-V,--version}'[display version number]'
  '--tls-connect[how to connect to agent. Values:]'
  '--tls-ca-file[full pathname of a file containing the top-level]'
  '--tls-crl-file[full pathname of a file containing revoked]'
  '--tls-agent-cert-issuer[allowed agent certificate issuer]'
  '--tls-agent-cert-subject[allowed agent certificate subject]'
  '--tls-cert-file[full pathname of a file containing the certificate]'
  '--tls-key-file[full pathname of a file containing the private key]'
  '--tls-psk-identity[unique, case sensitive string used to]'
  '--tls-psk-file[full pathname of a file containing the pre-shared]'
  {--tls-connect,--tls-ca-file}'[/zabbix/zabbix_ca_file \]'
  '--tls-agent-cert-issuer["CN=Signing CA,OU=IT operations,O=Example Corp,DC=example,DC=com" \]'
  '--tls-agent-cert-subject["CN=server1,OU=IT operations,O=Example Corp,DC=example,DC=com" \]'
  '--tls-cert-file[/zabbix/zabbix_get.crt \]'
  '--tls-key-file[/zabbix/zabbix_get.key]'
  {--tls-connect,--tls-psk-identity}'[ID Zabbix agentd" \]'
  '--tls-psk-file[/zabbix/zabbix_agentd.psk]'
  '*:filename:_files'
)

_arguments -s $arguments
