#compdef smp_rep_zone_perm_tbl

local arguments

arguments=(
  '--append[|-a append to FN with --permf option]'
  '--bits[COL output table as bit array with COL columns]'
  '--help[|-h print out usage message]'
  '--hex[|-H print response in hexadecimal]'
  '--interface[PARAMS specify or override interface]'
  '--multiple[|-m issue multiple function requests until all]'
  '--nocomma[|-N output descriptors as long string of hex]'
  '--num[MD maximum number of descriptors in one response]'
  '--permf[FN write descriptors to file FN (default: write]'
  '--raw[|-r output response in binary]'
  '--report[RT report type (default: 0). 0 -> current;]'
  '--sa[SAS_ADDR SAS address of SMP target (use leading]'
  '--start[SS starting (first) source zone group (default: 0)]'
  '--verbose[|-v increase verbosity]'
  '--version[|-V print version string and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
