#compdef cue2toc

local arguments

arguments=(
  '-h[display this help message]'
  '-n[do not write CD-Text information]'
  '-o[write output to tocfile]'
  '-q[quiet mode]'
  '-v[display version information]'
  '-w[name of WAVE file to be used in tocfile]'
  '*:filename:_files'
)

_arguments -s $arguments
