#compdef xorriso-tcltk

local arguments

arguments=(
  '-sync[Use synchronous mode for display server]::'
  '-colormap[Colormap for main window]'
  '-display[Display to use]::'
  '-geometry[Initial geometry for window]'
  '-name[Name to use for application]::'
  '-visual[Visual for main window]::'
  '-use[Id of window in which to embed application]::'
  '-help[Print summary of command-line options and abort]::'
  '--help[print this text and exit.]'
  '--stdio[establish connection to xorriso via stdin and stdout.]'
  '--named[_pipes cmd_fifo reply_fifo]'
  '--silent[_start]'
  '--no[_extract]'
  '--no[_bwidget]'
  '--geometry[+|-}X{+|-}Y]'
  '--click[_to_focus]'
  '--auto[_focus]'
  '--pipe[_log_file path]'
  '--script[_log_file path]'
  '--script[_log_all_commands]'
  '--use[_command_move]'
  '--use[_command_mv]'
  '*:filename:_files'
)

_arguments -s $arguments
