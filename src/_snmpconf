#compdef snmpconf

local arguments

arguments=(
  '-f[overwrite existing files without prompting]'
  '-i[install created files into /usr/share/snmp.]'
  '-p[install created files into /Users/wizard/.snmp.]'
  '-I[install created files into DIR.]'
  '-a[dont ask any questions, just read in current]'
  '-r[read in all or none of the .conf files found.]'
  '-R[... Read in a particular list of .conf files.]'
  '-g[ask a series of GROUPed questions.]'
  '-G[list known GROUPs.]'
  '-c[use alternate configuration directory.]'
  '-q[run more quietly with less advice.]'
  '-d[turn on debugging output.]'
  '-D[turn on debugging dumper output.]'
  '*:filename:_files'
)

_arguments -s $arguments
