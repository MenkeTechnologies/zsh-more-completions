#compdef inkscape

local arguments

arguments=(
  {-V,--version}'[print the]'
  {-z,--without-gui}'[do not use X]'
  {-g,--with-gui}'[try to use X]'
  {-f,--file}'[open specified]'
  {-p,--print}'[print]'
  {-e,--export-png}'[export document]'
  {-d,--export-dpi}'[resolution for]'
  {-a,--export-area}'[0:y0:x1:y1 Exported area]'
  {-D,--export-area-drawing}'[exported area]'
  {-C,--export-area-page}'[exported area]'
  '--export-margin[only for]'
  '--export-area-snap[snap the bitmap]'
  {-w,--export-width}'[the width of]'
  {-h,--export-height}'[the height of]'
  {-i,--export-id}'[the ID of the]'
  {-j,--export-id-only}'[export just the]'
  {-t,--export-use-hints}'[use stored]'
  {-b,--export-background}'[background]'
  {-y,--export-background-opacity}'[background]'
  {-l,--export-plain-svg}'[export document]'
  {-P,--export-ps}'[export document]'
  {-E,--export-eps}'[export document]'
  '--export-ps-level[level Choose the]'
  {-A,--export-pdf}'[export document]'
  '--export-pdf-version[export PDF to]'
  '--export-latex[export]'
  {-M,--export-emf}'[export document]'
  {-m,--export-wmf}'[export document]'
  {-T,--export-text-to-path}'[convert text]'
  '--export-ignore-filters[render filtered]'
  {-X,--query-x}'[query the X]'
  {-Y,--query-y}'[query the Y]'
  {-W,--query-width}'[query the width]'
  '--query-id[]'
  {-H,--query-height}'[query the]'
  '--query-id[]'
  {-S,--query-all}'[list id,x,y,w,h]'
  {-I,--query-id}'[the ID of the]'
  {-x,--extension-directory}'[print out the]'
  '--vacuum-defs[remove unused]'
  '--verb-list[list the IDs of]'
  '--verb[verb to call]'
  '--select[object ID to]'
  '--shell[start Inkscape]'
  '--no-convert-text-baseline-spacing[do not fix]'
  '--convert-dpi-method[method used to]'
  '--usage[display brief]'
  '*:filename:_files'
)

_arguments -s $arguments
