#compdef exrstdattr

local arguments

arguments=(
  '-part[if i is greater than or equal to zero, and less]'
  {-focus,-part,-aperture,-expTime}'[.01 -part any -owner luke]'
  '-chromaticities[f f f f f f f]'
  '-whiteLuminance[white luminance, in candelas per square meter]'
  '-adoptedNeutral[f]'
  '-renderingTransform[name of the CTL rendering transform for this]'
  '-lookModTransform[name of the CTL look modification transform for]'
  '-xDensity[horizontal output density, in pixels per inch]'
  '-owner[name of the owner of the image (string)]'
  '-comments[additional information about the image (string)]'
  '-capDate[date when the image was created or]'
  '-utcOffset[offset of local time at capDate from UTC, in]'
  '-longitude[]'
  '-latitude[]'
  '-altitude[location where the image was recorded, in]'
  '-focus[the cameras focus distance, in meters]'
  '-expTime[exposure time, in seconds (float, >= 0)]'
  '-aperture[lens apterture, in f-stops (float, >= 0)]'
  '-isoSpeed[effective speed of the film or image]'
  '-envmap[indicates that the image is an environment map]'
  '-framesPerSecond[i]'
  '-keyCode[i i i i i i]'
  '-timeCode[i]'
  '-wrapmodes[if the image is used as a texture map, specifies]'
  '-pixelAspectRatio[width divided by height of a pixel]'
  '-screenWindowWidth[width of the screen window (float, >= 0)]'
  '-screenWindowCenter[f]'
  '-string[s]'
  '-float[f]'
  '-int[i]'
  '-h[prints this message]'
  '*:filename:_files'
)

_arguments -s $arguments
