local arguments

arguments=(
    {-e,--extended}'[send extended frame.]'
    {-i,--identifier}'[CAN Identifier (default = 2).]':ID
    '--loop[send message COUNT times.]:COUNT'
    {-p,--poll}'[use poll(2) to wait for buffer space while sending.]'
    {-q,--quit}'[quit if <num> wrong sequences are encountered.]':num
    {-r,--receive}'[work as receiver.]'
    {-v,--verbose}'[be verbose (twice to be even more verbose.]'
    {-h,--help}'[this help.]'
    '--version[print version information and exit.]'
    '*:filename:_files'
)
_arguments -s $arguments
