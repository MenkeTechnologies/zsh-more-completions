#compdef swift-symbolgraph-extract

local arguments

arguments=(
  '--help[- Display available options (--help-hidden for more)]'
  '--help-list[- Display list of available options (--help-list-hidden for more)]'
  '--version[- Display the version of this program]'
  '-F[- Add a directory to the framework search paths]'
  '--Fsystem[- Add directory to system framework search path]'
  '-I[- Add directory to the import search paths]'
  '-L[- Add a directory to the library search paths]'
  '--Xcc[- Pass the following command-line flag to Clang]'
  '--minimum-access-level[- Include symbols with this access level or more]'
  '--module-cache-path[- Specifies a path to cache modules]'
  '--module-name[- Name of the module to extract (Required)]'
  '--output-dir[- Symbol Graph JSON Output Directory (Required)]'
  '--pretty-print[- Pretty-print the resulting Symbol Graph JSON]'
  '--resource-dir[- Override the directory that holds the compiler resource files]'
  '--sdk[- Path to the SDK]'
  '--skip-synthesized-members[- Skip members inherited through classes or default implementations]'
  '--swift-version[- Interpret input according to a specific Swift language version number]'
  '--target[- Target triple (Required)]'
  '*:filename:_files'
)

_arguments -s $arguments
