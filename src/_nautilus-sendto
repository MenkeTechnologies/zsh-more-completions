#compdef nautilus-sendto

local arguments

arguments=(
  '(- * :)'{-h,--help}'[show help options]'
  {-b,--run-from-build-dir}'[run from build directory (ignored)]'
  {-x,--xid}'[use XID as parent to the send dialogue (ignored)]'
  {-v,--version}'[output version information and exit]'
  '*:filename:_files'
)

_arguments -s $arguments
