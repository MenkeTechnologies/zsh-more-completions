#compdef srcredact

local arguments

arguments=(
  '-c[of comment patterns]'
  '-c[c, shell"]'
  '-d[debug mode on.]'
  '-e[extract the contents for the current audience into the file file.]'
  '-h[print help information and exit.]'
  '-l[list all audiences set in the file (one per line) and exit.]'
  '-u[take a redacted file intended for the audience (the second]'
  '-v[print version information and exit.]'
  '-w[1|0|true|false]'
  '*:filename:_files'
)

_arguments -s $arguments
