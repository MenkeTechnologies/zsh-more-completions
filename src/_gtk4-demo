#compdef gtk4-demo

local arguments

arguments=(
  {-h,--help}'[show help options]'
  '--help-all[show all help options]'
  '--help-gapplication[show GApplication options]'
  '--version[show program version]'
  '--run[run an example]'
  '--list[list examples]'
  '--autoquit[quit after a delay]'
  '*:filename:_files'
)

_arguments -s $arguments
