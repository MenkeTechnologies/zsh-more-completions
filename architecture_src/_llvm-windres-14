#compdef llvm-windres-14

local arguments

arguments=(
  '--codepage[default codepage to use]'
  '--define[define to pass to the preprocessor]'
  '--help[display this message and exit]'
  '--include-dir[include directory]'
  '--input-format[input format]'
  '--input[input file]'
  '--language[default language to use (0x0-0xffff)]'
  '--output-format[output format]'
  '--output[output file]'
  '--preprocessor-arg[preprocessor command argument]'
  '--preprocessor[custom preprocessor command]'
  '--target[target BFD format name]'
  '--undefine[undefine to pass to the preprocessor]'
  '--verbose[enable verbose output]'
  '--version[display version]'
  '*:filename:_files'
)

_arguments -s $arguments
