#compdef llvm-profgen

# Autogenerated from man page /usr/share/man/man1/llvm-profgen.1.gz

local arguments

arguments=(
    '--format[Specify the format of the generated profile.]:text|binary|extbinary|compbinary|gcc'
    '--show-mmap-events[Print mmap events.]'
    '--show-disassembly[Print disassembled code.]'
    '--x86-asm-syntax[Specify whether to print assembly code in AT&T syntax (the default) or Intel …]:att|intel'
    '--perfscript[Path of perf-script trace created by Linux perf tool with script command(the …]:stringstring...'
    '--binary[Path of the input profiled binary files.]:stringstring...'
    '--output[Path of the output profile file.]:string'
    '*:filename:_files'
)
_arguments -s $arguments
